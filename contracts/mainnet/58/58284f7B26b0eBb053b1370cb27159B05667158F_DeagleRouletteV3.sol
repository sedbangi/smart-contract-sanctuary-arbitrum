// This contract is initialized with DEAGLE inside that can't be removed except someone wins the roulette
// The roulette elects a winner after 10 hours from the last bet
// Every time a buyer buys a ticket into the roulette, it becomes the winner of the last bet and the counter resets
// The ticket is bought with ETH, half of it is used to buy and burn DEAGLE and the other half is used to increase the ETH prize
// Every ticket cost 0.001ETH
// Small wins: Every 10 bets, theres a chance to win a small prize of 0.02 ETH
// Discount ticket: If a user has bought 5 tickets in the latest 20 bets, the next ticket will cost 0.005 ETH less than usual
// High price high demand: If in the latest hour there has been 10 bets, each ticket will increase its price by 0.005 ETH
import "hardhat/console.sol";

pragma solidity ^0.8.17;
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@uniswap/v3-periphery/contracts/interfaces/ISwapRouter.sol";
import "../utils/Random.sol";

import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "../INonfungiblePositionManager.sol";
import "../IERC20Burnable.sol";
import "../IWETH.sol";

uint256 constant DECIMAL_POINT = 10 ** 3;
uint256 constant ONE_HUNDRED = 100 * DECIMAL_POINT;

contract DeagleRouletteV3 is Ownable, ReentrancyGuard {
    using EnumerableSet for EnumerableSet.UintSet;
    event RewardsCollected(
        address indexed recipient,
        uint256 tokenId,
        uint256 amount0,
        uint256 amount1
    );

    IERC20Burnable public deagleToken;
    IWETH public wethToken;
    ISwapRouter public immutable swapRouter;

    // Amount of $DEAGLE accumulated
    uint256 public deagleAmount;

    // Amount of ETH accumulated
    uint256 public ethAmount;

    // Buff cost
    uint256 buffCost = 0.01 ether;

    // Price of a ticket
    uint256 public ticketPrice = 0.001 ether;

    // Multiplier in 100 percentages
    uint256 private nextTicketBuffMultiplier = 100;

    // Points mapping for the players
    mapping(address => uint256) public points;

    // Total amount of points accumulated
    uint256 public totalPoints;

    // Indicates the amount of deagle per points remaining
    uint256 public deaglePerPoint;

    // Indicates that the roulette is finished
    bool public rouletteFinished = false;

    // Struct for a bet
    struct Bet {
        address player;
        uint256 timestamp;
        uint256 ethAmount;
        // Refundable amount of ETH if someone outbids the player
        uint256 ethRefundable;
    }

    // Index of the pointer of the bets array
    uint256 public betsPointer = 0;

    // Timestamp of the current waiting time (it will be reduced over time)
    uint256 public waitingTime = 10 hours;

    // Array of the latest 20 bets
    Bet[20] public bets;

    // function to add an element to the bets list implementing a circular buffer
    function addBet(Bet memory bet) internal {
        bets[betsPointer] = bet;
        betsPointer = (betsPointer + 1) % 20;
    }

    // Function to get the ordered list of bets
    function getBets() external view returns (Bet[20] memory) {
        Bet[20] memory result;
        uint256 index = 0;
        for (uint256 i = 0; i < 20; i++) {
            index = (betsPointer + i) % 20;
            result[i] = bets[index];
        }
        return result;
    }

    // struct for the player
    struct Player {
        address player;
        uint256 timestamp; // Last time the player bought a ticket
        uint256 ethAmount;
    }

    // events
    event TicketPurchased(
        string message,
        address indexed owner,
        uint256 timestamp,
        uint256 ethAmount
    );

    event SpellCasted(string message, address indexed owner, uint256 timestamp);

    event ExtraRewardEarned(address indexed owner, uint256 ethAmount);
    event DeagleBurned(address indexed owner, uint256 tokenId);
    event RouletteWon(
        address indexed owner,
        uint256 deagleAmount,
        uint256 ethAmount
    );

    constructor(
        address _deagleToken,
        address _wethToken,
        address _swapRouter,
        uint256 _buffCost
    ) {
        deagleToken = IERC20Burnable(_deagleToken);
        wethToken = IWETH(_wethToken);
        swapRouter = ISwapRouter(_swapRouter);
        buffCost = _buffCost;
    }

    // Deposit DEAGLE
    function depositDeagle(uint256 amount) external {
        // Check that roulette is not finished
        require(!rouletteFinished, "Roulette is finished");

        deagleToken.transferFrom(msg.sender, address(this), amount);

        // Recompute the deagle amount to make sure its accurate (in case someone else sends deagle in another way)
        deagleAmount = deagleToken.balanceOf(address(this));
    }

    // Public function that tells you the next ticket price for a player
    function nextTicketPrice(address player) public view returns (uint256) {
        uint256 highPrice = 0;
        uint256 hour = 3600;
        uint256 betsInHour = 0;
        for (uint256 i = 0; i < 20; i++) {
            if (block.timestamp - bets[i].timestamp < hour) {
                betsInHour++;
            }
        }

        // For each bet in the last hour, the price increases by 0.0005 ETH
        highPrice = betsInHour * 0.0005 ether;

        // Get the previous Bet (if any), and increment the price the longer it has been since the last bet
        Bet memory previousBet = getWinnerBet();

        // Ignore if there is no previous bet
        if (previousBet.player == address(0)) {
            return ((ticketPrice + highPrice) * nextTicketBuffMultiplier) / 100;
        }

        // Calculate the time since the last bet
        uint256 timeSinceLastBet = block.timestamp - previousBet.timestamp;
        uint256 priceIncrease = 0;
        // Every 10 minutes block increase the price by 0.001 ETH up to a max of 0.1 ETH
        if (timeSinceLastBet >= 10 minutes) {
            priceIncrease = (timeSinceLastBet / 10 minutes) * 0.00005 ether;
            priceIncrease = priceIncrease > 0.01 ether
                ? 0.01 ether
                : priceIncrease;
            console.log("Price increase: %s", priceIncrease);
        }

        // If the waiting time is less than 60 minutes, price adds 0.01 ETH
        if (waitingTime < 60 minutes) {
            priceIncrease += 0.01 ether;
        }

        return
            ((ticketPrice + highPrice + priceIncrease) *
                nextTicketBuffMultiplier) / 100;
    }

    // Buy ticket
    // Calculate the price of the ticket, we need to check the Discount ticket and High price high demand conditions
    function buyTicket() external payable nonReentrant {
        // Check that roulette is not finished
        require(!rouletteFinished, "Roulette is finished");

        uint256 ticketEthPrice = nextTicketPrice(msg.sender);
        require(msg.value >= ticketEthPrice, "Invalid ticket price");

        // Balance of DEAGLE before the buy
        uint256 deagleBalanceBefore = deagleToken.balanceOf(address(this));

        // We use half of the ETH price to buy DEAGLE
        uint256 ethForDeagle = msg.value / 2;

        // 50% of the remaining ETH is refundable
        uint256 refundableEth = ((ethForDeagle) * 50) / 100;

        console.log("Refundable ETH: %s", refundableEth);
        console.log("Full ticket price %s", ticketEthPrice);

        // The rest of the ETH is used to increase the prize pool
        uint256 restOfEth = msg.value - refundableEth - ethForDeagle;

        require(restOfEth >= 0, "Calculation error for remaining ETH");

        // Wrap the ETH
        wethToken.deposit{value: ethForDeagle}();

        // Market buy deagle
        ISwapRouter.ExactInputSingleParams memory params = ISwapRouter
            .ExactInputSingleParams({
                tokenIn: address(wethToken),
                tokenOut: address(deagleToken),
                fee: 10000, // Fee tier, e.g., 1%
                recipient: address(this),
                deadline: block.timestamp + 300, // 5 minutes from the current block
                amountIn: ethForDeagle,
                amountOutMinimum: 0,
                sqrtPriceLimitX96: 0 // No price limit
            });

        // Execute the swap
        swapRouter.exactInputSingle(params);

        // Deagle balance after the buy
        uint256 deagleBalanceAfter = deagleToken.balanceOf(address(this));

        // Calculate the amount of DEAGLE bought
        uint256 newDeagleAmount = deagleBalanceAfter - deagleBalanceBefore;

        // Burn 75 % of the DEAGLE bought and use the rest to increase the pot
        uint256 burnAmount = (newDeagleAmount * 75) / 100;
        deagleToken.burn(burnAmount);
        deagleAmount += newDeagleAmount - burnAmount;

        // Increase the ETH amount
        ethAmount += restOfEth;

        // Reset the nextTicketBuffMultiplier
        nextTicketBuffMultiplier = 100;

        // 5% chance of failing the bet
        uint256 randomNumber = getRandomNumber();
        console.log("Random number: %s", randomNumber);
        if (randomNumber < 50000) {
            emit TicketPurchased(
                "Failed to purchase ticket!",
                msg.sender,
                block.timestamp,
                msg.value
            );
        }

        // Get the previous Bet (if any)
        Bet memory previousBet = getWinnerBet();

        // If theres a previous bet, refund the refundable amount of ETH
        if (previousBet.player != address(0)) {
            console.log(
                "Refunding %s to %s",
                previousBet.ethRefundable,
                previousBet.player
            );
            previousBet.player.call{value: previousBet.ethRefundable}("");
            emit ExtraRewardEarned(
                previousBet.player,
                previousBet.ethRefundable
            );

            // Give points to the previous player based on the time since the last bet
            uint256 timeSinceLastBet = block.timestamp - previousBet.timestamp;
            points[previousBet.player] += timeSinceLastBet;

            // Increase the total points
            totalPoints += timeSinceLastBet;
        }

        // Add the bet to the array
        Bet memory bet = Bet(
            msg.sender,
            block.timestamp,
            msg.value,
            refundableEth
        );

        addBet(bet);

        // Reduce the waiting time by 5 minutes (if is more than 30 minutes)
        waitingTime = waitingTime > 30 minutes
            ? waitingTime - 5 minutes
            : waitingTime;

        // Emit an event
        emit TicketPurchased(
            "Ticket purchased correctly",
            msg.sender,
            block.timestamp,
            msg.value
        );
    }

    // Get the current winner bet
    function getWinnerBet() public view returns (Bet memory) {
        if (betsPointer == 0 && bets[0].player == address(0)) {
            // No bets have been made at all
            return Bet(address(0), 0, 0, 0);
        }
        // Handle the wrap-around of the circular buffer correctly
        uint256 index = betsPointer == 0 ? 19 : betsPointer - 1;
        return bets[index];
    }

    // Returns if the prize pool can be claimed (10 hours have passed since the last bet)
    function canClaimPrize() public view returns (bool) {
        Bet memory winnerBet = getWinnerBet();
        return block.timestamp - winnerBet.timestamp >= waitingTime;
    }

    // Claim the prize
    function claimPrize() external nonReentrant {
        // Check that roulette is not finished
        require(!rouletteFinished, "Roulette is finished");
        require(canClaimPrize(), "Prize can't be claimed yet");

        // Get the winner bet
        Bet memory winnerBet = getWinnerBet();

        // Calculate the amount of points for the winner based on the time since the last bet
        uint256 timeSinceLastBet = block.timestamp - winnerBet.timestamp;
        points[winnerBet.player] += timeSinceLastBet / 1 minutes;

        // Increase the total points
        totalPoints += timeSinceLastBet / 1 minutes;

        // Send the accumulated ETH to the winner
        winnerBet.player.call{value: ethAmount}("");
        emit ExtraRewardEarned(winnerBet.player, ethAmount);

        // Reset the eth amount
        ethAmount = 0;

        // Send half of the current DEAGLE balance to the winner
        uint256 prizeDeagle = deagleToken.balanceOf(address(this)) / 2;
        deagleToken.transfer(winnerBet.player, prizeDeagle);

        // Reset the deagle amount
        deagleAmount = deagleToken.balanceOf(address(this));

        // Emit an event
        emit RouletteWon(winnerBet.player, deagleAmount, ethAmount);

        // Set to finished
        rouletteFinished = true;

        // Calculate the amount of deagle per point
        deaglePerPoint = deagleAmount / totalPoints;

        // Claim the $DEAGLE corresponding to this player points
        claimPoints(winnerBet.player);
    }

    function getConsolidationPrizeAmount(
        address player
    ) public view returns (uint256) {
        // Returns the amount of deagle that the player can claim
        return points[player] * deaglePerPoint;
    }

    // Claim point rewards for a player
    function claimPoints(address player) public {
        // Check that roulette IS finished
        require(rouletteFinished, "Roulette is NOT finished");

        // Check that deaglePerPoint is greater than 0
        require(deaglePerPoint > 0, "No DEAGLE per point");

        // Get the points of the player
        uint256 playerPoints = points[player];

        // Check that the player has points
        require(playerPoints > 0, "No points to claim");

        // Calculate the reward
        uint256 reward = (playerPoints * deaglePerPoint);

        // Reset the points of the player
        points[player] = 0;

        // Transfer the reward
        deagleToken.transfer(player, reward);

        // Lower the deagle amount
        deagleAmount -= reward;
    }

    function getRemainingDeagle() external onlyOwner {
        // Check that roulette IS finished
        require(rouletteFinished, "Roulette is NOT finished");

        // A week has passed since last bet
        require(
            block.timestamp - getWinnerBet().timestamp >= 7 days,
            "A week has not passed since the last bet"
        );

        // Balance of deagle remaining
        uint256 deagleBalance = deagleToken.balanceOf(address(this));

        // Transfer the remaining DEAGLE to the owner
        deagleToken.transfer(owner(), deagleBalance);

        // Reset the deagle amount
        deagleAmount = 0;
    }

    // Approves infinite spending of WETH to the SwapRouter
    function approveWeth() external onlyOwner {
        wethToken.approve(address(swapRouter), type(uint256).max);
    }

    function getRandomNumber() internal returns (uint256) {
        uint256 baseId = Random.getInitialBase(msg.sender);
        uint256 randomBase = _generateRandomBase(baseId);
        uint256 randomNumber = 0;
        (randomNumber, ) = Random.getNextRandom(randomBase, ONE_HUNDRED);
        return randomNumber;
    }
    function _generateRandomBase(
        uint baseId
    ) internal view virtual returns (uint256) {
        return Random.startRandomBase(baseId, uint256(uint160(msg.sender)));
    }

    function castSpell() external {
        // Require that the roulette is not ended
        require(!rouletteFinished, "Roulette is finished");

        // Transfer the buff cost to the contract (DEAGLE)
        deagleToken.transferFrom(msg.sender, address(this), buffCost);

        // Get a random number
        uint256 randomNumber = getRandomNumber();
        console.log("Random number: %s", randomNumber);

        if (randomNumber < 20000) {
            // Deagle is send to owner
            deagleToken.transfer(owner(), buffCost);
        } else if (randomNumber < 50000) {
            // Deagle is added to the pot
            deagleAmount += buffCost;
        } else {
            // Deagle is burned
            deagleToken.burn(buffCost);
        }

        string memory buff = applyBuff(randomNumber);
        emit SpellCasted(buff, msg.sender, block.timestamp);
    }

    function applyBuff(uint256 randomNumber) public returns (string memory) {
        if (randomNumber < 25000) {
            // Buff failed
            return "Spell failed";
        } else if (randomNumber < 60000) {
            // Cut down time by 10 minutes
            waitingTime = waitingTime > 10 minutes
                ? waitingTime - 10 minutes
                : 10 minutes;
            return "Casted Fast Spell: Max Time reduced by 10 minutes";
        } else if (randomNumber < 65000) {
            // Burns 2% of the total deagle pot
            uint256 burnAmount = (deagleAmount * 2) / 100;
            deagleToken.burn(burnAmount);
            deagleAmount -= burnAmount;
            return
                "Spell failed to cast and exploded: 2% of the total deagle pot burned";
        } else if (randomNumber < 70000) {
            // Next ticket cost doubles!
            nextTicketBuffMultiplier = 200;
            return "Usury: Next ticket cost doubled!";
        } else if (randomNumber < 75000) {
            // Next ticket cost quadruples!
            nextTicketBuffMultiplier = 400;
            return "Usury: Next ticket cost quadrupled!";
        } else if (randomNumber < 80000) {
            // Next ticket cost is half!
            nextTicketBuffMultiplier = 50;
            return "Cheapskate: Next ticket cost halved!";
        } else if (randomNumber < 82000) {
            // You Steal the last bet
            Bet memory winnerBet = getWinnerBet();
            winnerBet.player = msg.sender;
            winnerBet.timestamp = block.timestamp;

            return "Thievery Spell Cast! You stole the last bet";
        } else if (randomNumber < 85000) {
            // Reset the timer to 10 hours
            waitingTime = 10 hours;
            return "Time Warp: Timer reset to 10 hours";
        } else if (randomNumber < 87000) {
            // Reset the timer to 1 hour
            waitingTime = 1 hours;
            return "Time Warp: Timer reset to 1 hour";
        } else if (randomNumber < 89000) {
            // Steals 10% of the ETH amount
            uint256 stealAmount = (ethAmount * 10) / 100;
            msg.sender.call{value: stealAmount}("");
            ethAmount -= stealAmount;
            return "Thievery Spell Cast! You stole 10% of the ETH amount";
        } else if (randomNumber < 90000) {
            // Steals 2% of the DEAGLE amount
            uint256 stealAmount = (deagleAmount * 2) / 100;
            deagleToken.transfer(msg.sender, stealAmount);
            deagleAmount -= stealAmount;
            return "Thievery Spell Cast! You stole 2% of the DEAGLE amount";
        } else if (randomNumber < 92000) {
            // Gain 2000 points
            points[msg.sender] += 2000;
            totalPoints += 200;
            return "Spell of Wisdom: You gained 2000 points";
        } else if (randomNumber < 95000) {
            // Lose all points
            totalPoints -= points[msg.sender];
            points[msg.sender] = 0;
            return "Spell of Amnesia: You lost all your points";
        } else if (randomNumber < 97000) {
            // Doubles the base cost of the ticket forever
            ticketPrice *= 2;
            return "Curse of Greed: Ticket price doubled forever";
        } else {
            // Halfs the base cost of the ticket forever
            ticketPrice /= 2;
            return "Blessing of Generosity: Ticket price halved forever";
        }
    }
}

