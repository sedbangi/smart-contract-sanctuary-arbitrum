import "./IWETH.sol";

import "hardhat/console.sol";

contract TokenFactory {
    int24 internal constant MIN_TICK = -887000;
    int24 internal constant MAX_TICK = -MIN_TICK;
    using EnumerableSet for EnumerableSet.AddressSet;

    INonfungiblePositionManager public positionManager;
    ISwapRouter public swapRouter;
    ILPLockerForever public lpLocker;
    IWETH public wethToken;

    // Store the tokens deployed by an user
    mapping(address => EnumerableSet.AddressSet) internal itemsOfOwner;

    IUniswapV3Factory constant factory =
        IUniswapV3Factory(0x1F98431c8aD98523631AE4a59f267346ea31F984);

    address public constant UNISWAP_POSITION_MANAGER =
        0xC36442b4a4522E871399CD717aBDD847Ab11FE88;

    address public constant UNISWAP_ROUTER =
        0xE592427A0AEce92De3Edee1F18E0157C05861564;

    event TokenCreated(
        address indexed tokenAddress,
        address indexed creator,
        uint256 indexed tokenId
    );

    constructor(address _wethToken, address _lpLocker) {
        wethToken = IWETH(_wethToken);
        lpLocker = ILPLockerForever(_lpLocker);
        positionManager = INonfungiblePositionManager(UNISWAP_POSITION_MANAGER);
        swapRouter = ISwapRouter(UNISWAP_ROUTER);
        wethToken.approve(address(swapRouter), type(uint256).max);
        wethToken.approve(address(positionManager), type(uint256).max);
    }

    // Calculates the square root of the price formatted in X96
    function calculateSqrtPriceX96(
        uint256 reserve1,
        uint256 reserve0
    ) public pure returns (uint160) {
        require(reserve0 != 0, "Reserve0 amount cannot be zero");
        require(reserve1 != 0, "Reserve1 amount cannot be zero");

        // Calculate the ratio as a fixed-point number with 18 decimal places
        uint256 ratio = (reserve1 * 1e18) / reserve0;

        // Adjust the ratio for taking the square root to avoid precision issues
        // Multiplying by 1e18 before the square root and then dividing the result by 1e9
        // This step is crucial to maintaining a balance in the multiplication and division scale
        uint256 adjustedRatio = ratio * 1e18;
        uint256 sqrtRatio = sqrt(adjustedRatio);

        // Now rescale to X96 format
        // Previously we shifted by too many bits causing underflow, now we'll adjust more cautiously
        uint256 sqrtPriceX96 = (sqrtRatio * (1 << 96)) / 1e18;

        return uint160(sqrtPriceX96);
    }

    function sqrt(uint y) internal pure returns (uint z) {
        if (y > 3) {
            z = y;
            uint x = y / 2 + 1;
            while (x < z) {
                z = x;
                x = (y / x + x) / 2;
            }
        } else if (y != 0) {
            z = 1;
        }
    }

    function createPool(
        address token0,
        address token1,
        uint256 initialSupply0,
        uint256 initialSupply1
    ) public returns (address) {
        uint160 sqrtPriceX96 = calculateSqrtPriceX96(
            initialSupply1,
            initialSupply0
        );
        return
            positionManager.createAndInitializePoolIfNecessary(
                token0,
                token1,
                10000,
                sqrtPriceX96
            );
    }

    function createToken(
        string memory name,
        string memory symbol,
        uint256 initialSupplyToken,
        uint256 ethBuyAmount
    ) public payable returns (address, uint256) {
        // Amount of ETH for the LP
        uint256 ethForLp = msg.value - ethBuyAmount;

        // Require that the amount of ETH is greater than 0
        require(ethForLp > 0, "ETH amount must be greater than 0");
        require(ethBuyAmount > 0, "ETH buy amount must be greater than 0");

        // convert eth to weth
        wethToken.deposit{value: msg.value}();

        // Deploy the token
        BaseERC20 newToken = new BaseERC20(name, symbol, initialSupplyToken);
        // Approve the position manager to handle all tokens for liquidity provision
        newToken.approve(address(positionManager), initialSupplyToken);

        // Determine the order of the tokens based on their addresses
        (
            address token0,
            uint256 initialSupply0,
            address token1,
            uint256 initialSupply1
        ) = address(wethToken) < address(newToken)
                ? (
                    address(wethToken),
                    ethForLp,
                    address(newToken),
                    initialSupplyToken
                )
                : (
                    address(newToken),
                    initialSupplyToken,
                    address(wethToken),
                    ethForLp
                );

        // Create pool
        address pool = createPool(
            token0,
            token1,
            initialSupply0,
            initialSupply1
        );

        uint256 tokenId = mintPoolNFT(
            token0,
            token1,
            initialSupply0,
            initialSupply1
        );

        // Emit an event that the token and liquidity were created
        emit TokenCreated(address(newToken), msg.sender, tokenId);

        // Buy some tokens from the pool with the ETH part
        buyInitialTokensFromPool(pool, newToken, ethBuyAmount);

        // Approve the position manager to transfer the LP to the LP Locker
        positionManager.approve(address(lpLocker), tokenId);

        console.log("Approved position manager");

        // Lock the LP token
        lpLocker.lockPosition(tokenId, msg.sender, token0, token1);

        console.log("Locked position");

        // Add the token to the list of tokens owned by the sender
        itemsOfOwner[msg.sender].add(address(newToken));

        // Return the token address
        return (address(newToken), tokenId);
    }

    function mintPoolNFT(
        address token0,
        address token1,
        uint256 amount0Desired,
        uint256 amount1Desired
    ) internal returns (uint256) {
        INonfungiblePositionManager.MintParams
            memory mintParams = INonfungiblePositionManager.MintParams({
                token0: token0,
                token1: token1,
                fee: 10000,
                tickLower: MIN_TICK,
                tickUpper: MAX_TICK,
                amount0Desired: amount0Desired,
                amount1Desired: amount1Desired,
                amount0Min: amount0Desired - (amount0Desired * 3) / 100,
                amount1Min: amount1Desired - (amount1Desired * 3) / 100,
                recipient: address(this),
                deadline: block.timestamp + 15 minutes
            });

        (uint256 tokenId, , , ) = positionManager.mint(mintParams);

        return tokenId;
    }

    function buyInitialTokensFromPool(
        address pool,
        ERC20 newToken,
        uint256 ethBuyAmount
    ) public {
        // Check the amount of newToken in the pool
        uint256 tokenBalancePool = newToken.balanceOf(address(pool));

        console.log("Token balance: %s", tokenBalancePool);

        // Buy some tokens from the pool with the ETH part
        ISwapRouter.ExactInputSingleParams memory swapParams = ISwapRouter
            .ExactInputSingleParams({
                tokenIn: address(wethToken),
                tokenOut: address(newToken),
                fee: 10000, // Fee tier, e.g., 1%
                recipient: address(this),
                deadline: block.timestamp + 300, // 5 minutes from the current block
                amountIn: ethBuyAmount,
                amountOutMinimum: 0,
                sqrtPriceLimitX96: 0 // No price limit
            });
        swapRouter.exactInputSingle(swapParams);
        console.log("Swap done");

        // Get the balance of this new token
        uint256 tokenBalance = newToken.balanceOf(address(this));

        console.log("Token balance after swap: %s", tokenBalance);

        // Transfer the tokens to the creator
        newToken.transfer(msg.sender, tokenBalance);
    }

    // Function to burn LP token
    function burnLpToken(uint256 tokenId) public {
        positionManager.burn(tokenId);
    }
}

