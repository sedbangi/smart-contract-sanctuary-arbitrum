pragma solidity ^0.8.9;

import 'hardhat/console.sol';

import {ERC20} from '@openzeppelin/contracts/token/ERC20/ERC20.sol';
import './UniswapV3PairCreate.sol';

struct ITokenOptions {
  address nftManager;
}

contract Token is ERC20, UniswapV3PairCreate {
  using Math for uint256;

  constructor(
    ITokenOptions memory opts
  ) ERC20('TestToken', 'TT') UniswapV3PairCreate(opts.nftManager) {
    _mint(msg.sender, 1000000e18);
  }

  function initializeUniswapPair(uint256 tokenAmount, uint24 fee) external payable {
    _transfer(msg.sender, address(this), tokenAmount);
    _initializeUniswapPair(msg.sender, tokenAmount, fee);
  }

  function encodePriceSqrt(uint256 r0, uint256 r1) external pure returns (uint160) {
    return _encodePriceSqrt(r0, r1);
  }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.9;

import '@openzeppelin/contracts/utils/math/Math.sol';
import '@openzeppelin/contracts/token/ERC20/IERC20.sol';

import './IUniswapV3PairCreate.sol';

abstract contract UniswapV3PairCreate is IERC20 {
  using Math for uint256;

  INonfungiblePositionManager internal immutable nft;
  IUniswapV3Pool public wethPool;

  constructor(address nftManager) {
    nft = INonfungiblePositionManager(nftManager);
  }

  function _initializeUniswapPair(
    address provider,
    uint256 tokenAmount,
    uint24 fee
  ) internal {
    _checkInitialConditions(tokenAmount);

    IUniswapV3Factory factory = IUniswapV3Factory(nft.factory());

    wethPool = IUniswapV3Pool(factory.createPool(nft.WETH9(), address(this), fee));

    (uint256 token0Balance, uint256 token1Balance) = _getPoolBalances(
      wethPool,
      nft.WETH9(),
      msg.value,
      tokenAmount
    );

    wethPool.initialize(_encodePriceSqrt(token0Balance, token1Balance));

    this.approve(address(nft), tokenAmount);

    (int24 tickLower, int24 tickUpper) = _getLowerUpperTicks(wethPool);

    (uint256 tokenId, , , ) = nft.mint{value: msg.value}(
      INonfungiblePositionManager.MintParams({
        token0: wethPool.token0(),
        token1: wethPool.token1(),
        fee: fee,
        tickLower: tickLower,
        tickUpper: tickUpper,
        amount0Desired: token0Balance,
        amount1Desired: token1Balance,
        amount0Min: 0,
        amount1Min: 0,
        recipient: address(this),
        deadline: block.timestamp
      })
    );

    nft.safeTransferFrom(address(this), provider, tokenId);
  }

  function _checkInitialConditions(uint256 tokenAmount) internal view {
    require(address(this).code.length > 0, 'UV3PC: cannot initialize within constructor');

    require(msg.value != 0, 'UV3PC: value must be positive');

    require(
      this.balanceOf(address(this)) >= tokenAmount,
      'UV3PC: contract balance too low'
    );
  }

  function _getPoolBalances(
    IUniswapV3Pool pool,
    address weth,
    uint256 wethBalance,
    uint256 tokenBalance
  ) internal view returns (uint256, uint256) {
    return
      pool.token0() == weth ? (wethBalance, tokenBalance) : (tokenBalance, wethBalance);
  }

  function _getLowerUpperTicks(IUniswapV3Pool pool) internal view returns (int24, int24) {
    (, int24 tick, , , , , ) = pool.slot0();
    int24 tickSpacing = pool.tickSpacing();

    int24 tickLower = tick - (tick % tickSpacing);
    int24 tickUpper = tick + (tickSpacing - (tick % tickSpacing));

    return (tickLower, tickUpper);
  }

  function _encodePriceSqrt(uint256 r0, uint256 r1) internal pure returns (uint160) {
    // (((r1 * 1e30) / r0)).sqrt(Math.Rounding.Trunc) * ((2 ** 96) / uint256(1e15))

    return
      uint160(((r1.mulDiv(1e30, r0))).sqrt(Math.Rounding.Trunc).mulDiv(2 ** 96, 1e15));
  }
}

