pragma solidity 0.8.20;

import { Initializable } from "lib/openzeppelin-contracts-upgradeable/contracts/proxy/utils/Initializable.sol";
import { EnumerableSet } from "lib/openzeppelin-contracts/contracts/utils/structs/EnumerableSet.sol";

import "src/abstract/AdminAbstract.sol";
import "src/libraries/Listing.sol";
import "src/interfaces/manage/IMarketManager.sol";
import "src/manage/FeeManager.sol";
import "src/interfaces/IOrderBook.sol";

import "src/interfaces/IPositionRegistry.sol";
import "src/interfaces/IVault.sol";
import "src/libraries/Errors.sol";
import "src/libraries/AbsoluteMath.sol";

contract PositionRegistry is AdminAbstract, Initializable, IPositionRegistry {
    using EnumerableSet for EnumerableSet.UintSet;
    using Listing for EnumerableSet.UintSet;
    using AbsoluteMath for int256;

    IMarketManager private marketManager;
    FeeManager private feeManager;
    IVault private vault;

    uint256 internal _countID;
    mapping(uint256 => Position) public positions; // positionID -> Position struct
    mapping(address => mapping(address => uint256)) public positionIDs; // orderBook -> user -> positionID
    mapping(address => EnumerableSet.UintSet) internal _userPositions; // user -> positionIDs

    modifier onlyOrderBook() {
        if (!marketManager.isOpenMarket(msg.sender)) revert Errors.NotOrderBook();
        _;
    }

    modifier nonZero(address _address) {
        if (_address == address(0)) revert Errors.WrongAddress();
        _;
    }

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(address _adminStructureAddress, address _marketManagerAddress, address _feeManager)
        external
        initializer
    {
        _setAdminStructure(_adminStructureAddress);
        _setMarketManager(_marketManagerAddress);
        _setFeeManager(_feeManager);

        _countID = 1;
    }

    // LISTING

    function countUserPositions(address _user) external view returns (uint256) {
        return _userPositions[_user].length();
    }

    /// @dev use with countPositions()
    function listUserPositions(uint256 _offset, uint256 _limit, address _user)
        external
        view
        returns (uint256[] memory userPositionsList)
    {
        return _userPositions[_user].listUint(_offset, _limit);
    }

    // POSITIONS

    function lockCollateral(address _user, uint256 _margin) external override onlyOrderBook {
        if (_margin > 0) {
            _lockCollateral(msg.sender, _user, _margin);
        }
    }

    function unlockCollateral(address _user, uint256 _margin) external override onlyOrderBook {
        if (_margin > 0) {
            _unlockCollateral(msg.sender, _user, _margin);
        }
    }

    function matchPosition(address _user, int256 _quantity)
        external
        view
        override
        onlyOrderBook
        returns (uint256 _matchedQuantity)
    {
        uint256 _positionID = positionIDs[msg.sender][_user];

        if (_hasPosition(_positionID, _user)) {
            Position memory _position = positions[_positionID];

            if ((_position.quantity > 0 && _quantity < 0) || (_position.quantity < 0 && _quantity > 0)) {
                if (_position.quantity.abs() > _quantity.abs()) {
                    return _quantity.abs();
                } else {
                    return _position.quantity.abs();
                }
            }
        }
    }

    function makePosition(address _user, int256 _quantity, uint256 _margin) external override onlyOrderBook {
        uint256 _positionID = positionIDs[msg.sender][_user];

        if (!_hasPosition(_positionID, _user)) {
            _openPosition(msg.sender, _user, _quantity, _margin);
        } else {
            _updatePosition(_positionID, _quantity, _margin);
        }
    }

    function closePosition(uint256 _positionID, address _user, int256 _quantity, uint256 _price, int256 _pnl)
        external
        onlyAdmin
    {
        if (!_hasPosition(_positionID, _user)) revert Errors.WrongPosition();

        Position memory _position = positions[_positionID];
        if (_quantity.abs() > _position.quantity.abs()) revert Errors.WrongQuantity();

        uint256 _margin = _position.margin * _quantity.abs() / _position.quantity.abs();

        IOrderBook(_position.orderBookAddress).placeOrder(_user, _quantity, _price, _margin);

        // TODO
        if (_price == 0) {
            _closePosition(_positionID, _user, _margin, _pnl);
        }
    }

    function _openPosition(address _orderBookAddress, address _user, int256 _quantity, uint256 _margin) private {
        uint256 _newPositionID = _countID++;
        uint16 _positionFee = feeManager.getPositionFee();
        uint16 _maxBPS = feeManager.maxBPS();
        uint256 _positionFeeInCollateral = _margin * _positionFee / _maxBPS;

        positions[_newPositionID] = Position({
            orderBookAddress: _orderBookAddress,
            user: _user,
            quantity: _quantity - int256(_quantity * int256(uint256(_positionFee)) / int256(uint256(_maxBPS))),
            entryPrice: 0, // TODO
            liqPrice: 0, // TODO
            margin: _margin - _positionFeeInCollateral,
            pnl: 0,
            open: true
        });

        vault.cutFee(_orderBookAddress, _user, _positionFeeInCollateral, feeManager.getFeeWallet());

        positionIDs[_orderBookAddress][_user] = _newPositionID;
        _userPositions[_user].add(_newPositionID);

        emit PositionOpened(
            _orderBookAddress,
            _user,
            _quantity - int256(_quantity * int256(uint256(_positionFee)) / int256(uint256(_maxBPS))),
            _margin - _positionFeeInCollateral
        );
    }

    function _updatePosition(uint256 _positionID, int256 _quantity, uint256 _margin) private {
        Position storage _position = positions[_positionID];

        _position.quantity += _quantity;
        _position.margin += _margin;

        emit PositionUpdated(_position.orderBookAddress, _position.user, _position.quantity, _position.margin);
    }

    function _closePosition(uint256 _positionID, address _user, uint256 _margin, int256 _pnl) private {
        Position storage _position = positions[_positionID];

        vault.realizePNL(_position.orderBookAddress, _user, _pnl, _margin);

        _position.margin -= _margin;
        _position.pnl = _pnl;

        if (_position.quantity == 0) {
            _position.open = false;
            emit PositionClosed(_position.orderBookAddress, _user, _margin);
        }
    }

    function _lockCollateral(address _orderBookAddress, address _user, uint256 _margin) internal {
        vault.lockMargin(_orderBookAddress, _user, _margin);
    }

    function _unlockCollateral(address _orderBookAddress, address _user, uint256 _margin) internal {
        vault.unlockMargin(_orderBookAddress, _user, _margin);
    }

    // SETTINGS

    function setVault(address _vaultAddress) external onlyAdmin {
        _setVault(_vaultAddress);
    }

    function setMarketManager(address _marketManagerAddress) external onlyAdmin {
        _setMarketManager(_marketManagerAddress);
    }

    function setFeeManager(address _feeManager) external onlyAdmin {
        _setFeeManager(_feeManager);
    }

    function _setVault(address _vaultAddress) private nonZero(_vaultAddress) {
        vault = IVault(_vaultAddress);

        emit VaultSet(_vaultAddress);
    }

    function _setMarketManager(address _marketManagerAddress) private nonZero(_marketManagerAddress) {
        marketManager = IMarketManager(_marketManagerAddress);

        emit MarketManagerSet(_marketManagerAddress);
    }

    function _setFeeManager(address _feeManager) private nonZero(_feeManager) {
        feeManager = FeeManager(_feeManager);

        emit FeeManagerSet(_feeManager);
    }

    // GETTERS

    function getMarketManager() external view returns (address _marketManagerAddress) {
        return address(marketManager);
    }

    function getVault() external view returns (address _vaultAddress) {
        return address(vault);
    }

    function getFeeManager() external view returns (address _feeManager) {
        return address(feeManager);
    }

    function positionQuantity(address _orderBookAddress, address _user)
        external
        view
        override
        returns (int256 _quantity)
    {
        uint256 _positionID = positionIDs[_orderBookAddress][_user];
        return positions[_positionID].quantity;
    }

    function _hasPosition(uint256 _positionID, address _user) internal view returns (bool) {
        return _userPositions[_user].contains(_positionID) && positions[_positionID].open;
    }
}

