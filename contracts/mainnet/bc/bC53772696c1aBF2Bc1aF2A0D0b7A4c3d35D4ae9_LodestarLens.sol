//SPDX-License-Identifier: BSD-3-Clause
pragma solidity 0.8.20;

import "@openzeppelin/contracts/access/Ownable2Step.sol";
import "lodestar-helper/contracts/Interfaces/Oracle/AggregatorV3Interface.sol";
import "lodestar-helper/contracts/Interfaces/Oracle/IPOPE.sol";
import "lodestar-helper/contracts/Interfaces/ComptrollerInterface.sol";
import "lodestar-helper/contracts/Interfaces/cToken/ICERC20.sol";
import "lodestar-helper/contracts/Interfaces/ERC20/IERC20Extended.sol";
import "lodestar-helper/contracts/Interfaces/ILODE.sol";
import "lodestar-helper/contracts/Interfaces/StakingRewardsInterface.sol";
import "lodestar-helper/contracts/Interfaces/VotingPowerInterface.sol";
import "lodestar-helper/contracts/Interfaces/GLPInterfaces.sol";

import "hardhat/console.sol";

contract LodestarLens is Ownable2Step {
    IPOPE public ORACLE;
    ComptrollerInterface public COMPTROLLER;
    address public ETHER_MARKET;
    AggregatorV3Interface public ETH_AGGREGATOR;
    StakingRewardsInterface public STAKING;
    VotingPowerInterface public VOTING;
    IGLPVault public GLP_VAULT;
    IGLPManager public GLP_MANAGER;
    IGLPRewardTracker public GLP_REWARD_TRACKER;

    uint256 public USD_DECIMAL_FACTOR = 1e30;
    uint256 public BASE = 1e18;

    ILODE public constant LODE = ILODE(0xF19547f9ED24aA66b03c3a552D181Ae334FBb8DB);
    IGLP public constant GLP = IGLP(0x4277f8F2c384827B5273592FF7CeBd9f2C1ac258);
    IERC20 public constant esLODE = IERC20(0x501f4f041655a3CEDd4211FE5ECdC27738D88aF2);

    struct MarketInfo {
        ICERC20 marketAddress;
        address underlyingAddress;
        uint256 underlyingPrice;
        uint256 priceInUSD;
        uint256 totalSuppliedUnderlying;
        uint256 totalSuppliedUSD;
        uint256 totalBorrowedUnderlying;
        uint256 totalBorrowedUSD;
        uint256 cash;
        uint256 reserves;
        uint256 borrowRatePerBlock;
        uint256 supplyRatePerBlock;
        uint256 collateralFactor;
        uint256 exchangeRate;
        address interestRateModel;
        string symbol;
        uint256 accrualBlockNumber;
        uint256 supplySpeed;
        uint256 borrowSpeed;
        uint256 borrowCap;
        uint256 supplyCap;
        uint256 reserveFactorMantissa;
    }

    struct MarketsInfo {
        MarketInfo[] markets;
        uint256 totalSuppliedUSD;
        uint256 totalBorrowedUSD;
    }

    struct UserMarketInfo {
        ICERC20 marketAddress;
        string symbol;
        uint256 totalSuppliedUnderlying;
        uint256 totalSuppliedETH;
        uint256 totalSuppliedUSD;
        uint256 totalBorrowedUnderlying;
        uint256 totalBorrowedETH;
        uint256 totalBorrowedUSD;
        uint256 totalCollateralETH;
    }

    struct UserMarketsInfo {
        address userAddress;
        UserMarketInfo[] userMarkets;
        uint256 health;
        uint256 liquidity;
        uint256 shortfall;
        uint256 totalCollateralETH;
        uint256 totalSuppliedUSD;
        uint256 totalBorrowedETH;
        uint256 totalBorrowedUSD;
        uint256 underlyingBalance;
    }

    struct StakerInfo {
        uint256 stLode;
        uint256 lodeStaked;
        uint256 lockTime;
        uint256 startTime;
        uint256 esLodeStaked;
        uint256 pendingRewards;
        uint256 totalLodeStaked;
        uint256 totalEsLodeStaked;
        uint256 totalSupply;
        uint256 weeklyRewards;
        uint256 totalESLodeSupply;
    }

    struct VotingInfo {
        uint256 votingPower;
        address delegate;
        uint256 lastVotedWeek;
        uint256 lastVotedTimestamp;
        uint256 userVotes;
        uint256 userVoteShare;
        uint256 currentWeek;
        bool previouslyVoted;
        string[] votingMarkets;
        VotingPowerInterface.OperationType[] votingOperations;
        uint256[] votingResults;
        uint256 lodeSpeed;
    }

    struct GLPInfo {
        uint256 aum1;
        uint256 aum2;
        uint256 tokensPerInterval;
        uint256 nativeTokenPrice;
        uint256 totalSupply;
    }

    constructor(IPOPE oracle_, ComptrollerInterface comptroller_, address ether_market_, AggregatorV3Interface ether_aggregator_, StakingRewardsInterface staking_, VotingPowerInterface voting_, IGLPVault glp_vault_, IGLPManager glp_manager_, IGLPRewardTracker glp_rewardtracker_) Ownable(msg.sender) {
        ORACLE = oracle_;
        COMPTROLLER = comptroller_;
        ETHER_MARKET = ether_market_;
        ETH_AGGREGATOR = ether_aggregator_;
        STAKING = staking_;
        VOTING = voting_;
        GLP_VAULT = glp_vault_;
        GLP_MANAGER = glp_manager_;
        GLP_REWARD_TRACKER = glp_rewardtracker_;
    }

    function getNativeBalance (address user) public view returns (uint256 balance) {
        balance = user.balance;
    }

    function getTokenBalance (address user, IERC20Extended token) public view returns (uint256 balance) {
        balance = token.balanceOf(user);
    }

    //logic to convert price returned from oracle in ETH with decimal conversion to USD scaled to 6 decimals
    function getPriceUSD (ICERC20 market) public view returns (uint256) {
        uint256 priceInETH = ORACLE.getUnderlyingPrice(address(market));
        //console.log('oracle return: ', priceInETH);
        IERC20Extended underlying;
        uint8 decimals;
        if(address(market) == ETHER_MARKET) {
            decimals = 18;
        } else {
            underlying = IERC20Extended(market.underlying());
            decimals = underlying.decimals();
        }
        uint256 ether_price = getEtherPriceInUsd();
        if(decimals != 18) {
            //remove padded zeroes if they are there
            uint256 priceScaled = priceInETH / (10 ** (18 - decimals));
            //console.log((priceScaled * ether_price) / USD_DECIMAL_FACTOR);
            return (priceScaled * ether_price) / USD_DECIMAL_FACTOR;
        }
        else {
            //console.log((priceInETH * ether_price) / USD_DECIMAL_FACTOR);
            return (priceInETH * ether_price) / USD_DECIMAL_FACTOR;
        }
    }

    //function to return price of ETH in USD scaled to 18 decimals
    function getEtherPriceInUsd() public view returns (uint256) {
        ( , int256 answer, , ,) = ETH_AGGREGATOR.latestRoundData();
        uint8 decimals = ETH_AGGREGATOR.decimals();
        //console.log(uint256(answer) * 10 ** (18 - decimals));
        return uint256(answer) * 10 ** (18 - decimals);
    }

    function convertCTokensToUnderlying(uint256 cTokens, ICERC20 market) internal view returns (uint256) {
        uint256 exchangeRate = market.exchangeRateStored();
        return (cTokens * exchangeRate) / BASE;
    }

    function convertCTokensToUSD (uint256 cTokens, ICERC20 market) public view returns (uint256) {
        //logic to convert an amount of cTokens to an amount of USD given the balance of cTokens and the market
        uint256 exchangeRate = market.exchangeRateStored();
        //console.log('exchange rate: ', exchangeRate);
        uint256 amountUnderlying = (cTokens * exchangeRate) / BASE;
        //console.log('amount underlying: ', amountUnderlying);
        IERC20Extended underlying;
        uint256 underlyingDecimals;
        if(address(market) != ETHER_MARKET) {
            underlying = IERC20Extended(market.underlying());
            underlyingDecimals = underlying.decimals();
        } else {
            underlyingDecimals = 18;
        }
        uint256 priceUSD = getPriceUSD(market);
        //console.log('asset price in USD: ', priceUSD);
        //console.log('total supply return: ', (amountUnderlying * priceUSD) / (10 ** underlyingDecimals));
        //console.log('numerator: ', amountUnderlying * priceUSD);
        return (amountUnderlying * priceUSD) / (10 ** underlyingDecimals);
    }

    function convertUnderlyingToUSD (uint256 underlying, ICERC20 market) public view returns (uint256) {
        //logic to convert an amount of underlying assets to an amount of USD given the balance of tokens and the market
        uint256 underlyingDecimals;
        if(address(market) != ETHER_MARKET) {
            IERC20Extended underlyingToken = IERC20Extended(market.underlying());
            underlyingDecimals = underlyingToken.decimals();
        } else {
            underlyingDecimals = 18;
        }
        uint256 priceUSD = getPriceUSD(market);
        return(underlying * priceUSD) / (10 ** underlyingDecimals);
    }

    function getCollateralFactor (ICERC20 market) public view returns (uint256) {
        ( , uint256 cFactor, ) = COMPTROLLER.markets(address(market));
        return cFactor;
    }

    function getUserLiquidity(address user) public view returns (uint256, uint256) {
        (,uint256 liquidity, uint256 shortfall) = COMPTROLLER.getAccountLiquidity(user);
        return (liquidity, shortfall);
    }

    function getPendingLODERewards (address user) public returns (uint256) {
        uint balance = LODE.balanceOf(user);
        COMPTROLLER.claimComp(user);
        uint newBalance = LODE.balanceOf(user);
        uint accrued = COMPTROLLER.compAccrued(user);
        uint total = accrued + newBalance;
        uint allocated = total - balance;
        return allocated;
    }

    function getMarketInfo(ICERC20 market) public view returns (MarketInfo memory marketInfo) {
        marketInfo.marketAddress = market;
        if(address(market) != ETHER_MARKET) {
            marketInfo.underlyingAddress = address(market.underlying());
        } else {
            marketInfo.underlyingAddress = address(0);
        }
        marketInfo.underlyingPrice = ORACLE.getUnderlyingPrice(address(market));
        marketInfo.priceInUSD = getPriceUSD(market);
        marketInfo.totalSuppliedUnderlying = convertCTokensToUnderlying(market.totalSupply(), market);
        marketInfo.totalSuppliedUSD = convertCTokensToUSD(market.totalSupply(), market);
        marketInfo.totalBorrowedUnderlying = market.totalBorrows();
        marketInfo.totalBorrowedUSD = convertUnderlyingToUSD(market.totalBorrows(), market);
        marketInfo.cash = market.getCash();
        marketInfo.reserves = market.totalReserves();
        marketInfo.borrowRatePerBlock = market.borrowRatePerBlock();
        marketInfo.supplyRatePerBlock = market.supplyRatePerBlock();
        marketInfo.collateralFactor = getCollateralFactor(market);
        marketInfo.exchangeRate = market.exchangeRateStored();
        marketInfo.interestRateModel = market.interestRateModel();
        marketInfo.symbol = market.symbol();
        marketInfo.accrualBlockNumber = market.accrualBlockNumber();
        marketInfo.supplySpeed = COMPTROLLER.compSupplySpeeds(address(market));
        marketInfo.borrowSpeed = COMPTROLLER.compBorrowSpeeds(address(market));
        marketInfo.borrowCap = COMPTROLLER.borrowCaps(address(market));
        marketInfo.supplyCap = COMPTROLLER.supplyCaps(address(market));
        marketInfo.reserveFactorMantissa = market.reserveFactorMantissa();
    }

    function getMarketsInfo(ICERC20[] memory markets) external view returns (MarketsInfo memory marketsInfo) {
        MarketInfo[] memory marketInfoArray = new MarketInfo[](markets.length);
        uint256 totalSuppliedUSD;
        uint256 totalBorrowedUSD;
        for(uint i = 0; i < markets.length; i++) {
            MarketInfo memory marketInfo = getMarketInfo(markets[i]);
            marketInfoArray[i] = marketInfo;
            totalSuppliedUSD += marketInfo.totalSuppliedUSD;
            totalBorrowedUSD += marketInfo.totalBorrowedUSD;
        }
        marketsInfo.totalSuppliedUSD = totalSuppliedUSD;
        marketsInfo.totalBorrowedUSD = totalBorrowedUSD;
        marketsInfo.markets = marketInfoArray;
    }

    function getUserMarketInfo (address user, ICERC20 market) public view returns (UserMarketInfo memory userMarketInfo) {
        userMarketInfo.marketAddress = market;
        userMarketInfo.symbol = market.symbol();
        uint256 userCTokenBalance = market.balanceOf(user);
        console.log(userCTokenBalance);
        uint256 totalSuppliedUnderlying = convertCTokensToUnderlying(userCTokenBalance, market);
        userMarketInfo.totalSuppliedUnderlying = totalSuppliedUnderlying;
        uint256 exchangeRate = market.exchangeRateStored();
        uint256 oraclePrice = ORACLE.getUnderlyingPrice(address(market));
        uint256 totalSuppliedETH = (userCTokenBalance * oraclePrice * exchangeRate) / 1e36;
        userMarketInfo.totalSuppliedETH = totalSuppliedETH;
        userMarketInfo.totalSuppliedUSD = convertCTokensToUSD(userCTokenBalance, market);
        uint256 userBorrowBalance = market.borrowBalanceStored(user);
        userMarketInfo.totalBorrowedETH = userBorrowBalance * oraclePrice;
        userMarketInfo.totalBorrowedUSD = convertUnderlyingToUSD(userBorrowBalance, market);
        userMarketInfo.totalCollateralETH = getCollateralFactor(market) * totalSuppliedETH;
    }

    function getUserMarketsInfo (address user, ICERC20[] memory markets) external view returns (UserMarketsInfo memory userMarketsInfo) {
        UserMarketInfo[] memory userMarketInfoArray = new UserMarketInfo[](markets.length);
        uint256 totalCollateralETH;
        uint256 totalBorrowedETH;
        uint256 totalSuppliedUSD;
        uint256 totalBorrowedUSD;

        for(uint i =0; i < markets.length; i++) {
            UserMarketInfo memory marketInfo = getUserMarketInfo(user, markets[i]);
            userMarketInfoArray[i] = marketInfo;
            totalCollateralETH += userMarketInfoArray[i].totalCollateralETH;
            totalBorrowedETH += userMarketInfoArray[i].totalBorrowedETH;

            totalSuppliedUSD += userMarketInfoArray[i].totalBorrowedUSD;
            totalBorrowedUSD += userMarketInfoArray[i].totalSuppliedUSD;
            
        }

        userMarketsInfo.totalBorrowedUSD = totalSuppliedUSD;
        userMarketsInfo.totalSuppliedUSD = totalBorrowedUSD;
        userMarketsInfo.totalCollateralETH = totalCollateralETH;
        userMarketsInfo.totalBorrowedETH = totalBorrowedETH;

        console.log('total collateral: ', totalCollateralETH);
        console.log('total borrowed: ', totalBorrowedETH);
        if(totalBorrowedETH != 0) {
            userMarketsInfo.health = (totalCollateralETH * BASE) / totalBorrowedETH;
        } else {
            userMarketsInfo.health = 0;
        }
        (userMarketsInfo.liquidity, userMarketsInfo.shortfall) = getUserLiquidity(user);
        userMarketsInfo.userMarkets = userMarketInfoArray;
    }

    function getStakingInfo (address user) external view returns (StakerInfo memory stakingInfo) {
        StakingRewardsInterface.StakingInfo memory stakingContractInfo = STAKING.stakers(user);
        stakingInfo.stLode = stakingContractInfo.stLODEAmount;
        stakingInfo.lodeStaked = stakingContractInfo.lodeAmount;
        stakingInfo.lockTime = stakingContractInfo.lockTime;
        stakingInfo.startTime = stakingContractInfo.startTime;
        stakingInfo.esLodeStaked = stakingContractInfo.totalEsLODEStakedByUser;
        stakingInfo.pendingRewards = STAKING.pendingRewards(user);
        stakingInfo.totalLodeStaked = STAKING.totalStaked();
        stakingInfo.totalEsLodeStaked = STAKING.totalEsLODEStaked();
        stakingInfo.totalSupply = STAKING.totalSupply();
        stakingInfo.weeklyRewards = STAKING.weeklyRewards();
        stakingInfo.totalESLodeSupply = esLODE.balanceOf(0x0000000000000000000000000000000000000000);
    }

    function getVotingInfo (address user) external view returns (VotingInfo memory votingInfo) {
        votingInfo.delegate = VOTING.delegates(user);
        votingInfo.lastVotedWeek = VOTING.lastVotedWeek(user);
        uint256 votingPeriod = VOTING.votingPeriod();
        votingInfo.userVotes = VOTING.userVotes(user, votingPeriod);
        uint256 clock = VOTING.clock();
        votingInfo.userVoteShare = VOTING.getPastVotes(user, clock - 1);
        votingInfo.currentWeek = VOTING.getCurrentWeek();
        votingInfo.previouslyVoted = VOTING.previouslyVoted(user);
        (votingInfo.votingMarkets, votingInfo.votingOperations, votingInfo.votingResults) = VOTING.getResults();
        votingInfo.lodeSpeed = VOTING.LODE_SPEED();
    }

    function getGLPInfo(address _token) public view returns (GLPInfo memory glpInfo) {
        uint256[] memory amounts = GLP_MANAGER.getAums();
        glpInfo.aum1 = amounts[0];
        glpInfo.aum2 = amounts[1];
        glpInfo.tokensPerInterval = GLP_REWARD_TRACKER.tokensPerInterval();
        glpInfo.nativeTokenPrice = GLP_VAULT.getMinPrice(_token); // usually WETH is passed in here
        glpInfo.totalSupply = GLP.totalSupply();
    }
}

