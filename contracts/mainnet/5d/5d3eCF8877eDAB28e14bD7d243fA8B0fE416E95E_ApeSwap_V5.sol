        uint256 bidId;
        uint256 flashLoanAmount;
        uint256 borrowerAmount;
        uint256 commitmentId;
        uint256 principalAmount;
        uint256 collateralAmount;
        uint256 collateralTokenId;
        uint16 interestRate;
        uint32 loanDuration;
    }

