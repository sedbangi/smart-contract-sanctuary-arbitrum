        uint256 bidId;
        uint256 commitmentId;
        uint256 principalAmount;
        uint256 collateralAmount;
        uint256 collateralTokenId;
        uint16 interestRate;
        uint32 loanDuration;
    }

    struct Aave_FlashLoanParams {
        address lendingToken;
        uint256 flashAmount;
        ApeHop subHop;
    }

    enum UniswapV3_SwapType {
        ExactInput,
        ExactOutput
    }

    struct UniswapV3_FlashSwapParams {
        ApeSwapHelper.PoolInfo poolInfo;
        uint256 flashAmount;
        UniswapV3_SwapType swapType;
        ApeHop subHop;
    }

    struct UniswapV3_FlashSwapCallbackData {
        ApeSwapHelper.PoolInfo poolInfo;
        ApeHop subHop;
    }

    struct Aave_FlashLoanCallbackData {
        ApeHop subHop;
    }

    function loanBorrower(uint256 _loanId) external view returns (address);

    function loanInfo(uint256 _loanId) external view returns (LoanInfo memory);

    function estimatePositionReturns(
        ApeSwap_ClosePositionParams memory _params
    )  external returns (int256) ;

}

