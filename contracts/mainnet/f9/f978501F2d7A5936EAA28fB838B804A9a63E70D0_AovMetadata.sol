pragma solidity ^0.8.4;

import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/Base64.sol";

import "./IAdventurerMetadata.sol";
import "../Adventurer/IAdventurerData.sol";
import "../Adventurer/TraitConstants.sol";

import "../Manager/ManagerModifier.sol";
import "./IBatchAdventurerData.sol";
import "./IAdventurerName.sol";
import "../Utils/MetadataUtils.sol";
import "../Renown/IRenown.sol";

abstract contract AdventurerMetadata is IAdventurerMetadata, ManagerModifier {
  using Strings for uint256;
  //=======================================
  // Immutables
  //=======================================
  IBatchAdventurerData public immutable ADVENTURER_DATA;
  IAdventurerName public immutable ADVENTURER_NAME;
  IRenown public immutable RENOWN;

  //=======================================
  // Strings
  //=======================================
  string public baseURI;

  //=======================================
  // Constructor
  //=======================================
  constructor(
    address _manager,
    address _adventurerData,
    address _adventurerName,
    address _renown
  ) ManagerModifier(_manager) {
    ADVENTURER_DATA = IBatchAdventurerData(_adventurerData);
    ADVENTURER_NAME = IAdventurerName(_adventurerName);
    RENOWN = IRenown(_renown);
  }

  function _collectionName() internal view virtual returns (string memory);

  function _collectionDescription()
    internal
    view
    virtual
    returns (string memory);

  function _attributeKeys() internal pure returns (string[] memory result) {
    result = new string[](16);
    result[0] = "Transcendence Level";
    result[1] = "Class";
    result[2] = "XP";
    result[3] = "HP";
    result[4] = "Strength";
    result[5] = "Dexterity";
    result[6] = "Constitution";
    result[7] = "Intelligence";
    result[8] = "Wisdom";
    result[9] = "Charisma";
    result[10] = "Name";
    result[11] = "Origin";
    result[12] = "Inheritance";
    result[13] = "Archetype";
    result[14] = "Renown";
    result[15] = "Training Points";
  }

  function _attributeValues(
    address _addr,
    uint256 _tokenId
  ) internal view returns (string[] memory result) {
    result = new string[](16);
    uint level = uint(
      ADVENTURER_DATA.STORAGE(
        _addr,
        _tokenId,
        traits.ADV_TRAIT_GROUP_BASE,
        traits.ADV_TRAIT_BASE_LEVEL
      )
    );
    result[0] = Strings.toString(level);
    result[1] = _class(
      uint(
        ADVENTURER_DATA.STORAGE(
          _addr,
          _tokenId,
          traits.ADV_TRAIT_GROUP_BASE,
          traits.ADV_TRAIT_BASE_CLASS
        )
      )
    );
    result[2] = Strings.toString(
      uint(
        ADVENTURER_DATA.STORAGE(
          _addr,
          _tokenId,
          traits.ADV_TRAIT_GROUP_BASE,
          traits.ADV_TRAIT_BASE_XP
        )
      )
    );
    result[3] = "100"; // HP
    result[4] = Strings.toString(
      uint(
        ADVENTURER_DATA.STORAGE(
          _addr,
          _tokenId,
          traits.ADV_TRAIT_GROUP_BASE,
          traits.ADV_TRAIT_BASE_STRENGTH
        )
      )
    );
    result[5] = Strings.toString(
      uint(
        ADVENTURER_DATA.STORAGE(
          _addr,
          _tokenId,
          traits.ADV_TRAIT_GROUP_BASE,
          traits.ADV_TRAIT_BASE_DEXTERITY
        )
      )
    );
    result[6] = Strings.toString(
      uint(
        ADVENTURER_DATA.STORAGE(
          _addr,
          _tokenId,
          traits.ADV_TRAIT_GROUP_BASE,
          traits.ADV_TRAIT_BASE_CONSTITUTION
        )
      )
    );
    result[7] = Strings.toString(
      uint(
        ADVENTURER_DATA.STORAGE(
          _addr,
          _tokenId,
          traits.ADV_TRAIT_GROUP_BASE,
          traits.ADV_TRAIT_BASE_INTELLIGENCE
        )
      )
    );
    result[8] = Strings.toString(
      uint(
        ADVENTURER_DATA.STORAGE(
          _addr,
          _tokenId,
          traits.ADV_TRAIT_GROUP_BASE,
          traits.ADV_TRAIT_BASE_WISDOM
        )
      )
    );
    result[9] = Strings.toString(
      uint(
        ADVENTURER_DATA.STORAGE(
          _addr,
          _tokenId,
          traits.ADV_TRAIT_GROUP_BASE,
          traits.ADV_TRAIT_BASE_CHARISMA
        )
      )
    );
    result[10] = ADVENTURER_NAME.names(_addr, _tokenId);
    if (bytes(result[10]).length == 0) {
      result[10] = "Forgotten"; // NAME
    }
    result[11] = "Unknown"; //Origin
    result[12] = "Undiscovered"; // Inheritance
    result[13] = Strings.toString(
      uint(
        ADVENTURER_DATA.STORAGE(
          _addr,
          _tokenId,
          traits.ADV_TRAIT_GROUP_ADVANCED,
          traits.ADV_TRAIT_ADVANCED_ARCHETYPE
        )
      )
    );
    result[14] = Strings.toString(
      uint(RENOWN.currentRenown(_addr, _tokenId, level))
    );
    result[15] = Strings.toString(
      uint(
        ADVENTURER_DATA.STORAGE(
          _addr,
          _tokenId,
          traits.ADV_TRAIT_GROUP_ADVANCED,
          traits.ADV_TRAIT_ADVANCED_TRAINING_POINTS
        )
      )
    );
  }

  //=======================================
  // External
  //=======================================
  function uri(
    address _addr,
    uint256 _tokenId
  ) external view override returns (string memory) {
    uint archetype = uint(
      ADVENTURER_DATA.STORAGE(
        _addr,
        _tokenId,
        traits.ADV_TRAIT_GROUP_ADVANCED,
        traits.ADV_TRAIT_ADVANCED_ARCHETYPE
      )
    );

    return
      metadata.convertToMetadata(
        string.concat(_collectionName(), " #", Strings.toString(_tokenId)),
        _collectionDescription(),
        string.concat(baseURI, Strings.toString(archetype), ".jpg"),
        _attributeKeys(),
        _attributeValues(_addr, _tokenId)
      );
  }

  //=======================================
  // Admin
  //=======================================
  function setBaseURI(string calldata _baseURI) external onlyAdmin {
    baseURI = _baseURI;
  }

  function _class(uint256 _classId) internal pure returns (string memory) {
    if (_classId == 1) {
      return "Chaos";
    } else if (_classId == 2) {
      return "Mischief";
    } else {
      return "Tranquility";
    }
  }
}

