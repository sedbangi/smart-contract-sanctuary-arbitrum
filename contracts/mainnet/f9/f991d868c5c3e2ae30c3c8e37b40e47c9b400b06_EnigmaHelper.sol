    function getMaxLiquidityForRange(
        address enigma,
        address _unifactory,
        int24 tickLower,
        int24 tickUpper,
        uint24 feeTier
    ) external view returns (uint256 maxLiquidity) {
        IEnigma _enigma = IEnigma(enigma);
        IERC20 token0 = _enigma.token0();
        IERC20 token1 = _enigma.token1();
        uint256 amount0 = token0.balanceOf(address(_enigma));
        uint256 amount1 = token1.balanceOf(address(_enigma));

        address pool = IAlgebraFactory(_unifactory).poolByPair(address(token0), address(token1));
        require(pool != address(0), "POOL(0)");

        maxLiquidity = getLiquidityForAmounts(pool, tickLower, tickUpper, amount0, amount1);
    }

    function getTotalUnusedAmounts(address enigma) external view returns (uint256 amount0, uint256 amount1) {
        IEnigma _enigma = IEnigma(enigma);
        //returns the total of unused tokens
        IERC20 token0 = _enigma.token0();
        IERC20 token1 = _enigma.token1();
        amount0 = token0.balanceOf(address(_enigma));
        amount1 = token1.balanceOf(address(_enigma));
    }

    /// @notice Private view function to sort 2 tokens in ascending order
    /// @param tokenX The first token
    /// @param tokenY The second token
    /// @return token0 The sorted first token
    /// @return token1 The sorted second token
    function sortTokens(address tokenX, address tokenY) external pure returns (address token0, address token1) {
        require(tokenX != tokenY, "same token");
        (token0, token1) = tokenX < tokenY ? (tokenX, tokenY) : (tokenY, tokenX);
        require(token0 != address(0), "no address zero");
    }

    /// @notice Calculates the vault's total holdings of token0 and token1 - in
    /// other words, how much of each token the vault would hold if it withdrew
    /// all its liquidity from Uniswap. (does not include pending fees)
    function getTotalAmounts(address _enigma, Range[] memory _ranges, IAlgebraFactory factory)
        external
        view
        returns (uint256 total0, uint256 total1)
    {
        IERC20 token0 = IEnigma(_enigma).token0();
        IERC20 token1 = IEnigma(_enigma).token1();
        //add any currently unused values contained in this contract
        total0 = token0.balanceOf(_enigma);
        total1 = token1.balanceOf(_enigma);

        //loop over the ranges in the strategy
        uint256 rangeLength = _ranges.length;
        for (uint256 index; index < rangeLength;) {
            Range memory _range = _ranges[index];

            //get the applicable uniswap pool (safe as fee tier is checked when adding)
            address _pool = factory.poolByPair(address(token0), address(token1));

            (uint128 liquidity, uint256 owed0, uint256 owed1) =
                position(address(this), _pool, _range.tickLower, _range.tickUpper);

            (uint256 amount0, uint256 amount1) =
                getAmountsForLiquidity(_pool, _range.tickLower, _range.tickUpper, liquidity);

            //increment the index prior to the continue to prevent unlimted gas issue
            unchecked {
                ++index; // Increment in an unchecked block
            }

            if (amount0 == 0 && amount1 == 0 && owed0 == 0 && owed1 == 0) continue;

            total0 += amount0 + owed0;
            total1 += amount1 + owed1;
        }
    }

    /// @notice Calculates the amount of liquidity to add to a range based on existing amounts in the range
    /// used for direct depositing to underlying pools in enigma deposit fn.
    /// @param pool the uniswapV3 pool
    /// @param range the underlying enigma range
    /// @param totalShares the total amount of shares that already exist
    /// @param mintShares the new amount of shares being minted
    /// @return liquidity of the new position that is to be minted
    function getScaledLiquidityForRange(address pool, Range memory range, uint256 totalShares, uint256 mintShares)
        external
        view
        returns (uint128 liquidity)
    {
        //get the current liquidity in the univ3 position
        (uint128 _liquidity,,) = position(address(this), pool, range.tickLower, range.tickUpper);

        // get the underrlying amounts of token0 and token1 for that amount of liquidity
        (uint256 amt0, uint256 amt1) = getAmountsForLiquidity(pool, range.tickLower, range.tickUpper, _liquidity);

        //scale the amounts that are to be added based on the number of new shares to mint and round down.
        uint256 amt0Act = FullMath.mulDiv(amt0, mintShares, totalShares + 1);
        uint256 amt1Act = FullMath.mulDiv(amt1, mintShares, totalShares + 1);

        //get the liquidity for the scaled amounts from uniV3.
        liquidity = getLiquidityForAmounts(pool, range.tickLower, range.tickUpper, amt0Act, amt1Act);
    }

