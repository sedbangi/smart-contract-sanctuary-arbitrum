pragma solidity 0.8.19;

import {IERC20Upgradeable} from '@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol';
import {ERC721Upgradeable} from '@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol';
import {OwnableUpgradeable} from '@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol';
import {SafeERC20Upgradeable} from '@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol';
import {ReentrancyGuardUpgradeable} from '@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol';
import {IPoolFactory} from './interfaces/IPoolFactory.sol';
import {IPoolMaster, PoolStatus} from './interfaces/IPoolMaster.sol';
import {Decimal} from './libraries/Decimal.sol';
import {UtilsGuard} from './utils/Utils.sol';
import {IAuction} from './interfaces/IAuction.sol';

contract Auction is
  IAuction,
  ERC721Upgradeable,
  OwnableUpgradeable,
  ReentrancyGuardUpgradeable,
  UtilsGuard
{
  using SafeERC20Upgradeable for IERC20Upgradeable;
  using Decimal for uint256;

  struct AuctionIncrementInfo {
    uint256 percent; // the percentage by which the bid increases
    uint256 maxAmount; // the maximum amount allowed to increase the bid
  }

  /// @notice Structure storing information about auction for some pool
  struct AuctionInfo {
    uint96 tokenId;
    address lastBidder;
    uint256 lastBid;
    uint256 lastBlock;
    uint256 start;
    uint256 end;
    uint256 incrementalAmount;
  }

  /// @notice Structure storing details of some token, representint pool debt
  struct TokenInfo {
    address pool;
    uint256 borrowsAtClaim;
    uint256 interestRate;
  }

  /// @notice PoolFactory contract
  IPoolFactory public factory;

  /// @notice Debt auction duration (in seconds)
  uint256 public auctionDuration;

  /// @notice Minimal ratio of initial bid to pool insurance (as 18-digit decimal)
  uint256 public minBidFactor;

  AuctionIncrementInfo public bidIncrementInfo; // see {AuctionIncrementInfo}

  /// @notice Last debt token ID
  uint96 public lastTokenId;

  /// @notice Mapping of pool addresses to their debt auction info
  mapping(address => AuctionInfo) public auctionInfo;

  /// @notice Mapping of addresses to flags if they are whitelisted bidders
  mapping(address => bool) public isWhitelistedBidder;

  /// @notice Mapping of token IDs to their token info
  mapping(uint256 => TokenInfo) public tokenInfo;

  // timestamp variables
  uint256 public lastBidDuration;
  uint256 public bidIncrementDuration;
  uint256 public resolutionDuration;

  // events
  event AuctionStarted(address indexed poolAddress, address indexed bidder);
  event WhitelistedBidderSet(address indexed bidder, bool whitelisted);
  event Bid(address indexed poolAddress, address indexed bidder, uint256 amount);
  event AuctionResolved(address indexed pool, bool resolution, address winner);
  event AuctionDurationSet(uint256 duration);
  event BidIncrementalPercentChanged(uint256 percent);
  event BidIncrementalMaxAmountChanged(uint256 maxAmount);
  //errors
  error InvalidIncrementalInfo();
  error NotInDefault();
  error InsufficientBidAmount();
  error NotAllowed();
  error NotWhitelisted();
  error AlreadyBid();
  error AuctionClosed();
  error AuctionNotStarted();
  error NotPool();
  error InvalidBidIncrementalPercentage();
  error NotLastBidder();
  error AuctionNotClosed();
  error AlreadyResolved();
  error DecisionDateNotPassed();
  error WrongDuration();

  // modifiers
  modifier checkBidder(address pool) {
    if (msg.sender == IPoolMaster(pool).borrower()) revert NotAllowed();
    if (!isWhitelistedBidder[msg.sender]) revert NotWhitelisted();
    _;
  }

  modifier onlyPoolAddr(address pool) {
    if (!factory.isPool(pool)) revert NotPool();
    _;
  }

  modifier onlyActive(address pool) {
    if (auctionInfo[pool].end != 0 && block.timestamp > auctionInfo[pool].end)
      revert AuctionClosed();
    _;
  }

  /// @custom:oz-upgrades-unsafe-allow constructor
  constructor() {
    _disableInitializers();
  }

  function initialize(
    address factory_,
    uint256 auctionDuration_,
    uint256 minBidFactor_,
    AuctionIncrementInfo calldata incrementInfo_
  ) external initializer nonZeroAddress(factory_) {
    if (
      (incrementInfo_.percent < 1e16 || incrementInfo_.percent > 1e18) ||
      incrementInfo_.maxAmount == 0
    ) revert InvalidIncrementalInfo();

    if (auctionDuration_ < 5 days) revert WrongDuration();

    __Ownable_init();
    __ERC721_init('Clearpool Debt', 'CPDEBT');
    __ReentrancyGuard_init();

    factory = IPoolFactory(factory_);
    auctionDuration = auctionDuration_;
    minBidFactor = minBidFactor_;
    bidIncrementInfo = incrementInfo_;

    lastBidDuration = 12 hours;
    bidIncrementDuration = 1 days;
    resolutionDuration = 10 days;
  }

  /**
   * @notice Makes a bid on a pool
   * @param pool Address of a pool
   * @param amount Amount of the bid
   */
  function bid(
    address pool,
    uint256 amount
  )
    external
    nonReentrant
    onlyPoolAddr(pool)
    onlyActive(pool)
    checkBidder(pool)
    nonZeroValue(amount)
  {
    AuctionInfo storage currentAuction = auctionInfo[pool];
    if (currentAuction.lastBidder == msg.sender) revert AlreadyBid();
    if (currentAuction.lastBid > amount) revert InsufficientBidAmount();

    IERC20Upgradeable currency = IERC20Upgradeable(IPoolMaster(pool).asset());
    // If no bids available yet, start the auction
    if (currentAuction.lastBidder == address(0)) {
      _startAuction(pool, amount);
    } else {
      if (currentAuction.lastBid + currentAuction.incrementalAmount > amount)
        revert InsufficientBidAmount();
      // Return the bid of the last bidder
      currency.safeTransfer(currentAuction.lastBidder, currentAuction.lastBid);
    }
    if (currentAuction.end - block.timestamp <= lastBidDuration)
      currentAuction.end += bidIncrementDuration;

    currentAuction.lastBidder = msg.sender;
    (currentAuction.lastBid, currentAuction.lastBlock) = (amount, block.number);
    currency.safeTransferFrom(msg.sender, address(this), amount);

    emit Bid(pool, msg.sender, amount);
  }

  /**
   * @notice Increases bid on a pool
   * @param pool Address of a pool
   * @param amount Amount to add
   */
  function increaseBid(
    address pool,
    uint256 amount
  ) external nonReentrant onlyPoolAddr(pool) onlyActive(pool) nonZeroValue(amount) checkBidder(pool) {
    AuctionInfo storage currentAuction = auctionInfo[pool];
    IERC20Upgradeable currency = IERC20Upgradeable(IPoolMaster(pool).asset());

    if (msg.sender == currentAuction.lastBidder) {
      if (currentAuction.incrementalAmount > amount) revert InsufficientBidAmount();
      if (currentAuction.end - block.timestamp <= lastBidDuration)
        currentAuction.end += bidIncrementDuration;

      currentAuction.lastBid += amount;
      currentAuction.lastBlock = block.number;
      currency.safeTransferFrom(msg.sender, address(this), amount);

      emit Bid(pool, msg.sender, currentAuction.lastBid);
    } else {
      revert NotLastBidder();
    }
  }

  /**
   * @notice Resolves auction in case there is no government decision for 10 days
   * @param pool Address of a pool
   */
  function resolveAuctionWithoutGovernment(address pool) external nonReentrant {
    AuctionInfo storage currentAuction = auctionInfo[pool];

    if (currentAuction.end == 0) revert AuctionNotStarted();
    if (currentAuction.end > block.timestamp) revert AuctionNotClosed();
    if (block.timestamp - currentAuction.end < resolutionDuration) revert DecisionDateNotPassed();
    if (currentAuction.tokenId != 0) revert AlreadyResolved();

    IERC20Upgradeable currency = IERC20Upgradeable(IPoolMaster(pool).asset());

    currency.safeTransfer(currentAuction.lastBidder, currentAuction.lastBid);

    delete auctionInfo[pool];
    auctionInfo[pool].tokenId = type(uint96).max;

    factory.processPoolAuctionEnd(pool, 0);
    emit AuctionResolved(pool, false, address(0));
  }

  /**
   * @notice Resolves auction after it's end - accepts or rejects winning bid
   * @param pool Address of a pool
   * @param resolution True to accept, false to reject
   */
  function resolveAuction(address pool, bool resolution) external nonReentrant onlyOwner {
    AuctionInfo storage currentAuction = auctionInfo[pool];
 
    if (currentAuction.end == 0) revert AuctionNotStarted();
    if (currentAuction.end > block.timestamp) revert AuctionNotClosed();
    if (currentAuction.tokenId != 0) revert AlreadyResolved();

    IERC20Upgradeable currency = IERC20Upgradeable(IPoolMaster(pool).asset());

    if (resolution) {
      // In case of acceptance, mint debt NFT and transfer bid to the pool
      lastTokenId++;

      tokenInfo[lastTokenId] = TokenInfo({
        pool: pool,
        borrowsAtClaim: IPoolMaster(pool).poolSize(),
        interestRate: IPoolMaster(pool).lendAPR()
      });
      currentAuction.tokenId = lastTokenId;

      _mint(auctionInfo[pool].lastBidder, lastTokenId);
      currency.safeTransfer(pool, currentAuction.lastBid);

      factory.processPoolAuctionEnd(pool, currentAuction.lastBid);
      emit AuctionResolved(pool, resolution, auctionInfo[pool].lastBidder);
    } else {
      // In case of rejection, distribute remaining cash between lenders and refund bids
      currency.safeTransfer(currentAuction.lastBidder, currentAuction.lastBid);
      delete auctionInfo[pool];

      auctionInfo[pool].tokenId = type(uint96).max;

      factory.processPoolAuctionEnd(pool, 0);
      emit AuctionResolved(pool, resolution, address(0));
    }
  }

  /**
   * @notice Used to set white listed bidders
   * @param bidder The address of the whitelisted bidder
   * @param whitelisted True if bidder should be whitelisted false otherwise
   */
  function setWhitelistedBidder(
    address bidder,
    bool whitelisted
  ) external onlyOwner nonZeroAddress(bidder) {
    isWhitelistedBidder[bidder] = whitelisted;
    emit WhitelistedBidderSet(bidder, whitelisted);
  }

  /*
   * @notice Function is used to set new value for auction duration
   * @param _auctionDuration Auction duration in seconds
   */
  function setAuctionDuration(
    uint256 _auctionDuration
  )
    external
    virtual
    onlyOwner
    nonSameValue(auctionDuration, _auctionDuration)
    nonZeroValue(_auctionDuration)
  {
    if (_auctionDuration < 5 days) revert WrongDuration();

    auctionDuration = _auctionDuration;
    emit AuctionDurationSet(_auctionDuration);
  }

  /**
   * @notice Function is used to set new value for bid incremental percent
   * @param bidIncrementalPercent_ Percent value in mantissa
   */
  function setBidIncrementalPercent(
    uint256 bidIncrementalPercent_
  ) external onlyOwner nonSameValue(bidIncrementInfo.percent, bidIncrementalPercent_) {
    if (bidIncrementalPercent_ < 1e16 || bidIncrementalPercent_ > 1e18)
      revert InvalidBidIncrementalPercentage();
    bidIncrementInfo.percent = bidIncrementalPercent_;
    emit BidIncrementalPercentChanged(bidIncrementalPercent_);
  }

  /**
   * @notice Function is used to set new value for bid incremental max amount
   * @param bidIncrementMaxAmount_ value
   */

  function setBidIncrementalMaxAmount(
    uint256 bidIncrementMaxAmount_
  )
    external
    onlyOwner
    nonZeroValue(bidIncrementMaxAmount_)
    nonSameValue(bidIncrementInfo.maxAmount, bidIncrementMaxAmount_)
  {
    bidIncrementInfo.maxAmount = bidIncrementMaxAmount_;
    emit BidIncrementalMaxAmountChanged(bidIncrementMaxAmount_);
  }

  /**
   * @notice Returns owner of a debt
   * @param pool Address of a pool
   * @return Address of the owner
   */
  function ownerOfDebt(address pool) external view returns (address) {
    uint96 tokenId = auctionInfo[pool].tokenId;
    return
      (tokenId != 0 && tokenId != type(uint96).max)
        ? ownerOf(auctionInfo[pool].tokenId)
        : address(0);
  }

  /**
   * @notice Returns state of a pool auction
   * @param pool Address of a pool
   * @return state of a pool auction
   */
  function state(address pool) external view returns (State) {
    if (
      IPoolMaster(pool).status() != PoolStatus.Default &&
      IPoolMaster(pool).status() != PoolStatus.Closed
    ) {
      return State.None;
    } else if (auctionInfo[pool].lastBidder == address(0)) {
      return State.NotStarted;
    } else if (block.timestamp < auctionInfo[pool].end) {
      return State.Active;
    } else if (block.timestamp >= auctionInfo[pool].end && auctionInfo[pool].tokenId == 0) {
      return State.Finished;
    } else {
      return State.Closed;
    }
  }

  /**
   * @notice Internal function used to start auction for a specific pool after first bid
   * @param poolAddress Address of the pool
   */
  function _startAuction(address poolAddress, uint256 amount) private {
    IPoolMaster pool = IPoolMaster(poolAddress);

    if (pool.status() != PoolStatus.Default) revert NotInDefault();

    AuctionInfo storage info = auctionInfo[poolAddress];

    factory.processPoolAuctionStart(poolAddress);
    uint256 poolDecimals = pool.decimals(); // e.g. 6 for USDC
    uint256 poolSize = pool.poolSize();
    if (poolSize.mulDecimal(minBidFactor) > amount) revert InsufficientBidAmount();

    uint256 maxAmount = bidIncrementInfo.maxAmount; // is in 18 decimals
    uint256 weiPoolSize = toWei(poolSize, poolDecimals); // 18 decimals amount
    uint256 incrementalAmount = weiPoolSize.mulDecimal(bidIncrementInfo.percent);
    if (incrementalAmount > maxAmount) {
      info.incrementalAmount = fromWei(maxAmount, poolDecimals);
    } else {
      info.incrementalAmount = fromWei(incrementalAmount, poolDecimals);
    }

    info.start = block.timestamp;
    info.end = block.timestamp + auctionDuration;

    emit AuctionStarted(poolAddress, msg.sender);
  }
}

