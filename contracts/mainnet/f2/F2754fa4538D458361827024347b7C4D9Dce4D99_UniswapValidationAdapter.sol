pragma solidity ^0.8.17;

import { IUniswapV3Factory } from '@uniswap/v3-core/contracts/interfaces/IUniswapV3Factory.sol';
import { IUniswapV3Pool } from '@uniswap/v3-core/contracts/interfaces/IUniswapV3Pool.sol';
import { OracleLibrary } from '../../libraries/uniswap/OracleLibrary.sol';

contract UniswapValidationAdapter {
    error INVALID_PRICE_LTE();
    error INVALID_PRICE_GTE();

    address immutable uniswapFactory;
    bool constant VALIDATION_ADAPTER = true;

    function isValidationAdapter() external pure returns (bool) {
        return VALIDATION_ADAPTER;
    }

    constructor(address _uniswapFactory) {
        uniswapFactory = _uniswapFactory;
    }

    function checkLte(address tokenIn, address tokenOut, uint256 triggerLtePrice) external view returns (bool canExec) {
        uint256 currentPrice = _checkPrice(tokenIn, tokenOut, 3000);
        if (currentPrice >= triggerLtePrice) {
            return true;
        }
        revert INVALID_PRICE_LTE();
    }

    function checkGte(address tokenIn, address tokenOut, uint256 triggerLtePrice) external view returns (bool canExec) {
        uint256 currentPrice = _checkPrice(tokenIn, tokenOut, 3000);
        if (currentPrice <= triggerLtePrice) {
            return true;
        }
        revert INVALID_PRICE_GTE();
    }

    function _checkPrice(address tokenA, address tokenB, uint24 fee) internal view returns (uint256 price) {
        address poolAddress = IUniswapV3Factory(uniswapFactory).getPool(tokenA, tokenB, fee);

        if (poolAddress == address(0)) {
            return 0; // Pool does not exist
        }

        IUniswapV3Pool pool = IUniswapV3Pool(poolAddress);
        (, int24 tick, , , , , ) = pool.slot0();
        return OracleLibrary.getQuoteAtTick(tick, 1e18, pool.token0(), pool.token1());
    }
}

