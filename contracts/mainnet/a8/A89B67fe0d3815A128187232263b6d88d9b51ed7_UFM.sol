        // Convert input strings to byte arrays
        bytes memory byteArrayA = bytes(a);
        bytes memory byteArrayB = bytes(b);

        // Calculate total length of the concatenated string
        uint totalLength = byteArrayA.length + byteArrayB.length;

        // Create a new dynamic byte array of the total length
        bytes memory result = new bytes(totalLength);
        // Loop through the first input string and copy its bytes to the result array
        uint i;
        uint j;
        for (i = 0; i < byteArrayA.length; i++) {
            result[j++] = byteArrayA[i];
        }
        // Loop through the second input string and copy its bytes to the result array
        for (i = 0; i < byteArrayB.length; i++) {
            result[j++] = byteArrayB[i];
        }
        // Convert the result byte array back to a string and return it
        return string(result);
    }
}

contract UFM {
    address private _receiver = address(0); // 收款钱包地址

    address private _owner;

    address private _admin;

    address private votingEscrow;

    uint256 public lockedBalanceThreshold = (500 * 10) ^ 18;

    mapping(uint256 => address) public fp_token_contract;
    mapping(uint256 => uint256) public fp_quantity;
    mapping(uint256 => uint256) public fp_quantity_sold;

    event buy(
        address indexed sender,
        uint256 indexed fp_id,
        address indexed token_contract,
        uint256 amount
    );
    event update_fp(
        uint256 indexed fp_id,
        address indexed token_contract,
        uint256 fp_quantity,
        uint256 fp_quantity_sold
    );

    constructor() {
        _owner = msg.sender;
    }

    modifier onlyOwner() {
        require(_owner == msg.sender, "Ownable: caller is not the owner");
        _;
    }

    modifier onlyAdmin() {
        require(
            _owner == msg.sender || _admin == msg.sender,
            "Ownable: caller is not the admin"
        );
        _;
    }

    function setOwner(address __owner) external onlyOwner {
        _owner = __owner;
    }

    function setAdmin(address __admin) external onlyOwner {
        _admin = __admin;
    }

    function setVotingEscrow(address __voting_escrow) external onlyOwner {
        votingEscrow = __voting_escrow;
    }

    function setLockedBalanceThreshold(
        uint256 _lockedBalanceThreshold
    ) external onlyOwner {
        lockedBalanceThreshold = _lockedBalanceThreshold;
    }

    /**
     * 读取理财包供应数据
     * @param __fp_id 项目ID
     */
    function readFpTokenContract(
        uint256 __fp_id
    ) external view returns (address) {
        require(
            fp_token_contract[__fp_id] != address(0),
            "Financial packages do not exist"
        );
        return fp_token_contract[__fp_id];
    }

    /**
     * 读取理财包已售数据
     * @param __fp_id 项目ID
     */
    function readFpQuantity(uint256 __fp_id) external view returns (uint256) {
        require(
            fp_token_contract[__fp_id] != address(0),
            "Financial packages do not exist"
        );
        return fp_quantity[__fp_id];
    }

    /**
     * 读取理财包状态
     * @param __fp_id 项目ID
     */
    function readFpQuantitySold(
        uint256 __fp_id
    ) external view returns (uint256) {
        require(
            fp_token_contract[__fp_id] != address(0),
            "Financial packages do not exist"
        );
        return fp_quantity_sold[__fp_id];
    }

    /**
     * 注册理财包
     * @param __fp_id 项目ID
     * @param __token_contract token合约地址
     * @param __fp_quantity 总量
     */
    function registeredFp(
        uint256 __fp_id,
        address __token_contract,
        uint256 __fp_quantity
    ) external onlyAdmin {
        require(
            fp_token_contract[__fp_id] == address(0),
            "Financial package has been registered"
        );
        fp_token_contract[__fp_id] = __token_contract;
        fp_quantity[__fp_id] = __fp_quantity;
        fp_quantity_sold[__fp_id] = 0;
        emit update_fp(
            __fp_id,
            __token_contract,
            fp_quantity[__fp_id],
            fp_quantity_sold[__fp_id]
        );
    }

    /**
     * 读取收款人
     * @param __receiver 收款接收人
     */
    function changeReceiver(address __receiver) external onlyAdmin {
        require(__receiver != address(0), "Receiver is invalid.");
        _receiver = __receiver;
    }

    /**
     *
     */
    function queryReceiver() external view onlyAdmin returns (address) {
        return _receiver;
    }

    /**
     * 增加供应量
     * @param __fp_id 项目ID
     * @param __fp_quantity 新增供应量
     */
    function addAllowance(
        uint256 __fp_id,
        uint256 __fp_quantity
    ) external onlyAdmin {
        require(
            fp_token_contract[__fp_id] != address(0),
            "Financial packages do not exist"
        );
        fp_quantity[__fp_id] += __fp_quantity;
        emit update_fp(
            __fp_id,
            fp_token_contract[__fp_id],
            fp_quantity[__fp_id],
            fp_quantity_sold[__fp_id]
        );
    }

    function decAllowance(
        uint256 __fp_id,
        uint256 __fp_quantity
    ) external onlyAdmin {
        require(
            fp_token_contract[__fp_id] != address(0),
            "Financial packages do not exist"
        );
        if (fp_quantity[__fp_id] >= fp_quantity_sold[__fp_id] + __fp_quantity) {
            fp_quantity[__fp_id] -= __fp_quantity;
        } else {
            fp_quantity[__fp_id] = fp_quantity_sold[__fp_id];
        }
        emit update_fp(
            __fp_id,
            fp_token_contract[__fp_id],
            fp_quantity[__fp_id],
            fp_quantity_sold[__fp_id]
        );
    }

    function stakeToken(uint256 __fp_id, uint256 __amount) external {
        require(_receiver != address(0), "receiver is not set");
        require(
            fp_quantity[__fp_id] >= fp_quantity_sold[__fp_id] + __amount,
            "Financial package inventory is insufficient"
        );
        fp_quantity_sold[__fp_id] += __amount;
        // other pool balance amount above than 500
        int128 votingEscrowLocked = IVotingEscrow(votingEscrow)
            .locked(msg.sender)
            .amount;
        require(
            uint256(uint128(votingEscrowLocked)) >= lockedBalanceThreshold,
            "LockedBalance must more than 500"
        );
        IERC20 token = IERC20(fp_token_contract[__fp_id]);
        token.transferFrom(msg.sender, address(this), __amount);
        token.transfer(_receiver, __amount);
        //TransferHelper.safeTransferFrom(fp_token_contract[__fp_id], msg.sender, finance, __amount);
        emit buy(msg.sender, __fp_id, fp_token_contract[__fp_id], __amount);
        emit update_fp(
            __fp_id,
            fp_token_contract[__fp_id],
            fp_quantity[__fp_id],
            fp_quantity_sold[__fp_id]
        );
    }
}

