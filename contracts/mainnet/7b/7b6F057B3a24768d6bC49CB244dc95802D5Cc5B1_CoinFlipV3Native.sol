pragma solidity ^0.8.0;
import "hardhat/console.sol";

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "../utils/Random.sol";
uint256 constant DECIMAL_POINT = 10 ** 3;
uint256 constant ONE_HUNDRED = 100 * DECIMAL_POINT;

contract CoinFlipV3Native is Ownable, ReentrancyGuard {
    address public authorizedCaller;

    // Max amount of tokens that can be betted
    uint256 public maxBetAmount = 1 * 10 ** 18;

    // Total token deposited
    uint256 public tokenBank;
    uint256 public feeAmount;

    // Event for a flip
    event Flip(address indexed user, uint256 amount, uint256 choose, bool win);

    // Event for deposit
    event Deposit(address indexed user, uint256 amount);
    // Event for claim from bank
    event ClaimFromBank(address indexed user, uint256 amount);

    constructor(address _authorized) {
        authorizedCaller = _authorized;
        feeAmount = 6; // Percent fee on flip
    }

    // Set a new authorized caller
    function setAuthorizedCaller(address _authorized) public onlyOwner {
        authorizedCaller = _authorized;
    }

    // Set max bet amount
    function setMaxBetAmount(uint256 _amount) public onlyOwner {
        maxBetAmount = _amount;
    }

    // Only authorized or owner modifier
    modifier onlyAuthorized() {
        require(
            msg.sender == authorizedCaller || msg.sender == owner(),
            "Not authorized"
        );
        _;
    }

    // returns the amount of eth in the contract
    function getNativeBalance() public view returns (uint256) {
        return address(this).balance;
    }

    // Flip coin!
    function flip(uint256 _choose) public payable {
        // Amount is the value sent with the transaction
        uint256 _amount = msg.value;

        require(_choose == 0 || _choose == 1, "Invalid choose");
        // Require that the amount is less than the max bet amount
        require(_amount <= maxBetAmount, "Amount too high");

        // Require that the contract holds at least double the amount of the bet
        require(
            getNativeBalance() >= _amount * 2,
            "Insufficient funds in bank"
        );

        uint256 randomNumber = getRandomNumber();
        console.log("Random number: %s", randomNumber);

        bool win = randomNumber > 50000;
        tokenBank += _amount;

        if (!win) {
            emit Flip(msg.sender, _amount, _choose, false);
        } else {
            // Remove the fee
            uint256 fee = (_amount * feeAmount) / 100;

            uint256 total = _amount + _amount - fee;

            // Modify the bank
            tokenBank -= total;

            // Send the tokens to the user
            payable(msg.sender).transfer(total);

            emit Flip(msg.sender, _amount, _choose, true);
        }
    }

    function getRandomNumber() internal view returns (uint256) {
        uint256 baseId = Random.getInitialBase(msg.sender);
        uint256 randomBase = _generateRandomBase(baseId);
        uint256 randomNumber = 0;
        (randomNumber, ) = Random.getNextRandom(randomBase, ONE_HUNDRED);
        return randomNumber;
    }

    function _generateRandomBase(
        uint baseId
    ) internal view virtual returns (uint256) {
        return Random.startRandomBase(baseId, uint256(uint160(msg.sender)));
    }

    // Claim the fees
    function claimFromBank(uint256 amount) public onlyOwner {
        // Check that we dont claim more than the bank amount (user funds safu)
        require(amount <= tokenBank, "Amount too high");
        tokenBank -= amount;

        payable(owner()).transfer(amount);

        emit ClaimFromBank(owner(), amount);
    }

    // Deposit in the BANK
    function depositInBank() public payable {
        tokenBank += msg.value;
        emit Deposit(msg.sender, msg.value);
    }
}

