pragma solidity ^0.8.0;
interface IDISNNFT {
    function totalSupply() external view returns (uint256);
}
contract NFTWrapper is Ownable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    address public nftAddress = address(0x3C56F9B2425391131459509A00bCF0c13d0FC2Bc);
    address public usdtContract = address(0xFd086bC7CD5C481DCC9C85ebE478A1C0b69FCbb9);
    address public msgAddress = address(0x9B69a41A5a66AD5105cA3B7057782CB84Bbb1B96);
    uint256 public nftUNum = 0;
    constructor(){}
    receive() external payable {}
    function setTotal(uint256 t) external {
        require(_msgSender() == msgAddress, "Ownable: caller is not the owner");
        nftUNum = nftUNum+t;
    }
    function setNFTRewardCondition(uint256 amount) public onlyOwner {
        nftRewardCondition = amount;
    }
    function getErrorErc200(address coin,address account,uint256 amount) public onlyOwner {
        if(coin == address(0)){
            payable(account).transfer(amount);
        }else{
            IERC20(coin).transfer(account, amount);
        }
    }
    function setmsgAddress(address maddress) public onlyOwner {
        msgAddress = maddress;
    }
    function ospenTracker() public view returns(uint256) {
        return IDISNNFT(nftAddress).totalSupply();
    }
    function ospenTracker2(uint256 sa) public view returns(address) {
        return IERC721(nftAddress).ownerOf(sa);
    }
    uint256 nftRewardCondition = 516 * 10**5;
    uint256 public currentNFTIndex = 1;
    function updateNftAddress(address _setNftAddress) public onlyOwner {
        nftAddress = _setNftAddress;
    }
    function process(uint256 distributorGas) external {
        require(_msgSender() == msgAddress, "Ownable: caller is not the owner");
         if (nftUNum == 0){
            return;
        }
        address shareHolder;
        uint256 amount;

        uint256 shareholderCount = IDISNNFT(nftAddress).totalSupply();

        uint256 gasUsed = 0;
        uint256 iterations = 0;
        uint256 gasLeft = gasleft();
        IERC20 USDT = IERC20(usdtContract);

        while (gasUsed < distributorGas && iterations < shareholderCount) {
            if (currentNFTIndex > shareholderCount) {
                currentNFTIndex = 1;
            }
            shareHolder = IERC721(nftAddress).ownerOf(currentNFTIndex);
            amount = nftRewardCondition / shareholderCount;
            if (amount > 0) {
                if (nftUNum < amount){
                    break;
                } else {
                    USDT.transfer(shareHolder, amount);
                    nftUNum -= amount;
                }
            }

            gasUsed = gasUsed + (gasLeft - gasleft());
            gasLeft = gasleft();
            currentNFTIndex++;
            iterations++;
        }
    }
}