pragma solidity ^0.8.20;

import "@openzeppelin/contracts/proxy/Clones.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "./INodeSaleInit.sol";
import "./ITokenLimitedTransferInit.sol";
import "./IFeeTreasury.sol";

/**
 * @title NodeSaleFactory contract.
 * @dev This contract is for creating clones of NodeSale and TokenLimitedTransfer contracts.
 */
contract NodeSaleFactory is IFeeTreasury, AccessControl {
    using EnumerableSet for EnumerableSet.AddressSet;
    using Clones for address;

    /// Address of the NodeSale contract implementation.
    address public immutable nodeSaleImplementation;
    /// Address of the TokenLimitedTransfer contract implementation.
    address public immutable tokenLimitedTransferImplementation;

    /// Fee amount in basis points.
    uint96 public fee;

    /// Treasury address for the fee.
    address private _feeTreasury;
    /// Set of addresses of NodeSale contract clones.
    EnumerableSet.AddressSet private _nodeSaleClones;
    /// Set of addresses of TokenLimitedTransfer contract clones.
    EnumerableSet.AddressSet private _tokenLimitedTransferClones;

    /**
     * @dev Event emitted when a new clone is created.
     * @param creator Address of the creator.
     * @param nodeSaleClone Address of the new NodeSale contract clone.
     * @param tokenLimitedTransferClone Address of the new TokenLimitedTransfer contract clone.
     */
    event CloneCreated(
        address indexed creator,
        address indexed nodeSaleClone,
        address indexed tokenLimitedTransferClone
    );

    constructor(
        address _nodeSaleImplementation,
        address _tokenLimitedTransferImplementation,
        address feeTreasury,
        uint96 _fee
    ) {
        nodeSaleImplementation = _nodeSaleImplementation;
        tokenLimitedTransferImplementation = _tokenLimitedTransferImplementation;
        _feeTreasury = feeTreasury;
        fee = _fee;

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    /**
     * @dev Sets the fee amount.
     * @param _fee Fee amount in basis points (ex: 1000 = %10).
     */
    function setFee(uint96 _fee) external onlyRole(DEFAULT_ADMIN_ROLE) {
        fee = _fee;
    }

    /**
     * @dev Sets the treasury address.
     * @param feeTreasury Address of the treasury.
     */
    function setFeeTreasury(address feeTreasury) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _feeTreasury = feeTreasury;
    }

    /**
     * @dev Creates a clone of NodeSale and TokenLimitedTransfer contracts.
     * See {NodeSale} and {TokenLimitedTransfer} contracts for more details.
     */
    function createClone(
        string memory tokenName,
        string memory tokenSymbol,
        uint256 transferUnlockTime,
        uint96 defaultReferralDiscount
    ) external returns (address nodeSaleClone, address tokenLimitedTransferClone) {
        nodeSaleClone = nodeSaleImplementation.clone();
        _nodeSaleClones.add(nodeSaleClone);

        tokenLimitedTransferClone = tokenLimitedTransferImplementation.clone();
        _tokenLimitedTransferClones.add(tokenLimitedTransferClone);

        INodeSaleInit(nodeSaleClone).init(
            msg.sender,
            tokenLimitedTransferClone,
            address(this),
            fee,
            defaultReferralDiscount
        );
        ITokenLimitedTransferInit(tokenLimitedTransferClone).init(
            msg.sender,
            nodeSaleClone,
            tokenName,
            tokenSymbol,
            transferUnlockTime
        );

        emit CloneCreated(msg.sender, nodeSaleClone, tokenLimitedTransferClone);
    }

    /**
     * @dev Checks if an address is a clone of the NodeSale contract.
     * @param clone Address to check.
     * @return isAddressClone True if the address is a clone, false otherwise.
     */
    function isNodeSaleClone(address clone) external view returns (bool isAddressClone) {
        isAddressClone = _nodeSaleClones.contains(clone);
    }

    /**
     * @dev Checks if an address is a clone of the TokenLimitedTransfer contract.
     * @param clone Address to check.
     * @return isAddressClone True if the address is a clone, false otherwise.
     */
    function isTokenLimitedTransferClone(address clone) external view returns (bool isAddressClone) {
        isAddressClone = _tokenLimitedTransferClones.contains(clone);
    }

    /**
     * @dev Returns the total number of clones created.
     * @return totalAmount Total number of clones.
     */
    function totalCloneAmount() external view returns (uint256 totalAmount) {
        // must be same amount for both
        totalAmount = _nodeSaleClones.length();
    }

    /**
     * @dev See {IFeeTreasury-getFeeTreasury}.
     */
    function getFeeTreasury() external view override returns (address treasury) {
        treasury = _feeTreasury;
    }
}