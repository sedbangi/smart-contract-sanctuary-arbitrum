import "../Libraries/ABDKMath64x64.sol";

library OptionMath {
    using ABDKMath64x64 for int128;

    // 64x64 fixed point integer constants
    int128 internal constant ONE_64x64 = 0x10000000000000000;
    int128 internal constant THREE_64x64 = 0x30000000000000000;

    // 64x64 fixed point constants used in Choudhury’s approximation of the Black-Scholes CDF
    int128 private constant CDF_CONST_0 = 0x09109f285df452394; // 2260 / 3989
    int128 private constant CDF_CONST_1 = 0x19abac0ea1da65036; // 6400 / 3989
    int128 private constant CDF_CONST_2 = 0x0d3c84b78b749bd6b; // 3300 / 3989

    /**
     * @notice calculate the exponential decay coefficient for a given interval
     * @param oldTimestamp timestamp of previous update
     * @param newTimestamp current timestamp
     * @return 64x64 fixed point representation of exponential decay coefficient
     */
    function _decay(
        uint256 oldTimestamp,
        uint256 newTimestamp
    ) internal pure returns (int128) {
        return
            ONE_64x64.sub(
                (-ABDKMath64x64.divu(newTimestamp - oldTimestamp, 7 days)).exp()
            );
    }

    /**
     * @notice calculate Choudhury’s approximation of the Black-Scholes CDF
     * @param input64x64 64x64 fixed point representation of random variable
     * @return 64x64 fixed point representation of the approximated CDF of x
     */
    function _N(int128 input64x64) internal pure returns (int128) {
        // squaring via mul is cheaper than via pow
        int128 inputSquared64x64 = input64x64.mul(input64x64);

        int128 value64x64 = (-inputSquared64x64 >> 1).exp().div(
            CDF_CONST_0.add(CDF_CONST_1.mul(input64x64.abs())).add(
                CDF_CONST_2.mul(inputSquared64x64.add(THREE_64x64).sqrt())
            )
        );

        return input64x64 > 0 ? ONE_64x64.sub(value64x64) : value64x64;
    }

    /**
     * @notice calculate the price of an option using the Black-Scholes model
     * @param impliedVol uint256 representation of annualized impliedVol with a factor of 1e4
     * @param strike uint256 representation of strike price with a factor of 1e8
     * @param spot uint256 representation of spot price with a factor of 1e8
     * @param period uint256 representation of duration of option contract (in seconds)
     * @param isYes whether to price "call" or "put" option
     * @param isAbove whether to the user bets the price will stay above this strike or not
     * @return uint256 representation of Black-Scholes option price with a factor of 1e8
     */
    function blackScholesPriceBinary(
        uint256 impliedVol,
        uint256 strike,
        uint256 spot,
        uint256 period,
        bool isYes,
        bool isAbove
    ) internal pure returns (uint256) {
        int128 D8 = ABDKMath64x64.fromUInt(10 ** 8);
        int128 D4 = ABDKMath64x64.fromUInt(10 ** 4);
        int128 impliedVol64x64 = ABDKMath64x64.fromUInt(impliedVol).div(D4);
        int128 variance64x64 = impliedVol64x64.mul(impliedVol64x64);
        int128 strike64x64 = ABDKMath64x64.fromUInt(strike).div(D8);
        int128 spot64x64 = ABDKMath64x64.fromUInt(spot).div(D8);
        int128 maturity64x64 = ABDKMath64x64.fromUInt(period).div(
            ABDKMath64x64.fromUInt(365 days)
        );

        int128 premium64x64 = _blackScholesPriceBinary(
            variance64x64,
            strike64x64,
            spot64x64,
            maturity64x64,
            isYes,
            isAbove
        );
        return ABDKMath64x64.toUInt(premium64x64.mul(D8));
    }

    /**
     * @notice calculate the price of an option using the Black-Scholes model
     * @param varianceAnnualized64x64 64x64 fixed point representation of annualized variance
     * @param strike64x64 64x64 fixed point representation of strike price
     * @param spot64x64 64x64 fixed point representation of spot price
     * @param timeToMaturity64x64 64x64 fixed point representation of duration of option contract (in years)
     * @param isYes whether to price "call" or "put" option
     * @param isAbove whether to the user bets the price will stay above this strike or not
     * @return 64x64 fixed point representation of Black-Scholes option price
     */
    function _blackScholesPriceBinary(
        int128 varianceAnnualized64x64,
        int128 strike64x64,
        int128 spot64x64,
        int128 timeToMaturity64x64,
        bool isYes,
        bool isAbove
    ) internal pure returns (int128) {
        int128 cumulativeVariance64x64 = timeToMaturity64x64.mul(
            varianceAnnualized64x64
        );
        int128 cumulativeVarianceSqrt64x64 = cumulativeVariance64x64.sqrt();

        int128 d1_64x64 = spot64x64
            .div(strike64x64)
            .ln()
            .add(cumulativeVariance64x64 >> 1)
            .div(cumulativeVarianceSqrt64x64);
        int128 d2_64x64 = d1_64x64.sub(cumulativeVarianceSqrt64x64);

        if (isYes) {
            if (isAbove) {
                return _N(d2_64x64);
            } else {
                return _N(-d2_64x64);
            }
        } else {
            if (isAbove) {
                return ABDKMath64x64.fromUInt(1).sub(_N(d2_64x64));
            } else {
                return ABDKMath64x64.fromUInt(1).sub(_N(-d2_64x64));
            }
        }
    }
}

// SPDX-License-Identifier: MIT

pragma solidity ^0.8.0;

import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

import "@pythnetwork/pyth-sdk-solidity/IPyth.sol";
import "@pythnetwork/pyth-sdk-solidity/PythStructs.sol";

import "./Store.sol";

import "./interfaces/IRouter.sol";
import "./interfaces/IPool.sol";
import "./OptionMath.sol";

contract Options is ReentrancyGuard, Ownable {
    uint256 public BPS_DIVIDER = 10000;
    uint256 public size = 10000;
    uint256 public constant UNIT = 10 ** 18;

    uint32 public maxPeriod = 24 hours;
    uint32 public minPeriod = 1 minutes;

    address public gov;
    address public router;
    uint256 public fee = 80;

    uint256 public payoutBP = 8000;

    uint256 public iv;

    DataStore public Store;
    IPyth public pyth;

    int256 public fundingTracker;
    uint256 public utilizationMultiplier = 100; // in bps

    address public treasury;

    uint256 public baseSettlementFeePercentageForAbove = 50e2; // Factor of 10e2
    uint256 public baseSettlementFeePercentageForBelow = 50e2; // Factor of 10e2
    uint256 public stepSize = 25;
    uint256 public collateriaztionRatio = 100;

    uint256 public expMultiplier = 10 ** 8;

    // Events
    event PositionCreated(
        uint256 orderId,
        address indexed user,
        address indexed currency,
        uint256 marketId,
        uint256 entry,
        uint256 expiry,
        uint256 duration,
        uint256 timestamp,
        bool isLong,
        uint256 orderType,
        uint256 margin,
        bool isActive
    );
    event FeeGenerated(
        address indexed user,
        uint256 amount,
        address indexed currency,
        uint256 timestamp
    );

    event ClosePosition(
        uint256 orderId,
        address user,
        uint256 entry,
        uint256 exit,
        address currency,
        uint256 marketId,
        bool isLong,
        uint256 margin,
        bool isActive,
        uint256 earning
    );

    fallback() external payable {}

    receive() external payable {}

    constructor(address pythContract) {
        pyth = IPyth(pythContract);
    }

    function setTreasury(address _treasury) external onlyOwner {
        treasury = _treasury;
    }

    function setPyth(address _p) external onlyOwner {
        pyth = IPyth(_p);
    }

    function setPayout(uint256 x) external onlyOwner {
        require(x > 1000);
        payoutBP = x;
    }

    function setRouter(address _router) external onlyOwner {
        router = _router;
        treasury = IRouter(router).treasury();
        gov = IRouter(router).oracle();
    }

    function setKeeper(address _g) external onlyOwner {
        gov = _g;
    }

    function link(address _store) external onlyOwner {
        Store = DataStore(_store);
    }

    function estimateFee(
        uint256 margin
    ) private view returns (uint256, uint256) {
        uint256 charge = (fee * margin) / BPS_DIVIDER;
        uint256 currentMargin = margin - charge;
        return (charge, currentMargin);
    }

    function setSize(uint256 x) external onlyOwner {
        size = x;
    }

    function setPrices(
        address sender,
        bytes[] calldata priceUpdateData
    ) public payable {
        uint256 fee = IPyth(pyth).getUpdateFee(priceUpdateData);
        require(msg.value >= fee, "!fee");
        IPyth(pyth).updatePriceFeeds{value: fee}(priceUpdateData);
        if (msg.value - fee > 0) {
            (bool success, ) = payable(sender).call{value: msg.value - fee}("");
            require(success, "refund failed");
        }
    }

    function setMaxPeriod(uint32 _maxPeriod) external onlyOwner {
        require(
            _maxPeriod <= 1 days,
            "MaxPeriod should be less than or equal to 1 day"
        );
        require(
            _maxPeriod >= minPeriod,
            "MaxPeriod needs to be greater than or equal the min period"
        );
        maxPeriod = _maxPeriod;
    }

    function setMinPeriod(uint32 _minPeriod) external onlyOwner {
        require(
            _minPeriod >= 1 minutes,
            "MinPeriod needs to be greater than 1 minute"
        );
        minPeriod = _minPeriod;
    }

    function setIV(uint256 _iv) external onlyOwner {
        iv = _iv;
    }

    function configure(
        uint256 _baseSettlementFeePercentageForAbove,
        uint256 _baseSettlementFeePercentageForBelow,
        uint256 _exp
    ) external onlyOwner {
        require(10e2 <= _baseSettlementFeePercentageForAbove, "minimum payout");
        require(_baseSettlementFeePercentageForAbove <= 50e2, "!exceeds max");
        baseSettlementFeePercentageForAbove = _baseSettlementFeePercentageForAbove; // Percent with a factor of 1e2

        require(10e2 <= _baseSettlementFeePercentageForBelow, "minimum payout");
        require(
            _baseSettlementFeePercentageForBelow <= 50e2,
            "!exceeds max payout"
        );
        baseSettlementFeePercentageForBelow = _baseSettlementFeePercentageForBelow;

        expMultiplier = _exp;
    }

    function getMinMargin(address token) public view returns (uint256) {
        DataStore.CurrenciesData memory currencies = Store.getCurrencyData(
            token
        );
        return currencies.minMargin;
    }

    function getUtilization(address token) public view returns (uint256) {
        address pool = IRouter(router).getPool(token);
        (uint256 poolOI, uint256 multiplier) = IPool(pool).getPoolOI();
        uint256 utilization = (poolOI * multiplier) /
            IERC20(token).balanceOf(pool);
        return utilization;
    }

    function CreateOption(
        DataStore.OptionData memory optionsData,
        bytes[] calldata priceUpdateData
    ) public payable nonReentrant {
        require(Store.isSupportedCurrency(optionsData.currency) != false, "!supported");

        require(optionsData.user != address(0), "!adress");

        require(optionsData.duration <= maxPeriod, "! max period");
        require(optionsData.duration >= minPeriod, "!min period");

        setPrices(msg.sender, priceUpdateData);

        (uint256 feeCharge, uint256 currentMargin) = estimateFee(
            optionsData.margin
        );

        require(optionsData.margin > feeCharge, "!fee amount");

        DataStore.MarketData memory market = Store.getMarket(
            optionsData.marketId
        );
 
        require(optionsData.margin >= getMinMargin(optionsData.currency), "!margin");

        uint256 poolUtilization = getUtilization(optionsData.currency);
        require(poolUtilization < 10 ** 4, "!utilization");

        (uint256 pythPrice, ) = _getPythPrice(market.pythFeed);

        require(pythPrice > 0, "!price zero");

        optionsData.strikePrice = pythPrice;
        optionsData.expiration = block.timestamp + optionsData.duration;
        optionsData.margin = currentMargin;
        optionsData.user = msg.sender;
        optionsData.isExpired = false;
        optionsData.expiryPrice = 0;

        emit FeeGenerated(
            msg.sender,
            feeCharge,
            optionsData.currency,
            block.timestamp
        );

        transferToTreasury(optionsData.currency, feeCharge);
        _transferIn(optionsData.currency, currentMargin);

        // market entry
        uint256 orderId;
        optionsData.user = msg.sender;
        orderId = Store.addOrder(optionsData);

        Store.incrementOpenInterest(
            optionsData.marketId,
            optionsData.currency,
            currentMargin,
            optionsData.isLong
        );

        IncrementPoolOpenInterest(currentMargin, optionsData.currency, false);

        emit PositionCreated(
            orderId,
            msg.sender,
            optionsData.currency,
            optionsData.marketId,
            pythPrice,
            optionsData.expiration,
            block.timestamp,
            optionsData.duration,
            optionsData.isLong,
            optionsData.orderType,
            currentMargin,
            optionsData.isActive
        );
    }

    function evaluatePayout(
        uint256 margin
    ) public view returns (uint256 payout) {
        payout = (payoutBP * margin) / BPS_DIVIDER;
        return payout;
    }

    function evaluateProfit(
        uint256 id
    ) public view returns (uint256 profit, bool isNegative) {
        DataStore.OptionData memory option = Store.getOrder(id);
        require(option.user != address(this), "!zero address");
        require(option.expiryPrice > 0, "!expiry price");

        if (option.isLong == true) {
            if (option.expiryPrice >= option.strikePrice) {
                isNegative = false;
            } else {
                isNegative = true;
            }
        } else {
            if (option.strikePrice >= option.expiryPrice) {
                isNegative = false;
            } else {
                isNegative = true;
            }
        }

        if (isNegative == false) {
            profit = evaluatePayout(option.margin);
        } else {
            profit = 0;
        }

        return (profit, isNegative);
    }

    function exerciseOption(
        uint256 id,
        bytes[] calldata priceUpdateData
    ) public payable onlyGov  {
        DataStore.OptionData memory option = Store.getOrder(id);

        require(option.user != address(this), "!contract address");
        require(option.user != address(0),"!zero address");
        require(option.margin > 0, "!margin");
        require(option.isActive == true, "!not active");
        require(option.isExpired == true, "!not expired");
        require(block.timestamp >= option.expiration, "!expiration time");

        address pool = IRouter(router).getPool(option.currency);

        setPrices(msg.sender, priceUpdateData);

        (uint256 earning, bool status) = evaluateProfit(id);

        if (status == false) {
            Store.removeOrder(id);
            _transferOut(option.currency, option.user, option.margin);
            if (earning > 0) {
                IPool(pool).creditUserProfit(option.user, earning);
            }
            IncrementPoolOpenInterest(option.margin, option.currency, true);
            Store.decreaseOpenIntrest(
                option.marketId,
                option.currency,
                option.margin,
                option.isLong
            );

            emit ClosePosition(
                id,
                option.user,
                option.strikePrice,
                option.expiryPrice,
                option.currency,
                option.marketId,
                option.isLong,
                option.margin,
                option.isActive,
                earning
            );
        } else {
             _transferOut(option.currency, treasury, option.margin);
            Store.removeOrder(id);
            emit ClosePosition(
                id,
                option.user,
                option.strikePrice,
                option.expiryPrice,
                option.currency,
                option.marketId,
                option.isLong,
                option.margin,
                option.isActive,
                earning
            );

            IncrementPoolOpenInterest(option.margin, option.currency, true);
            Store.decreaseOpenIntrest(
                option.marketId,
                option.currency,
                option.margin,
                option.isLong
            );



        }
    }

    function checkExpiry(uint256 id) public view returns (bool) {
        DataStore.OptionData memory option = Store.getOrder(id);
        require(option.user != address(0));
        if (block.timestamp >= option.expiration) {
            return true;
        } else {
            return false;
        }
    }

    function getExpiredOptions(
        uint256[] memory orderIds
    ) external view returns (uint256[] memory isExpired) {
        for (uint256 i = 0; i < orderIds.length; i++) {
            uint256 id = orderIds[i];
            isExpired = new uint256[](orderIds.length);
            DataStore.OptionData memory option = Store.getOrder(id);
            if (option.user != address(0)) {
                bool check = checkExpiry(id);
                if (check == true) {
                    isExpired[i] = id;
                }
            }
        }
        return isExpired;
    }

    function updateOption(
        uint256[] memory orderIds,
        bytes[] calldata priceUpdateData
    ) public payable  onlyGov {
        setPrices(msg.sender, priceUpdateData);
        for (uint256 i = 0; i < orderIds.length; i++) {
            uint256 id = orderIds[i];
            DataStore.OptionData memory option = Store.getOrder(id);
            bool check = checkExpiry(id);
            if (
                option.user != address(0) &&
                check == true 
            ) {
                uint256 closePrice = getPythPrice(option.marketId);
                require(closePrice > 0,"!zero price for updating expiry");
                Store.setOptionExpiry(option.orderId, closePrice);
            }
        }
    }

    function currencyData(
        address token
    ) external view returns (DataStore.CurrenciesData memory currencies) {
        currencies = Store.getCurrencyData(token);
        return currencies;
    }

    function getOrder(
        uint256 id
    ) external view returns (DataStore.OptionData memory _orders) {
        DataStore.OptionData memory order = Store.getOrder(id);
        return order;
    }

    function getAllOrders()
        external
        view
        returns (DataStore.OptionData[] memory _orders)
    {
        DataStore.OptionData[] memory orders = Store.getOrders();
        return orders;
    }

    function getMarket(
        uint256 id
    ) external view returns (DataStore.MarketData memory market) {
        DataStore.MarketData memory data = Store.getMarket(id);
        return data;
    }

    function getUserOrdersFromStore(
        address user
    ) public view returns (DataStore.OptionData[] memory _users) {
        return Store.getUserOrders(user);
    }

    function getOILong(
        uint256 marketId,
        address currency
    ) external view returns (uint256) {
        return Store.getOpenIntrestLong(marketId, currency);
    }

    function getOIShort(
        uint256 marketId,
        address currency
    ) external view returns (uint256) {
        return Store.getOpenIntrestShort(marketId, currency);
    }

    /// @dev Returns pyth price converted to 18 decimals
    function _getPythPrice(
        bytes32 priceFeedId
    ) public view returns (uint256, uint256) {
        // It will revert if the price is older than maxAge
        PythStructs.Price memory retrievedPrice = pyth.getPriceUnsafe(
            priceFeedId
        );
        uint256 baseConversion = 10 **
            uint256(int256(18) + retrievedPrice.expo);

        // Convert price to 18 decimals
        uint256 price = uint256(retrievedPrice.price * int256(baseConversion));
        uint256 publishTime = retrievedPrice.publishTime;

        return (price, publishTime);
    }

    function getPythPrice(uint256 marketId) public view returns (uint256) {
        DataStore.MarketData memory market = Store.getMarket(marketId);
        (uint256 data, ) = _getPythPrice(market.pythFeed);
        return data;
    }

    function IncrementPoolOpenInterest(
        uint amount,
        address currency,
        bool isDecrease
    ) private {
        address pool = IRouter(router).getPool(currency);
        IPool(pool).updateOpenInterest(amount, isDecrease);
    }

    function _transferOut(
        address currency,
        address to,
        uint256 amount
    ) internal {
        if (amount == 0 || currency == address(0) || to == address(0)) return;
        IERC20(currency).transfer(to, amount);
    }

    function _transferIn(address currency, uint256 amount) internal {
        if (amount == 0 || currency == address(0)) return;
        IERC20(currency).transferFrom(msg.sender, address(this), amount);
    }

    function transferToTreasury(address currency, uint256 amount) internal {
        if (amount == 0 || currency == address(0)) return;
        IERC20(currency).transferFrom(msg.sender, treasury, amount);
    }

    function sendToTreasury(address currency, uint256 amount) internal {
        if (amount == 0 || currency == address(0)) return;
        IERC20(currency).transfer(treasury, amount);
    }

    modifier onlyGov() {
        require(msg.sender == gov, "!gov");
        _;
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";

contract DataStore {
    using EnumerableSet for EnumerableSet.UintSet;
    using EnumerableSet for EnumerableSet.Bytes32Set;
    using EnumerableSet for EnumerableSet.AddressSet;
    using SafeERC20 for IERC20;

    uint256 public constant BPS_DIVIDER = 10000;
    address public gov;

    address public trade;
    address public pool;

    uint256 public poolWithdrawalFee = 10; // in bps
    uint256 public minimumMarginLevel = 2000; // 20% in bps, at which account is liquidated

    address[] public currencies;
    mapping(address => bool) public supported;
    mapping(uint256 => bool ) public isActive;

    // Funding
    uint256 public constant fundingInterval = 1 hours; // In seconds.

    mapping(address => mapping(uint256 => int256)) private fundingTrackers; // asset => market => funding tracker (long) (short is opposite)
    mapping(address => mapping(uint256 => uint256)) private fundingLastUpdated; // asset => market => last time fundingTracker was updated. In seconds.

    mapping(address => mapping(uint256 => uint256))
        public currencyOpenIntrestLong;
    mapping(address => mapping(uint256 => uint256))
        public currencyOpenIntrestShort;

    mapping(uint256 => uint256) public openIntrestLong;
    mapping(uint256 => uint256) public openIntrestShort;

    mapping(bytes32 => bool) public isMarketAdded;
    mapping(address => bool) public tokenAdded;


    struct CurrenciesData {
        address token;
        uint256 minMargin;
        bool supported;
    }

    struct MarketData {
        uint256 marketType; // 1 = crypto , 2 = forex
        string symbol;
        bytes32 pythFeed;
        uint256 maxOI;
        uint256 fee; // in bps
        uint256 fundingFactor; // Yearly funding rate if OI is completely skewed to one side. In bps.
        uint256 minSize;
        uint256 minSettlementTime; // time before keepers can execute order (price finality) if chainlink price didn't change
        uint256 maxDeviation;
    }

    struct OptionData {
        uint256 orderId;
        address user;
        address currency;
        uint256 marketId;
        uint256 strikePrice; 
        bool isLong; 
        uint8 orderType; // 0 = market, 1 = limit, 2 = stop
        uint256 margin;
        uint256 timestamp;
        uint256 duration;
        uint256 expiration;
        bool isActive;
        bool isExpired;
        uint256 expiryPrice;
    }

    struct PositionData {
        address user;
        string market;
        uint256 marketId;
        bool isLong;
        uint256 size;
        uint256 margin;
        int256 fundingTracker;
        uint256 price;
        uint256 timestamp;
    }

    mapping(uint256 => PositionData) public userPositonItem;
    EnumerableSet.UintSet private positionKeys; // [position keys..]
    mapping(address => EnumerableSet.UintSet) private userPositionIds;

    mapping(uint256 => OptionData) public orders;
    mapping(address => EnumerableSet.UintSet) private userOrderIds;
    EnumerableSet.UintSet private orderIdsSet;

    uint256 public markertPairId;
    uint256 public tokenId;


    uint256[] public marketList; // "ETH-USD", "BTC-USD", etc
    mapping(uint256 => MarketData) public marketItems;
    mapping(address => CurrenciesData) public currencyData;

    mapping(address => uint256) private balances; // user => amount
    mapping(address => uint256) private lockedMargins; // user => amount

    uint256 public orderId;

    constructor() {
        gov = msg.sender;
    }

    function linkData(address _trading) external onlyGov {
        trade = _trading;
        // pool = _pool;
    }


    function addMarket(MarketData memory marketInfo) external onlyGov {
        uint256 id = markertPairId++;
        require(
            isMarketAdded[marketInfo.pythFeed] == false,
            "feed has been added"
        );
        marketItems[id] = marketInfo;
        marketList.push(id);
        isMarketAdded[marketInfo.pythFeed] = true;
    }

    function updateMarket(
        uint256 martketId,
        MarketData memory marketInfo
    ) external onlyGov {
        require(
            isMarketAdded[marketInfo.pythFeed] == true,
            "feed has not been added"
        );
        marketItems[martketId] = marketInfo;
    }

    function removeMarket(bytes32 feed, uint256 id) external onlyGov {
        require(isMarketAdded[feed] == true, "feed has not been added");
        delete marketItems[id];
        isMarketAdded[feed] = false;
        marketList[id] = marketList[marketList.length - 1];
        marketList.pop();
    }

    function getMarketPairs()
        external
        view
        returns (MarketData[] memory pairs)
    {
        uint256 length = marketList.length;
        pairs = new MarketData[](length);
        for (uint256 i = 0; i < length; i++) {
            pairs[i] = marketItems[marketList[i]];
        }
        return pairs;
    }

    function getMarketPairsData() external view returns (uint256[] memory) {
        return marketList;
    }


    function getMarket(
        uint256 id
    ) external view returns (MarketData memory _marketData) {
        return marketItems[id];
    }


    function incrementOpenInterest(
        uint256 marketId,
        address currency,
        uint256 amount,
        bool isLong
    ) external onlyContract {
        if (isLong) {
            openIntrestLong[marketId] += amount;
            currencyOpenIntrestLong[currency][marketId] += amount;
        } else {
            openIntrestShort[marketId] += amount;
            currencyOpenIntrestShort[currency][marketId] += amount;
        }
    }

    function decreaseOpenIntrest(
        uint256 marketId,
        address currency,
        uint256 amount,
        bool isLong
    ) external onlyContract {
        if (isLong) {
            if (amount > currencyOpenIntrestLong[currency][marketId]) {
                currencyOpenIntrestLong[currency][marketId] = 0;
            } else {
                currencyOpenIntrestLong[currency][marketId] -= amount;
            }
        } else {
            if (amount > currencyOpenIntrestShort[currency][marketId]) {
                currencyOpenIntrestShort[currency][marketId] = 0;
            } else {
                currencyOpenIntrestShort[currency][marketId] -= amount;
            }
        }
    }

    function getOpenIntrestLong(
        uint256 marketId,
        address currency
    ) external view returns (uint256) {
        return currencyOpenIntrestLong[currency][marketId];
    }

    function getOpenIntrestShort(
        uint256 marketId,
        address currency
    ) external view returns (uint256) {
        return currencyOpenIntrestShort[currency][marketId];
    }


    function getOrder(
        uint256 id
    ) external view returns (OptionData memory _orders) {
        return orders[id];
    }

    function getCurrencyData (address token) public view
     returns (CurrenciesData memory currency){
        currency = currencyData[token];
        return currency;
    }

    function getOrders() external view returns (OptionData[] memory _orders) {
        uint256 length = orderIdsSet.length();
        _orders = new OptionData[](length);
        for (uint256 i = 0; i < length; i++) {
            _orders[i] = orders[orderIdsSet.at(i)];
        }
        return _orders;
    }

    function getLimitOrders() external view returns (OptionData[] memory _orders) {
         uint256 length = orderIdsSet.length();
        _orders = new OptionData[](length);
        for (uint256 i = 0; i < length; i++) {
            if (orders[orderIdsSet.at(i)].isActive == false){
            _orders[i] = orders[orderIdsSet.at(i)];
            }
        }
        return _orders;
    }

    function activateLimitOrders(uint256 id) external onlyContract {
        OptionData storage order = orders[id];
        require(order.strikePrice > 0, "!price");
        require(order.orderType == 1, "!limit");
        order.isActive = true;
    }

    function setOptionExpiry(uint256 id,uint256 exitPrice) external onlyContract {
        OptionData storage order = orders[id];
        require(order.strikePrice > 0, "!price");
        order.isExpired = true;
        order.expiryPrice = exitPrice;
    }

    function getUserOrders(
        address user
    ) external view returns (OptionData[] memory _userOrders) {
        uint256 length = userOrderIds[user].length();
        _userOrders = new OptionData[](length);

        for (uint256 i = 0; i < length; i++) {
            _userOrders[i] = orders[userOrderIds[user].at(i)];
        }
        return _userOrders;
    }

    function getUserPositions(
        address user
    ) external view returns (PositionData[] memory _positions) {
        uint256 length = userPositionIds[user].length();
        _positions = new PositionData[](length);
        for (uint256 i = 0; i < length; i++) {
            _positions[i] = userPositonItem[userPositionIds[user].at(i)];
        }
        return _positions;
    }

    function addOrder(
        OptionData memory order
    ) external onlyContract returns (uint256) {
        uint256 nextOrderId = ++orderId;
        order.orderId = nextOrderId;
        userOrderIds[order.user].add(nextOrderId);
        orders[nextOrderId] = order;
        orderIdsSet.add(nextOrderId);
        return nextOrderId;
    }

    function removeOrder(uint256 _orderId) external onlyContract {
        OptionData memory order = orders[_orderId];
        userOrderIds[order.user].remove(_orderId);
        orderIdsSet.remove(_orderId);
        delete orders[_orderId];
    }

    function addOrUpdateUserPosition(
        address user,
        uint256 nextPositionId
    ) external onlyContract {
        userPositionIds[user].add(nextPositionId);
        positionKeys.add(nextPositionId);
    }

    function isSupportedCurrency(address token) external view returns (bool) {
        require(token != address(0), "!currency");
        return supported[token];
    }

    function addCurrency(CurrenciesData memory data) external onlyGov {
        require(data.token != address(0), "!currency");
        require(supported[data.token] == false, "!add");
        currencyData[data.token] = data;
        supported[data.token] = true;
    }

    function removeCurrency(address token) external onlyGov {
        require(token != address(0), "!currency");
        delete currencyData[token];
        supported[token] = false;
    }

    function updateCurrency(CurrenciesData memory data) external onlyGov {
        require(data.token != address(0), "!currency");
        require(supported[data.token] == true, "!not added");
        currencyData[data.token] = data;
        supported[data.token] = true;
    }

    function removeUserPosition(
        address user,
        uint256 id
    ) external onlyContract {
        userPositionIds[user].remove(id);
    }

    function getFundingFactor(
        uint256 marketId
    ) external view returns (uint256) {
        return marketItems[marketId].fundingFactor;
    }

    function getFundingLastUpdated(
        uint256 marketId,
        address currency
    ) external view returns (uint256) {
        return fundingLastUpdated[currency][marketId];
    }

    function getFundingTracker(
        uint256 marketId,
        address currency
    ) external view returns (int256) {
        return fundingTrackers[currency][marketId];
    }

    function setFundingLastUpdated(
        uint256 marketId,
        address currency,
        uint256 timestamp
    ) external onlyContract {
        fundingLastUpdated[currency][marketId] = timestamp;
    }

    function updateFundingTracker(
        uint256 marketId,
        address currency,
        int256 fundingIncrement
    ) external onlyContract {
        fundingTrackers[currency][marketId] += fundingIncrement;
    }

    // Mods
    modifier onlyContract() {
        require(
            msg.sender == trade || msg.sender == pool || msg.sender == gov,
            "!contract"
        );
        _;
    }

    modifier onlyGov() {
        require(msg.sender == gov, "!gov");
        _;
    }
}

