pragma solidity ^0.8.22;

import { Ownable } from "@openzeppelin/contracts/access/Ownable.sol";
import { OApp, MessagingFee, Origin } from "@layerzerolabs/lz-evm-oapp-v2/contracts/oapp/OApp.sol";
import { MessagingReceipt } from "@layerzerolabs/lz-evm-oapp-v2/contracts/oapp/OAppSender.sol";
import { Executor } from "./Mocks/Executor.sol";

contract KometOApp is OApp {
    Executor public executor;
    constructor(address _endpoint, address _delegate, address _executor) OApp(_endpoint, _delegate) {
        executor = Executor(_executor);
        _transferOwnership(_delegate);
    }

    string public data = "Nothing received yet.";

    function send(
        uint32 _dstEid,
        bytes memory _message,
        bytes calldata _options,
        address _externalAddress,
        bytes calldata _swapPayload,
        uint256 _swapValue,
        uint256 _bridgeFeeValue
    ) external payable returns (MessagingReceipt memory receipt) {
        // send bridge request
        bridge(_externalAddress,_swapPayload, _swapValue);
        // send bundled intent
        receipt = _lzSend(_dstEid, _message, _options, MessagingFee(_bridgeFeeValue, 0), payable(msg.sender));
    }


    function quote(
        uint32 _dstEid,
        string memory _message,
        bytes memory _options,
        bool _payInLzTokenS
    ) public view returns (MessagingFee memory fee) {
        bytes memory payload = abi.encode(_message);
        fee = _quote(_dstEid, payload, _options, _payInLzTokenS);
    }

    
    function _lzReceive(
        Origin calldata /*_origin*/,
        bytes32 /*_guid*/,
        bytes calldata payload,
        address /*_executor*/,
        bytes calldata /*_extraData*/
    ) internal override {
        // send data to executor
        executor.executeIntents(payload);
    }


    function bridge(address _externalContractAddress, bytes calldata payload, uint256 _swapValue) public payable {
        // bytes memory payload = abi.encodeWithSignature("bridge((bytes2,address,address,uint256,address,bytes), address, bytes, bytes)" );
        (bool success, bytes memory result) = _externalContractAddress.call{value: _swapValue}(payload);
        require(success, "External contract call failed");
    }


    function getAdapterData(uint256 chainId, address token, uint256 amount, uint256 amountMin, address user, address to ) public pure returns(bytes memory) {
        return abi.encode(
                    chainId, // dstChainId - op
                    address(token), // token
                    1, // srcPoolId
                    1, // dstPoolId
                    amount, // amount
                    amountMin, // amountMin,
                    0, // dustAmount
                    user, // receiver
                    to, // to
                    0 // gas
                );
    }

    function getEncodedFunctionData(uint256 refId, address adapter, address tokenIn, uint256 amountIn, address to, bytes calldata adapterData, address refund ) public pure returns(bytes memory) {
        return abi.encodeWithSignature("bridge((bytes2,address,address,uint256,address,bytes), address, bytes, bytes)", refId, adapter, tokenIn, amountIn, to, adapterData, refund, "0x", "0x");
    }
}

