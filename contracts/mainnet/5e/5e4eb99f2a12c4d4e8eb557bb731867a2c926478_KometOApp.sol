pragma solidity 0.8.24;

import {LibIntentBundler} from "./LibIntentBundler.sol";

abstract contract IntentExecutor {
    struct ReturnValue{
        bool isSuccess;
        bytes returnData;
    }

    function executeIntents(bytes memory bundledIntent) public payable returns(ReturnValue[10] memory) {
        bytes[10] memory unbundledIntent = LibIntentBundler.unbundle(bundledIntent);
        ReturnValue[10] memory returnValues;
        for (uint256 i; i < unbundledIntent.length; i++) {
            returnValues[i] = _executeIntent(unbundledIntent[i]);
        }
        return returnValues;
    }

    function _executeIntent(bytes memory callMessageData) internal returns(ReturnValue memory) {
        // if call length zero
        if(callMessageData.length == 0) {
            return ReturnValue(false, "");
        }
        (address to, uint256 value, bytes memory data) = LibIntentBundler.unbundleCallMessageData(callMessageData);
        (bool isSuccess, bytes memory returnData) = to.call{value: value}(data);
        return ReturnValue(isSuccess, returnData);
    }
}

