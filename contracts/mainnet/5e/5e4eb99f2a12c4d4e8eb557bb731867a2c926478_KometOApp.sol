                lengthOfIntentData := shr(224, lengthDataBytes)
            }
            intents[i] = slice(
                bundledIntent,
                currentScannedLength + 4,
                lengthOfIntentData
            );
            currentScannedLength += 4 + lengthOfIntentData;
        }
    }

    function bundleCallMessageData(address to, uint256 value, bytes memory data) external pure returns(bytes memory callMessageData) {
        return abi.encode(to, value, data);
