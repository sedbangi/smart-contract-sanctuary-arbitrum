    error LibIntentBundler_IntentSizeExceeds();
    function bundle(
        bytes[] memory intents
    ) external pure returns (bytes memory bundledIntent) {
        // structure the message
        // first 4 bytes - number of intent - N
        // next N x 4 bytes - size of the intent - Si
        // Bytes after 4 + (N x 4) will represent the intent message
        bytes4 numberOfIntent;
        if(intents.length > 10) {
            revert LibIntentBundler_IntentSizeExceeds();
