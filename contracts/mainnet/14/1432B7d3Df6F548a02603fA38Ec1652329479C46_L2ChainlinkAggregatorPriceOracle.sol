pragma solidity ^0.8.20;

import { AggregatorV3Interface } from "../../dependencies/@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";
import { Math } from "../../dependencies/@openzeppelin/contracts/utils/math/Math.sol";

import { IOracle } from "../../interfaces/oracle/IOracle.sol";
import { IPriceOracle } from "../../interfaces/oracle/IPriceOracle.sol";
import { USD, USDMath } from "../../libraries/USDMath.sol";
import { Normalizer } from "../../libraries/Normalizer.sol";

import "hardhat/console.sol";

contract ChainlinkAggregatorPriceOracle is IPriceOracle {

  using Normalizer for uint256;
  using USDMath for USD;

  AggregatorV3Interface immutable public _baseAggregator;

  AggregatorV3Interface immutable public _ratioAggregator;

  uint256 public immutable _timeout;

  constructor(AggregatorV3Interface baseAggregator, AggregatorV3Interface ratioAggregator, uint256 timeout) {
    _baseAggregator = baseAggregator;
    _ratioAggregator = ratioAggregator;
    _timeout = timeout;
  }

  /// @inheritdoc IPriceOracle
  function getLatestPrice() external view returns (USD price, IOracle.Confidence confidence) {
    require(_baseAggregator.decimals() == USDMath.DECIMALS, "invalid price feed decimals");

    ( , int256 answer, , uint256 timestamp, ) = _baseAggregator.latestRoundData();

    price = USD.wrap(answer < 0 ? 0 : uint256(answer));

    confidence = getConfidence(answer, timestamp);

    if (address(_ratioAggregator) != address(0x0)) {
      ( , answer, , timestamp, ) = _ratioAggregator.latestRoundData();

      price = normalize(price, answer);

      confidence = confidence == IOracle.Confidence.STALE
        ? confidence
        : getConfidence(answer, timestamp); 
    }
  }

  function normalize(USD current, int256 answer) private view returns (USD) {
    uint8 decimals = _ratioAggregator.decimals();

    uint8 max = decimals > USDMath.DECIMALS ? decimals : USDMath.DECIMALS;

    uint256 ratio = (answer < 0 ? 0 : uint256(answer)).normalize(decimals, max);

    uint256 price = current.normalize(max) * ratio / 10 ** max;

    return USD.wrap(price.normalize(max, USDMath.DECIMALS));
  }

  function getConfidence(int256 answer, uint256 timestamp) internal virtual view returns (IOracle.Confidence) {
    bool isStale = timestamp > block.timestamp 
      ? true 
      : block.timestamp - timestamp > _timeout;

    return answer < 0 || isStale
      ? IOracle.Confidence.STALE
      : IOracle.Confidence.OK;
  }

  /// @inheritdoc IOracle
  function getStatus() public virtual view returns (Status status, uint256 changedAt) {
    return (IOracle.Status.ONLINE, 0);
  }  
}

// SPDX-License-Identifier: GPL-3.0
pragma solidity ^0.8.20;

import { AggregatorV3Interface } from "../../dependencies/@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";
import { IOracle } from "../../interfaces/oracle/IOracle.sol";
import { IPriceOracle } from "../../interfaces/oracle/IPriceOracle.sol";
import { USD, USDMath } from "../../libraries/USDMath.sol";
import { ChainlinkAggregatorPriceOracle } from "./ChainlinkAggregatorPriceOracle.sol";

contract L2ChainlinkAggregatorPriceOracle is ChainlinkAggregatorPriceOracle {

  AggregatorV3Interface immutable public _sequencerUptime;

  uint256 public immutable _gracePeriod;

  constructor(
    AggregatorV3Interface baseAggregator, 
    AggregatorV3Interface ratioAggregator, 
    uint256 timeout, 
    AggregatorV3Interface sequencerUptime, 
    uint256 gracePeriod) 
  ChainlinkAggregatorPriceOracle(baseAggregator, ratioAggregator, timeout) 
  {
    _sequencerUptime = sequencerUptime;
    _gracePeriod = gracePeriod;
  }

  function getConfidence(int256 answer, uint256 timestamp) internal override view returns (IOracle.Confidence) {
    if (answer < 0) {
      return IOracle.Confidence.STALE;
    }

    // check the sequencer status 
    (IOracle.Status status, uint256 changedAt) = getStatus();

    if (status == IOracle.Status.OFFLINE) {
      return IOracle.Confidence.OFFLINE;
    }

    // if the sequencer is online but the grace period hasnt expired
    // then we still treat this as being OFFLINE from a confidence 
    if (block.timestamp - changedAt <= _gracePeriod) {
      return IOracle.Confidence.OFFLINE;
    }

    // check for staleness of the price
    if (block.timestamp - timestamp > _timeout) {
      return IOracle.Confidence.STALE;
    }

    return IOracle.Confidence.OK;
  }

  /// @inheritdoc IOracle
  function getStatus() public override view returns (IOracle.Status status, uint256 changedAt) {
    (, int256 answer, uint256 startedAt, , ) = _sequencerUptime.latestRoundData();

    status = answer == 0 ? IOracle.Status.ONLINE : IOracle.Status.OFFLINE;
    changedAt = startedAt;
  }  
}

