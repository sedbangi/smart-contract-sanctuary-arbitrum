/* assume fees to be paid in ETH */
abstract contract ERC20HXFeesOracle is ERC20HX, ReentrancyGuard {
    address WETH9 = 0x82aF49447D8a07e3bd95BD0d56f35241523fBab1;
    address UNISWAP_V3_FACTORY = 0x1F98431c8aD98523631AE4a59f267346ea31F984;
    uint32 twapPeriod = 3600;
    uint24 uniswapV3PoolFeeTier = 3000;

    bool enableOracle;
    uint256 fixedFee;

    struct FeesInfo {
        address payable receiver;
        uint96 weight;
    }

    FeesInfo[] public feesInfo;

    // Public functions callable by the owner only
    function setWETH9(address _WETH9) public onlyOwner {
        _setWETH9(_WETH9);
    }

    function setUniV3Factory(address _UNISWAP_V3_FACTORY) public onlyOwner {
        _setUniV3Factory(_UNISWAP_V3_FACTORY);
    }

    function setUniV3PoolFeeTier(uint24 _uniswapV3PoolFeeTier) public onlyOwner {
        _setUniV3PoolFeeTier(_uniswapV3PoolFeeTier);
    }

    function setTWAPPeriod(uint32 _period) public onlyOwner {
        _setTWAPPeriod(_period);
    }

    // Internal functions that perform the actual assignments
    function _setWETH9(address _WETH9) internal {
        WETH9 = _WETH9;
    }

    function _setUniV3Factory(address _UNISWAP_V3_FACTORY) internal {
        UNISWAP_V3_FACTORY = _UNISWAP_V3_FACTORY;
    }

    function _setUniV3PoolFeeTier(uint24 _uniswapV3PoolFeeTier) internal {
        uniswapV3PoolFeeTier = _uniswapV3PoolFeeTier;
    }

    function _setTWAPPeriod(uint32 _period) internal {
        twapPeriod = _period;
    }

    function _totalWeight() internal view virtual returns (uint96) {
        uint96 totalWeight = 0;
        for (uint i = 0; i < feesInfo.length; i++)
            totalWeight += feesInfo[i].weight;

        return totalWeight;
    }

    function _pushFeeInfo(
        address payable receiver,
        uint96 feeNumerator
    ) internal virtual {
        require(receiver != address(0), "ERC20HXFees: Invalid parameters");

        feesInfo.push(FeesInfo(receiver, feeNumerator));
    }

    function _setEnableOracle(bool value) internal {
        address _pool = getDefaultPool();
        require(
            Address.isContract(_pool),
            "ERC20HXFeesOracle: Invalid pool"
        );
        enableOracle = value;
    }

    function setEnableOracle(bool value) public onlyOwner {
        _setEnableOracle(value);
    }

    function pushFeeInfo(
        address payable[] calldata receivers,
        uint96[] calldata weights
    ) public virtual onlyOwner {
        require(
            receivers.length == weights.length,
            "ERC20HXFees: receivers length must be equal to feeumerators"
        );

        for (uint i = 0; i < receivers.length; i++)
            _pushFeeInfo(receivers[i], weights[i]);
    }

    function deleteFeeInfo() public virtual onlyOwner {
        delete feesInfo;
    }

    function setFixedFee(uint256 weiFormatAmount) public onlyOwner {
        fixedFee = weiFormatAmount;
    }

    function getDefaultPool() public view virtual returns (address) {
        return
            IUniswapV3Factory(UNISWAP_V3_FACTORY).getPool(
                address(this),
                WETH9,
                uniswapV3PoolFeeTier
            );
    }

    function _getFeeOracle() internal view returns (uint256 feeAmount) {
        address _pool = getDefaultPool();
        require(
            Address.isContract(_pool),
            "ERC20HXFeesOracle: Not a contract"
        );
        uint32 oldestSecondsAgo = oldestObservationSecondsAgo(_pool);
        int24 tick;

        if (oldestSecondsAgo < twapPeriod) {
            (tick, ) = OracleLibrary.consult(_pool, oldestSecondsAgo);
        } else {
            (tick, ) = OracleLibrary.consult(_pool, twapPeriod);
        }

        return
            OracleLibrary.getQuoteAtTick(
                tick,
                1_000_000_000_000_000_000,
                address(this),
                WETH9
            ) / 100;
    }

    function oldestObservationSecondsAgo(
        address pool
    ) public view returns (uint32) {
        return OracleLibrary.getOldestObservationSecondsAgo(pool);
    }

    function getFeeOracle() public view returns (uint256 feeAmount) {
        return _getFeeOracle();
    }

    function fee(uint256 amount) public view virtual returns (uint256) {
        if (!enableOracle) {
            return (amount * fixedFee);
        }

        uint256 twapFee = _getFeeOracle();

        return (amount * twapFee);
    }

    function _handleFeesEth(uint256 _fee) internal virtual {
        require(msg.value >= _fee, "ERC20HXFees: Insufficient fees");

        if (msg.value > _fee) {
            uint256 refund = (msg.value - _fee);
            (bool sent, ) = _msgSender().call{value: refund}("");

            require(sent, "ERC20HXFees: Failure to refund");
        }

        uint256 totalWeight = _totalWeight();
        for (uint i = 0; i < feesInfo.length; i++) {
            uint256 recv = (_fee * feesInfo[i].weight) / totalWeight;
            (bool sent, ) = (feesInfo[i].receiver).call{value: recv}("");

            require(sent, "ERC20HXFees: Failure to transfer ETH");
        }
    }

    function burnNFT(
        uint256[] calldata nftIds
    ) public payable override nonReentrant {
        uint256 _fee = fee(nftIds.length);

        _handleFeesEth(_fee);

        super.burnNFT(nftIds);
    }

    function mintNFT(uint256 amount) public payable override nonReentrant {
        uint256 _fee = fee(amount);

        _handleFeesEth(_fee);

        super.mintNFT(amount);
    }

    function burnNFTFrom(
        address from,
        address to,
        uint256[] calldata nftIds
    ) public payable override nonReentrant {
        uint256 _fee = fee(nftIds.length);

        _handleFeesEth(_fee);

        super.burnNFTFrom(from, to, nftIds);
    }

    function mintNFTFrom(
        address from,
        address to,
        uint256 amount
    ) public payable override nonReentrant {
        uint256 _fee = fee(amount);

        _handleFeesEth(_fee);

        super.mintNFTFrom(from, to, amount);
    }
}


