pragma solidity 0.8.17;

import "../interfaces/IBridge.sol";
import "../interfaces/IRouter.sol";
import "../Helpers/ReentrancyGuard.sol";
import "../libraries/SafeERC20.sol";
import "../libraries/LibData.sol";
import "../libraries/LibPlexusUtil.sol";
// import "@openzeppelin/[email protected]/utils/Strings.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "hardhat/console.sol";

contract RouterFacet is IBridge, ReentrancyGuard {
    using SafeERC20 for IERC20;

    IRouter private immutable router;

    constructor(IRouter _assetForwarder) {
        router = _assetForwarder;
    }

    function bridgeToRouter(BridgeData memory _bridgeData, RouterData memory _routerData) external payable nonReentrant {
        LibPlexusUtil._isTokenDeposit(_bridgeData.srcToken, _bridgeData.amount);
        _RouterStart(_bridgeData, _routerData);
    }

    function swapAndBridgeToRouter(
        SwapData calldata _swap,
        BridgeData memory _bridgeData,
        RouterData memory _routerData
    ) external payable nonReentrant {
        LibPlexusUtil._isSwapTokenDeposit(_swap.input);
        _bridgeData.amount = LibPlexusUtil._bridgeSwapStart(_swap);
        if (_swap.dup[0].token == _swap.output[0].dstToken) {
            LibPlexusUtil._isTokenDeposit(_swap.dup[0].token, _swap.dup[0].amount);
            _bridgeData.amount = _bridgeData.amount + _swap.dup[0].amount;
        }
        _RouterStart(_bridgeData, _routerData);
    }

    function _RouterStart(BridgeData memory _bridgeData, RouterData memory _routerData) internal {
        bool isNotNative = !LibPlexusUtil._isNative(_bridgeData.srcToken);

        DepositData memory depositData = DepositData({
            partnerId: 1,
            amount: _bridgeData.amount,
            destAmount: _bridgeData.amount,
            srcToken: _bridgeData.srcToken,
            refundRecipient: _bridgeData.recipient,
            destChainIdBytes: _toBytes32(uint256(_bridgeData.dstChainId))
        });

        bytes memory destTokenBytes = abi.encodePacked(bytes20(_routerData.destToken));
        bytes memory recipientBytes = abi.encodePacked(bytes20(_bridgeData.recipient));

        if (isNotNative) {
            IERC20(_bridgeData.srcToken).safeApprove(address(router), _bridgeData.amount);
            router.iDeposit(depositData, destTokenBytes, recipientBytes);
            IERC20(_bridgeData.srcToken).safeApprove(address(router), 0);
        } else {
            router.iDeposit{value: _bridgeData.amount}(depositData, destTokenBytes, recipientBytes);
        }
        emit LibData.Bridge(msg.sender, _bridgeData.dstChainId, _bridgeData.srcToken, _bridgeData.amount, _bridgeData.plexusData, "Router");
    }

    function _toBytes32(uint256 num) public pure returns (bytes32) {
        string memory numAsString = Strings.toString(num);
        bytes memory tempEmptyStringTest = bytes(numAsString);
        bytes32 converted = 0x0;
        for (uint i = 0; i < tempEmptyStringTest.length; i++) {
            converted |= bytes32(tempEmptyStringTest[i] & 0xFF) >> (i * 8);
        }
        return converted;
    }
}

