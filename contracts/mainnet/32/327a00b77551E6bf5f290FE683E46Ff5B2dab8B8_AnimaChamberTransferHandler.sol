pragma solidity ^0.8.17;

import "../Manager/ManagerModifier.sol";
import "../Utils/MetadataUtils.sol";
import "../ERC721/AbstractERC721Metadata.sol";
import "./IAnimaChamberData.sol";
import "../Armory/IArmory.sol";
import "./IAnimaChamberTransferHandler.sol";
import "./IAnimaStaker.sol";
import "../Armory/IArmoryERC721OwnerCheck.sol";

contract AnimaChamberTransferHandler is
  IAnimaChamberTransferHandler,
  ManagerModifier
{
  IArmory public ARMORY;
  IArmoryERC721OwnerCheck public OWNER_CHECK;
  address public REALM;

  constructor(
    address _manager,
    address _armory,
    address _ownerCheck,
    address _realm
  ) ManagerModifier(_manager) {
    ARMORY = IArmory(_armory);
    OWNER_CHECK = IArmoryERC721OwnerCheck(_ownerCheck);
    REALM = _realm;
  }

  // @dev Unstake all entities from the ARMORY when transferring a chamber
  function handleBeforeTransfer(
    address _sender,
    address _from,
    address _to,
    uint256 _startTokenId,
    uint256 _quantity
  ) external override onlyManager {
    for (uint256 i = 0; i < _quantity; i++) {
      uint256 tokenId = _startTokenId + i;
      (bool isStaked, address ownerAddress, uint ownerId) = OWNER_CHECK.ownerOf(
        msg.sender,
        tokenId
      );

      if (!isStaked) {
        continue;
      }

      ARMORY.unstake(
        _from,
        ownerAddress,
        ownerId,
        new bytes32[](0),
        msg.sender,
        tokenId,
        1
      );
    }
  }
}

