// SPDX-License-Identifier: UNLICENSED
pragma solidity ^0.8.13;
import {IERC20} from "@openzeppelin-contracts/interfaces/IERC20.sol";
import {ERC20} from "@openzeppelin-contracts/token/ERC20/ERC20.sol";
import {Ownable} from "@openzeppelin-contracts/access/Ownable.sol";


contract LPToken is ERC20, Ownable{

    struct Fifo {
        uint128 head;
        uint128 tail;
    }

    struct TimeCert {
        uint until;
        uint256 amount;
    }

    mapping(address=>Fifo) public lockQueue;
    mapping(address=>uint256) public freeAmount;
    mapping(uint256=>TimeCert) public timeCerts;
    uint lockTime;

    constructor(string memory name_, uint lockTime_) ERC20(name_, name_) Ownable(msg.sender) {
        lockTime = lockTime_;
    }
    
    function _free_to_spend(uint until) private view returns (bool) {
        return until <= block.timestamp;
    }

    function _addr_to_uint(address account) private pure returns (uint256) {
        return uint256(uint160(account));
    }

    function _get_head(address account) private view returns (TimeCert storage) {
        Fifo storage q = lockQueue[account];
        return timeCerts[_addr_to_uint(account) + q.head];
    }

    function freeBalanceOf(address account) public view returns (uint256 amount) {
        // todo: 到期数额 + 自由数额
        Fifo storage q = lockQueue[account];
        amount = freeAmount[account];
        for(uint i = q.head; i < q.tail; i++){
            TimeCert storage tc = timeCerts[_addr_to_uint(account) + i];
            if (!_free_to_spend(tc.until))
                break;
            amount += timeCerts[_addr_to_uint(account) + i].amount;
        }
    }

    function _append_lock_queue(address account, uint256 value) private {
        Fifo storage q = lockQueue[account];
        uint until =  block.timestamp + lockTime;
        timeCerts[_addr_to_uint(account) + q.tail] = TimeCert(until, value);
        q.tail += 1;
        // _mint(account, value);
    }

    function _pop_lock_queue(address account, uint256 value) private returns (uint256 remain) {
        Fifo storage q = lockQueue[account];
        if (q.head < q.tail) { // not empty
            TimeCert storage tc = timeCerts[_addr_to_uint(account) + q.head];
            if (_free_to_spend(tc.until)) { // ok to pop
                if (tc.amount <= value) { // pop
                    q.head += 1;
                    remain = value - tc.amount;
                } else { // subtract
                    tc.amount -= value;
                    remain = 0;
                }
                // _burn(account, value - remain);
                return remain;
            }
        }
        // did not pop, try free amount
        require(freeAmount[account] >= value, "insufficient free balance");
        freeAmount[account] -= value;
        // _burn(account, value);
        remain = 0;
        return remain;
    }

    function mint(address account, uint256 value) public onlyOwner {
        _mint(account, value);
    }

    function burn(address account, uint256 value) public onlyOwner {
        _burn(account, value);
    }

    function _update(address from, address to, uint256 value) internal override {
        if (from == address(0)){
            _append_lock_queue(to, value);
            super._update(from, to, value);
        } else {
            uint current = value;
            while(current > 0) {
                uint remain = _pop_lock_queue(from, current);
                current = remain;
            }
            super._update(from, to, value);
            if (to != address(0)){
                freeAmount[to] += value;
            }
        }

        
    }
}

