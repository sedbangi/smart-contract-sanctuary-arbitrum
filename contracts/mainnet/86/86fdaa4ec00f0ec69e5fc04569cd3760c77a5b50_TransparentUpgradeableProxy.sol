pragma solidity ^0.8.0;

import {PointsAuction} from "src/PointsAuction.sol";
import {OwnableUpgradeable} from "@openzeppelin-contracts-upgradeable/access/OwnableUpgradeable.sol";
import {Initializable} from "@openzeppelin-contracts-upgradeable/proxy/utils/Initializable.sol";
import {PointType, Bid, AuctionInfo, AuctionState} from "../src/types/Bid.sol";
import {Reward} from "../src/Reward.sol";
import {BankVault} from "../src/BankVault.sol";

contract Controller is PointsAuction, Reward, BankVault, Initializable, OwnableUpgradeable {
    
    /* 与 owner 分离，负责开启、关闭拍卖、更新分数。 */
    address public manager;
    event Charge(address from, PointType pt, uint price, uint points);
    event Refund(address to, PointType pt, uint price, uint points);

    /**
     * @dev Throws if called by any account other than the owner.
     */
    modifier onlyManager() {
        if (msg.sender != manager) {
            revert("unauthorized error");
        }
        _;
    }

    function initialize(address ezEth, address manager_, uint lockTime) public initializer {
        __Ownable_init(msg.sender);
        __BankVault_init(ezEth, lockTime);
        _addPointsTracker(PointType.ElPoints, "ELP");
        _addPointsTracker(PointType.EzPoints, "EZP");
        manager = manager_;
    }

    function assignManager(address newManager) external onlyOwner {
        manager = newManager;
    }
    function updatePoints(PointType[] memory pointType, int256[] memory _currentPoints) external onlyManager {
        _updatePoints(pointType, _currentPoints);
    }

    function createAuction(PointType[] memory _pointType, AuctionInfo[] memory _auctionInfo) external onlyManager {
        _createAuction(_pointType, _auctionInfo);
    }

    function cancelAuction(PointType[] memory pointType) external onlyManager {
        _cancelAuction(pointType);
    }

    function _trackPoints(address to, PointType pt, int amount) internal override {
        require(amount > 0);
        _mintTracker(to, pt, uint(amount));
    }

    function _refund(address to, PointType pt, uint price, uint points) internal override {
        if (price * points > 0){
            _refund(to, price * points / 1 ether);
            emit Refund(to, pt, price, points);
        }
    }

    function _safeToCommit() internal view override returns(bool){
        return _activeAuctionCount() == 0;
    }

    function endAuction(PointType pointType) external {
        _endAuction(pointType);
        _commitPending();
    }

    // price: wei/point
    function placeBid(uint256 price, int256 points, PointType pointType) external{
        require(msg.sender != owner(), "Owner not allow to bid");
        require(points > 0);
        uint totalPrice = price * uint(points) / 1e18;
        require(totalPrice > 0);
        _charge(msg.sender, totalPrice);
        emit Charge(msg.sender, pointType, price, uint(points));
        _placeBid(price, points, pointType);
    }

    // suppliers

    function deposit(uint amountInEzEth) external {
        _deposit(amountInEzEth);
    }

    function withdraw(uint amountInLPToken) external {
        _withdraw(amountInLPToken);
    } 

    // claim

    // 设置空投token，关联特定点数
    function addRedeemable(PointType pt, address token) external onlyOwner {
        _addRedeemable(pt, token);
    }

    // 添加点数类型
    function addPointType(PointType pt) external onlyOwner { 
        _addPointType(pt, _totalSupplyOfTracker(pt)); 
    }

    // 开放兑换流程
    function openClaim() external onlyOwner {
        _openClaim();
    }

    // 奖励领取前，扣除点数
    function _burnPoints(address owner, PointType pt, uint balance) internal override {
        _burnTracker(owner, pt, balance);
    }
    // 获取点数
    function _getBalanceOf(address owner, PointType pt) internal override view returns (uint) {
        return _balanceOfTracker(owner, pt);
    }

    // version 
    function version() public virtual pure returns (uint) {
        return 1;
    }
}

