pragma solidity ^0.8.0;

import {PointType, Bid, BidTracker, AuctionInfo, AuctionState} from  "src/types/Bid.sol";
import {SafeERC20} from "@openzeppelin-contracts/token/ERC20/utils/SafeERC20.sol";
import {IERC20} from "@openzeppelin-contracts/interfaces/IERC20.sol";

struct Redeemable {
    address token;
    uint totalValue;
}
abstract contract Reward {
    using SafeERC20 for IERC20;
    mapping(PointType => Redeemable[]) public redeemables;
    mapping(PointType => uint) public totalPoints;
    mapping(address=>uint) internal tokenUsed;
    PointType[] public pointTypes;
    uint public claimOpened;
    function _getBalanceOf(address owner, PointType pt) internal virtual view returns (uint);
    function _burnPoints(address owner, PointType pt, uint balance) internal virtual;
    function _claimTo(address to, Redeemable storage redeemable, uint balance, uint total) private {
        require(claimOpened > 0);
        uint value = balance * redeemable.totalValue / total; // todo: overflow??
        IERC20(redeemable.token).safeTransfer(to, value);
    }

    function _claimTo(address to) private {
        address owner = msg.sender;
        for (uint i = 0; i < pointTypes.length; ++i){
            uint balance = _getBalanceOf(owner, pointTypes[i]);
            if (balance > 0 && totalPoints[pointTypes[i]] > 0) {
                 _burnPoints(owner, pointTypes[i], balance);
                for (uint j = 0; j < redeemables[pointTypes[i]].length; j++){
                    _claimTo(to, redeemables[pointTypes[i]][j], balance, totalPoints[pointTypes[i]]);
                }
            }
        }
    }

    function claim() public {
        _claimTo(msg.sender);
    }
    
    function claimTo(address to) public {
        _claimTo(to);
    }

    function _addPointType(PointType pt, uint total) internal {
        require(claimOpened == 0);
        require(totalPoints[pt] == 0, "PointType already added");
        require(total > 0);
        totalPoints[pt] = total;
        pointTypes.push(pt);
    }
    
    function _addRedeemable(PointType pt, address token) internal {
        require(claimOpened == 0);
        require(tokenUsed[token] == 0);
        redeemables[pt].push(Redeemable(
            token,
            IERC20(token).balanceOf(address(this))
        ));
        tokenUsed[token] = 1;
    }

    function _openClaim() internal {
        claimOpened = 1;
    }
}

