    * @notice The function inserts the bid into the bid tracker's heap.
    * @notice The remaining points to sell are updated by subtracting the bid's points.
    * @notice The heap is then sorted based on the price of the bids, with the highest price at the top.
    * @notice If the remaining points to sell become non-positive after inserting the bid, the function evicts bids from the bottom of the heap until the remaining points become positive again.
    * @notice The evicted bids are returned in an array.
    * @notice The bid tracker's remaining points to sell are updated with the final value.
    */
    function _insertBid(PointType pt, Bid memory bid) private {
        BidTracker storage bt = bidTrackers[pt];
        // 剩余未出售点数、可能为负数，最后在 endAuction 处理
        int256 remainingPoints = bidTrackers[pt].info.pointsToSell;
        bt.heap.push(bid);
        // 先占坑
        remainingPoints -= bid.points;

        // console2.log("sender", msg.sender);
        // console2.log("  remainingPoints", remainingPoints);
        // console2.log("  heap.length", bt.heap.length);

        // 按价格冒泡
        for(uint i = bt.heap.length - 1; i > 0; i--){
            if (bt.heap[i].price > bt.heap[i-1].price){
                _swap(bt.heap[i], bt.heap[i-1]);
                // console2.log("  swap happened");
            }
        }

        // console2.log("  heap[0] is", bt.heap[0].bidder);
        uint currentLen = bt.heap.length;
        // 如果退款了点数还是负数，说明是多出来的
        while(remainingPoints + bt.heap[currentLen - 1].points <= 0){
            remainingPoints += bt.heap[currentLen - 1].points;
            currentLen--;
        }
        // 多出来的退款
        while (currentLen < bt.heap.length) {
            _refund(bt.heap[bt.heap.length - 1].bidder, pt, bt.heap[bt.heap.length - 1].price, uint(bt.heap[bt.heap.length - 1].points));
            bt.heap.pop();
        }
        bidTrackers[pt].info.pointsToSell = remainingPoints;
        if (bt.heap.length > 500){
            revert("Too many bids");
        }
    }

