contract PTToken is LocalToken {
    constructor(string memory name_) LocalToken(name_) {}
    function _update(address from, address to, uint256 value) internal override onlyOwner {
        super._update(from, to, value);
    }
}

abstract contract BankVault {

    using SafeERC20 for IERC20;
    event Dividend(uint amount, uint current);
    event Withdraw(address to, uint lpt, uint ez);
    event Deposit(address from, uint lpt, uint ez);

    IERC20 public eth; 
    LPToken public lpt;
    uint public trackedBalance;
    uint internal pending;
    mapping(PointType=>PTToken) internal pointsTrackers;

    function __BankVault_init(address ethToken, uint lockTime) internal {
        eth = IERC20(ethToken);
        lpt = new LPToken("LPT", lockTime);
    }

    function _addPointsTracker(PointType pt, string memory name_) internal {
        require(address(pointsTrackers[pt]) == address(0));
        pointsTrackers[pt] = new PTToken(name_);
    }

    // ratio = lpt.totalSupply() / eth.blanceOf(this)
    function _deposit(uint amountInEth) internal {
        eth.safeTransferFrom(msg.sender, address(this), amountInEth);
        uint lptToMint = amountInEth;
        if (lpt.totalSupply() == 0) trackedBalance = 0;
        if (trackedBalance > 0) {
            lptToMint = lpt.totalSupply() * amountInEth / trackedBalance;
        }
        lpt.mint(msg.sender, lptToMint);
        trackedBalance += amountInEth;
        emit Deposit(msg.sender, lptToMint, amountInEth);
    }

    function _withdraw(uint amountInLpt) internal {
        require(lpt.totalSupply() > 0);
        uint ethToWithdraw = amountInLpt * trackedBalance / lpt.totalSupply();
        lpt.burn(msg.sender, amountInLpt);
        eth.safeTransfer(msg.sender, ethToWithdraw);
        trackedBalance -= ethToWithdraw;
        emit Withdraw(msg.sender, amountInLpt, ethToWithdraw);
        if (lpt.totalSupply() == 0) trackedBalance = 0;
    }

    function _charge(address from, uint amountInEth) internal {
        eth.safeTransferFrom(from, address(this), amountInEth);
        pending += amountInEth;
    }

    function _refund(address to, uint amountInEth) internal {
        eth.safeTransfer(to, amountInEth);
        pending -= amountInEth;
    }

    function _addInterest(uint amountInEth) internal {
        if (lpt.totalSupply() > 0 && amountInEth > 0){
            emit Dividend(amountInEth, trackedBalance);
            trackedBalance += amountInEth;
        }
    }

    function _safeToCommit() internal virtual returns(bool);
    function _commitPending() internal {
        if (_safeToCommit()){
            _addInterest(pending);
            pending = 0;
        }
    } 

    function _mintTracker(address to, PointType pt, uint amount) internal {
        pointsTrackers[pt].mint(to, amount);
    }

    function _burnTracker(address to, PointType pt, uint amount) internal {
        pointsTrackers[pt].burn(to, amount);
    }

    function _totalSupplyOfTracker(PointType pt) internal view returns (uint){
        return pointsTrackers[pt].totalSupply();
    }
    function _balanceOfTracker(address owner, PointType pt) internal view returns (uint){
        return pointsTrackers[pt].balanceOf(owner);
    }

    function getTracker(PointType pt) public view returns (address) {
        return address(pointsTrackers[pt]);
    }

    function getMainToken() public view returns (address){
        return address(eth);
    }

    function getLpToken() public view returns (address){
        return address(lpt);
    }

    function toMainToken(uint amountInLpt) public view returns (uint) {
        if (lpt.totalSupply() == 0) return 0;
        return amountInLpt * trackedBalance / lpt.totalSupply();
    }

    function toLpToken(uint amountInEth) public view returns (uint) {
        if (trackedBalance == 0) return 0;
        return amountInEth * lpt.totalSupply() / trackedBalance;
    }
}

