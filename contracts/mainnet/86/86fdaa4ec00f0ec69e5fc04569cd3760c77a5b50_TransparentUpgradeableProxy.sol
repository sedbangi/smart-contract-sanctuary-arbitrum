import {console2} from "@forge-std/console2.sol";
import {PointType, Bid, BidTracker, AuctionInfo, AuctionState} from  "src/types/Bid.sol";


abstract contract PointsAuction {

    event Deal(address bidder, PointType pt, uint price, uint amount);

    mapping(PointType => BidTracker) public bidTrackers;

    function _swap(Bid storage a, Bid storage b) private {
        Bid memory tmp = Bid(b.bidder, b.price, b.points);
        b.bidder = a.bidder;
        b.points = a.points;
        b.price = a.price;
        a.bidder = tmp.bidder;
        a.points = tmp.points;
        a.price = tmp.price;
    }

    function _refund(address bidder, PointType pt, uint price, uint amount) internal virtual;
    /**
    * @dev Inserts a bid into the bid tracker for the specified point type.
    * @param pt The type of points the bid is for.
