contract LocalToken is ERC20, Ownable{
    constructor(string memory name_) ERC20(name_, name_) Ownable(msg.sender) {}
    function mint(address account, uint256 value) public onlyOwner { _mint(account, value); }
    function burn(address account, uint256 value) public onlyOwner { _burn(account, value); }
