    * @notice The duration of the auction must be less than 24 hours.
    * @notice The start time of the auction must be within 24 hours from the current block timestamp.
    */
    function _createAuction(
            PointType[] memory _pointType,
            AuctionInfo[] memory _auctionInfo
        ) internal {
        for(uint8 i = 0; i < _pointType.length; i++){
            BidTracker storage bt = bidTrackers[_pointType[i]];
            require(bt.info.state == AuctionState.Idle, "Auction already created");
            require(block.timestamp < _auctionInfo[i].startTime, "Invalid start time");
            require(_auctionInfo[i].duration < 3600 * 24, "Duration too long");
            require(_auctionInfo[i].startTime - block.timestamp < 3600 * 24, "Start time too far away");
