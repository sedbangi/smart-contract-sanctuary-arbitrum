pragma solidity ^0.8.4;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "./GameTicketNFT.sol";
import "hardhat/console.sol";
import "./IGame.sol";
import "../utils/VRFConsumerBaseV2Upgradeable.sol";

contract Game is IGame, Initializable, OwnableUpgradeable, VRFConsumerBaseV2Upgradeable, ReentrancyGuardUpgradeable {

    // List of ticket types
    Ticket[] public tickets;

    // Mapping from ticket ID to ticket instance
    mapping(uint256 => TicketInstance) public ticketInstances;

    // Mapping from user address to array of ticket IDs
    mapping(address => uint256[]) public userTickets;

    // Mapping from user address to total won value
    mapping(address => uint256) public userTotalWonValue;

    event ReceivedEther(address sender, uint256 amount);

    VRFCoordinatorV2Interface private coordinator;
    uint64 private subscriptionId;
    bytes32 private keyHash;
    uint16 private minimumRequestConfirmations;
    uint32 private callbackGasLimit;

    function initialize(
        address _coordinator,
        uint64 _subscriptionId,
        bytes32 _keyHash,
        uint16 _minimumRequestConfirmations
    ) public initializer {
        __Ownable_init();
        __ReentrancyGuard_init();
        __VRFConsumerBaseV2Upgradeable_init(_coordinator);

        // Your initialization logic
        callbackGasLimit = 500000;
        coordinator = VRFCoordinatorV2Interface(_coordinator);
        subscriptionId = _subscriptionId;
        keyHash = _keyHash;
        minimumRequestConfirmations = _minimumRequestConfirmations;
    }

    receive() external payable nonReentrant {
        emit ReceivedEther(msg.sender, msg.value);
    }

    function withdraw(uint256 amount) external onlyOwner {
        require(address(this).balance >= amount, "Insufficient balance in the contract");
        payable(owner()).transfer(amount);
    }

    function createTicketType(uint256 price, address nftAddress, uint256 nftId) external onlyOwner {
        Ticket storage newTicket = tickets.push();
        newTicket.price = price;
        newTicket.nftAddress = nftAddress;
        newTicket.nftId = nftId;
    }

    function updateTicketType(uint256 ticketType, uint256 price, address nftAddress, uint256 nftId) external onlyOwner {
        tickets[ticketType].price = price;
        tickets[ticketType].nftAddress = nftAddress;
        tickets[ticketType].nftId = nftId;
    }


    function addPrizeToTicketType(uint256 ticketType, uint256 prizeType, uint256 amount, uint256 winRate, address tokenAddress, uint256 tokenId, uint256 estimatedValue) external onlyOwner {
        require(winRate <= 10000, "winRate should be between 0 and 10000");
        Ticket storage ticket = tickets[ticketType];
        ticket.prizes.push(Prize(ticket.prizes.length, prizeType, amount, winRate, tokenAddress, tokenId, estimatedValue));
    }

    function updatePrizeInTicket(uint256 ticketType, uint256 prizeId, uint256 prizeType, uint256 amount, uint256 winRate, address tokenAddress, uint256 tokenId, uint256 estimatedValue) external onlyOwner {
        require(winRate <= 10000, "winRate should be between 0 and 10000");
        Ticket storage ticket = tickets[ticketType];
        require(prizeId < ticket.prizes.length, "Invalid prize index");
        ticket.prizes[prizeId].prizeType = prizeType;
        ticket.prizes[prizeId].amount = amount;
        ticket.prizes[prizeId].winRate = winRate;
        ticket.prizes[prizeId].tokenAddress = tokenAddress;
        ticket.prizes[prizeId].tokenId = tokenId;
        ticket.prizes[prizeId].estimatedValue = estimatedValue;
    }

    function updateTicketPrice(uint256 ticketType, uint256 newPrice) internal onlyOwner {
        require(ticketType < tickets.length, "Invalid ticketType");
        tickets[ticketType].price = newPrice;
    }

    function updatePrizeAmount(uint256 ticketType, uint256 prizeIndex, uint256 newAmount) internal onlyOwner {
        require(ticketType < tickets.length, "Invalid ticketType");
        require(prizeIndex < tickets[ticketType].prizes.length, "Invalid prizeIndex");
        tickets[ticketType].prizes[prizeIndex].amount = newAmount;
    }

    function updateAllPricesAndAmounts(
        uint256[] memory prices,
        uint256[][] memory prizes
    ) external onlyOwner {
        require(
            tickets.length == prices.length &&
            tickets.length == prizes.length,
            "All input arrays must have the same length"
        );

        for (uint256 i = 0; i < tickets.length; i++) {
            updateTicketPrice(i, prices[i]);
            for (uint256 j = 0; j < prizes[i].length; j++) {
                updatePrizeAmount(i, j, prizes[i][j]);
            }
        }
    }

    function buyTicket(uint256 ticketType) external payable nonReentrant override returns (uint256) {
        require(ticketType < tickets.length, "Invalid ticketType");
        Ticket memory ticket = tickets[ticketType];

        bool usedNFT = false;
        if(ticket.nftAddress != address(0) && GameTicketNFT(ticket.nftAddress).balanceOf(msg.sender, ticket.nftId) >= 1) {
            GameTicketNFT(ticket.nftAddress).burn(msg.sender, ticket.nftId, 1); // Assuming burning 1 NFT per ticket
            usedNFT = true;
        } else {
            require(msg.value >= ticket.price, "Insufficient funds sent");
        }

        uint256 ticketId = _requestRandomWords();
        _createTicketInstance(ticketId, msg.sender, ticketType);
        userTickets[msg.sender].push(ticketId);

        if (usedNFT) {
            emit TicketBoughtWithNFT(ticketId, msg.sender, ticketType, ticket.nftAddress, ticket.nftId);
        } else {
            emit TicketBoughtWithETH(ticketId, msg.sender, ticketType, msg.value);
        }
        return ticketId;
    }

    function _requestRandomWords() private returns (uint256) {
        return coordinator.requestRandomWords(
            keyHash,
            subscriptionId,
            minimumRequestConfirmations,
            callbackGasLimit,
            1
        );
    }

    // Helper function to manage ticketInstances creation
    function _createTicketInstance(uint256 ticketId, address owner, uint256 ticketType) private {
        ticketInstances[ticketId] = TicketInstance({
            owner: owner,
            ticketType: ticketType,
            fulfilled: false,
            error: "",
            timestamp: block.timestamp,
            wonPrize: Prize({prizeId: 0, prizeType: 0, amount: 0, winRate: 0, tokenAddress: address(0), tokenId: 0, estimatedValue: 0})
        });
    }


    function _assignPrize(uint256 ticketId, TicketInstance storage ticketInstance, Prize memory prize) private returns (bool, string memory) {
        Prize storage wonPrize = ticketInstance.wonPrize;
        wonPrize.prizeId = prize.prizeId;
        wonPrize.prizeType = prize.prizeType;
        wonPrize.amount = prize.amount;
        wonPrize.winRate = prize.winRate;
        wonPrize.tokenAddress = prize.tokenAddress;
        wonPrize.tokenId = prize.tokenId;
        wonPrize.estimatedValue = prize.estimatedValue;

        address winner = ticketInstance.owner;
        if (prize.prizeType == 1) {
            //console.log("[Game] _assignPrize prizeType 1", prize.amount);
            (bool success, ) = payable(winner).call{value: prize.amount}("");
            if (!success) {
                emit ErrorOccurred(ticketId, "Failed to transfer Ether");
                return (false, "Failed to transfer Ether");
            }
        } else if (prize.prizeType == 2) {
            //console.log("[Game] _assignPrize prizeType 2", prize.amount);
            try IERC20(prize.tokenAddress).transfer(winner, prize.amount) {} catch {
                emit ErrorOccurred(ticketId, "Failed to transfer ERC20 Token");
                return (false, "Failed to transfer ERC20 Token");
            }
        } else if (prize.prizeType == 3) {
            //console.log("[Game] _assignPrize prizeType 3", prize.amount);
            try IERC1155(prize.tokenAddress).safeTransferFrom(address(this), winner, prize.tokenId, prize.amount, "") {} catch {
                emit ErrorOccurred(ticketId, "Failed to transfer ERC1155 Token");
                return (false, "Failed to transfer ERC1155 Token");
            }
        } else if (prize.prizeType == 4) {
            try GameTicketNFT(prize.tokenAddress).mint(winner, prize.tokenId, prize.amount, "") {} catch {
                emit ErrorOccurred(ticketId, "Failed to mint ERC1155 Ticket");
                return (false, "Failed to mint ERC1155 Ticket");
            }
        }

        // Update the user's total won value
        userTotalWonValue[ticketInstance.owner] += prize.estimatedValue;

        //console.log("[Game] _assignPrize emit PrizeAssigned", ticketId, winner, prize.prizeType);
        emit PrizeAssigned(ticketId, winner, prize.prizeId);
        return (true, "");
    }

    function _determinePrize(uint256 ticketId, uint256 randomness, Ticket memory ticket) internal returns (bool, Prize memory, string memory) {
        uint256 totalWinRate;
        for (uint256 i = 0; i < ticket.prizes.length; i++) {
            totalWinRate += ticket.prizes[i].winRate;
        }
        if(totalWinRate > 10000) {
            emit ErrorOccurred(ticketId, "Total winRate cannot exceed 100");
            return (false, Prize({prizeId: 0, prizeType: 0, amount: 0, winRate: 0, tokenAddress: address(0), tokenId: 0, estimatedValue: 0}), "Total winRate cannot exceed 100");
        }

        uint256 random = randomness % 10000;
        uint256 accumulatedWinRate = 0;
        for (uint256 i = 0; i < ticket.prizes.length; i++) {
            accumulatedWinRate += ticket.prizes[i].winRate;
            if (random < accumulatedWinRate) {
                return (true, ticket.prizes[i], "");
            }
        }


        return (true, Prize({prizeId: 0, prizeType: 0, amount: 0, winRate: 0, tokenAddress: address(0), tokenId: 0, estimatedValue:0 }), "No prize won");
    }

    function _determineAndAssignPrize(uint256 ticketId, uint256 randomness, TicketInstance storage ticketInstance) internal returns (bool, string memory) {
        Ticket memory ticket = tickets[ticketInstance.ticketType];

        // console.log("[Game] _determineAndAssignPrize", ticketId, ticketInstance.ticketType);
        (bool success, Prize memory wonPrize, string memory error) = _determinePrize(ticketId, randomness, ticket);
        if(!success) {
            // console.log("[Game] _determineAndAssignPrize error:", error);
            return (false, error);
        }

        // console.log("[Game] _determineAndAssignPrize _assignPrize");
        (bool assignSuccess, string memory assignError) = _assignPrize(ticketId, ticketInstance, wonPrize);
        if(!assignSuccess) {
            // console.log("[Game] _determineAndAssignPrize _assignPrize error:", assignError);
            return (false, assignError);
        }

        return (true, "");
    }

    function fulfillRandomWords(uint256 ticketId, uint256[] memory randomWords) internal override {
        // console.log("[Game] fulfillRandomWords", ticketId, randomWords[0]);
        TicketInstance storage instance = ticketInstances[ticketId];
        if (instance.owner == address(0) || instance.fulfilled) {
            instance.error = "Invalid request ID or already fulfilled";
            // console.log("[Game] fulfillRandomWords error:", instance.error);
            return;
        }
        instance.fulfilled = true;
        instance.timestamp = block.timestamp;

        (bool success, string memory error) = _determineAndAssignPrize(ticketId, randomWords[0], instance);
        if(!success) {
            instance.error = error;
            // console.log("[Game] fulfillRandomWords _determineAndAssignPrize error:", instance.error);
        }
    }


    function getTicketInstance(uint256 ticketId) external view override returns (TicketInstance memory) {
        return ticketInstances[ticketId];
    }

    function getTickets() external view override returns (Ticket[] memory) {
        return tickets;
    }

    function getPrizesForTicketType(uint256 ticketType) external view override returns (Prize[] memory) {
        return tickets[ticketType].prizes;
    }

    // Function to retrieve the latest tickets for a user
    function getLatestUserTickets(address user, uint256 offset, uint256 limit) external view override returns (TicketHistoryInstance[] memory) {
        uint256[] memory ticketIds = userTickets[user];
        uint256 count = ticketIds.length;

        // If the offset is greater than the number of tickets, return an empty array
        if (offset >= count) {
            return new TicketHistoryInstance[](0);
        }

        // Calculate the number of tickets to return
        uint256 resultCount = count - offset;
        if (resultCount > limit) {
            resultCount = limit;
        }

        // Create an array to hold the result
        TicketHistoryInstance[] memory result = new TicketHistoryInstance[](resultCount);

        // Iterate over the user's tickets in reverse order, starting from the offset
        for (uint256 i = 0; i < resultCount; i++) {
            result[i] = TicketHistoryInstance(
                ticketIds[count - offset - i - 1],
                ticketInstances[ticketIds[count - offset - i - 1]]
            );
        }

        return result;
    }

    function getTotalWonValue(address user) external view override returns (uint256) {
        return userTotalWonValue[user];
    }

}

