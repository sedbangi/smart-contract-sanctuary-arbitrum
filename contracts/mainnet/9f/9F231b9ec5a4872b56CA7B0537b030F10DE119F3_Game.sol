interface IGame {
    event TicketBoughtWithNFT(uint256 ticketId, address buyer, uint256 ticketType, address nftAddress, uint256 nftId);
    event TicketBoughtWithETH(uint256 ticketId, address buyer, uint256 ticketType, uint256 value);
    event PrizeAssigned(uint256 ticketId, address winner, uint256 prizeId);
    event ErrorOccurred(uint256 ticketId, string error);

    struct Ticket {
        uint256 price;
        Prize[] prizes;
        address nftAddress; // The address of the associated NFT contract.
        uint256 nftId;      // The id of the associated NFT.
    }

    struct TicketInstance {
        address owner;
        uint256 ticketType;
        bool fulfilled;
        string error;
        uint256 timestamp;
        Prize wonPrize; // added to store the prize that was won
    }

    struct TicketHistoryInstance {
        uint256 ticketId;
        TicketInstance ticketInstance;
    }

    struct Prize {
        uint256 prizeId;
        uint256 prizeType; // 0: No prize, 1: ETH, 2: ERC20, 3: ERC1155, 4: Mint New ERC1155 Ticket
        uint256 amount;
        uint256 winRate;
        address tokenAddress;
        uint256 tokenId;
        uint256 estimatedValue;
    }

    function getTickets() external view returns (Ticket[] memory);
    function getPrizesForTicketType(uint256 ticketType) external view returns (Prize[] memory);

    function buyTicket(uint256 ticketType) external payable returns (uint256);
    function getTicketInstance(uint256 ticketId) external view returns (TicketInstance memory);
    function getLatestUserTickets(address user, uint256 offset, uint256 limit) external view returns (TicketHistoryInstance[] memory);
    function getTotalWonValue(address user) external view returns (uint256);
}

