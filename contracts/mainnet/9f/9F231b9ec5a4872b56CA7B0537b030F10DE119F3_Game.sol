import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Burnable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract GameTicketNFT is ERC1155Burnable, Ownable {

    constructor() ERC1155("https://openvault.win/api/token/{id}.json") {}

    function mint(address account, uint256 id, uint256 amount, bytes memory data) external onlyOwner  {
        _mint(account, id, amount, data);
    }

    function burn(
        address account,
        uint256 id,
        uint256 amount
    ) public override {
        require(_msgSender() == account || owner() == _msgSender() || isApprovedForAll(account, _msgSender()), "Not owner or approved");
        _burn(account, id, amount);
    }
}

