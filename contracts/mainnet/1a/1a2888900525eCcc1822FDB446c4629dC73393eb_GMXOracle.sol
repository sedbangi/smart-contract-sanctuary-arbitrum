    PriceProps memory tokenOutPrice
  ) external view returns (int256, int256);

  function getOpenInterestWithPnl(
    address dataStore,
    MarketProps memory market,
    PriceProps memory indexTokenPrice,
    bool isLong,
    bool maximize
  ) external view returns (int256);
}

