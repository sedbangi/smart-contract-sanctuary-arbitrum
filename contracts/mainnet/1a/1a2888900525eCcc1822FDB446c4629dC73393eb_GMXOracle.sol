    uint256 feeAmountForPool;
    uint256 amountAfterFees;

    address uiFeeReceiver;
    uint256 uiFeeReceiverFactor;
    uint256 uiFeeAmount;
  }

  struct ExecutionPriceResult {
    int256 priceImpactUsd;
    uint256 priceImpactDiffUsd;
    uint256 executionPrice;
  }

  function getMarkets(
