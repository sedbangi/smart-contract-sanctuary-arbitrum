pragma solidity 0.8.24;

import {IERC20Meta} from "../interfaces/IERC20.sol";

library Util {
    /**
     * 
     * @param _returnData T
     */
    function _getRevertMsg(bytes memory _returnData) internal pure returns (string memory) {
        // If the _res length is less than 68, then the transaction failed silently (without a revert message)
        if (_returnData.length < 68) return 'Transaction reverted silently';

        assembly {
            // Slice the sighash.
            _returnData := add(_returnData, 0x04)
        }
        return abi.decode(_returnData, (string)); // All that remains is the revert string
    }
    function AddrToString(address _addr) internal pure returns (string memory) {
        bytes32 value = bytes32(uint256(uint160(_addr)));
        bytes memory alphabet = "0123456789abcdef";
         
        bytes memory str = new bytes(42);
        str[0] = '0';
        str[1] = 'x';
         
        for (uint256 i = 0; i < 20; i++) {
            str[2 + i * 2] = alphabet[uint8(value[i + 12] >> 4)];
            str[3 + i * 2] = alphabet[uint8(value[i + 12] & 0x0f)];
        }
         
        return string(str);
    }
    function sortTokens (address _t0, address _t1) internal pure returns (address token0, address token1) {
        if (_t0 < _t1) {
            token0 = _t0;
            token1 = _t1;
        } else {
            token1 = _t0;
            token0 = _t1;
        }
    }
    function getTicker (address _token0, address _token1) internal view returns (string memory ticker) {
        (address token0, address token1) = sortTokens(_token0, _token1);
        ticker = string.concat(string.concat(IERC20Meta(token0).symbol(), "_"), IERC20Meta(token1).symbol());
    } 
    
    function toString(uint256 value) internal pure returns (string memory) {
        if (value == 0) {
            return "0";
        }
 
        uint256 temp = value;
        uint256 digits;
 
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
 
        bytes memory buffer = new bytes(digits);
 
        while (value != 0) {
            digits--;
            buffer[digits] = bytes1(uint8(48 + (value % 10)));
            value /= 10;
        }
 
        return string(buffer);
    }

}

