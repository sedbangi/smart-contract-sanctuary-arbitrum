error InvalidExchangePoolRangeValues(uint start, uint stop);

error ExchangeAlreadyAdded(ExchangeInfo exchange);
error ExchangePoolAlreadyAdded();
error ExchangeNotYetImplemented();

enum ExchangeType {
    UNISWAP_V2, UNISWAP_V3, BALANCER_V2, CAMELOT_V3
}

enum ExchangePoolType {
    UNISWAP_V2_PAIR,
    UNISWAP_V3_POOL,
    BALANCER_V2_POOL,
    CAMELOT_V3_POOL
}
struct ReserveInfo {
    uint reserve0;
