import {ERC721} from "@solady/src/tokens/ERC721.sol";
import {ECDSA} from "@solady/src/utils/ECDSA.sol";
import {EIP712Upgradeable} from
    "@openzeppelin/contracts-upgradeable/utils/cryptography/EIP712Upgradeable.sol";
import {Initializable} from "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import {AccessControlUpgradeable} from
    "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import {IAccessControl} from "@openzeppelin/contracts/access/IAccessControl.sol";
import {IERC721} from "@openzeppelin/contracts/interfaces/IERC721.sol";
import {UUPSUpgradeable} from "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import {ReentrancyGuardUpgradeable} from
    "@openzeppelin/contracts-upgradeable/utils/ReentrancyGuardUpgradeable.sol";

/// @title Infinity
contract Infinity is
    ERC721,
    AccessControlUpgradeable,
    UUPSUpgradeable,
    EIP712Upgradeable,
    ReentrancyGuardUpgradeable
{
    using ECDSA for bytes32;

    error Infinity__IsNotSigner();
    error Infinity__NonceAlreadyUsed();
    error Infinity__TransferFailed();

    uint256 internal s_nextTokenId;

    bytes32 public constant SIGNER_ROLE = keccak256("SIGNER");
    bytes32 public constant UPGRADER_ROLE = keccak256("UPGRADER");

    string public constant TOKEN_URI = "ipfs://QmYZQHUHa2ZeP2GZL4MkJJHQm8KDyhcmMM8WG4NAKfLKzJ";

    bytes32 internal constant REQ_HASH = keccak256("MintRequest(uint256 nonce,address toAddress)");

    struct MintRequest {
        uint256 nonce;
        address toAddress;
    }

    mapping(uint256 => uint256) internal s_questIssueNumbers;
    mapping(uint256 => string) internal s_tokenURIs;
    mapping(uint256 nonce => bool isConsumed) internal s_nonces;

    /// @custom:oz-upgrades-unsafe-allow constructor
    // solhint-disable-next-line func-visibility
    constructor() {
        _disableInitializers();
    }

    /// @notice Initializes the contract
    function initialize(
        string memory _signingDomain,
        string memory _signatureVersion,
        address _admin
    ) external initializer {
        __EIP712_init(_signingDomain, _signatureVersion);
        __AccessControl_init();
        __UUPSUpgradeable_init();
        __ReentrancyGuard_init();

        _grantRole(DEFAULT_ADMIN_ROLE, _admin);
    }

    /// @notice Authorizes an upgrade to a new contract implementation
    /// @dev Overrides the UUPSUpgradeable internal function with access control.
    /// @param newImplementation Address of the new contract implementation
    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyRole(UPGRADER_ROLE)
    {}

    /// @dev Returns the token collection name.
    function name() public pure override returns (string memory) {
        return "Layer3 Infinity CUBE";
    }

    /// @dev Returns the token collection symbol.
    function symbol() public pure override returns (string memory) {
        return "POWER";
    }

    /// @notice Retrieves the URI for a given token
    /// @dev Overrides the ERC721's tokenURI method.
    /// @param _tokenId The ID of the token
    /// @return TOKEN_URI The URI of the specified token (they're all the same)
    function tokenURI(uint256 _tokenId) public pure override returns (string memory) {
        return TOKEN_URI;
    }

    /// @notice Internal function to handle the logic of minting an Infinity CUBE
    /// @param data The MintRequest struct
    /// @param signature The signature for verification
    function mintInfinityCube(MintRequest memory data, bytes memory signature)
        external
        nonReentrant
    {
        // Validate the signature to ensure the mint request is authorized
        _validateSignature(data, signature);

        // Perform the actual minting of the Infinity CUBE
        _safeMint(data.toAddress, s_nextTokenId);

        unchecked {
            ++s_nextTokenId;
        }
    }

    /// @notice Validates the signature for a minting request
    /// @dev Ensures that the signature is from a valid signer and the nonce hasn't been used before
    /// @param data The MintRequest struct containing minting details
    /// @param signature The signature to be validated
    function _validateSignature(MintRequest memory data, bytes memory signature) internal {
        address signer = _getSigner(data, signature);
        if (!hasRole(SIGNER_ROLE, signer)) {
            revert Infinity__IsNotSigner();
        }
        if (s_nonces[data.nonce]) {
            revert Infinity__NonceAlreadyUsed();
        }
        s_nonces[data.nonce] = true;
    }

    /// @notice Recovers the signer's address from the MintRequest and its associated signature
    /// @dev Utilizes EIP-712 typed data hashing and ECDSA signature recovery
    /// @param data The MintRequest struct containing the details of the minting request
    /// @param sig The signature associated with the MintRequest
    /// @return The address of the signer who signed the MintRequest
    function _getSigner(MintRequest memory data, bytes memory sig)
        internal
        view
        returns (address)
    {
        bytes32 digest = _computeDigest(data);
        return digest.recover(sig);
    }

    /// @notice Internal function to compute the EIP712 digest for MintRequest
    /// @dev Generates the digest that must be signed by the signer.
    /// @param data The MintRequest to generate a digest for
    /// @return The computed EIP712 digest
    function _computeDigest(MintRequest memory data) internal view returns (bytes32) {
        return _hashTypedDataV4(keccak256(_getStructHash(data)));
    }

    /// @notice Internal function to generate the struct hash for MintRequest
    /// @dev Encodes the MintRequest struct into a hash as per EIP712 standard.
    /// @param data The MintRequest struct to hash
    /// @return A hash representing the encoded MintRequest
    function _getStructHash(MintRequest memory data) internal pure returns (bytes memory) {
        return abi.encode(REQ_HASH, data.nonce, data.toAddress);
    }

    /// @notice Burns `tokenId`.
    /// @dev Can only be called by the owner of the token or an approved operator.
    function burn(uint256 tokenId) public {
        if (!_isApprovedOrOwner(msg.sender, tokenId)) {
            revert NotOwnerNorApproved();
        }
        _burn(tokenId);
    }

    /// @dev Hook that is called before any token transfer.
    function _beforeTokenTransfer(address from, address to, uint256)
        internal
        pure
        virtual
        override
    {
        // Allow minting (from == address(0)) and burning (to == address(0))
        if (from != address(0) && to != address(0)) {
            revert Infinity__TransferFailed();
        }
    }

    /// @notice Checks if the contract implements an interface
    /// @dev Overrides the supportsInterface function of ERC721 and AccessControlUpgradeable.
    /// @param interfaceId The interface identifier, as specified in ERC-165
    /// @return True if the contract implements the interface, false otherwise
    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721, AccessControlUpgradeable)
        returns (bool)
    {
        return interfaceId == type(IERC721).interfaceId
            || interfaceId == type(IAccessControl).interfaceId || super.supportsInterface(interfaceId);
    }
}

