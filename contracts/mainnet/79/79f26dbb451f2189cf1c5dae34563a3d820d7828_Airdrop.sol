pragma solidity 0.8.23;

import {IERC20, SafeERC20} from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import {Ownable2Step, Ownable} from "@openzeppelin/contracts/access/Ownable2Step.sol";

/**
 * @title Airdrop contract
 * @author CacaoSwap
 * @notice This contract is used for both airdrops and voldrops. The contract admin can provide a list of addresses
 * and claimable amounts, and address owners are able to sign a transaction and receive the allocated
 * token amount. All the work calculating who is to receive what off chain and submit it as a list.
 */
contract Airdrop is Ownable2Step {
  using SafeERC20 for IERC20;

  error ArrayLengthMismatch();
  error AirdropHasNotStarted();
  error AirdropInProgress();
  error AirdropEnded();
  error NoTokensToClaim();

  IERC20 internal immutable YUM;
  uint256 public immutable startDate;
  uint256 public immutable endDate;

  mapping(address user => uint256 claimableAmount) public claimableAmounts;

  /**
   * @notice The constructor sets the owner, the token address, and the start and end dates.
   * @param initialOwner The initial owner of the contract.
   * @param token The address of the token to be used for the airdrop.
   * @param _startDate The start date of the airdrop.
   * @param _endDate The end date of the airdrop.
   */
  constructor(address initialOwner, address token, uint256 _startDate, uint256 _endDate) Ownable(initialOwner) {
    YUM = IERC20(token); // https://arbiscan.io/token/0x9F41b34f42058a7b74672055a5fae22c4b113Fd1
    startDate = _startDate;
    endDate = _endDate;
  }

  /**
   * @notice Allow users to claim their claimable amounts.
   */
  function claim() external {
    if (block.timestamp < startDate) revert AirdropHasNotStarted();
    if (block.timestamp >= endDate) revert AirdropEnded();
    uint256 amount = claimableAmounts[msg.sender];
    if (amount == 0) revert NoTokensToClaim();
    delete claimableAmounts[msg.sender];
    YUM.safeTransfer(msg.sender, amount);
  }

  /**
   * @notice Submit list of claimable amounts, indexed by public address.
   * @notice Owner should have already approved this contract to spend the claimable amounts.
   * @dev If the address already has a claimable amount, add the submitted amount to the existing amount.
   * @param users Array of public addresses
   * @param amounts Array of claimable amounts
   */
  function updateClaimAmounts(address[] calldata users, uint256[] calldata amounts) external onlyOwner {
    if (block.timestamp >= startDate) revert AirdropInProgress();
    if (users.length != amounts.length) revert ArrayLengthMismatch();
    uint256 currentBalance = YUM.balanceOf(address(this));
    uint256 totalAmount;

    for (uint256 i; i < users.length; ) {
      claimableAmounts[users[i]] += amounts[i];
      totalAmount += amounts[i];
      unchecked {
        ++i;
      }
    }

    YUM.safeTransferFrom(msg.sender, address(this), totalAmount);
    assert(YUM.balanceOf(address(this)) >= currentBalance + totalAmount);
  }

  /**
   * @notice Delete a claimable amount by address
   * @param users Address of the user for whom to delete the claimable amount
   */
  function deleteClaimAmounts(address[] calldata users) external onlyOwner {
    if (block.timestamp >= startDate) revert AirdropInProgress();
    uint256 totalAmount;

    for (uint256 i; i < users.length; ) {
      totalAmount += claimableAmounts[users[i]]; // NOTE: Expensive as it makes an SLOAD on every iteration
      delete claimableAmounts[users[i]];

      unchecked {
        ++i;
      }
    }
    YUM.safeTransfer(msg.sender, totalAmount);
  }

  /**
   * @notice Withdraws all the remaining tokens after airdrop has finished to the owner.
   */
  function withdraw() external onlyOwner {
    if (block.timestamp < endDate) revert AirdropInProgress();
    YUM.safeTransfer(msg.sender, YUM.balanceOf(address(this)));
  }
}

