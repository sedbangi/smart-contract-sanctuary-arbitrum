pragma solidity 0.8.19;

import "@openzeppelin/contracts/access/Ownable.sol";

import {IAlgebraFactory} from "../algebra/interfaces/IAlgebraFactory.sol";
import {IERC20, SafeERC20} from "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import {EnumerableSet} from "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

import "../libs/ERC20Sol.sol";
import {Range} from "../types/EnigmaStructs.sol";
import {IEnigma} from "../interfaces/IEnigma.sol";
import {Errors} from "../libs/Errors.sol";

/// @title Enigma base contract containing all Enigma pools storage variables.
abstract contract EnigmaStorage is IEnigma, ERC20Sol, Ownable {
    using SafeERC20 for IERC20;
    using EnumerableSet for EnumerableSet.AddressSet;

    // #region modifiers
    address public operatorAddress;

    function _onlyOperator() private view {
        if (operatorAddress != msg.sender && owner() != msg.sender) {
            revert Errors.Enigma_NotOperator(msg.sender);
        }
    }

    modifier onlyOperator() {
        _onlyOperator();
        _;
    }
    // #end region modifiers

    //tracks if the enigma has been initalized
    bytes32 internal _parameters;

    uint256 internal constant PRECISION = 1e18;
    uint256 internal constant MIN_INITIAL_SHARES = 1e6;

    IAlgebraFactory public factory;
    IERC20 public token0;
    IERC20 public token1;

    //fee params
    uint256 public constant FEE_LIMIT = 1e18; //represents 100% fee
    uint256 public SELECTED_FEE = 0.1e18; //initial fee of 10%

    //storage of the enigma ranges
    Range[] public ranges;
    EnumerableSet.AddressSet internal _pools;

    //limit total supply
    uint256 public maxTotalSupply = 0;
    uint256 public deposit0Max;
    uint256 public deposit1Max;

    ///is this a private pool
    bool public isPrivate;
    /// list of allowed depositors
    mapping(address => bool) public privateList;

    //uniswapV3mintCallback
    function _uniswapV3CallBack(uint256 amount0_, uint256 amount1_) internal {
        if (!_pools.contains(msg.sender)) revert Errors.Enigma_UniCallbackFail(msg.sender);

        if (amount0_ > 0) token0.safeTransfer(msg.sender, amount0_);
        if (amount1_ > 0) token1.safeTransfer(msg.sender, amount1_);
    }

    /// -----------------------------------------------------------
    /// Init Functions
    /// -----------------------------------------------------------

    /// @param _uniFactory Uniswap V3 pool for which liquidity is managed
    function initialize(
        address _uniFactory,
        address _token0,
        address _token1,
        uint24[] calldata _feeTiers,
        uint256 _selectedFee,
        address _owner,
        bool _isPrivate,
        address _operator
    ) external {
        bytes32 parameters = _parameters;
        if (parameters != 0) revert Errors.Enigma_AlreadyInit();
        if (_uniFactory == address(0)) revert Errors.Enigma_InitInvalidFactory(_uniFactory);
        if (_owner == address(0)) revert Errors.Enigma_InvalidOwner(_owner);
        if (_operator == address(0)) revert Errors.Enigma_InvalidOperator(_operator);

        if (_token0 == address(0)) revert Errors.Enigma_InvalidAddress(_token0);
        if (_token1 == address(0)) revert Errors.Enigma_InvalidAddress(_token1);

        token0 = IERC20(_token0);
        token1 = IERC20(_token1);

        factory = IAlgebraFactory(_uniFactory);
        _setSelectedFee(_selectedFee);
        _transferOwnership(_owner);

        /// add the pools to the enumerable set only upon factory creation
        /// operators cannot add any further pools once init
        _addPools(_feeTiers, _token0, _token1);

        /// no cap on deposits or supply of enigma token
        maxTotalSupply = 0;
        deposit0Max = type(uint256).max;
        deposit1Max = type(uint256).max;

        operatorAddress = _operator;
        isPrivate = _isPrivate;

        // Disable the initialize function
        _parameters = bytes32(uint256(1));
    }

    /// -----------------------------------------------------------
    /// View Functions
    /// -----------------------------------------------------------

    /// @notice Returns the name of the token.
    /// @return The name of the token.
    function name() public view virtual override returns (string memory) {
        return "Enigma LP Token";
    }

    /// @notice Returns the symbol of the token, usually a shorter version of the name.
    /// @return symbol of the token.
    function symbol() public view virtual override returns (string memory) {
        return "LP_ENIGMA";
    }

    /// @notice Returns all the addresses of the underlying uniV3 pool
    function getPools() external view returns (address[] memory) {
        uint256 len = _pools.length();
        address[] memory output = new address[](len);
        for (uint256 i; i < len;) {
            output[i] = _pools.at(i);
            //iterate
            unchecked {
                ++i;
            }
        }

        return output;
    }

    /// @notice get the length of the ranges array
    /// @return length of the ranges array
    function getRangeLength() external view returns (uint256) {
        return ranges.length;
    }

    /// @notice get all the ranges on the enigma
    function getRanges() external view returns (Range[] memory ranges_) {
        return ranges;
    }

    /// -----------------------------------------------------------
    /// Internal Functions
    /// -----------------------------------------------------------

    /// @notice add a new pool to the mapping
    /// only ever called once upon enigma pool creation
    /// a new pool can only ever be added at init time
    function _addPools(uint24[] calldata feeTiers_, address token0Addr_, address token1Addr_) internal {
        for (uint256 i = 0; i < feeTiers_.length;) {
            address pool = factory.poolByPair(token0Addr_, token1Addr_);

            if (pool == address(0)) revert Errors.Enigma_InvalidPool(pool);
            if (_pools.contains(pool)) revert Errors.Enigma_PoolExists(pool);

            // explicit.
            _pools.add(pool);

            unchecked {
                ++i;
            }
        }
    }

    /// @notice set the max fee that can be taken by the enigma
    /// @notice should be < than 1e18 and > 0
    /// @dev the max is 1e18 so setting the _newSelectedFee to this
    /// @dev means that fees are 100% of the lp fees earned
    function _setSelectedFee(uint256 _newSelectedFee) internal {
        if (_newSelectedFee == 0) revert Errors.Enigma_InvalidFee(_newSelectedFee);
        if (_newSelectedFee > FEE_LIMIT) revert Errors.Enigma_InvalidFee(_newSelectedFee);

        SELECTED_FEE = _newSelectedFee;
        //emit update the fee
        emit Log_SetEnigmaFee(_newSelectedFee);
    }

    /// -----------------------------------------------------------
    /// Owner / Operator Functions
    /// -----------------------------------------------------------

    /// @notice sets restrictions around deposits for receipt token supply
    /// @param _maxTotalSupply The maximum liquidity token supply the contract allows
    function setMaxTotalSupply(uint256 _maxTotalSupply) external onlyOperator {
        if (maxTotalSupply != _maxTotalSupply) {
            maxTotalSupply = _maxTotalSupply;
            emit Log_MaxTotalSupplySet(_maxTotalSupply);
        }
    }

    /// @notice sets restrictions around deposits
    /// @notice i.e the max amount of tokens that can exist in the enigma
    /// @param _deposit0Max The maximum amount of token0 allowed in a deposit
    /// @param _deposit1Max The maximum amount of token1 allowed in a deposit
    function setDepositMax(uint256 _deposit0Max, uint256 _deposit1Max) external onlyOperator {
        if (deposit0Max != _deposit0Max) {
            deposit0Max = _deposit0Max;
        }
        if (deposit1Max != _deposit1Max) {
            deposit1Max = _deposit1Max;
        }
        emit Log_DepositMaxSet(_deposit0Max, _deposit1Max);
    }

    /// @notice adds a set of addresses to the allowlist
    /// @param listed Array of addresses to be appended
    function appendList(address[] calldata listed) external onlyOperator {
        for (uint256 i; i < listed.length;) {
            privateList[listed[i]] = true;
            emit Log_SetIsWhitelisted(listed[i], true);

            unchecked {
                ++i;
            }
        }
    }

    /// @notice removes an addresses from the allowlist
    /// @param listed Address of listed to remove
    function removeListed(address listed) external onlyOperator {
        privateList[listed] = false;
        emit Log_SetIsWhitelisted(listed, false);
    }

    /// @notice sets if the enigma pool should use the whitelist
    /// users can always withdraw from the enigmaPool
    /// however only deposits will be restricted
    function togglePrivate() external onlyOperator {
        isPrivate = !isPrivate;
        emit Log_IsPrivate(isPrivate);
    }

    /// @notice set the max fee that can be taken by the enigma
    /// @notice should be < than 1e18 and > ENIGMA_TREASURY_FEE
    /// @dev the max is 1e18 so setting the _newSelectedFee to this
    /// @dev means that fees are 100% of the lp fees earned
    function setSelectedFee(uint256 _newSelectedFee) external onlyOperator {
        _setSelectedFee(_newSelectedFee);
    }

    /// @notice set operator address
    /// @param _operator of the enigma pool
    /// @dev only callable by owner or existing operator.
    function setOperator(address _operator) external onlyOperator {
        if (_operator == address(0)) revert Errors.Enigma_InvalidOperator(_operator);
        operatorAddress = _operator;

        emit Log_SetOperator(_operator);
    }

    /// @notice Rescues funds stuck or tokens not compatible with the enigma
    /// @param _token address of the token to rescue.
    function inCaseTokensGetStuck(address _token) external onlyOwner {
        if (_token == address(token0) || _token == address(token1)) revert Errors.Enigma_RescueInvalidTokens();
        uint256 amount = IERC20(_token).balanceOf(address(this));
        IERC20(_token).safeTransfer(msg.sender, amount);
    }

    /// -----------------------------------------------------------
    /// END Enigma by SteakHut Labs
    /// -----------------------------------------------------------
}

