/// @title Enigma vault contracts
/// @notice Next generation liquidity management protocol ontop of Uniswap v3
/// @author by SteakHut Labs © 2023
contract Enigma is EnigmaStorage, IAlgebraMintCallback, ReentrancyGuard {
    using SafeERC20 for IERC20;
    using SafeCast for uint256;
    using FullMath for uint256;

    IEnigmaFactory private immutable _factory;

    /// -----------------------------------------------------------
    /// Events
    /// -----------------------------------------------------------

    ///@notice events shall be contained in the IEnigma.sol

    /// -----------------------------------------------------------
    /// Constructor / Initialize (contained in storage)
    /// -----------------------------------------------------------

    /// @dev Constructor for the Enigma Pool contract that sets the Enigma Factory
    /// @param factory_ The Enigma Factory Contract
    constructor(IEnigmaFactory factory_) {
        //perform basic sanitization on factory
        if (address(factory_) == address(0)) revert Errors.Enigma_InvalidImplementationAddress(address(factory_));

        //set the factory
        _factory = factory_;

        // Disable the initialize function
        _parameters = bytes32(uint256(1));
    }

    /// -----------------------------------------------------------
    /// Algebra Mint Callback
    /// -----------------------------------------------------------

    /// @notice implement the Algebra V3 callback fn, called when minting uniswap LP position
    function algebraMintCallback(uint256 amount0, uint256 amount1, bytes calldata /*data*/ ) external override {
        //function protected as checks msg.sender is one of the enigmas current v3 pools
        _uniswapV3CallBack(amount0, amount1);
    }

    /// -----------------------------------------------------------
    /// External Functions
    /// -----------------------------------------------------------

    /// @notice Primary entry point into the pool
    /// @param params deposit parameters
    /// @return shares The amount of shares provided to the recipient
    /// @return amount0 amount of token0 actually deposited
    /// @return amount1 amount of token1 actually deposited
    function deposit(DepositParams calldata params)
        external
        virtual
        override
        nonReentrant
        returns (uint256 shares, uint256 amount0, uint256 amount1)
    {
        if (params.recipient == address(0)) {
            revert Errors.Enigma_InvalidToAddress(params.recipient);
        }

        if (params.amount0Desired == 0 && params.amount1Desired == 0) {
            revert Errors.Enigma_InvalidDesiredAmounts(params.amount0Desired, params.amount1Desired);
        }

        if (params.amount0Desired > deposit0Max || params.amount1Desired > deposit1Max) {
            revert Errors.Enigma_InvalidAmounts(params.amount0Desired, params.amount1Desired);
        }

        if (isPrivate && !privateList[msg.sender]) revert Errors.Enigma_Private();

        if (params.deadline < block.timestamp) revert Errors.Enigma_Deadline();

        //we should poke fee's to ensure that flash theft of yield is not possible
        _pokeFees();
        uint256 ts = totalSupply();

        //calculate the amount of shares to mint the user
        (uint256 total0, uint256 total1) = getTotalAmounts();

        (uint256 _sharesToMint, uint256 amount0Actual, uint256 amount1Actual) =
            EnigmaHelper.calcSharesAndAmounts(ts, total0, total1, params.amount0Desired, params.amount1Desired);

        //check that shares received are expected amount
        if (_sharesToMint < params.minSharesToMint) revert Errors.Enigma_MinimumSharesToMint();

        //check that if first deposit we are minting the minimum amount of inital shares
        if (ts == 0 && _sharesToMint < MIN_INITIAL_SHARES) revert Errors.Enigma_MinInitialAmount();

        //set the amount returned
        amount0 = amount0Actual;
        amount1 = amount1Actual;

        //transfer the required amounts into this contract from sender
        if (amount0Actual > 0) {
            token0.safeTransferFrom(msg.sender, address(this), amount0Actual);
        }
        if (amount1Actual > 0) {
            token1.safeTransferFrom(msg.sender, address(this), amount1Actual);
        }

        //loop over the ranges in the strategy and add these too the pool
        if (ts > 0) {
            uint256 rangeLength = ranges.length;
            for (uint256 index; index < rangeLength;) {
                Range memory _range = ranges[index];

                //add liquidty into pools based on underlying liquidity in the ranges
                //get the applicable uniswap pool (safe as pool is checked when adding ranges)
                address _pool = factory.poolByPair(address(token0), address(token1));

                //get the scaled amount of liquidity for the range
                (uint128 _liquidity) = EnigmaHelper.getScaledLiquidityForRange(_pool, _range, ts, _sharesToMint);

                //increment loop before continue to prevent freeze
                unchecked {
                    index++;
                }

                if (_liquidity == 0) continue;

                //mint liquidity
                _mintLiquidity(_pool, _range.tickLower, _range.tickUpper, _liquidity);
            }
        }

        //mint the underlying receipt tokens
        _mint(params.recipient, _sharesToMint);
        shares = _sharesToMint;

        /// Check total supply cap not exceeded. A value of 0 means no limit.
        //cannot use the cached ts here as needs to be the updated amount
        if (maxTotalSupply > 0 && totalSupply() > maxTotalSupply) revert Errors.Enigma_MaxTotalSupply();

        //emit a deposit event
        emit Log_Deposit(params.recipient, shares, amount0, amount1);
    }

    /// @param shares Number of liquidity tokens to redeem as pool underlying assets
    /// @param to Address to which redeemed pool assets are sent
    /// @return amount0 Amount of token0 redeemed by the submitted liquidity tokens
    /// @return amount1 Amount of token1 redeemed by the submitted liquidity tokens
    function withdraw(uint256 shares, address to, uint256 deadline)
        external
        nonReentrant
        returns (uint256 amount0, uint256 amount1)
    {
        if (balanceOf(msg.sender) < shares || shares == 0) revert Errors.Enigma_WithdrawInvalid(shares);
        if (to == address(0)) revert Errors.Enigma_InvalidToAddress(to);
        if (deadline < block.timestamp) revert Errors.Enigma_Deadline();

        uint256 totalSupply = totalSupply();

        //burn share tokens
        _burn(msg.sender, shares);

        uint256 fee0Accumulator;
        uint256 fee1Accumulator;

        //loop over the ranges in the strategy and remove the required amounts
        uint256 rangeLength = ranges.length;
        for (uint256 index; index < rangeLength;) {
            Range memory _range = ranges[index];
            //fetch the required uni pool
            address _pool = factory.poolByPair(address(token0), address(token1));

            //liquidity from position @ index
            (uint256 liquidity,,) = EnigmaHelper.position(address(this), _pool, _range.tickLower, _range.tickUpper);

            //attributableLiquidity to the amount of shares being removed
            uint256 attributableLiquidity = FullMath.mulDiv(shares, liquidity, totalSupply);

            //burn liquidity from the pools as required.
            BurnParams memory _params = BurnParams(
                _pool, _range.tickLower, _range.tickUpper, attributableLiquidity.toUint128(), address(this), false
            );
            WithdrawParams memory payload = _burnLiquidity(_params);

            //update the totals
            //amount0 and amount1 are the burn amounts
            amount0 += payload.burn0;
            amount1 += payload.burn1;
            fee0Accumulator += payload.fee0;
            fee1Accumulator += payload.fee1;

            //increment loop
            unchecked {
                ++index;
            }
        }

        //calculate and distribute the required fees
        _applyFeesDistribute(fee0Accumulator, fee1Accumulator);

        // distribute the unused tokens to address(to);
        // Push tokens proportional to unused balances (i.e liquidity idle in strategy)
        // currently liquidity is pushed back here and then distributed as unusedliquidity
        uint256 idleAmount0 = FullMath.mulDiv(token0.balanceOf(address(this)) - amount0, shares, totalSupply);
        uint256 idleAmount1 = FullMath.mulDiv(token1.balanceOf(address(this)) - amount1, shares, totalSupply);

        //transfer amounts to the user.
        if (idleAmount0 > 0 || amount0 > 0) token0.safeTransfer(to, idleAmount0 + amount0);
        if (idleAmount1 > 0 || amount1 > 0) token1.safeTransfer(to, idleAmount1 + amount1);

        //emit event
        emit Log_CollectFees(fee0Accumulator, fee1Accumulator);
        emit Log_Withdraw(to, shares, amount0, amount1);
    }

    /// -----------------------------------------------------------
    /// Operator Functions
    /// -----------------------------------------------------------

    /// @notice collects fees from Enigma's underlying UniswapV3 positions
    /// distributes the fees to the fee collector
    function harvest() external onlyOperator {
        _pokeFees();
    }

    /// @notice rebalance Enigma's UniswapV3 positions
    /// removes liquidity from rebalance params, swaps and then adds liquidity
    /// @dev onlyOperator contract can call this function.
    function rebalance(Rebalance calldata rebalanceData) external onlyOperator nonReentrant {
        ////////////////////////////////////////////////////////////////////////////////
        //remove all the required liquidty
        ////////////////////////////////////////////////////////////////////////////////
        uint256 burn0Amount;
        uint256 burn1Amount;
        uint256 fee0Accumulator;
        uint256 fee1Accumulator;

        for (uint256 index; index < rebalanceData.burns.length;) {
            //burn the required liquidities
            Position memory _burn = rebalanceData.burns[index];

            //fetch the required uni pool
            address _pool = factory.poolByPair(address(token0), address(token1));

            (uint256 liquidity,,) =
                EnigmaHelper.position(address(this), _pool, _burn.range.tickLower, _burn.range.tickUpper);

            uint256 liquidityToRemove;
            if (_burn.liquidity == type(uint128).max) {
                liquidityToRemove = liquidity;
            } else {
                liquidityToRemove = _burn.liquidity;
            }

            if (liquidity > 0) {
                //burn the required liquidity
                BurnParams memory _params = BurnParams({
                    _pool: _pool,
                    tickLower: _burn.range.tickLower,
                    tickUpper: _burn.range.tickUpper,
                    liquidity: liquidityToRemove.toUint128(),
                    to: address(this),
                    isZeroBurn: false
                });

                //burn liquidity from the pools as required.
                WithdrawParams memory payload = _burnLiquidity(_params);

                //take a fee on the position
                burn0Amount += payload.burn0;
                burn1Amount += payload.burn1;
                fee0Accumulator += payload.fee0;
                fee1Accumulator += payload.fee1;
            }

            //if we remove all the liquidity we should also remove the range from storage
            //don't remove the range if we are just performing a 0 burn (i.e harvesting)
            //if burn uint128.max then we want to remove the range
            if (liquidityToRemove >= liquidity && liquidityToRemove != 0 || _burn.liquidity == type(uint128).max) {
                (bool rangeExists, uint256 i) = _rangeExists(rebalanceData.burns[index].range);
                if (!rangeExists) revert Errors.Enigma_InvalidBurnRange(i);
                //gas optimization
                if (i != ranges.length - 1) {
                    ranges[i] = ranges[ranges.length - 1];
                }
                ranges.pop();
            }

            //increment loop
            unchecked {
                ++index;
            }
        }

        //take the required fees (gas saving)
        _applyFeesDistribute(fee0Accumulator, fee1Accumulator);

        //perform final checks on burn amounts
        if (burn0Amount < rebalanceData.minBurn0) {
            revert Errors.Enigma_RebalanceBurnAmounts(burn0Amount, rebalanceData.minBurn0);
        }
        if (burn1Amount < rebalanceData.minBurn1) {
            revert Errors.Enigma_RebalanceBurnAmounts(burn1Amount, rebalanceData.minBurn1);
        }

        //emit events
        emit Log_CollectFees(fee0Accumulator, fee1Accumulator);

        ////////////////////////////////////////////////////////////////////////////////
        //Perform swaps if required
        ////////////////////////////////////////////////////////////////////////////////

        if (rebalanceData.swap.amountIn > 0) {
            //a swap has been commissioned by the rebalance function
            uint256 balance0Before = token0.balanceOf(address(this));
            uint256 balance1Before = token1.balanceOf(address(this));

            //grant approvals for token to the router
            token0.safeApprove(address(rebalanceData.swap.router), balance0Before);
            token1.safeApprove(address(rebalanceData.swap.router), balance1Before);

            //check to see the router is allowed
            if (!_factory.isRouterAllowed(rebalanceData.swap.router)) {
                revert Errors.Enigma_InvalidSwapRouter(rebalanceData.swap.router);
            }
            //perform the swap
            (bool success,) = rebalanceData.swap.router.call(rebalanceData.swap.payload);
            if (!success) revert Errors.Enigma_RebalanceSwapError();

            //start perform swap checks
            uint256 balance0After = token0.balanceOf(address(this));
            uint256 balance1After = token1.balanceOf(address(this));

            //check that we have received at least the expected amounts
            if (rebalanceData.swap.zeroForOne) {
                if (balance1After < balance1Before + rebalanceData.swap.expectedMinReturn) {
                    revert Errors.Enigma_RebalanceInsufficientSwap();
                }
                if (balance0After < balance0Before - rebalanceData.swap.amountIn) {
                    revert Errors.Enigma_RebalanceInsufficientSwap();
                }
            } else {
                if (balance0After < balance0Before + rebalanceData.swap.expectedMinReturn) {
                    revert Errors.Enigma_RebalanceInsufficientSwap();
                }
                if (balance1After < balance1Before - rebalanceData.swap.amountIn) {
                    revert Errors.Enigma_RebalanceInsufficientSwap();
                }
            }

            //remove approvals for token to the router
            token0.safeApprove(address(rebalanceData.swap.router), 0);
            token1.safeApprove(address(rebalanceData.swap.router), 0);

            emit Log_Rebalance(rebalanceData, balance0After, balance1After);
        } else {
            //no swaps required log (0,0) and ignore this in offchain data
            emit Log_Rebalance(rebalanceData, 0, 0);
        }

        ////////////////////////////////////////////////////////////////////////////////
        //Add liquidity to new ranges
        ////////////////////////////////////////////////////////////////////////////////

        uint256 mint0Amount;
        uint256 mint1Amount;

        for (uint256 index; index < rebalanceData.mints.length;) {
            //add the liquidity in the strategy to the new ranges
            Position memory position = rebalanceData.mints[index];

            //increment loop
            unchecked {
                index++;
            }

            //check if the range exists already and if not add it to the array
            Range memory range_ = Range(position.range.tickLower, position.range.tickUpper, position.range.feeTier);

            //fetch the required uni pool
            address _pool = factory.poolByPair(address(token0), address(token1));
            if (_pool == address(0)) revert Errors.Enigma_RebalanceNoPool();

            if (!EnigmaHelper.verifyTickSpacing(_pool, range_)) {
                revert Errors.Enigma_RebalanceInvalidTickSpacing();
            }

            //verify the pool was whitelisted upon creation otherwise revert
            bool _isWhitelisted = EnigmaHelper.poolWhitelisted(address(this), _pool);
            if (!_isWhitelisted) revert Errors.Enigma_RebalanceNotWLPool();

            if (position.liquidity == 0) continue;

            //check if the range exists if not push it to the array (ensure no empty ranges)
            (bool exists,) = _rangeExists(range_);
            if (!exists) ranges.push(range_);

            //mint liquidity
            (uint256 amount0Minted, uint256 amount1Minted) =
                _mintLiquidity(_pool, position.range.tickLower, position.range.tickUpper, position.liquidity);

            mint0Amount += amount0Minted;
            mint1Amount += amount1Minted;
        }

        if (mint0Amount < rebalanceData.minDeposit0) {
            revert Errors.Enigma_RebalanceMintAmounts(mint0Amount, rebalanceData.minDeposit0);
        }
        if (mint1Amount < rebalanceData.minDeposit1) {
            revert Errors.Enigma_RebalanceMintAmounts(mint1Amount, rebalanceData.minDeposit1);
        }

        //check if the cap has been reached on the number of positions in the enigma
        if (ranges.length > _factory.ENIGMA_MAX_POS()) revert Errors.Enigma_MaxCapReached(ranges.length);
    }

    /// -----------------------------------------------------------
    /// Internal Functions
    /// -----------------------------------------------------------

    /// @notice check if the range exists in the enigma
    /// @param range_ the range to check if exists in the enigma
    function _rangeExists(Range memory range_) internal view returns (bool ok, uint256 index) {
        for (uint256 i; i < ranges.length;) {
            //evaluate if the range exists
            if (
                range_.tickLower == ranges[i].tickLower && range_.tickUpper == ranges[i].tickUpper
                    && range_.feeTier == ranges[i].feeTier
            ) return (true, i);
            unchecked {
                ++i;
            }
        }
        return (false, 0);
    }

    /// @notice Adds the liquidity for the given position
    /// @param tickLower The lower tick of the position in which to add liquidity
    /// @param tickUpper The upper tick of the position in which to add liquidity
    /// @param liquidity The amount of liquidity to mint
    /// @return amount0 The amount of token0 that was paid to mint the given amount of liquidity
    /// @return amount1 The amount of token1 that was paid to mint the given amount of liquidity
    function _mintLiquidity(address _pool, int24 tickLower, int24 tickUpper, uint128 liquidity)
        internal
        returns (uint256 amount0, uint256 amount1)
    {
        if (liquidity > 0) {
            (amount0, amount1, liquidity) =
                IAlgebraPool(_pool).mint(address(this), address(this), tickLower, tickUpper, liquidity, "");
        }
    }

    /// @notice Burn liquidity from the sender and collect tokens owed for the liquidity
    function _burnLiquidity(BurnParams memory _burnParams) internal returns (WithdrawParams memory withdrawPayload) {
        /// Burn liquidity if required
        if (_burnParams.liquidity > 0 || _burnParams.isZeroBurn) {
            (withdrawPayload.burn0, withdrawPayload.burn1) = IAlgebraPool(_burnParams._pool).burn(
                _burnParams.tickLower, _burnParams.tickUpper, _burnParams.liquidity
            );
        }

        //collecting swap fees
        (uint256 collect0, uint256 collect1) = IAlgebraPool(_burnParams._pool).collect(
            _burnParams.to, _burnParams.tickLower, _burnParams.tickUpper, type(uint128).max, type(uint128).max
        );

        //apply manager fees
        //fee is the difference between collect and what was burnt.
        if (collect0 > 0) withdrawPayload.fee0 = collect0 - withdrawPayload.burn0;
        if (collect1 > 0) withdrawPayload.fee1 = collect1 - withdrawPayload.burn1;
    }

    /// @notice applies the enigma fees and distributes
    /// @return fee0 amount of token0 taken as fee
    /// @return fee1 amount of token1 taken as fee
    function _applyFeesDistribute(uint256 fee0_, uint256 fee1_) internal returns (uint256 fee0, uint256 fee1) {
        //assign storage vars to memory
        uint256 fl = FEE_LIMIT;
        uint256 sf = SELECTED_FEE;

        fee0 = (fee0_ * sf) / fl;
        fee1 = (fee1_ * sf) / fl;

        address enigmaFeeRecipient = _factory.enigmaTreasury();
        uint256 enigmaTreasuryFee = _factory.ENIGMA_TREASURY_FEE();

        uint256 enigmaFee0;
        uint256 operatorFee0;
        uint256 enigmaFee1;
        uint256 operatorFee1;

        if (fee0 > 0) {
            //enigmaShare of fees
            enigmaFee0 = FullMath.mulDiv(fee0, enigmaTreasuryFee, fl);

            //operatorShare
            operatorFee0 = fee0 - enigmaFee0;

            //transfer amounts if non zero
            if (enigmaFee0 > 0) {
                //transfer fee to enigma treasury
                token0.safeTransfer(enigmaFeeRecipient, enigmaFee0);
            }
            if (operatorFee0 > 0) {
                //transfer fee to enigma operator
                token0.safeTransfer(operatorAddress, operatorFee0);
            }
        }

        if (fee1 > 0) {
            //enigmaShare of fees
            enigmaFee1 = FullMath.mulDiv(fee1, enigmaTreasuryFee, fl);
            //operatorShare
            operatorFee1 = fee1 - enigmaFee1;

            //transfer amounts if non zero
            if (enigmaFee1 > 0) {
                //transfer fee to enigma treasury
                token1.safeTransfer(enigmaFeeRecipient, enigmaFee1);
            }
            if (operatorFee1 > 0) {
                //transfer fee to enigma operator
                token1.safeTransfer(operatorAddress, operatorFee1);
            }
        }

        emit Log_DistributeFees(operatorFee0, operatorFee1, enigmaFee0, enigmaFee1);
    }

    /// @notice pokes the enigma fees and distributes to collector
    /// used by harvest() function for collecting fees
    /// used by deposit() to prevent flash theft of yield
    function _pokeFees() internal {
        uint256 fee0Accumulator;
        uint256 fee1Accumulator;

        //loop over the ranges in the strategy to harvest fees from underyling position
        uint256 rangeLength = ranges.length;
        for (uint256 index; index < rangeLength;) {
            Range memory _range = ranges[index];

            //increment loop before continue to prevent freeze
            unchecked {
                ++index;
            }

            //get the applicable uniswap pool (safe as pool is checked when adding ranges)
            address _pool = factory.poolByPair(address(token0), address(token1));
            (uint128 liquidity,,) = EnigmaHelper.position(address(this), _pool, _range.tickLower, _range.tickUpper);

            if (liquidity == 0) continue;

            //burn 0 liquidity from the pools as required to collect fees
            BurnParams memory _params = BurnParams({
                _pool: _pool,
                tickLower: _range.tickLower,
                tickUpper: _range.tickUpper,
                liquidity: 0,
                to: address(this),
                isZeroBurn: true
            });

            WithdrawParams memory payload = _burnLiquidity(_params);

            fee0Accumulator += payload.fee0;
            fee1Accumulator += payload.fee1;
        }

        //calculate and distribute the required fees
        _applyFeesDistribute(fee0Accumulator, fee1Accumulator);
        emit Log_CollectFees(fee0Accumulator, fee1Accumulator);
    }

    /// -----------------------------------------------------------
    /// View Functions
    /// -----------------------------------------------------------

    /// @notice gets the factory stored
    /// @return factory_ enigma factory address
    function getFactory() public view returns (address factory_) {
        factory_ = address(_factory);
    }

    /// @notice Calculates the vault's total holdings of token0 and token1 - in
    /// other words, how much of each token the vault would hold if it withdrew
    /// all its liquidity from Uniswap. (does not include pending fees)
    function getTotalAmounts() public view returns (uint256 total0, uint256 total1) {
        //add any currently unused values contained in this contract
        (total0, total1) = EnigmaHelper.getTotalAmounts(address(this), ranges, factory);
    }

    /// -----------------------------------------------------------
    /// END Enigma by SteakHut Labs
    /// -----------------------------------------------------------
}

