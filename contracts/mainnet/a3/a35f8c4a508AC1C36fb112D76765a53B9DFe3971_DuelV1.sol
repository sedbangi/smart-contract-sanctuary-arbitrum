// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;
import "hardhat/console.sol";

import "../IERC20Burnable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "../utils/Random.sol";

uint256 constant ONE_HUNDRED = 100;

contract DuelV1 is Ownable, ReentrancyGuard {
    IERC20Burnable public token;
    address public authorizedCaller;

    // Max amount of tokens that can be betted
    uint256 public maxBetAmount = 10000 * 10 ** 18;

    // Total token deposited
    uint256 public tokenBank;
    uint256 public feeAmount;

    // Event for a flip
    event ShootResult(
        string player,
        string target,
        string where,
        uint256 damage,
        bool hit,
        bool targetDead,
        uint256 bulletsPicked,
        uint256 grenadesPicked,
        bool medpackPicked
    );

    // Grenade result
    event GrenadeResult(
        string player,
        string target,
        bool hit,
        bool miss,
        bool bounceBack,
        uint256 bulletsPicked,
        uint256 grenadesPicked,
        bool medpackPicked
    );

    event GameFinished(address winner);

    struct Game {
        // Is finished
        bool finished;
        bool started;
        // Winner
        address winner;
        // Entry fee
        uint256 entryFee;
        // Uint amount of players
        uint256 playerCount;
        // Timestamp of start
        uint256 startTimestamp;
        // Duration of the game (1 hour for example)
        uint256 duration;
    }

    // GUn
    struct Gun {
        string name;
        uint256 precission;
        uint256 damage;
    }

    // Structure to keep track of players
    struct Player {
        string name;
        address owner;
        uint256 health;
        uint256 bullets;
        uint256 grenades;
        Gun gun;
    }

    // Current game (or null)
    Game public currentGame;

    //  Mapping of players
    mapping(address => Player) public players;

    mapping(uint256 => address) public playersByIndex;

    constructor(address _token, address _authorized) {
        token = IERC20Burnable(_token);
        authorizedCaller = _authorized;
        feeAmount = 4; // Percent fee on games
    }

    // Set a new authorized caller
    function setAuthorizedCaller(address _authorized) public onlyOwner {
        authorizedCaller = _authorized;
    }

    // Set max bet amount
    function setMaxBetAmount(uint256 _amount) public onlyOwner {
        maxBetAmount = _amount;
    }

    // Only authorized or owner modifier
    modifier onlyAuthorized() {
        require(
            msg.sender == authorizedCaller || msg.sender == owner(),
            "Not authorized"
        );
        _;
    }

    // Create a game, can only be called if theres no active current game
    function createGame(uint256 _entryFee, uint256 _duration) public {
        // ONly the first time there will not be a current game
        if (currentGame.startTimestamp > 0) {
            require(currentGame.finished, "There is an active game");
        }

        // Make sure the max bet amount is not exceeded
        require(_entryFee <= maxBetAmount, "Amount too high");

        // Make sure that _duration is more than the current timestamp but less than 12 hours
        require(
            _duration > block.timestamp &&
                _duration < block.timestamp + 12 hours,
            "Invalid duration"
        );

        currentGame = Game({
            finished: false,
            started: false,
            winner: address(0),
            entryFee: _entryFee,
            playerCount: 0,
            startTimestamp: block.timestamp,
            duration: _duration
        });
    }

    // Add a player to the current game (public so anyone can join)
    function joinGame(string memory playerName) public nonReentrant {
        require(currentGame.startTimestamp > 0, "No active game");
        require(!currentGame.finished, "Game already finished");
        require(currentGame.playerCount < 4, "Game is full");

        // Do not allow the same player to join twice
        bool playerAlreadyJoined = false;
        for (uint256 i = 0; i < currentGame.playerCount; i++) {
            if (playersByIndex[i] == msg.sender) {
                playerAlreadyJoined = true;
                break;
            }
        }

        require(!playerAlreadyJoined, "Player already joined");

        _addPlayer(msg.sender, playerName);
    }

    // Add a player to the current game
    function _addPlayer(address _player, string memory playerName) private {
        // Create the player struct
        Player memory player = Player({
            name: playerName,
            owner: _player,
            health: 200,
            bullets: 3,
            gun: _getRandomGun(),
            grenades: 1
        });

        players[_player] = player;
        playersByIndex[currentGame.playerCount] = _player;
        currentGame.playerCount++;

        // Transfer the entry fee
        token.transferFrom(_player, address(this), currentGame.entryFee);
        tokenBank += currentGame.entryFee;
    }

    function _getRandomGun() private view returns (Gun memory) {
        uint256 baseId = Random.getInitialBase(msg.sender);
        uint256 randomBase = _generateRandomBase(baseId);
        uint256 randomNumberPrecission = 0;
        uint256 randomNumberDamage = 0;
        uint256 base = 0;
        (randomNumberPrecission, base) = Random.getNextRandom(
            randomBase,
            ONE_HUNDRED
        );
        (randomNumberDamage, base) = Random.getNextRandom(base, ONE_HUNDRED);

        uint256 minimumPrecision = 50;
        uint256 minimumDamage = 50;

        if (randomNumberPrecission < minimumPrecision) {
            randomNumberPrecission = minimumPrecision;
        }

        if (randomNumberDamage < minimumDamage) {
            randomNumberDamage = minimumDamage;
        }

        string memory gunName = "Desert Eagle";
        string memory precissionName = "Not accurate";
        string memory damageName = "Normal";

        if (randomNumberPrecission > 80) {
            precissionName = "Precise";
        }

        if (randomNumberPrecission > 90) {
            precissionName = "Ultra Precise";
        }

        if (randomNumberDamage > 80) {
            damageName = "Powerful";
        }

        if (randomNumberDamage > 90) {
            damageName = "Ultra Powerful";
        }

        Gun memory gun = Gun({
            name: string(
                abi.encodePacked(precissionName, " ", damageName, " ", gunName)
            ),
            precission: randomNumberPrecission,
            damage: randomNumberDamage
        });

        return gun;
    }

    // Start the game
    function startGame() public nonReentrant {
        require(currentGame.startTimestamp > 0, "No active game");
        require(!currentGame.finished, "Game already finished");
        require(!currentGame.started, "Game already started");
        require(currentGame.playerCount == 4, "Not enough players");

        currentGame.started = true;
        currentGame.startTimestamp = block.timestamp;
    }

    // Reload (buys a bullet for each alive player, it requires all player to have 0 bullets)
    function reload() public nonReentrant {
        require(currentGame.started, "Game not started");
        require(!currentGame.finished, "Game already finished");

        // Check if all players have 0 bullets
        bool allAlivePlayersHave0Bullets = true;
        for (uint256 i = 0; i < currentGame.playerCount; i++) {
            if (
                players[playersByIndex[i]].bullets > 0 &&
                players[playersByIndex[i]].health > 0
            ) {
                allAlivePlayersHave0Bullets = false;
                break;
            }
        }

        require(allAlivePlayersHave0Bullets, "Not all players have 0 bullets");

        // Reload the bullets
        for (uint256 i = 0; i < currentGame.playerCount; i++) {
            if (players[playersByIndex[i]].health > 0) {
                players[playersByIndex[i]].bullets = 1;
            }
        }
    }

    // Shoot
    function shoot(uint256 target) public nonReentrant {
        require(currentGame.started, "Game not started");
        require(!currentGame.finished, "Game already finished");
        require(target < currentGame.playerCount, "Invalid target");
        // check the user is in the current game
        bool isPlayer = false;
        // Iterate over the players
        for (uint256 i = 0; i < currentGame.playerCount; i++) {
            if (playersByIndex[i] == msg.sender) {
                isPlayer = true;
                break;
            }
        }

        require(isPlayer, "Not a player");

        Player storage player = players[msg.sender];

        require(player.bullets > 0, "No bullets left");

        // The player must be alive
        require(player.health > 0, "Player is dead");

        player.bullets--;

        // Get a random number
        uint256 baseId = Random.getInitialBase(msg.sender);
        uint256 randomBase = _generateRandomBase(baseId);
        uint256 precissionRandomNumber = 0;
        uint256 whereToShoot = 0;
        uint256 base = 0;
        (precissionRandomNumber, base) = Random.getNextRandom(
            randomBase,
            ONE_HUNDRED
        );
        (whereToShoot, base) = Random.getNextRandom(base, ONE_HUNDRED);
        // Decide where the player gets shoot
        string memory where = whereToShoot > 80
            ? "head"
            : whereToShoot > 10
                ? "body"
                : "feet";
        // Percents
        uint256 whereToShootDamage = whereToShoot > 80
            ? 200
            : whereToShoot > 10
                ? 150
                : 100;

        uint256 damage = (player.gun.damage * whereToShootDamage) / 100;
        Player storage targetPlayer = players[playersByIndex[target]];

        // Check if the player hits (precission is a percentage of hitting)
        if (precissionRandomNumber < player.gun.precission) {
            // Hit
            console.log("Player %s damage", damage);

            bool killed = false;

            if (targetPlayer.health > 0) {
                if (targetPlayer.health > damage) {
                    targetPlayer.health -= damage;
                } else {
                    targetPlayer.health = 0;
                    killed = true;
                    console.log("Player %s killed", targetPlayer.name);
                }
            }

            console.log("Player %s health", targetPlayer.health);

            if (killed) {
                // Random chance of getting a medpack
                (uint256 medpack, ) = Random.getNextRandom(base, ONE_HUNDRED);
                bool medpackPicked = medpack > 80;

                if (medpackPicked) {
                    player.health = 200;
                }

                emit ShootResult(
                    player.name,
                    targetPlayer.name,
                    where,
                    damage,
                    true,
                    killed,
                    targetPlayer.bullets,
                    targetPlayer.grenades,
                    medpackPicked
                );

                // Pick the bullets
                player.bullets += targetPlayer.bullets;
                targetPlayer.bullets = 0;

                // Pick the grenades
                player.grenades += targetPlayer.grenades;
                targetPlayer.grenades = 0;

                _checkWinCondition();
            } else {
                emit ShootResult(
                    player.name,
                    targetPlayer.name,
                    where,
                    damage,
                    true,
                    false,
                    0,
                    0,
                    false
                );
            }
        } else {
            // Emit a result
            emit ShootResult(
                player.name,
                targetPlayer.name,
                where,
                damage,
                false,
                false,
                0,
                0,
                false
            );
        }
    }

    function _checkWinCondition() internal {
        // Player is dead
        // Check if there is only one player left
        uint256 alivePlayers = 0;
        address winner;
        for (uint256 i = 0; i < currentGame.playerCount; i++) {
            if (players[playersByIndex[i]].health > 0) {
                alivePlayers++;
                winner = playersByIndex[i];
            }
        }

        if (alivePlayers == 1) {
            // Game is finished
            currentGame.finished = true;
            currentGame.winner = winner;

            // Emit win
            emit GameFinished(winner);
            _claimPrize(winner);
        }
    }

    function grenade(uint256 target) public nonReentrant {
        // Throws a grenade , but theres a chance of bouncing back and hitting the player
        // Also theres a chance of missing
        require(currentGame.started, "Game not started");
        require(!currentGame.finished, "Game already finished");
        require(target < currentGame.playerCount, "Invalid target");
        // check the user is in the current game
        bool isPlayer = false;
        // Iterate over the players
        for (uint256 i = 0; i < currentGame.playerCount; i++) {
            if (playersByIndex[i] == msg.sender) {
                isPlayer = true;
                break;
            }
        }

        require(isPlayer, "Not a player");

        Player storage player = players[msg.sender];

        require(player.grenades > 0, "No grenades left");

        // The player must be alive
        require(player.health > 0, "Player is dead");

        player.grenades--;

        // Get a random number
        uint256 baseId = Random.getInitialBase(msg.sender);
        uint256 randomBase = _generateRandomBase(baseId);
        uint256 result = 0;
        uint256 base = 0;
        (result, base) = Random.getNextRandom(randomBase, ONE_HUNDRED);
        Player storage targetPlayer = players[playersByIndex[target]];

        // If result < 20 the grenade bounces back and hits the player
        // If result > 80 the grenade hits the target
        // If result > 20 and result < 80 the grenade misses
        if (result < 20) {
            // Bounce back
            player.health = 0;
            emit GrenadeResult(
                player.name,
                targetPlayer.name,
                true,
                false,
                true,
                0,
                0,
                false
            );
            _checkWinCondition();
        } else if (result > 80) {
            // Hit the target

            if (targetPlayer.health > 0) {
                targetPlayer.health = 0;
            }

            // Random chance of getting a medpack
            (uint256 medpack, ) = Random.getNextRandom(base, ONE_HUNDRED);
            bool medpackPicked = medpack > 80;

            if (medpackPicked) {
                player.health = 200;
            }

            emit GrenadeResult(
                player.name,
                targetPlayer.name,
                true,
                false,
                false,
                targetPlayer.bullets,
                targetPlayer.grenades,
                medpackPicked
            );

            // Pick the bullets
            player.bullets += targetPlayer.bullets;
            targetPlayer.bullets = 0;

            // Pick the grenades
            player.grenades += targetPlayer.grenades;
            targetPlayer.grenades = 0;

            _checkWinCondition();
        } else {
            // Miss
            emit GrenadeResult(
                player.name,
                targetPlayer.name,
                false,
                true,
                false,
                0,
                0,
                false
            );
        }
    }

    function _claimPrize(address winner) private {
        // Transfer the prize to the winner
        uint256 totalPrize = currentGame.entryFee * 4;

        // Take a fee
        uint256 fee = (totalPrize * feeAmount) / 100;

        // Burn the fee
        token.burn(fee);

        token.transfer(winner, totalPrize - fee);

        // Reset the bank amount
        tokenBank = 0;
    }

    // PUblic function to end the game if 1 hour has passed since start and theres no winner
    function endGame() public {
        require(currentGame.started, "Game not started");
        require(!currentGame.finished, "Game already finished");
        require(block.timestamp > currentGame.duration, "Game not finished");

        // Game is finished
        currentGame.finished = true;

        // Distribute the money back to each player, but take a fee that gets burned
        uint256 totalPrize = currentGame.entryFee * 4;

        // Take a fee
        uint256 fee = (totalPrize * feeAmount) / 100;

        // Burn the fee
        token.burn(fee);

        // Distribute the rest
        for (uint256 i = 0; i < currentGame.playerCount; i++) {
            token.transfer(
                playersByIndex[i],
                (totalPrize - fee) / currentGame.playerCount
            );
        }

        // Reset the bank amount
        tokenBank = 0;
    }

    function _generateRandomBase(
        uint baseId
    ) internal view virtual returns (uint256) {
        return Random.startRandomBase(baseId, uint256(uint160(msg.sender)));
    }
}

