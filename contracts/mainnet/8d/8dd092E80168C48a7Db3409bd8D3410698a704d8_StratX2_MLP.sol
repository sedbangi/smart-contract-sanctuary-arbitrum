// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "../../BaseStrategyV2.sol";

interface IMUXRouter {
    function mlpFeeTracker() external view returns (address);
    function mlpMuxTracker() external view returns (address);
    function claimableReward(address user) external view returns (uint256);
    function mlp() external view returns (address);
    function claimFromMlp() external;
    function stakeMlp(uint256 _amount) external returns (uint256);
    function unstakeMlp(uint256 _amount) external returns (uint256);
}

contract StratX2_MLP is BaseStrategyV2 {
    using SafeERC20 for IERC20;

    IMUXRouter public router;
    address public fMLP;
    address public sbfMUX;

    function initialize(
        address[] memory _addresses,
        address _router,
        bool _isAutoComp
    ) public virtual {
        super.__initialize(_addresses, 0, false, _isAutoComp);

        router = IMUXRouter(_router);
        fMLP = IMUXRouter(_router).mlpFeeTracker();
        sbfMUX = IMUXRouter(_router).mlpMuxTracker();

        // reward MUX(0) + WETH(1)
        _addRewardToken(IERC20(wethAddress));
    }

    function _vaultDeposit(uint256 _amount) internal override {
        if (_amount > 0) {
            IERC20(router.mlp()).safeIncreaseAllowance(fMLP, _amount);
            router.stakeMlp(_amount);
        }
    }

    function _vaultWithdraw(uint256 _amount) internal override {
        router.unstakeMlp(_amount);
    }

    function _vaultHarvest() public override {
        router.claimFromMlp();
    }

    function _vaultPending() public view override returns (uint256[] memory pending)
    {
        uint256 rewardWETH = IMUXRouter(fMLP).claimableReward(address(this));
        uint256 rewardMUX = IMUXRouter(sbfMUX).claimableReward(address(this));

		pending = new uint256[](2);
        pending[0] = rewardMUX;
        pending[1] = rewardWETH;
    }

    receive() external payable { }
}

