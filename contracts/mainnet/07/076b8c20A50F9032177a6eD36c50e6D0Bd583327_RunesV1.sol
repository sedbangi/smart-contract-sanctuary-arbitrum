import "hardhat/console.sol";

pragma solidity ^0.8.17;
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "../IERC20Burnable.sol";

contract RunesV1 is Ownable, ReentrancyGuard {
    // Address authorized to check bets
    address public authorizedAddress;

    IERC20Burnable public deagleToken;

    // Amount of $DEAGLE accumulated
    uint256 public deagleAmount;

    // Max bet amount
    uint256 public maxBetAmount = 1000 ether;

    // Struct for a bet
    struct Bet {
        address player;
        uint256 timestamp;
        uint256 tokens;
        uint256 rounds;
        uint256 reward;
        bool checked;
    }

    // Struct address -> bet
    mapping(address => Bet) public bets;

    // events
    event BetPlaced(
        address indexed owner,
        uint256 timestamp,
        uint256 tokens,
        uint256 rounds
    );

    event BetChecked(
        address indexed owner,
        uint256 timestamp,
        uint256 tokens,
        uint256 rounds,
        uint256 reward
    );

    event BetPrizeClaimed(
        address indexed owner,
        uint256 timestamp,
        uint256 tokens,
        uint256 rounds,
        uint256 reward
    );

    event BetRemoved(
        address indexed owner,
        uint256 timestamp,
        uint256 tokens,
        uint256 rounds
    );

    constructor(address _deagleToken, address _authorizedAddress) {
        deagleToken = IERC20Burnable(_deagleToken);
        authorizedAddress = _authorizedAddress;
    }

    // Set max bet amount
    function setMaxBetAmount(uint256 _maxBetAmount) external onlyOwner {
        maxBetAmount = _maxBetAmount;
    }

    // Deposit DEAGLE
    function depositDeagle(uint256 amount) external {
        deagleToken.transferFrom(msg.sender, address(this), amount);

        deagleAmount += amount;
    }

    // Place a bet
    function placeBet(uint256 tokens, uint256 rounds) external nonReentrant {
        require(tokens > 0, "Invalid tokens");
        require(rounds > 0, "Invalid rounds");

        // Require the bet is less than the max bet amount
        require(tokens <= maxBetAmount, "Bet exceeds max amount");

        // Require the bank has at least triple the amount of the bet
        require(deagleAmount >= tokens * 3, "Bank does not have enough DEAGLE");

        // Make sure the user does not have a bet already
        require(bets[msg.sender].player == address(0), "Bet already placed");

        // Transfer the tokens to the contract
        deagleToken.transferFrom(msg.sender, address(this), tokens);

        // Add the bet to the mapping
        bets[msg.sender] = Bet(
            msg.sender,
            block.timestamp,
            tokens,
            rounds,
            0,
            false
        );

        emit BetPlaced(msg.sender, block.timestamp, tokens, rounds);
    }

    // Check a bet
    function checkBet(address player, uint256 reward) external {
        require(msg.sender == authorizedAddress, "Unauthorized");

        Bet storage bet = bets[player];
        require(bet.player != address(0), "Bet not found");
        require(!bet.checked, "Bet already checked");

        bet.reward = reward;
        bet.checked = true;

        emit BetChecked(
            player,
            block.timestamp,
            bet.tokens,
            bet.rounds,
            reward
        );
    }

    // Claim the prize of a bet
    function claimPrize(address player) public {
        Bet storage bet = bets[player];
        require(bet.player != address(0), "Bet not found");
        require(bet.checked, "Bet not checked");
        // Bet reward cant be more than the deagle amount
        require(bet.reward <= deagleAmount, "Bet reward exceeds bank amount");

        deagleAmount += bet.tokens;

        // Transfer the reward to the player
        if (bet.reward > 0) {
            deagleAmount -= bet.reward;
            deagleToken.transfer(player, bet.reward);
        }

        // Remove the bet
        delete bets[player];

        emit BetPrizeClaimed(
            player,
            block.timestamp,
            bet.tokens,
            bet.rounds,
            bet.reward
        );
    }

    // Withdraw bet (only if one hour has passed and is not checked)
    function withdrawBet() external {
        Bet storage bet = bets[msg.sender];
        require(bet.player != address(0), "Bet not found");

        if (bet.checked) {
            claimPrize(msg.sender);
        } else {
            // Check that one hour has passed
            require(
                block.timestamp - bet.timestamp >= 1 hours,
                "One hour has not passed"
            );

            // Transfer the tokens back to the player
            deagleToken.transfer(msg.sender, bet.tokens);

            // Remove the bet
            delete bets[msg.sender];

            emit BetRemoved(
                msg.sender,
                block.timestamp,
                bet.tokens,
                bet.rounds
            );
        }
    }

    // We can claim funds from the bank but never from the players bet amounts
    function claimFromBank(uint256 _amount) external onlyOwner {
        // Make sure we dont claim mroe than the bank has
        require(_amount <= deagleAmount, "Amount exceeds bank amount");

        deagleToken.transfer(msg.sender, _amount);

        deagleAmount -= _amount;
    }
}

