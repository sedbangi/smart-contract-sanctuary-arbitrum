pragma solidity 0.8.10;

library LibTokens {
  enum WopMintStage {
    NOT_STARTED,
    WHITELIST,
    WAITLIST,
    COMMUNITY,
    FINAL
  }

  enum WopUpgradeTier {
    RARE,
    EPIC,
    LEGENDARY
  }

  uint64 public constant WOP_BASE_ID = 1;
  uint64 public constant TOTAL_WOP_COUNT = 10_000;
  uint64 public constant RESERVED_WOP_COUNT = 500;
}

