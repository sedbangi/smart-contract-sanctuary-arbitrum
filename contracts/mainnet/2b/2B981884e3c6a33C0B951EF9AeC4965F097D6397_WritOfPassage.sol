pragma solidity 0.8.10;
import {LibAccessControl} from './lib/LibAccessControl.sol';
import {LibTokens} from './lib/LibTokens.sol';
import {EnumerableSet} from '@openzeppelin/contracts/utils/structs/EnumerableSet.sol';
import {ERC721URIStorageUpgradeable, ERC721Upgradeable} from '@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721URIStorageUpgradeable.sol';
import {ERC721PausableUpgradeable} from '@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721PausableUpgradeable.sol';
import {ERC2981Upgradeable} from '@openzeppelin/contracts-upgradeable/token/common/ERC2981Upgradeable.sol';
import {ERC721EnumerableUpgradeable} from '@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol';
import {IERC721Upgradeable} from '@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol';

struct InitArgs {
  uint32 maxWopPurchasesWaitlist;
  uint32 maxWopPurchasesWaitlistLandSeeker;
  uint32 maxWopPurchasesCommunity;
  uint32 wopCost;
  uint256 royaltiesPercentage;
  string legendaryWopUri;
  string epicWopUri;
  string rareWopUri;
  string contractUri;
  address contractFundsRecipient;
  address royaltiesRecipient;
  address[] adminAddresses;
}

/// @title WritOfPassage
/// @notice ERC721 contract for Writ of Passage NFTs.
contract WritOfPassage is
  ERC721URIStorageUpgradeable,
  ERC721PausableUpgradeable,
  ERC2981Upgradeable,
  ERC721EnumerableUpgradeable
{
  using EnumerableSet for EnumerableSet.UintSet;

  address public contractOwner;
  address public contractFundsRecipient;
  address public royaltiesRecipient;
  uint32 public maxWopPurchasesWaitlist;
  uint32 public maxWopPurchasesWaitlistLandSeeker;
  uint32 public maxWopPurchasesCommunity;
  // NOTE: 1000 = 1 eth.
  uint32 public wopCost;
  uint256 public wopIndex;
  uint256 public royaltiesPercentage;
  string public legendaryWopUri;
  string public epicWopUri;
  string public rareWopUri;
  string public contractUri;
  // NOTE: 0 = Null, 1 = Whitelist, 2 = Waitlist, 3 = Community.
  LibTokens.WopMintStage public wopMintStage;

  // NOTE: 0 = Rare, 1 = Epic, 2 = Legendary.
  mapping(uint256 => LibTokens.WopUpgradeTier) public upgradeTier;
  mapping(address => uint16) public wopAmountPurchasedByAddress;
  mapping(address => uint16) public wopAmountPurchasedByAddressWaitlist;
  mapping(address => uint16) public wopAmountPurchasedByAddressCommunity;
  mapping(address => uint16) public whitelists;
  mapping(address => bool) public waitlisted;
  mapping(address => bool) public isLandSeeker;
  mapping(address => bool) public isOperatorRestricted;
  // NOTE: 0 = Null, 1 = Admin.
  mapping(address => EnumerableSet.UintSet) rolesByAddress;

  modifier ownerOnly() {
    require(msg.sender == contractOwner, 'Only owner allowed');
    _;
  }

  modifier roleOnly(LibAccessControl.Roles role) {
    require(
      rolesByAddress[msg.sender].contains(uint256(role)) || msg.sender == address(this),
      'Missing role'
    );
    _;
  }

  function initialize(InitArgs memory args) public initializer {
    __ERC721_init('WritOfPassage', 'WOP');
    __ERC721Pausable_init();

    contractOwner = msg.sender;
    wopIndex = LibTokens.WOP_BASE_ID;
    wopMintStage = LibTokens.WopMintStage.NOT_STARTED;

    contractFundsRecipient = args.contractFundsRecipient;
    royaltiesRecipient = args.royaltiesRecipient;
    maxWopPurchasesWaitlist = args.maxWopPurchasesWaitlist;
    maxWopPurchasesWaitlistLandSeeker = args.maxWopPurchasesWaitlistLandSeeker;
    maxWopPurchasesCommunity = args.maxWopPurchasesCommunity;
    wopCost = args.wopCost;
    royaltiesPercentage = args.royaltiesPercentage;
    legendaryWopUri = args.legendaryWopUri;
    epicWopUri = args.epicWopUri;
    rareWopUri = args.rareWopUri;
    contractUri = args.contractUri;

    for (uint256 i; i < args.adminAddresses.length; i++) {
      rolesByAddress[args.adminAddresses[i]].add(uint256(LibAccessControl.Roles.ADMIN));
    }

    _setDefaultRoyalty(args.royaltiesRecipient, uint96(args.royaltiesPercentage * 100));
  }
  function setContractUri(string memory newContractUri) public ownerOnly {
    contractUri = newContractUri;
  }

  /// @notice Returns the metadata URI of a token
  function tokenURI(
    uint256 tokenId
  ) public view override(ERC721URIStorageUpgradeable, ERC721Upgradeable) returns (string memory) {
    if (upgradeTier[tokenId] == LibTokens.WopUpgradeTier.LEGENDARY) {
      return legendaryWopUri;
    } else if (upgradeTier[tokenId] == LibTokens.WopUpgradeTier.EPIC) {
      return epicWopUri;
    } else if (upgradeTier[tokenId] == LibTokens.WopUpgradeTier.RARE) {
      return rareWopUri;
    } else {
      return super.tokenURI(tokenId);
    }
  }

  function supportsInterface(
    bytes4 interfaceId
  )
    public
    view
    virtual
    override(
      ERC721Upgradeable,
      ERC721URIStorageUpgradeable,
      ERC2981Upgradeable,
      ERC721EnumerableUpgradeable
    )
    returns (bool)
  {
    return super.supportsInterface(interfaceId);
  }

  function setRestrictedOperator(
    address operator,
    bool restricted
  ) external roleOnly(LibAccessControl.Roles.ADMIN) {
    isOperatorRestricted[operator] = restricted;
  }

  function approve(
    address to,
    uint256 tokenId
  ) public virtual override(ERC721Upgradeable, IERC721Upgradeable) {
    require(isOperatorRestricted[to] == false, 'Approval to restricted operator');

    super.approve(to, tokenId);
  }

  function setApprovalForAll(
    address operator,
    bool approved
  ) public virtual override(ERC721Upgradeable, IERC721Upgradeable) {
    require(isOperatorRestricted[operator] == false, 'Approval to restricted operator');

    super.setApprovalForAll(operator, approved);
  }

  function tokensOfOwner(address owner) external view returns (uint256[] memory) {
    uint256 tokenCount = balanceOf(owner);
    uint256[] memory tokens = new uint256[](tokenCount);
    for (uint256 i = 0; i < tokenCount; i++) {
      tokens[i] = tokenOfOwnerByIndex(owner, i);
    }
    return tokens;
  }

  /// @notice Overrides so no tokens can be transferred, or burned when contract is paused or transfers are locked.
  function _beforeTokenTransfer(
    address from,
    address to,
    uint256 tokenId,
    uint256 batchSize
  )
    internal
    virtual
    override(ERC721PausableUpgradeable, ERC721Upgradeable, ERC721EnumerableUpgradeable)
    whenNotPaused
  {
    require(
      wopMintStage == LibTokens.WopMintStage.FINAL || from == address(0),
      'Token transfers locked'
    );

    super._beforeTokenTransfer(from, to, tokenId, batchSize);
  }

  /// @notice Toggles the contract between paused and unpaused states
  function pauseContract() external roleOnly(LibAccessControl.Roles.ADMIN) {
    if (!paused()) {
      _pause();
    } else {
      _unpause();
    }
  }

  /// @notice Gets roles assigned to an address
  /// @param user Address to get role from
  function getRoles(address user) external view returns (uint256[] memory) {
    uint256 length = rolesByAddress[user].length();
    uint256[] memory values = new uint256[](length);

    for (uint256 i = 0; i < length; i++) {
      values[i] = rolesByAddress[user].at(i);
    }

    return values;
  }

  /// @notice Assigns a role to an address
  /// @param recipient Address to set role to
  /// @param role Role to assign
  function setRole(address recipient, LibAccessControl.Roles role) external ownerOnly {
    require(!rolesByAddress[recipient].contains(uint256(role)), 'Role already set');
    rolesByAddress[recipient].add(uint256(role));
  }

  /// @notice Revokes a role from an address
  /// @param recipient Address to set role to
  /// @param role Role to assign
  function revokeRole(address recipient, LibAccessControl.Roles role) external ownerOnly {
    require(rolesByAddress[recipient].contains(uint256(role)), 'Role not set');
    rolesByAddress[recipient].remove(uint256(role));
  }

  /// @notice Sets addresses to a certain amount of whitelists
  /// @param addresses Addresses to set
  /// @param whitelistsAmount Amount of whitelists to set
  function setWhitelisted(
    address[] calldata addresses,
    uint16 whitelistsAmount
  ) external roleOnly(LibAccessControl.Roles.ADMIN) {
    for (uint16 i; i < addresses.length; i++) {
      whitelists[addresses[i]] = whitelistsAmount;
    }
  }

  /// @notice Sets addresses to be waitlisted or not
  /// @param addresses Addresses to set
  /// @param value Whether to set the address to be waitlisted or not
  function setWaitlisted(
    address[] calldata addresses,
    bool value
  ) external roleOnly(LibAccessControl.Roles.ADMIN) {
    for (uint16 i; i < addresses.length; i++) {
      waitlisted[addresses[i]] = value;
    }
  }

  /// @notice Sets addresses to be land seekers or not
  /// @param addresses Addresses to set
  /// @param value Whether to set the address to be land seekers or not
  function setLandSeeker(
    address[] calldata addresses,
    bool value
  ) external roleOnly(LibAccessControl.Roles.ADMIN) {
    for (uint16 i; i < addresses.length; i++) {
      isLandSeeker[addresses[i]] = value;
    }
  }

  /// @notice Mints one or more rare WOPs
  function mintWop(uint256 amount) external payable whenNotPaused {
    LibTokens.WopMintStage mintStage = wopMintStage;

    require(mintStage != LibTokens.WopMintStage.NOT_STARTED, 'Minting disabled');
    require(mintStage != LibTokens.WopMintStage.FINAL, 'Minting finished');
    require(amount > 0, 'Amount must be greater than 0');
    require(
      totalSupply() + amount <= LibTokens.TOTAL_WOP_COUNT - LibTokens.RESERVED_WOP_COUNT,
      'Amount exceeds max WOPs'
    );

    uint16 whitelistsAmount = whitelists[msg.sender];
    uint256 cost = (wopCost * amount * 1 ether) / 1000;

    if (mintStage == LibTokens.WopMintStage.WHITELIST) {
      require(amount <= whitelistsAmount, 'Not enough whitelists');
    } else if (mintStage == LibTokens.WopMintStage.WAITLIST) {
      require(waitlisted[msg.sender], 'Address not waitlisted');

      if (isLandSeeker[msg.sender]) {
        require(
          amount <=
            maxWopPurchasesWaitlistLandSeeker - wopAmountPurchasedByAddressWaitlist[msg.sender],
          'Max purchases reached LS'
        );
      } else {
        require(
          amount <= maxWopPurchasesWaitlist - wopAmountPurchasedByAddressWaitlist[msg.sender],
          'Max purchases reached NLS'
        );
      }
    } else if (mintStage == LibTokens.WopMintStage.COMMUNITY) {
      require(
        amount <= maxWopPurchasesCommunity - wopAmountPurchasedByAddressCommunity[msg.sender],
        'Amount exceeds max purchases'
      );
    } else {
      revert('Invalid mint stage');
    }

    require(msg.value == cost, 'Payment amount mismatch');

    if (mintStage == LibTokens.WopMintStage.WHITELIST) {
      whitelists[msg.sender] -= uint16(amount);
    }

    if (mintStage == LibTokens.WopMintStage.WAITLIST) {
      wopAmountPurchasedByAddressWaitlist[msg.sender] += uint16(amount);
    }

    if (mintStage == LibTokens.WopMintStage.COMMUNITY) {
      wopAmountPurchasedByAddressCommunity[msg.sender] += uint16(amount);
    }

    wopAmountPurchasedByAddress[msg.sender] += uint16(amount);

    for (uint256 i; i < amount; i++) {
      uint256 tokenId = _generateWopTokenId();
      _safeMint(msg.sender, tokenId);
    }
  }

  /// @notice Mints one or more team WOPs
  function mintTeamWop(
    uint256 amount
  ) external payable whenNotPaused roleOnly(LibAccessControl.Roles.ADMIN) {
    LibTokens.WopMintStage mintStage = wopMintStage;

    require(mintStage == LibTokens.WopMintStage.FINAL, 'Mint stage must be final');
    require(amount > 0, 'Amount must be greater than 0');
    require(totalSupply() + amount <= LibTokens.TOTAL_WOP_COUNT, 'Amount exceeds max WOPs');

    wopAmountPurchasedByAddress[msg.sender] += uint16(amount);

    for (uint256 i; i < amount; i++) {
      uint256 tokenId = _generateWopTokenId();
      _safeMint(msg.sender, tokenId);
    }
  }

  /// @notice Sets an upgrade tier for a list of WOPs
  /// @param tokenIds IDs of tokens to set upgrade tier for
  /// @param value Upgrade tier to set
  function upgradeWops(
    uint256[] memory tokenIds,
    LibTokens.WopUpgradeTier value
  ) external roleOnly(LibAccessControl.Roles.ADMIN) {
    for (uint16 i; i < tokenIds.length; i++) {
      upgradeTier[tokenIds[i]] = value;
      emit MetadataUpdate(tokenIds[i]);
    }
  }

  /// @notice Transfers ownership of the contract to a new address
  /// @param newOwner Address of the new owner
  function transferContractOwnership(address newOwner) public ownerOnly {
    require(newOwner != address(0), 'New owner cannot be the zero address');

    contractOwner = newOwner;
  }

  /// @notice Gets the base token ids
  function getBaseTokenIds()
    public
    pure
    returns (uint64 wopBaseId, uint64 totalWopCount, uint64 reservedWopCount)
  {
    return (LibTokens.WOP_BASE_ID, LibTokens.TOTAL_WOP_COUNT, LibTokens.RESERVED_WOP_COUNT);
  }

  /// @notice Gets the upgrade status of multiple tokens
  function getTokensUpgradeStatus(
    uint256[] memory tokenIds
  ) public view returns (LibTokens.WopUpgradeTier[] memory) {
    LibTokens.WopUpgradeTier[] memory values = new LibTokens.WopUpgradeTier[](tokenIds.length);

    for (uint256 i; i < tokenIds.length; i++) {
      values[i] = upgradeTier[tokenIds[i]];
    }

    return values;
  }

  /// @notice Toggles legendary WOP minting
  function setWopMintStage(
    LibTokens.WopMintStage stage
  ) external roleOnly(LibAccessControl.Roles.ADMIN) {
    wopMintStage = stage;
  }

  /// @notice Sets the cost of minting a WOP (1000 = 1 eth)
  function setWopCost(uint32 newCost) external roleOnly(LibAccessControl.Roles.ADMIN) {
    wopCost = newCost;
  }

  /// @notice Sets the maximum purchase limit for those purchasing in the waitlist phase
  function setMaxWopPurchasesWaitlist(
    uint32 newMax
  ) external roleOnly(LibAccessControl.Roles.ADMIN) {
    maxWopPurchasesWaitlist = newMax;
  }

  /// @notice Sets the maximum purchase limit for those purchasing in the waitlist phase for land seekers
  function setMaxWopPurchasesWaitlistLandSeeker(
    uint32 newMax
  ) external roleOnly(LibAccessControl.Roles.ADMIN) {
    maxWopPurchasesWaitlistLandSeeker = newMax;
  }

  /// @notice Sets the maximum purchase limit for those purchasing in the community phase
  function setMaxWopPurchasesCommunity(
    uint32 newMax
  ) external roleOnly(LibAccessControl.Roles.ADMIN) {
    maxWopPurchasesCommunity = newMax;
  }

  /// @notice Emits a MetadataUpdate for the max uint256. Marketplaces interpret this as a signal to refresh the whole collection
  function refreshCollectionMetadata() external roleOnly(LibAccessControl.Roles.ADMIN) {
    emit BatchMetadataUpdate(1, type(uint256).max);
  }

  /// @notice Sets the royalties recipient
  function setRoyaltiesRecipient(address newRecipient) external ownerOnly {
    _setRoyaltiesRecipient(newRecipient);
    _setDefaultRoyalty(newRecipient, uint96(royaltiesPercentage * 100));
  }

  /// @notice Sets the royalties percentage
  function setRoyaltiesPercentage(uint96 newPercentage) external ownerOnly {
    _setRoyaltiesPercentage(newPercentage);
    _setDefaultRoyalty(royaltiesRecipient, (newPercentage * 100));
  }

  /// @notice Sets the contract funds recipient
  function setContractFundsRecipient(address newRecipient) external ownerOnly {
    _setContractFundsRecipient(newRecipient);
  }

  function setLegendaryWopUri(
    string memory newUri
  ) external roleOnly(LibAccessControl.Roles.ADMIN) {
    legendaryWopUri = newUri;
  }

  function setEpicWopUri(string memory newUri) external roleOnly(LibAccessControl.Roles.ADMIN) {
    epicWopUri = newUri;
  }

  function setRareWopUri(string memory newUri) external roleOnly(LibAccessControl.Roles.ADMIN) {
    rareWopUri = newUri;
  }

  /// @notice Transfers contract funds to the contract funds recipient
  function transferContractFunds(uint256 funds) external roleOnly(LibAccessControl.Roles.ADMIN) {
    require(contractFundsRecipient != address(0), 'Contract Funds recipient is the zero address');
    payable(contractFundsRecipient).transfer(funds);
  }

  function _setRoyaltiesRecipient(address newRecipient) internal {
    require(newRecipient != address(0), 'Royalties: new recipient is the zero address');
    royaltiesRecipient = newRecipient;
  }

  function _setRoyaltiesPercentage(uint96 newPercentage) internal {
    require(newPercentage >= 0 && newPercentage <= 100, 'Invalid percentage');
    royaltiesPercentage = newPercentage;
  }

  function _setContractFundsRecipient(address newRecipient) internal {
    require(newRecipient != address(0), 'Contract Funds: new recipient is the zero address');
    contractFundsRecipient = newRecipient;
  }

  /// @dev Generates a unique incremental id for a WOP
  function _generateWopTokenId() internal whenNotPaused returns (uint256) {
    require(wopIndex <= LibTokens.TOTAL_WOP_COUNT, 'Max WOPs minted');

    return wopIndex++;
  }

  function _burn(
    uint256 tokenId
  ) internal override(ERC721URIStorageUpgradeable, ERC721Upgradeable) {
    super._burn(tokenId);
  }
}

