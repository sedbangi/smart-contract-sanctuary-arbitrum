    RequestSignature calldata signature
  )
    internal
    view
    returns(
      bytes32 hashed
    )
  {
    _checkContext(signature);
    hashed = keccak256(
      abi.encode(
        CONTEXT_TYPEHASH,
        _msgSender(),
        signature.expiry,
        signature.nonce
      )
    );
  }

