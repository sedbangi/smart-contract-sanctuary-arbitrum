pragma solidity ^0.8.13;

import { MoonpassInitializer, RequestSignature } from "../interfaces/IMoonpassContract.sol";
import { IMoonpassNFTTieredCollection, SetTokenTierRequest, ClaimRequest, AirdropRequest } from "../interfaces/nft/IMoonpassNFTTieredCollection.sol";
import { NFTCollectionInitializer } from "../interfaces/nft/IMoonpassNFTCollection.sol";
import { MoonpassNFTCollection } from "./MoonpassNFTCollection.sol";
import { MoonpassRoles } from "../auth/MoonpassRoles.sol";

contract MoonpassNFTTieredCollection is
  IMoonpassNFTTieredCollection,
  MoonpassNFTCollection
{
  bytes32 internal constant SET_TOKEN_TIER_TYPEHASH = keccak256("SetTokenTierRequest(uint256 tokenId,uint256 tier,RequestContext context)RequestContext(address requester,uint256 expiry,uint256 nonce)");
  bytes32 internal constant CLAIM_TYPEHASH = keccak256("ClaimRequest(address to,uint256 quantity,uint256 tier,RequestContext context)RequestContext(address requester,uint256 expiry,uint256 nonce)");
  bytes32 internal constant AIRDROP_TYPEHASH = keccak256("AirdropRequest(address[] to,uint256[] quantity,uint256[] tier,RequestContext context)RequestContext(address requester,uint256 expiry,uint256 nonce)");

  struct MoonpassNFTTieredCollectionStorage {
    mapping(uint256 => uint256) _tokenTiers;
    uint256 _currentTier;
  }

  bytes32 private constant MoonpassNFTTieredCollectionStorageLocation = 0x21971352363936334edc4314bbaf920ce102073132ed4afd3a00708ccc6bbb78;

  function _getMoonpassNFTTieredCollectionStorage()
    private
    pure 
    returns (MoonpassNFTTieredCollectionStorage storage $) 
  {
    assembly {
      $.slot := MoonpassNFTTieredCollectionStorageLocation
    }
  }

  function initialize(
    NFTCollectionInitializer calldata data,
    MoonpassInitializer calldata moonpass
  )
    public
    initializer
    initializerERC721A
  {
    __MoonpassNFTCollection_init(data, moonpass);
    __Nonces_init();
  }

  function tierOf(
    uint256 tokenId
  )
    public
    view
    returns(
      uint256 tier
    )
  {
    MoonpassNFTTieredCollectionStorage storage $ = _getMoonpassNFTTieredCollectionStorage();
    tier = ($._tokenTiers[tokenId / 128] >> (tokenId % 128 * 2) & 3);
  }

  function setTokenTier(
    uint256 tokenId,
    uint256 tier
  )
    external
    onlyRole(MoonpassRoles.NFT_MANAGER_ROLE)
  {
    _setTier(tokenId, tier);
  }

  function setTokenTier(
    SetTokenTierRequest calldata request,
    RequestSignature calldata signature
  )
    external
  {
    bytes memory data = abi.encode(
      SET_TOKEN_TIER_TYPEHASH,
      request.tokenId,
      request.tier,
      _encodeContext(signature)
    );
    _checkRole(MoonpassRoles.NFT_TRUSTEE_ROLE, _signerFrom(data, signature));
    _useCheckedNonce(_msgSender(), signature.nonce);
    _setTier(request.tokenId, request.tier);
  }

  function _setTier(
    uint256 tokenId,
    uint256 tier
  )
    internal
  {
    if(tier > 3) {
      revert InvalidTokenTierError();
    }
    uint256 slot = tokenId / 128;
    uint256 bits = tokenId % 128 * 2;
    MoonpassNFTTieredCollectionStorage storage $ = _getMoonpassNFTTieredCollectionStorage();
    $._tokenTiers[slot] = ($._tokenTiers[slot] & ~(3 << bits)) | (tier << bits);

    emit NFTTokenTierChange(tokenId, tier);
  }

  function claim(
    address to,
    uint256 quantity,
    uint256 tier
  )
    external
    onlyRole(MoonpassRoles.NFT_MINTER_ROLE)
  {
    _claim(to, quantity, tier);
  }

  function claim(
    ClaimRequest calldata request,
    RequestSignature calldata signature
  )
    external
  {
    bytes memory data = abi.encode(
      CLAIM_TYPEHASH,
      request.to,
      request.quantity,
      _encodeContext(signature)
    );
    _checkRole(MoonpassRoles.NFT_MINTER_TRUSTEE_ROLE, _signerFrom(data, signature));
    _useCheckedNonce(_msgSender(), signature.nonce);
    _claim(request.to, request.quantity, request.tier);
  }

  function _claim(
    address to,
    uint256 quantity,
    uint256 tier
  )
    internal
  {
    MoonpassNFTTieredCollectionStorage storage $ = _getMoonpassNFTTieredCollectionStorage();
    $._currentTier = tier;
    _mint(to, quantity);
  }

  function airdrop(
    address[] calldata to,
    uint256[] calldata quantity,
    uint256[] calldata tier
  )
    external
    onlyRole(MoonpassRoles.NFT_MINTER_ROLE)
  {
    _airdrop(to, quantity, tier);
  }

  function airdrop(
    AirdropRequest calldata request,
    RequestSignature calldata signature
  )
    external
  {
    bytes memory data = abi.encode(
      AIRDROP_TYPEHASH,
      request.to,
      request.quantity,
      _encodeContext(signature)
    );
    _checkRole(MoonpassRoles.NFT_MINTER_TRUSTEE_ROLE, _signerFrom(data, signature));
    _useCheckedNonce(_msgSender(), signature.nonce);
    _airdrop(request.to, request.quantity, request.tier);
  }

  function _airdrop(
    address[] calldata to,
    uint256[] calldata quantity,
    uint256[] calldata tier
  )
    internal
  {
    uint256 len = to.length;
    if(len != quantity.length || len != tier.length) {
      revert InvalidAirdropRequest();
    }
    MoonpassNFTTieredCollectionStorage storage $ = _getMoonpassNFTTieredCollectionStorage();
    for(uint256 i = 0; i < len;) {
      $._currentTier = tier[i];
      _mint(to[i], quantity[i]);
      unchecked {
        i++;
      }
    }
  }

  function _afterTokenTransfers(
    address from,
    address to,
    uint256 startTokenId,
    uint256 quantity
  )
    internal
    override
  {
    if(to == address(0)) {
      return;
    }
    uint256 tier = _getMoonpassNFTTieredCollectionStorage()._currentTier;
    for(uint256 i = 0; i < quantity; i++) {
      _setTier(startTokenId + i, tier);
    }
  }

}

