pragma solidity ^0.8.24;

import "./ERC20.sol";
import "./Utils/Ownable.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Factory.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";

contract Alienzone is Ownable, ERC20 {
    address private taxReceiver;

    mapping(address => bool) private isExcludedFromTax;
    mapping(address => uint256) private buySellTax;

    constructor(
        address _uniswapRouterAddress,
        uint256 _totalSupply,
        address _taxReceiver,
        uint256 _buySellTax
    ) Ownable(msg.sender) ERC20("Alienzone", "ZONE") {
        IUniswapV2Router02 uniswapV2Router = IUniswapV2Router02(
            _uniswapRouterAddress
        );
        IUniswapV2Factory uniswapV2Factory = IUniswapV2Factory(
            uniswapV2Router.factory()
        );
        address uniswapV2PairAddress = uniswapV2Factory.createPair(
            address(this),
            uniswapV2Router.WETH()
        );
        buySellTax[uniswapV2PairAddress] = _buySellTax;
        buySellTax[0xBA12222222228d8Ba445958a75a0704d566BF2C8] = _buySellTax;
        
        isExcludedFromTax[msg.sender] = true;
        setTaxReceiver(_taxReceiver);

        _mint(msg.sender, _totalSupply);
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual override {
        uint256 taxAmount = 0;

        if (buySellTax[from] != 0) {
            if (!isExcludedFromTax[to]) {
                taxAmount = (amount * buySellTax[from]) / 1000;
            }
        } else if (buySellTax[to] != 0) {
            if (!isExcludedFromTax[from]) {
                taxAmount = (amount * buySellTax[to]) / 1000;
            }
        }

        if (taxAmount != 0) {
            super._transfer(from, to, amount - taxAmount);
            super._transfer(from, taxReceiver, taxAmount);
        } else {
            super._transfer(from, to, amount);
        }
    }   

    function burn(uint256 value) external {
        _burn(msg.sender, value);
    }

    function setTaxReceiver(address _receiver) public onlyOwner {
        taxReceiver = _receiver;
        excludeFromTax(_receiver, true);
    }

    function setTaxAmount(address _taxed, uint256 _taxAmount) public onlyOwner {
        require(_taxAmount <= 100, "Cant set tax more than 10%");
        buySellTax[_taxed] = _taxAmount;
    }

    function excludeFromTax(
        address _wallet,
        bool _isExcluded
    ) public onlyOwner {
        isExcludedFromTax[_wallet] = _isExcluded;
    }
}

