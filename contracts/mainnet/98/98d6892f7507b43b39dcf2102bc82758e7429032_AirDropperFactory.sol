// import "@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol";

contract AirDropper is ReentrancyGuard {
    using EnumerableSet for EnumerableSet.Bytes32Set;
    using SafeERC20 for IERC20;

    error ClaimEnded();
    error AlreadyClaimed();
    error IncorrectMerkle();
    error PriceNotReached();

    bytes32 public _root;
    uint256 private _deadline;
    address private _token;
    address private _treasury;

    EnumerableSet.Bytes32Set private claimedUsers;

    constructor() {}

    function initialize(
        bytes32 root_,
        uint256 duration_,
        address token_,
        address treasury_
    ) external {
        _root = root_;
        _deadline = block.timestamp + duration_;
        _token = token_;
        _treasury = treasury_;
    }

    function deadline() external view returns (uint256) {
        return _deadline;
    }

    function hasClaimed() external view returns (bool) {
        return claimedUsers.contains(keccak256(abi.encode(msg.sender)));
    }

    function token() external view returns (address) {
        return address(this);
    }

    function symbol() external pure returns (string memory) {
        return "airdrop";
    }

    function decimals() external pure returns (uint) {
        return 18;
    }

    function claim(
        bytes32[] calldata _proof,
        uint256 amount
    ) external nonReentrant {
        if (block.timestamp > _deadline) {
            revert ClaimEnded();
        }

        if (claimedUsers.contains(keccak256(abi.encode(msg.sender)))) {
            revert AlreadyClaimed();
        }

        bytes32 leaf = keccak256(bytes.concat(keccak256(abi.encode(msg.sender, amount))));
        if (!MerkleProof.verify(_proof, _root, leaf)) {
            revert IncorrectMerkle();
        }

        claimedUsers.add(keccak256(abi.encode(msg.sender)));

        IERC20(_token).safeTransferFrom(_treasury, msg.sender, amount);
    }
}

