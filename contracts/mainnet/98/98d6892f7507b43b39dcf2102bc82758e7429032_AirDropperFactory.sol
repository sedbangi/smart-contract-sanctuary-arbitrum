// File: AirDropperFactory.sol



pragma solidity 0.8.25;




contract AirDropperFactory is Initializable {
    using SafeERC20 for IERC20;

    // AggregatorV3Interface internal priceFeed; // Chainlink AVAX/USD Price Feed
    address public immutable airdropperIpml;

    mapping(address => address[]) private _airdroppers;

    constructor() {
        airdropperIpml = address(new AirDropper());
        // priceFeed = AggregatorV3Interface(_priceFeedAddress);
    }

    function createAirdrop(
        bytes32 root_,
        uint256 duration_,
        address token_,
        address treasury_
    ) external {
        bytes32 salt = keccak256(
            abi.encodePacked(msg.sender, _airdroppers[msg.sender].length)
        );

        address airdropperNew = Clones.cloneDeterministic(airdropperIpml, salt);

        _airdroppers[msg.sender].push(airdropperNew);

        AirDropper(airdropperNew).initialize(
            root_,
            duration_,
            token_,
            treasury_
        );
    }

    function getNumAidroppers(address owner) external view returns (uint256) {
        return _airdroppers[owner].length;
    }

    function getAidroppers(
        uint256 cursor,
        uint256 howMany,
        address owner
    ) external view returns (address[] memory airdroppers, uint256 newCursor) {
        unchecked {
            address[] storage _airdroppersByOwner = _airdroppers[owner];
            uint256 numAirdroppers = _airdroppersByOwner.length;
            if (numAirdroppers == 0) {
                return (new address[](0), 0);
            }
            if (cursor >= numAirdroppers) {
                return (new address[](0), numAirdroppers);
            }

            uint256 length = numAirdroppers - cursor;
            if (length > howMany) {
                length = howMany;
            }

            airdroppers = new address[](length);
            for (uint256 i; i != length; ++i) {
                airdroppers[i] = _airdroppersByOwner[cursor];
                ++cursor;
            }

            newCursor = cursor;
        }
    }
}