    uint24[] memory fee = new uint24[](3);
    fee[0] = 100;
    fee[1] = 500;
    fee[2] = 3000;
    fee[3] = 10000;
    for (uint256 i = 0; i < fee.length; i++) {
      params.fee = fee[i];
