contract UniswapSniper is Ownable2StepUpgradeable, UUPSUpgradeable {
  mapping(address => bool) public snipers;
  ISwapRouter public swapRouter;
  IERC20Metadata public tokenIn;
  IERC20Metadata public tokenOut;
  uint256[50] private __gap;

  constructor() {
    _disableInitializers();
  }

  modifier onlySniper() {
    require(snipers[msg.sender], "Sniper: caller is not the sniper");
    _;
  }

  function initialize(
    ISwapRouter _swapRouter,
    IERC20Metadata _tokenOut,
    IERC20Metadata _tokenIn,
    address _owner,
    address[] memory _snipers
  ) external initializer {
    for (uint256 i = 0; i < _snipers.length; i++) {
      snipers[_snipers[i]] = true;
    }
    swapRouter = _swapRouter;
    tokenIn = _tokenIn;
    tokenOut = _tokenOut;
    __Ownable2Step_init();
    __UUPSUpgradeable_init();
    _transferOwnership(_owner);
  }

  function snipe() external onlySniper {
    buy(tokenOut.balanceOf(address(this)));
  }

  function snipeWithMinOut(uint256 minTokenOut) external onlySniper {
    uint256 startingBal = tokenIn.balanceOf(address(this));
    buy(tokenOut.balanceOf(address(this)));
    require(
      tokenIn.balanceOf(address(this)) - startingBal >= minTokenOut,
      "Sniper: minimum token out out not met"
    );
  }

  function addSnipers(address[] memory newSnipers) external onlyOwner {
    for (uint256 i = 0; i < newSnipers.length; i++) {
      snipers[newSnipers[i]] = true;
    }
  }

  function removeSnipers(address[] memory oldSnipers) external onlyOwner {
    for (uint256 i = 0; i < oldSnipers.length; i++) {
      snipers[oldSnipers[i]] = false;
    }
  }

  function setTokenIn(address _tokenIn) external onlyOwner {
    tokenIn = IERC20Metadata(_tokenIn);
  }

  function setTokenOut(address _tokenOut) external onlyOwner {
    tokenOut = IERC20Metadata(_tokenOut);
  }

  function withdrawERC20(
    address token,
    address to,
    uint256 amount
  ) external onlyOwner {
    IERC20Metadata(token).transfer(to, amount);
  }

  function withdrawEth(address to, uint256 amount) external onlyOwner {
    (bool sent, bytes memory data) = to.call{value: amount}("");
    require(sent, "Sniper: failed to send Ether");
  }

  receive() external payable {}

  function _authorizeUpgrade(address) internal override onlyOwner {}

  function buy(uint256 amountIn) public returns (uint256 amountOut) {
    // Approve the router to spend DAI.
    TransferHelper.safeApprove(address(tokenIn), address(swapRouter), amountIn);

    ISwapRouter.ExactInputSingleParams memory params = ISwapRouter
      .ExactInputSingleParams({
        tokenIn: address(tokenIn),
        tokenOut: address(tokenOut),
        fee: 3000,
        recipient: address(this),
        deadline: block.timestamp,
        amountIn: amountIn,
        amountOutMinimum: 0,
        sqrtPriceLimitX96: 0
