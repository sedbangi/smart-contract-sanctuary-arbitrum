    function setTransferAllowed(
        bool allowed
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _transferAllowed = allowed;
        emit TransferAllowedChanged(msg.sender, allowed);
    }

    function setBaseURI(
        string memory newBaseUri
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _baseUri = newBaseUri;
    }

    function setValidator(
        address validator
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(validator != address(0), "Cannot set to 0 address");
        _validator = validator;
    }

    function setFundsRecipient(
        address payable newFundsRecipient
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(newFundsRecipient != address(0), "Cannot set to 0 address");
        _fundsRecipient = newFundsRecipient;
        emit FundsRecipientChanged(msg.sender, newFundsRecipient);
    }

    function setOrAddTierInfo(
        uint256 index,
        uint256 price,
        uint256 totalQuantity,
        uint256 earlySaleQuantity,
        uint256 publicSaleLimitPerAddress
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(index <= _tiersInfo.length, "Index out of bounds");
        require(
            earlySaleQuantity < totalQuantity,
            "Early sale quantity must be less than total quantity"
        );

        Tier memory newTier = Tier(
            price,
            totalQuantity,
            earlySaleQuantity,
            publicSaleLimitPerAddress
        );
        if (index < _tiersInfo.length) {
            _maxSupply -= _tiersInfo[index].totalQuantity;
            _tiersInfo[index] = newTier;
        } else {
            _tiersInfo.push(newTier);
        }

        _maxSupply += totalQuantity;
        emit TierInfoSetOrAdded(index, price, totalQuantity, earlySaleQuantity);
    }

    function _setSaleTime(
        uint256 startTime,
        uint256 endTime,
        bool isEarlySale
    ) internal {
        require(
            startTime <= type(uint64).max &&
                endTime <= type(uint64).max &&
                startTime < endTime,
            "Invalid time"
        );

        TimeRange memory newTimeRange = TimeRange(
            uint64(startTime),
            uint64(endTime)
        );

        if (isEarlySale) {
            _earlySaleTime = newTimeRange;
        } else {
            _publicSaleTime = newTimeRange;
        }
    }

    function setPublicSaleTime(
        uint256 publicSaleStartTime,
        uint256 publicSaleEndTime
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _setSaleTime(publicSaleStartTime, publicSaleEndTime, false);
    }

    function setEarlySaleTime(
        uint256 earlySaleStartTime,
        uint256 earlySaleEndTime
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _setSaleTime(earlySaleStartTime, earlySaleEndTime, true);
    }

    function setDefaultRoyalty(
        address receiver,
        uint96 feeNumerator
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _setDefaultRoyalty(receiver, feeNumerator);
    }

    function setTokenRoyalty(
        uint256 tokenId,
        address receiver,
        uint96 feeNumerator
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _setTokenRoyalty(tokenId, receiver, feeNumerator);
    }

    function withdrawETH(
        address payable recipient,
        uint256 amount
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(amount <= address(this).balance, "Insufficient balance");
        (bool success, ) = recipient.call{value: amount}("");
        require(success, "Transfer failed");
    }

    function withdrawErc20(
        address tokenAddress,
        address to,
        uint256 amount
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        IERC20 token = IERC20(tokenAddress);
        uint256 balance = token.balanceOf(address(this));
        require(amount <= balance, "Insufficient balance");

        token.transfer(to, amount);
    }

    function withdrawErc721(
        address tokenAddress,
        address to,
        uint256 tokenId
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        IERC721 token = IERC721(tokenAddress);
        require(
            token.ownerOf(tokenId) == address(this),
            "Token is not owned by contract"
        );

        token.safeTransferFrom(address(this), to, tokenId);
    }

    function withdrawErc1155(
        address tokenAddress,
        address to,
        uint256 tokenId,
        uint256 amount,
        bytes memory data
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        IERC1155 token = IERC1155(tokenAddress);
        uint256 balance = token.balanceOf(address(this), tokenId);
        require(amount <= balance, "Insufficient balance");

        token.safeTransferFrom(address(this), to, tokenId, amount, data);
    }

    /* View & Pure & Getter Functions */
    function _verifySignature(
        PurchaseType purchaseType,
        uint256 quantity,
        uint256 purchaseLimit,
        uint256 tierIndex,
        address to,
        bytes32 referralCode,
        uint256 expireTimestamp,
        bytes calldata signature
    ) internal view {
        bytes32 digest;
        if (purchaseType == PurchaseType.EARLY_SALE) {
            digest = _hashTypedDataV4(
                keccak256(
                    abi.encode(
                        EARLY_SALE_TYPEHASH,
                        quantity,
                        purchaseLimit,
                        tierIndex,
                        to,
                        referralCode,
                        expireTimestamp
                    )
                )
            );
        } else if (purchaseType == PurchaseType.PUBLIC_SALE) {
            digest = _hashTypedDataV4(
                keccak256(
                    abi.encode(
                        PUBLIC_SALE_TYPEHASH,
                        quantity,
                        purchaseLimit,
                        tierIndex,
                        to,
                        referralCode,
                        expireTimestamp
                    )
                )
            );
        } else {
            digest = _hashTypedDataV4(
                keccak256(
                    abi.encode(
                        SOLANA_DELEGATOR_TYPEHASH,
                        quantity,
                        purchaseLimit,
                        tierIndex,
                        to,
                        referralCode,
                        expireTimestamp
                    )
                )
            );
        }

        require(
            ECDSA.recover(digest, signature) == _validator,
            "Invalid signature"
        );
    }

    function compatTierAddress(
        uint256 tierIndex,
        address userAddress
    ) public pure returns (bytes32) {
        return keccak256(abi.encode(tierIndex, userAddress));
    }

    function getTiersInfoLength() external view returns (uint256) {
        return _tiersInfo.length;
    }

    function getTierInfo(uint256 index) public view returns (Tier memory) {
        require(index < _tiersInfo.length, "Index out of bounds");
        return _tiersInfo[index];
    }

    function getNumberMinted(address account) public view returns (uint256) {
        return _numberMinted(account);
    }

    function getTotalMinted() public view returns (uint256) {
        return _totalMinted();
    }

    function getTierMintedEarlySale(
        uint256 tierIndex
    ) public view returns (uint256) {
        return _tiersMintedEarlySale[tierIndex];
    }

    function getAllTiersMintedEarlySale()
        public
        view
        returns (uint256[] memory)
    {
        uint256 tiersLength = _tiersInfo.length;
        uint256[] memory mintedEarlySales = new uint256[](tiersLength);

        for (uint256 i = 0; i < tiersLength; i++) {
            mintedEarlySales[i] = _tiersMintedEarlySale[i];
        }

        return mintedEarlySales;
    }

    function getTierMintedPublicSale(
        uint256 tierIndex
    ) public view returns (uint256) {
        return _tiersMintedPublicSale[tierIndex];
    }

    function getAllTiersMintedPublicSale()
        public
        view
        returns (uint256[] memory)
    {
        uint256 tiersLength = _tiersInfo.length;
        uint256[] memory mintedPublicSales = new uint256[](tiersLength);

        for (uint256 i = 0; i < tiersLength; i++) {
            mintedPublicSales[i] = _tiersMintedPublicSale[i];
        }

        return mintedPublicSales;
    }

    function getAllTiersQuantitiesEarlySale()
        public
        view
        returns (uint256[] memory)
    {
        uint256 tiersLength = _tiersInfo.length;
        uint256[] memory earlySaleQuantities = new uint256[](tiersLength);

        for (uint256 i = 0; i < tiersLength; i++) {
            earlySaleQuantities[i] = _tiersInfo[i].earlySaleQuantity;
        }

        return earlySaleQuantities;
    }

    function getAllTiersQuantitiesPublicSale()
        public
        view
        returns (uint256[] memory)
    {
        uint256 tiersLength = _tiersInfo.length;
        uint256[] memory publicSaleQuantities = new uint256[](tiersLength);

        for (uint256 i = 0; i < tiersLength; i++) {
            publicSaleQuantities[i] =
                _tiersInfo[i].totalQuantity -
                _tiersInfo[i].earlySaleQuantity;
        }

        return publicSaleQuantities;
    }

    function getAllTiersLimitPerAddressPublicSale()
        public
        view
        returns (uint256[] memory)
    {
        uint256 tiersLength = _tiersInfo.length;
        uint256[] memory addressLimits = new uint256[](tiersLength);

        for (uint256 i = 0; i < tiersLength; i++) {
            addressLimits[i] = _tiersInfo[i].publicSaleLimitPerAddress;
        }

        return addressLimits;
    }

    function getAllTiersPrices() public view returns (uint256[] memory) {
        uint256 tiersLength = _tiersInfo.length;
        uint256[] memory prices = new uint256[](tiersLength);

        for (uint256 i = 0; i < tiersLength; i++) {
            prices[i] = _tiersInfo[i].price;
        }

        return prices;
    }

    function getTierAddressMintedPublicSale(
        uint256 tierIndex,
        address userAddress
    ) public view returns (uint256) {
        return
            _tiersAddressesMintedPublicSale[
                compatTierAddress(tierIndex, userAddress)
            ];
    }

    function getAllTiersAddressMintedPublicSale(
        address userAddress
    ) public view returns (uint256[] memory) {
        uint256 tiersLength = _tiersInfo.length;
        uint256[] memory mintedPublicSales = new uint256[](tiersLength);

        for (uint256 i = 0; i < tiersLength; i++) {
            mintedPublicSales[i] = getTierAddressMintedPublicSale(
                i,
                userAddress
            );
        }

        return mintedPublicSales;
    }

    function getTierAddressMintedEarlySale(
        uint256 tierIndex,
        address userAddress
    ) public view returns (uint256) {
        return
            _tiersAddressesMintedEarlySale[
                compatTierAddress(tierIndex, userAddress)
            ];
    }

    function getAllTiersAddressMintedEarlySale(
        address userAddress
    ) public view returns (uint256[] memory) {
        uint256 tiersLength = _tiersInfo.length;
        uint256[] memory mintedEarlySales = new uint256[](tiersLength);

        for (uint256 i = 0; i < tiersLength; i++) {
            mintedEarlySales[i] = getTierAddressMintedEarlySale(i, userAddress);
        }

        return mintedEarlySales;
    }

    function getBaseURI() public view returns (string memory) {
        return _baseUri;
    }

    function _baseURI() internal view override returns (string memory) {
        return _baseUri;
    }

    function _isBetween(
        uint256 startTime,
        uint256 endTime
    ) internal view returns (bool) {
        return startTime <= block.timestamp && block.timestamp < endTime;
    }

    function isEarlySaleActive() public view returns (bool) {
        TimeRange memory earlyTime = _earlySaleTime;
        return _isBetween(earlyTime.beginTimestamp, earlyTime.endTimestamp);
    }

    function isPublicSaleActive() public view returns (bool) {
        TimeRange memory pubTime = _publicSaleTime;
        return _isBetween(pubTime.beginTimestamp, pubTime.endTimestamp);
    }

    function getEarlySaleTime() public view returns (TimeRange memory) {
        return _earlySaleTime;
    }

    function getPublicSaleTime() public view returns (TimeRange memory) {
        return _publicSaleTime;
    }

    function supportsInterface(
        bytes4 interfaceId
    )
        public
        view
        virtual
        override(ERC721A, ERC2981, AccessControl)
        returns (bool)
    {
        return
            ERC721A.supportsInterface(interfaceId) ||
            AccessControl.supportsInterface(interfaceId) ||
            ERC2981.supportsInterface(interfaceId);
    }
}

