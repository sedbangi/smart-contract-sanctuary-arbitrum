    event TransferAllowedChanged(address indexed admin, bool transferAllowed);
    event TierInfoSetOrAdded(
        uint256 index,
        uint256 price,
        uint256 totalQuantity,
        uint256 earlySaleQuantity
    );
    event EarlySalePurchased(
        uint256 quantity,
        uint256 tierPrice,
        uint256 indexed tierIndex,
        address indexed to,
        bytes32 indexed referralCode
    );
    event PublicSalePurchased(
        uint256 quantity,
        uint256 tierPrice,
        uint256 indexed tierIndex,
        address indexed to,
        bytes32 indexed referralCode
    );
    event SolanaDelegatorPurchased(
        uint256 indexed uuid,
        bytes solanaAddress,
        uint256 quantity,
        uint256 tierPrice,
        uint256 tierIndex,
        address to,
        bytes32 referralCode
    );

    using Strings for uint256;

    TimeRange private _publicSaleTime;
    TimeRange private _earlySaleTime;
    string private _baseUri;
    address private _validator;
    Tier[] private _tiersInfo;
    mapping(bytes32 => uint256) private _tiersAddressesMintedPublicSale;
    mapping(bytes32 => uint256) private _tiersAddressesMintedEarlySale;
    mapping(uint256 => uint256) private _tiersMintedEarlySale;
    mapping(uint256 => uint256) private _tiersMintedPublicSale;
    mapping(bytes32 => bool) private _signatureUsed;
    address payable public _fundsRecipient;
    uint256 public _maxSupply;
    bool public _transferAllowed;

    string private constant VERSION = "1";
    bytes32 public constant SOLANA_DELEGATOR_ROLE =
        keccak256("SOLANA_DELEGATOR_ROLE");
    bytes32 public constant IDO_DELEGATOR_ROLE =
        keccak256("IDO_DELEGATOR_ROLE");
    bytes32 private constant EARLY_SALE_TYPEHASH =
        keccak256(
            "earlySalePurchase(uint256 quantity,uint256 purchaseLimit,uint256 tierIndex,address to,bytes32 referralCode,uint256 expireTimestamp)"
        );
    bytes32 private constant PUBLIC_SALE_TYPEHASH =
        keccak256(
            "publicSalePurchase(uint256 quantity,uint256 purchaseLimit,uint256 tierIndex,address to,bytes32 referralCode,uint256 expireTimestamp)"
        );
    bytes32 private constant SOLANA_DELEGATOR_TYPEHASH =
        keccak256(
            "solanaDelegatorPurchase(uint256 quantity,uint256 purchaseLimit,uint256 tierIndex,address to,bytes32 referralCode,uint256 expireTimestamp)"
        );

    modifier earlySaleActive() {
        require(isEarlySaleActive(), "Sale inactive");
        _;
    }

    modifier publicSaleActive() {
        require(isPublicSaleActive(), "Sale inactive");
        _;
    }

    constructor(
        string memory name,
        string memory symbol,
        string memory baseUri,
        address payable fundsRecipient,
        address validator,
        uint256 earlySaleStartTime,
        uint256 earlySaleEndTime,
        uint256 publicSaleStartTime,
        uint256 publicSaleEndTime,
        address loyaltyRecipient,
        uint96 loyaltyFeeNumerator
    ) ERC721A(name, symbol) EIP712(name, VERSION) {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setRoleAdmin(SOLANA_DELEGATOR_ROLE, DEFAULT_ADMIN_ROLE);
        _setRoleAdmin(IDO_DELEGATOR_ROLE, DEFAULT_ADMIN_ROLE);

        _setSaleTime(earlySaleStartTime, earlySaleEndTime, true);
        _setSaleTime(publicSaleStartTime, publicSaleEndTime, false);
        _setDefaultRoyalty(loyaltyRecipient, loyaltyFeeNumerator);

        _baseUri = baseUri;
        require(
            fundsRecipient != address(0) && validator != address(0),
            "Cannot set to 0 address"
        );
        _fundsRecipient = fundsRecipient;
        _validator = validator;
    }

    function _handlePurchase(
        PurchaseType purchaseType,
        uint256 quantity,
        uint256 purchaseLimit,
        uint256 tierIndex,
        address to,
        bytes32 referralCode,
        uint256 expireTimestamp,
        bytes calldata signature
    ) internal {
        require(block.timestamp < expireTimestamp, "Purchase expired");
        require(
            _maxSupply == 0 || _totalMinted() + quantity <= _maxSupply,
            "Minted exceed max supply"
        );
        uint256 purchasePrice = hasRole(SOLANA_DELEGATOR_ROLE, msg.sender)
            ? 0
            : _tiersInfo[tierIndex].price * quantity;
        require(msg.value == purchasePrice, "Invalid amount");

        bytes32 signatureHash = keccak256(abi.encodePacked(signature));
        require(!_signatureUsed[signatureHash], "Signature used");
        _verifySignature(
            purchaseType,
            quantity,
            purchaseLimit,
            tierIndex,
            to,
            referralCode,
            expireTimestamp,
            signature
        );
        _signatureUsed[signatureHash] = true;

        _safeMint(to, quantity);
        if (purchasePrice > 0) {
            _payoutPurchasePrice(purchasePrice);
        }
    }

    function _payoutPurchasePrice(uint256 purchasePrice) internal {
        (bool success, ) = _fundsRecipient.call{value: purchasePrice}("");
        require(success, "Transfer failed");
    }

    function earlySalePurchase(
        uint256 quantity,
        uint256 purchaseLimit,
        uint256 tierIndex,
        address to,
        bytes32 referralCode,
        uint256 expireTimestamp,
        bytes calldata signature
    ) external payable earlySaleActive nonReentrant {
        require(tierIndex < _tiersInfo.length, "Invalid tier index");
        bytes32 tierAddressKey = compatTierAddress(tierIndex, to);
        require(
            quantity != 0 &&
                _tiersAddressesMintedEarlySale[tierAddressKey] + quantity <=
                purchaseLimit &&
                _tiersMintedEarlySale[tierIndex] + quantity <=
                _tiersInfo[tierIndex].earlySaleQuantity,
            "Invalid quantity or inactive tier"
        );
        _tiersAddressesMintedEarlySale[tierAddressKey] += quantity;
        _tiersMintedEarlySale[tierIndex] += quantity;
        _handlePurchase(
            PurchaseType.EARLY_SALE,
            quantity,
            purchaseLimit,
            tierIndex,
            to,
            referralCode,
            expireTimestamp,
            signature
        );
        emit EarlySalePurchased(
            quantity,
            _tiersInfo[tierIndex].price,
            tierIndex,
            to,
            referralCode
        );
    }

    function publicSalePurchase(
        uint256 quantity,
        uint256 purchaseLimit,
        uint256 tierIndex,
        address to,
        bytes32 referralCode,
        uint256 expireTimestamp,
        bytes calldata signature
    ) external payable publicSaleActive nonReentrant {
        require(tierIndex < _tiersInfo.length, "Invalid tier index");
        bytes32 tierAddressKey = compatTierAddress(tierIndex, to);
        require(
            quantity != 0 &&
                (purchaseLimit == 0 ||
                    _tiersAddressesMintedPublicSale[tierAddressKey] +
                        quantity <=
                    purchaseLimit) &&
                _tiersMintedPublicSale[tierIndex] + quantity <=
                _tiersInfo[tierIndex].totalQuantity -
                    _tiersInfo[tierIndex].earlySaleQuantity &&
                (_tiersInfo[tierIndex].publicSaleLimitPerAddress == 0 ||
                    _tiersAddressesMintedPublicSale[tierAddressKey] +
                        quantity <=
                    _tiersInfo[tierIndex].publicSaleLimitPerAddress),
            "Invalid quantity"
        );
        _tiersAddressesMintedPublicSale[tierAddressKey] += quantity;
        _tiersMintedPublicSale[tierIndex] += quantity;
        _handlePurchase(
            PurchaseType.PUBLIC_SALE,
            quantity,
            purchaseLimit,
            tierIndex,
            to,
            referralCode,
            expireTimestamp,
            signature
        );
        emit PublicSalePurchased(
            quantity,
            _tiersInfo[tierIndex].price,
            tierIndex,
            to,
            referralCode
        );
    }

