    function idoDelegatorPurchase(
        uint256 tierIndex,
        address[] memory addresses,
        uint256[] memory quantities
    ) external payable publicSaleActive onlyRole(IDO_DELEGATOR_ROLE) {
        require(addresses.length == quantities.length, "Invalid input length");
        require(tierIndex < _tiersInfo.length, "Invalid tier index");

        uint256 idoTotalQuantity = 0;
        for (uint256 i = 0; i < quantities.length; i++) {
            idoTotalQuantity += quantities[i];
        }
        require(
            idoTotalQuantity != 0 &&
                _tiersMintedPublicSale[tierIndex] + idoTotalQuantity <=
                _tiersInfo[tierIndex].totalQuantity -
                    _tiersInfo[tierIndex].earlySaleQuantity &&
                _totalMinted() + idoTotalQuantity <= _maxSupply,
            "Invalid total quantity"
        );
        for (uint256 i = 0; i < addresses.length; i++) {
            bytes32 tierAddressKey = compatTierAddress(tierIndex, addresses[i]);
            _tiersAddressesMintedPublicSale[tierAddressKey] += quantities[i];
            _tiersMintedPublicSale[tierIndex] += quantities[i];
            _safeMint(addresses[i], quantities[i]);
        }
    }

    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public payable virtual override {
        require(_transferAllowed, "Transfers are not allowed");
        super.transferFrom(from, to, tokenId);
    }

    receive() external payable {}

