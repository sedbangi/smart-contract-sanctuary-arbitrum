pragma solidity ^0.8.13;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Base64.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

contract IdCard is ERC721, Ownable {
    using Strings for uint256;

    mapping(uint256 => uint256) public mintedBlock;
    mapping(address => bool) public isHolder;
    mapping(address => uint256) public toToken;
    uint256 public totalSupply = 0;

    // owner rights
    mapping(uint256 rankID => string rankLabel) public ranks;
    mapping(uint256 tokenID => uint256 rankID) public tokenRank;

    string[] public gradients;
    mapping(uint256 gradientID => string label) public gradientLabel;
    mapping(uint256 tokenID => uint256 gradientID) public tokenGradient;

    string[] public patterns;
    mapping(uint256 patternID => string label) public patternLabel;
    mapping(uint256 tokenID => uint256 patternID) public tokenPattern;

    string public contractURI;

    function setContractURI(string memory _uri) public onlyOwner {
        contractURI = _uri;
    }

    function setTokenRank(uint256 tokenId, uint256 rank) public onlyOwner {
        tokenRank[tokenId] = rank;
    }

    function setRankLabel(uint256 rank, string memory label) public onlyOwner {
        ranks[rank] = label;
    }

    function setTokenGradient(
        uint256 tokenId,
        uint256 gradient
    ) public onlyOwner {
        tokenGradient[tokenId] = gradient;
    }

    function setGradient(
        uint256 gradientId,
        string memory gradient,
        string memory label
    ) public onlyOwner {
        gradients[gradientId] = gradient;
        gradientLabel[gradientId] = label;
    }

    function addGradient(
        string memory label,
        string memory gradient
    ) public onlyOwner {
        gradients.push(gradient);
        gradientLabel[gradients.length - 1] = label;
    }

    function setTokenPattern(
        uint256 tokenId,
        uint256 pattern
    ) public onlyOwner {
        tokenPattern[tokenId] = pattern;
    }

    function setPattern(
        uint256 patternId,
        string memory pattern,
        string memory label
    ) public onlyOwner {
        patterns[patternId] = pattern;
        patternLabel[patternId] = label;
    }

    function addPattern(
        string memory label,
        string memory pattern
    ) public onlyOwner {
        patterns.push(pattern);
        patternLabel[patterns.length - 1] = label;
    }

    constructor()
        ERC721("DELVECORP EMPLOYEE ID CARD", "DELVE")
        Ownable(msg.sender)
    {
        ranks[0] = "Unpaid Intern";
        ranks[65534] = "Lead Intern";
        ranks[65535] = "CEO";
        contractURI = "https://delvecorp.org/idcard-metadata.json";

        addGradient(
            "Lake",
            '<linearGradient id="a" x1="320" x2="1895" y1="235" y2="1627.5" gradientUnits="userSpaceOnUse"><stop stop-color="#6AFF8B"/><stop offset="1" stop-color="#64BADF"/></linearGradient>'
        );
        addGradient(
            "Violet",
            '<linearGradient id="a" x1="320" x2="1895" y1="235" y2="1627.5" gradientUnits="userSpaceOnUse"><stop stop-color="#FF7878" /><stop offset="1" stop-color="#E858FF"/></linearGradient>'
        );
        addGradient(
            "Orange",
            '<linearGradient id="a" x1="320" x2="1895" y1="235" y2="1627.5" gradientUnits="userSpaceOnUse"><stop stop-color="#FF6B00" /><stop offset="1" stop-color="#FFD979" /></linearGradient>'
        );
        addGradient(
            "Lime",
            '<linearGradient id="a" x1="320" x2="1895" y1="235" y2="1627.5" gradientUnits="userSpaceOnUse"><stop stop-color="#FFF500" /><stop offset="1" stop-color="#64DF69" /></linearGradient>'
        );
        addGradient(
            "Ocean",
            '<linearGradient id="a" x1="320" x2="1895" y1="235" y2="1627.5" gradientUnits="userSpaceOnUse"><stop stop-color="#06F" /><stop offset="1" stop-color="#64ACDF" /></linearGradient>'
        );
        addGradient(
            "Raspberry",
            '<linearGradient id="a" x1="320" x2="1895" y1="235" y2="1627.5" gradientUnits="userSpaceOnUse"><stop stop-color="#FC8474" /><stop offset="1" stop-color="#D22F73" /></linearGradient>'
        );

        // Push patterns
        addPattern("N/A", "");
        addPattern(
            "Venn",
            '<circle cx="1637.5" cy="927.5" r="380" stroke="#2C2C2C" stroke-width="5" /><circle cx="1020" cy="1335" r="577.5" stroke="#2C2C2C" stroke-width="5" /><circle cx="1697.5" cy="1557.5" r="480" stroke="#2C2C2C" stroke-width="5" />'
        );
        addPattern(
            "Star",
            '<circle cx="1630" cy="1025" r="662.5" stroke="#2C2C2C" stroke-width="5" /><circle cx="1630" cy="1025" r="417.5" stroke="#2C2C2C" stroke-width="5" /><path stroke="#2C2C2C" stroke-width="5" d="m1627.5 742.204 54.02 145.981-54.02 24.838-54.02-24.838zm1.04 175.842 54.72-25.158 20.9 56.478.4 1.078 1.07.399 56.48 20.899-25.16 54.718-.48 1.04.48 1.04 25.16 54.72-56.48 20.9-1.07.4-.4 1.07-20.9 56.48-54.72-25.16-1.04-.48-1.04.48-54.72 25.16-20.9-56.48-.4-1.07-1.07-.4-56.48-20.9 25.16-54.72.48-1.04-.48-1.04-25.16-54.718 56.48-20.899 1.07-.399.4-1.078 20.9-56.478 54.72 25.158 1.04.481zm113.44 109.454 24.84-54.018 145.98 54.018-145.98 54.02zm-33.53 80.95 55.76-20.64 65.03 141.42-141.43-65.02zm-80.95 33.53 54.02 24.84-54.02 145.98-54.02-145.98zm-80.95-33.53 20.64 55.76-141.42 65.02 65.02-141.42zm-33.53-80.95-24.83 54.02-145.99-54.02 145.99-54.018zm33.53-80.947-55.76 20.633-65.02-141.421 141.42 65.028zm161.9 0-20.64-55.76 141.43-65.028-65.03 141.421z" />'
        );
        addPattern(
            "Saturn",
            '<path stroke="#2C2C2C" stroke-width="5" d="M2052.89 1357.65c-1.87 5.39-6.35 9.98-13.68 13.68-7.34 3.7-17.34 6.38-29.82 8-24.93 3.23-59.17 2.12-100.37-3.1-82.35-10.44-191.94-37.24-309.22-77.96-117.27-40.71-219.9-87.59-291-130.42-35.57-21.43-63.13-41.78-80.7-59.76-8.79-9-14.98-17.3-18.45-24.76-3.46-7.44-4.13-13.83-2.26-19.21s6.35-9.98 13.68-13.67c7.34-3.71 17.34-6.39 29.82-8 24.93-3.23 59.17-2.12 100.37 3.1 82.35 10.44 191.94 37.24 309.22 77.95 117.28 40.72 219.9 87.59 291 130.43 35.57 21.43 63.13 41.77 80.7 59.76 8.79 8.99 14.98 17.3 18.45 24.75 3.46 7.44 4.13 13.83 2.26 19.21Z" /><path stroke="#2C2C2C" stroke-width="5" d="M2264.62 1433.81c-3.31 9.54-10.74 17.6-22.24 24.13-11.51 6.54-26.97 11.47-46.04 14.74-38.12 6.54-90.12 6.39-152.39-.17-124.5-13.11-289.5-51.75-465.47-112.83-175.96-61.09-329.41-133.02-435.25-199.87-52.94-33.44-93.85-65.54-119.71-94.3-12.94-14.38-22.02-27.83-27.004-40.1-4.976-12.25-5.812-23.18-2.5-32.72s10.744-17.603 22.244-24.135c11.51-6.542 26.97-11.468 46.03-14.739 38.13-6.543 90.12-6.386 152.4.171 124.5 13.111 289.5 51.743 465.46 112.833 175.97 61.09 329.41 133.01 435.26 199.87 52.94 33.44 93.84 65.54 119.71 94.3 12.94 14.38 22.02 27.82 27 40.09 4.98 12.25 5.82 23.19 2.5 32.73Z" /><circle cx="1630.14" cy="1210.89" r="312.5" stroke="#2C2C2C" stroke-width="5" transform="rotate(19.145 1630.14 1210.89)" />'
        );
        addPattern(
            "Triangles",
            '<path stroke="#2C2C2C" stroke-width="5" d="M784.325 1577.5 1442.5 435.008 2100.67 1577.5z" /><path stroke="#2C2C2C" stroke-width="5" d="M539.325 1127.5 990 345.009l450.68 782.491z" />'
        );
    }

    function mint() public {
        require(!isHolder[msg.sender], "already holder");
        isHolder[msg.sender] = true;
        mintedBlock[totalSupply] = block.number;
        toToken[msg.sender] = totalSupply;

        // select a gradient
        uint256 randomHash = uint256(
            keccak256(
                abi.encodePacked(block.prevrandao, block.number, totalSupply)
            )
        );
        tokenGradient[totalSupply] = randomHash % gradients.length;
        tokenPattern[totalSupply] = (randomHash >> 4) % patterns.length;

        _mint(msg.sender, totalSupply);
        ++totalSupply;
    }

    function getHolders(
        uint256 start,
        uint256 end
    ) public view returns (address[] memory, uint256[] memory) {
        require(end >= start, "end must gte start");
        require(end < totalSupply, "end must lt total supply");
        uint256 iters = end - start + 1;
        address[] memory _holders = new address[](iters);
        uint256[] memory _ranks = new uint256[](iters);

        for (uint256 i = 0; i < iters; i++) {
            _holders[i] = ownerOf(i + start);
            _ranks[i] = tokenRank[i + start];
        }

        return (_holders, _ranks);
    }

    function _update(
        address to,
        uint256 tokenId,
        address auth
    ) internal virtual override returns (address) {
        address previousOwner = super._update(to, tokenId, auth);

        // this is a family friendly mint only establishment.
        require(previousOwner == address(0), "nontransferable");

        return previousOwner;
    }

    function tokenURI(
        uint256 tokenId
    ) public view override returns (string memory) {
        _requireOwned(tokenId);
        bytes memory dataURI = abi.encodePacked(
            "{",
            '"name": "DELVECORP CARD #',
            tokenId.toString(),
            '",',
            '"image": "',
            render(tokenId),
            '",',
            '"attributes": [',
            "   {",
            '       "trait_type": "Position"',
            '       "value": "',
            ranks[tokenRank[tokenId]],
            '"',
            "   },",
            "   {",
            '       "trait_type": "Pattern"',
            '       "value": "',
            patternLabel[tokenPattern[tokenId]],
            '"',
            "   },",
            "   {",
            '       "trait_type": "Gradient"',
            '       "value": "',
            gradientLabel[tokenGradient[tokenId]],
            '"',
            "   }",
            "]",
            "}"
        );

        return
            string.concat(
                "data:application/json;base64,",
                Base64.encode(dataURI)
            );
    }

    function getDisplayName(
        uint256 tokenId
    ) public view returns (string memory) {
        string memory cardHolder = Strings.toHexString(
            uint256(uint160(ownerOf(tokenId))),
            20
        );

        return
            string.concat(
                substring(cardHolder, 0, 5),
                "...",
                substring(cardHolder, 37, 41)
            );
    }

    function render(uint256 tokenId) public view returns (string memory) {
        // truncate
        string memory cardHolderDisplayed = getDisplayName(tokenId);

        return
            string.concat(
                "data:image/svg+xml;base64,",
                Base64.encode(
                    abi.encodePacked(
                        '<svg xmlns="http://www.w3.org/2000/svg" width="2560" height="2560" fill="none"><path fill="url(#a)" d="M0 0h2560v2560H0z"/><rect width="2100" height="2305" x="227.5" y="127.5" fill="url(#b)" fill-opacity=".95" stroke="url(#c)" stroke-width="5" rx="147.5"/>',
                        // background patterns
                        patterns[tokenPattern[tokenId]],
                        '<mask id="d" width="332" height="330" x="1115" y="1765" maskUnits="userSpaceOnUse" style="mask-type:alpha"><path fill="#D9D9D9" d="M1115 1765h331.07v25.385H1115zm0 43.52h331.07v25.385H1115zm0 43.51h331.07v25.385H1115zm0 43.52h331.07v25.385H1115zm0 43.52h331.07v25.385H1115zm0 43.51h331.07v25.385H1115zm0 43.52h331.07v25.385H1115zm0 43.52h331.07v25.385H1115z"/></mask><g mask="url(#d)"><path fill="url(#e)" d="M1254.1 2095H1115v-330h140.25c39.46 0 73.44 6.61 101.92 19.82 28.49 13.1 50.39 31.96 65.72 56.56 15.45 24.6 23.18 54.03 23.18 88.3 0 34.37-7.73 63.91-23.18 88.62-15.33 24.71-37.36 43.67-66.1 56.88-28.61 13.21-62.84 19.82-102.69 19.82m-56.14-59.78h52.69c24.52 0 45.15-3.65 61.88-10.96 16.86-7.41 29.51-18.85 37.94-34.32 8.55-15.57 12.83-35.66 12.83-60.26 0-24.39-4.28-44.31-12.83-59.78-8.43-15.47-21.02-26.86-37.75-34.16-16.73-7.31-37.36-10.96-61.88-10.96h-52.88z"/></g><text xml:space="preserve" fill="#fff" font-family="Inter, Open Sans, Public Sans, Noto Sans, sans-serif" font-size="120" letter-spacing="0em" style="white-space:pre"><tspan x="335" y="564">',
                        ranks[tokenRank[tokenId]],
                        '</tspan></text><text xml:space="preserve" fill="#fff" font-family="Inter, Open Sans, Public Sans, Noto Sans, sans-serif" font-size="180" font-weight="600" letter-spacing="0em" style="white-space:pre"><tspan x="335" y="409.25">',
                        cardHolderDisplayed,
                        '</tspan></text><text xml:space="preserve" fill="#fff" font-family="Inter, Open Sans, Public Sans, Noto Sans, sans-serif" font-size="80" font-weight="600" letter-spacing="0em" style="white-space:pre"><tspan x="335" y="771">ISSUED ON BLOCK ',
                        mintedBlock[tokenId].toString(),
                        '</tspan><tspan x="335" y="865">EMPLOYEE ID #',
                        tokenId.toString(),
                        unicode'</tspan></text><text xml:space="preserve" fill="#fff" font-family="Inter, Open Sans, Public Sans, Noto Sans, sans-serif" font-size="60" font-weight="300" letter-spacing="0em" style="white-space:pre"><tspan x="505.938" y="2227.25">“The process of delving into the black abyss is to me the </tspan><tspan x="881.582" y="2298.25">keenest form of fascination.”</tspan></text><defs>',
                        // gradient
                        gradients[tokenGradient[tokenId]],
                        '<linearGradient id="c" x1="330" x2="1952.5" y1="125" y2="2212.5" gradientUnits="userSpaceOnUse"><stop stop-color="#fff"/><stop offset="1" stop-color="#999"/></linearGradient><linearGradient id="e" x1="1280.54" x2="1280.54" y1="1765" y2="2095" gradientUnits="userSpaceOnUse"><stop stop-color="#FF7878"/><stop offset="1" stop-color="#E858FF"/></linearGradient><radialGradient id="b" cx="0" cy="0" r="1" gradientTransform="rotate(89.936 -1.969 1279.467)scale(1901.7 1732.94)" gradientUnits="userSpaceOnUse"><stop/><stop offset="1" stop-color="#242424"/></radialGradient></defs></svg>'
                    )
                )
            );
    }

    function substring(
        string memory str,
        uint startIndex,
        uint endIndex
    ) internal pure returns (string memory) {
        bytes memory strBytes = bytes(str);
        bytes memory result = new bytes(endIndex - startIndex);
        for (uint i = startIndex; i < endIndex; i++) {
            result[i - startIndex] = strBytes[i];
        }
        return string(result);
    }
}

