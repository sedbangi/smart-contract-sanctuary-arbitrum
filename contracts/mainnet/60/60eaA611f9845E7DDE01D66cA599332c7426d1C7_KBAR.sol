        bytes memory encoded = encodeTransfer(payload);

        uint64 sequence = wormhole().publishMessage{value: msg.value}(
            nonce,
            encoded,
            finality()
        );

        emit bridgeOutEvent(
            payload.tokenID,
            payload.tokenChain,
            payload.toChain,
            addressToBytes(ownerOf(tokenId)),
            recipient
        );

        burn(tokenId);

        return sequence;
    }

