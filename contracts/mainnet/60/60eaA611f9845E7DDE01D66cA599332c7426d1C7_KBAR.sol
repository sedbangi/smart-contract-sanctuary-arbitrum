    ERC721EnumerableUpgradeable,
    CATERC721Governance,
    CATERC721Events,
    IERC721Receiver,
    PausableUpgradeable,
    AccessControlUpgradeable,
    UUPSUpgradeable
{
    using Strings for uint256;
    /**
     * @dev Oracle interface for retrieving the USD price of gold
     */
    AggregatorV3Interface public goldUsdpriceFeed;

    /**
     * @dev Oracle interface for retrieving the USD price of Ethereum
     */
    AggregatorV3Interface public ethUsdPriceFeed;

    /**
     * @dev Constant for the pauser role
     */
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");

    /**
     * @dev Constant for the minter role
     */
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    /**
     * @dev Constant for the controller role
     */
    bytes32 public constant CONTROLLER_ROLE = keccak256("CONTROLLER_ROLE");

    /**
     * @dev Constant for the fee controller role
     */
    bytes32 public constant FEE_ROLE = keccak256("FEE_ROLE");

    /**
     * @dev Constant for the updating upgrader role
     */
    bytes32 public constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");

    /**
     * @dev Keeps track of addresses that are frozen
     */
    mapping(address => bool) internal _frozen;

    /**
     * @dev Keeps track of addresses that are not paying
     */
    mapping(address => bool) internal _notpaying;

    /**
     * @dev Current fee rate
     */
    uint256 public feeRate;

    /**
     * @dev Address where fees get sent
     */
    address public feeAccumulator;

    /**
     * @dev Fee rate denominator
     */
    uint256 public feeParts;

    /**
     * @dev Internal link to the website
     */
    string internal _websiteLink;

    /**
     * @dev Status for individual tokenId
     */

    mapping(uint256 => bool) private tokenIdsFreezeStatus;


     /**
     * @dev Constant for the freeze controller role
     */
    bytes32 public constant FREEZE_ROLE = keccak256("FREEZE_ROLE");

    /**
     * @dev Emitted when an address is frozen
     */
    event AddressFrozen(address indexed addr);

    /**
     * @dev Emitted when an address is unfrozen
     */
    event AddressUnfrozen(address indexed addr);

    /**
     * @dev Emitted during transfer when fee is being transfered to accumulator
     */
    event FeeCollected(address indexed from, address indexed to, uint256 value);

    /**
     * @dev Emitted every time trasfer fee rate is changed
     */
    event FeeRateSet(uint256 indexed oldFeeRate, uint256 indexed newFeeRate);

    /**
     * @dev Emitted every time trasfer fee accumulator is changed
     */
    event FeeAccumulatorSet(
        address indexed oldFeeAccumulator,
        address indexed newFeeAccumulator
    );

    function initialize(
        uint16 chainId,
        uint8 finality,
        address wormhole,
        string memory nftName,
        string memory nftSymbol
    ) public initializer {
        require(isInitialized() == false, "Already Initialized");
        setEvmChainId(block.chainid);
        setChainId(chainId);
        setWormhole(wormhole);
        setFinality(finality);
        setMintedSupply(0);

        __ERC721Enumerable_init();
        __ERC721Burnable_init();
        __AccessControl_init();
        __UUPSUpgradeable_init();
        __ERC721_init(nftName, nftSymbol);
        __Pausable_init();
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(PAUSER_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);
        _grantRole(CONTROLLER_ROLE, msg.sender);
        _grantRole(FEE_ROLE, msg.sender);
        _grantRole(UPGRADER_ROLE, msg.sender);
        _grantRole(FREEZE_ROLE, msg.sender);
        goldUsdpriceFeed = AggregatorV3Interface(
            0x214eD9Da11D2fbe465a6fc601a91E62EbEc1a0D6
        ); 
        ethUsdPriceFeed = AggregatorV3Interface(
            0x5f4eC3Df9cbd43714FE2740f5E3616155c5b8419
        ); 
        feeRate = 1000;
        feeParts = 1000000;
        feeAccumulator = msg.sender;
        setIsInitialized();
    }

    /**
     * @dev Freezes the specified address, preventing certain functionalities.
     * @param addr The address to be frozen.
     * Caller: must have the CONTROLLER_ROLE.
     */
    function freezeAccount(address addr) public onlyRole(CONTROLLER_ROLE) {
        require(!_frozen[addr], "address already frozen");
        _frozen[addr] = true;
        emit AddressFrozen(addr);
    }

    /**
     * @dev Unfreezes the specified address, allowing certain functionalities.
     * @param addr The address to be unfrozen.
     * Caller: must have the CONTROLLER_ROLE.
     */
    function unfreezeAccount(address addr) public onlyRole(CONTROLLER_ROLE) {
        require(_frozen[addr], "address already unfrozen");
        _frozen[addr] = false;
        emit AddressUnfrozen(addr);
    }

     /**
     * @dev Checks if the specified address is frozen.
     * @param addr The address to check.
     * @return A boolean indicating whether the address is frozen.
     */
    function isAccountFrozen(address addr) public view returns (bool) {
        return _frozen[addr];
    }

    function freezeTokenId(uint256 tokenId) public onlyRole(FREEZE_ROLE){
        require(!tokenIdsFreezeStatus[tokenId], "Already tokenId frozen");
        tokenIdsFreezeStatus[tokenId] = true;
    }


    function unfreezeTokenId(uint256 tokenId) public onlyRole(FREEZE_ROLE){
        require(tokenIdsFreezeStatus[tokenId], "Already tokenId frozen");
        tokenIdsFreezeStatus[tokenId] = false;
    }

    function isTokenIdFrozen(uint256 tokenId) public view returns(bool) {
        return tokenIdsFreezeStatus[tokenId];
    }
   
    /**
     * @dev Adds an address to the list of nonpaying addresses
     * @param addr The address to add.
     * Caller: must have the FEE_ROLE.
     */
    function addNotPaying(address addr) public onlyRole(FEE_ROLE) {
        require(!_notpaying[addr], "address already notpaying");
        _notpaying[addr] = true;
    }

    /**
     * @dev Removes an address from notpaying.
     * @param addr The  address to remove.
     */
    function removeNotPaying(address addr) public onlyRole(FEE_ROLE) {
        require(_notpaying[addr], "address is not notpaying");
        _notpaying[addr] = false;
    }

    /**
     * @dev Gets whether the address is currently notpaying.
     * @param addr The address to check if notpaying.
     * @return A bool representing whether the given address is notpaying.
     */
    function isNotPaying(address addr) public view returns (bool) {
        return _notpaying[addr];
    }

    /**
     * @dev Pauses certain functionalities of the contract.
     * Caller: must have the PAUSER_ROLE.
     */
    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    /**
     * @dev Unpauses certain functionalities of the contract.
     * * Caller: must have the PAUSER_ROLE.
     */
    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    /**
     * @dev Safely mints a new token and assigns it to the specified address.
     * @param to The address to receive the minted token.
     * @param uri The token URI for the minted token.
     * Caller: must have the MINTER_ROLE.
     */
    function safeMint(
        address to,
        string memory uri, 
        bool active
    ) public onlyRole(MINTER_ROLE) {
        uint256 tokenId = mintedSupply();
        _safeMint(to, tokenId);
        _setTokenURI(tokenId, uri);
        setMintedSupply(mintedSupply() + 1);
        tokenIdsFreezeStatus[tokenId]= !active;
    }

    /**
     * @dev Safely mints a batch of new tokens and assigns them to the specified addresses.
     * @param to An array of addresses to receive the minted tokens.
     * @param uris An array of token URIs for the minted tokens.
     * Caller: must have the MINTER_ROLE.
     */
    function safeMintBatch(
        address[] memory to,
        string[] memory uris,
        bool[] memory active
    ) public onlyRole(MINTER_ROLE) {
        require(to.length == uris.length, "Arrays length mismatch");

        for (uint256 i = 0; i < to.length; i++) {
            safeMint(to[i], uris[i], active[i]);
        }
    }

    /**
     * @dev Sets a new fee recipient address.
     * @param newFeeAccumulator The address allowed to collect transfer fees for transfers.
     */
    function setFeeAccumulator(
        address newFeeAccumulator
    ) public onlyRole(FEE_ROLE) {
        require(
            newFeeAccumulator != address(0),
            "cannot set fee accumulator to null"
        );
        address oldFeeAccumulator = feeAccumulator;
        feeAccumulator = newFeeAccumulator;
        emit FeeAccumulatorSet(oldFeeAccumulator, newFeeAccumulator);
    }

    /**
     * @dev Sets a new fee rate.
     * @param _newFeeRate The new fee rate to collect as transfer fees for transfers.
     */
    function setFeeRate(uint256 _newFeeRate) public onlyRole(FEE_ROLE) {
        require(_newFeeRate <= (feeParts / 2), "cannot set fee rate above 50%");
        uint256 _oldFeeRate = feeRate;
        feeRate = _newFeeRate;
        emit FeeRateSet(_oldFeeRate, feeRate);
    }

    /**
     * @dev Gets a fee for a given value
     * ex: given feeRate = 200 and feeParts = 1,000,000 then getFeeFor(10000) = 2
     * @param value The amount to get the fee for.
     */
    function getFeeFor(uint256 value) public view returns (uint256) {
        if (feeRate == 0) {
            return 0;
        }
        return (value * feeRate) / feeParts;
    }

    /**
     * @dev Retrieves the current website link.
     * @return The website link as a string.
     */
    function websiteLink() public view returns (string memory) {
        return _websiteLink;
    }

    /**
     * @dev Sets the website link.
     * @param newLink The new website link to be set.
     * Caller: must have the UPGRADER_ROLE.
     * Effects:
     * - Updates the internal website link variable.
     */
    function setWebsiteLink(
        string memory newLink
    ) public onlyRole(UPGRADER_ROLE) {
        _websiteLink = newLink;
    }

    /**
     * @dev Retrieves the current price of one gram of gold in USD.
     * @return The price of one gram of gold in USD as an int256.
     */
    function getOneGramGoldPriceInUSD() public view returns (int256) {
        (, int256 price, , , ) = goldUsdpriceFeed.latestRoundData();
        int256 oneGramPrice = (price * 10000) / 283495; // 1 ounce = 28.3495 g
        return oneGramPrice; // as priceFeed has decimals = 10 ** 8
    }

    /**
     * @dev Retrieves the current price of one kilogram of gold in USD.
     * @return The price of one kilogram of gold in USD as an int256.
     */
    function getOneKgGoldPriceInUSD() public view returns (int256) {
        int256 oneKgPrice = getOneGramGoldPriceInUSD() * 1000; // 1 kg = 1000 grams
        return oneKgPrice;
    }

    /**
     * @dev Retrieves the current price of Ether in USD.
     * @return The price of Ether in USD as a uint256.
     */
    function getEthPriceInUSD() public view returns (uint256) {
        (, int256 price, , , ) = ethUsdPriceFeed.latestRoundData();
        return uint256(price); // as priceFeed has decimals = 10 ** 8
    }

    /**
     * @dev Retrieves the current price of gold per kilogram in Ether.
     * @return The price of gold per kilogram in Ether as a uint256.
     */
    function getGoldPricePerKgInEth() public view returns (uint256) {
        uint256 goldPriceInEth = (uint256(getOneKgGoldPriceInUSD()) *
            10 ** 18) / uint256(getEthPriceInUSD());
        return goldPriceInEth;
    }

    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public payable override(ERC721Upgradeable, IERC721) whenNotPaused {
        require(!_frozen[to] && !_frozen[from], "Address frozen");
          require(!tokenIdsFreezeStatus[tokenId], "TokenId frozen");
        uint256 feeAmount = 0;
        if (!_notpaying[from] && !_notpaying[to]) {
            uint256 nftCost = getGoldPricePerKgInEth();
            feeAmount = getFeeFor(nftCost);
            require(msg.value >= feeAmount, "Insufficient Eth Amount");
            (bool transfer, ) = payable(feeAccumulator).call{value: feeAmount}(
                ""
            );
            require(transfer, "Transfer fee not transferred");
        }

        if (msg.value > feeAmount) {
            uint256 extraEthAmount = msg.value - feeAmount;
            (bool send, ) = payable(msg.sender).call{value: extraEthAmount}("");
            require(send, "Excess Eth Amount not transferred");
        }
        super.transferFrom(from, to, tokenId);
    }

    function bulkTransferFrom(
        address from,
        address to,
        uint256[] memory tokenIds
    ) public payable whenNotPaused {
        require(!_frozen[to] && !_frozen[from], "Address frozen");
        _checkFreezeStatus(tokenIds);
      
        uint256 bulkTransferFee = 0;

        if (!_notpaying[from] && !_notpaying[to]) {
            bulkTransferFee = calculateBulkTransferFee(tokenIds.length);
            require(msg.value >= bulkTransferFee, "Insufficient Eth Amount");

            (bool transfer, ) = payable(feeAccumulator).call{
                value: bulkTransferFee
            }("");
            require(transfer, "Transfer fee not transferred");
        }

        if (msg.value > bulkTransferFee) {
            uint256 extraEthAmount = msg.value - bulkTransferFee;
            (bool send, ) = payable(msg.sender).call{value: extraEthAmount}("");
            require(send, "Excess Eth Amount not transferred");
        }

        for (uint256 i = 0; i < tokenIds.length; i++) {
            super.transferFrom(from, to, tokenIds[i]);
        }
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        bytes memory data
    ) public payable override(ERC721Upgradeable, IERC721) {
        transferFrom(from, to, tokenId);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public payable override(ERC721Upgradeable, IERC721) {
        transferFrom(from, to, tokenId);
    }

    function calculateBulkTransferFee(
        uint256 tokenIdsLength
    ) public view returns (uint256) {
        uint256 nftCost = getGoldPricePerKgInEth();
        return getFeeFor(tokenIdsLength * nftCost);
    }

    /**
     * @dev Registers a new blockchain with its corresponding token contract.
     * @param chainId The ID of the blockchain to register.
     * @param tokenContract The address of the token contract for the specified blockchain.
     * Caller must have the UPGRADER_ROLE.
     * Effects:
     * - Calls the parent contract's registerChain function.
     */
    function registerChain(
        uint16 chainId,
        bytes32 tokenContract
    ) public override onlyRole(UPGRADER_ROLE) {
        super.registerChain(chainId, tokenContract);
    }

    /**
     * @dev Registers multiple blockchains with their corresponding token contracts.
     * @param chainId An array of blockchain IDs to register.
     * @param tokenContract An array of token contract addresses for the specified blockchains.
     * Caller must have the UPGRADER_ROLE.
     * Effects:
     * - Calls the parent contract's registerChains function.
     */
    function registerChains(
        uint16[] memory chainId,
        bytes32[] memory tokenContract
    ) public override onlyRole(UPGRADER_ROLE) {
        super.registerChains(chainId, tokenContract);
    }

    /**
     * @dev Updates the finality setting for the contract.
     * @param finality The new finality setting.
     * Caller must have the UPGRADER_ROLE.
     * Effects:
     * - Calls the parent contract's updateFinality function.
     */
    function updateFinality(
        uint8 finality
    ) public override onlyRole(UPGRADER_ROLE) {
        super.updateFinality(finality);
    }

    /**
     * @dev Retrieves the token URI for a given token ID.
     * @param tokenId The ID of the token.
     * @return The token URI as a string.
     */
    function tokenURI(
        uint256 tokenId
    )
        public
        view
        override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }


    function _checkFreezeStatus(uint256[]memory tokenIds) private {
        for(uint256 index=0; index < tokenIds.length; index++){
            if(tokenIdsFreezeStatus[tokenIds[index]] == true) revert("TokenId frozen");
          }
    }

    /**
     * @dev Handles the receipt of an ERC721 token.
     * @param operator The address that called `safeTransferFrom` function.
