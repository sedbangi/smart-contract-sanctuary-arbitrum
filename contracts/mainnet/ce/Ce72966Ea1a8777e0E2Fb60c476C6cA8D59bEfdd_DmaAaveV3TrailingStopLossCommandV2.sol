pragma solidity ^0.8.0;
import { console } from "hardhat/console.sol";
import { AggregatorV3Interface } from "../interfaces/Chainlink/AggregatorV3Interface.sol";

/**
 * @dev Checks if the next round is the expected round based on the given parameters.
 * @param _proxyRoundId The round ID of the current round.
 * @param _nextProxyRoundId The round ID of the next round.
 * @param _proxy The AggregatorV3Interface contract instance.
 * @return A boolean indicating whether the next round is the expected round.
 */
function _checkIfNextRound(
    uint80 _proxyRoundId,
    uint256 _nextProxyRoundId,
    AggregatorV3Interface _proxy
) view returns (bool) {
    uint256 PHASE_OFFSET = 64;
    // if the roundId is the next roundId then we can return true
    if (_proxyRoundId + 1 == _nextProxyRoundId) {
        return true;
    }
    // in other case it means the round id is the last roundId of old aggregator and the next roundId is the first roundId of the new aggregator
    // to check if thats true we need to check if the next roundId is the first roundId of the new phase

    // based on proxyRoundId we can get the phaseId - with each aggregator deployment and update the phaseId increments by 1
    uint16 previousRoundIdPhaseId = uint16(_proxyRoundId >> PHASE_OFFSET);
    uint16 nextRoundIdPhaseId = uint16(_nextProxyRoundId >> PHASE_OFFSET);
    console.log("previousRoundIdPhaseId", previousRoundIdPhaseId);
    console.log("nextRoundIdPhaseId", nextRoundIdPhaseId);
    bool isNextPhaseIdNext = nextRoundIdPhaseId == previousRoundIdPhaseId + 1;

    // knowing the phaseId we can get the next aggregators aggregatorRoundId
    uint64 nextAggregatorRoundId = uint64(_nextProxyRoundId);
    // if the next roundId is the first roundId of the new phase and the next roundId is the first roundId of the new aggregator then we can return true
    bool isNextRoundIdNext = nextAggregatorRoundId == 1;
    // if the next round of the previous aggregator is returning 0 then we can assume that the next roundId is the first roundId of the new aggregator
    (, int256 answer, uint256 startedAt, uint256 updatedAt, ) = _proxy.getRoundData(
        _proxyRoundId + 1
    );
    bool isLastRoundOfPreviousAggregator = updatedAt == startedAt &&
        answer == 0 &&
        uint256(answer) == updatedAt;
    return isLastRoundOfPreviousAggregator && isNextPhaseIdNext && isNextRoundIdNext;
}

