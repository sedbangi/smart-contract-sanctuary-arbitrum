pragma solidity 0.8.22;

import { IServiceRegistry } from "../interfaces/IServiceRegistry.sol";
import { IPool } from "../interfaces/AAVE/IPool.sol";
import { IAccountImplementation } from "../interfaces/IAccountImplementation.sol";
import { BaseDMACommand, ICommand, CommonTriggerData } from "./BaseDMACommand.sol";
import { AggregatorV3Interface } from "../interfaces/Chainlink/AggregatorV3Interface.sol";
import { _checkIfNextRound } from "../helpers/ChainlinkHelpers.sol";

/**
 * @dev Struct representing the data for a AAVE sl trigger.
 */
struct TrailingStopLossTriggerData {
    CommonTriggerData commonTriggerData;
    address collateralOracle;
    uint80 collateralAddedRoundId;
    address debtOracle;
    uint80 debtAddedRoundId;
    uint256 trailingDistance;
    // since now we want to have mutliple choices - ho do we handle it ? close to eao, close to aave psoition, close to collateral etc
    bool closeToCollateral;
    // TODO ? shall we add a bool for close to EOA ?
    // bool closeToEOA;
}

struct TrailingStopLossOffchainData {
    uint80 collateralMaxPriceRoundId;
    uint80 debtClosestPriceRoundId;
    uint80 debtNextPriceRoundId;
}

struct ExecutionDataWithOffchainData {
    bytes executionData;
    bytes offchainData;
}

struct TrailinStopLossValidations {
    bool isTrailingThresholdExceeded;
    bool areMaxPricesRecordedAfterTriggerCreation;
    bool areDebtPricesSequential;
    bool isMaxPriceBetweenDebtPrices;
    bool isSameAggregatorWithNoNextPrice;
}

/**
 * @title DMA Trailing Stop Loss - (AAVE V3) Command for the AutomationBot V2
 */
contract DmaAaveV3TrailingStopLossCommandV2 is BaseDMACommand {
    IPool public immutable lendingPool;

    string private constant AAVE_V3_LENDING_POOL = "AAVE_V3_LENDING_POOL";
    uint16 private constant AAVE_V3_TRAILING_SL = 10005;
    uint256 private constant CHAINLINK_PRECISION = 10 ** 8;

    constructor(IServiceRegistry _serviceRegistry) BaseDMACommand(_serviceRegistry) {
        address lendingPoolAddress = _serviceRegistry.getRegisteredService(AAVE_V3_LENDING_POOL);
        if (lendingPoolAddress == address(0)) {
            revert EmptyAddress(AAVE_V3_LENDING_POOL);
        }
        lendingPool = IPool(lendingPoolAddress);
    }

    function isStopLossEligible(
        TrailingStopLossTriggerData memory stopLossTriggerData,
        AggregatorV3Interface[] memory oracles
    ) internal view returns (bool) {
        (, int collateralLatestPrice, , , ) = oracles[0].latestRoundData();
        (, int debtLatestPrice, , , ) = oracles[1].latestRoundData();
        uint256 collateralLatestPriceUint = uint256(collateralLatestPrice);
        uint256 debtLatestPriceUint = uint256(debtLatestPrice);

        (, int collateralPriceWhenAdded, , uint256 collateralTimestampWhenAdded, ) = oracles[0]
            .getRoundData(stopLossTriggerData.collateralAddedRoundId);

        (, int debtPriceWhenAdded, , uint256 debtTimestampWhenAdded, ) = oracles[1].getRoundData(
            stopLossTriggerData.debtAddedRoundId
        );
        uint256 collateralPriceWhenAddedUint = uint256(collateralPriceWhenAdded);
        uint256 debtPriceWhenAddedUint = uint256(debtPriceWhenAdded);
        uint256 latestPrice = (collateralLatestPriceUint * CHAINLINK_PRECISION) /
            debtLatestPriceUint;
        uint256 priceWhenAdded = (collateralPriceWhenAddedUint * CHAINLINK_PRECISION) /
            debtPriceWhenAddedUint;

        {
            bool isStopLossExecutionEligible = latestPrice <
                priceWhenAdded - stopLossTriggerData.trailingDistance;
            bool isLatestPriceAfterCreation = collateralTimestampWhenAdded > 0 &&
                debtTimestampWhenAdded > 0;

            return isStopLossExecutionEligible && isLatestPriceAfterCreation;
        }
    }

    function isTrailingStopLossEligible(
        TrailingStopLossTriggerData memory stopLossTriggerData,
        TrailingStopLossOffchainData memory stopLossOffchainData,
        AggregatorV3Interface[] memory oracles
    ) internal view returns (bool) {
        (
            uint256 collateralAddedTimestamp,
            uint256 debtAddedTimestamp,
            uint256 collateralMaxPriceTimestamp,
            uint256 debtClosestPriceTimestamp,
            uint256 debtNextPriceTimestamp,
            uint256 latestPrice,
            uint256 maximumPrice,
            uint256 latestDebtRoundId
        ) = getLatestAndMaximumPrices(stopLossTriggerData, stopLossOffchainData, oracles);

        {
            TrailinStopLossValidations memory stopLossValidations;
            {
                stopLossValidations.isTrailingThresholdExceeded =
                    latestPrice < maximumPrice - stopLossTriggerData.trailingDistance;
                // greater or equal as the base price can be the sameas on the date of creation (e.g. USDC/USD oracle is updated every 24h)
                stopLossValidations.areMaxPricesRecordedAfterTriggerCreation =
                    collateralMaxPriceTimestamp > collateralAddedTimestamp &&
                    debtClosestPriceTimestamp >= debtAddedTimestamp;
            }
            {
                stopLossValidations.areDebtPricesSequential = checkIfNextRound(
                    stopLossOffchainData.debtClosestPriceRoundId,
                    stopLossOffchainData.debtNextPriceRoundId,
                    oracles[1]
                );
                stopLossValidations.isSameAggregatorWithNoNextPrice = (latestDebtRoundId ==
                    stopLossOffchainData.debtClosestPriceRoundId &&
                    uint16(latestDebtRoundId >> 64) == oracles[1].phaseId());
                stopLossValidations.isMaxPriceBetweenDebtPrices =
                    collateralMaxPriceTimestamp > debtClosestPriceTimestamp &&
                    (collateralMaxPriceTimestamp < debtNextPriceTimestamp ||
                        stopLossValidations.isSameAggregatorWithNoNextPrice);
            }
            return
                stopLossValidations.isTrailingThresholdExceeded &&
                stopLossValidations.areMaxPricesRecordedAfterTriggerCreation &&
                stopLossValidations.isMaxPriceBetweenDebtPrices &&
                stopLossValidations.areDebtPricesSequential;
        }
    }

    function getLatestRoundData(
        AggregatorV3Interface[] memory oracles
    )
        public
        view
        returns (uint256[] memory prices, uint256[] memory timestamps, uint256[] memory roundIds)
    {
        uint256[] memory tempRoundIds = new uint256[](oracles.length);
        uint256[] memory tempPrices = new uint256[](oracles.length);
        uint256[] memory tempTimestamps = new uint256[](oracles.length);
        for (uint256 i = 0; i < oracles.length; i++) {
            (uint256 roundId, int collateralLatestPrice, , uint256 collateralAdded, ) = oracles[i]
                .latestRoundData();
            // TODO: shall we normalize the price? to either 8 or 18 decimals
            tempPrices[i] = uint256(collateralLatestPrice);
            tempTimestamps[i] = collateralAdded;
            tempRoundIds[i] = roundId;
        }
        prices = tempPrices;
        timestamps = tempTimestamps;
        roundIds = tempRoundIds;
    }

    function getRoundData(
        AggregatorV3Interface[] memory oracles,
        uint80[] memory roundIds
    ) public view returns (uint256[] memory prices, uint256[] memory timestamps) {
        uint256[] memory tempPrices = new uint256[](oracles.length);
        uint256[] memory tempTimestamps = new uint256[](oracles.length);
        for (uint256 i = 0; i < oracles.length; i++) {
            (, int latestPrice, , uint256 added, ) = oracles[i].getRoundData(roundIds[i]);
            tempPrices[i] = uint256(latestPrice);
            tempTimestamps[i] = added;
        }
        prices = tempPrices;
        timestamps = tempTimestamps;
    }

    function getLatestAndMaximumPrices(
        TrailingStopLossTriggerData memory stopLossTriggerData,
        TrailingStopLossOffchainData memory stopLossOffchainData,
        AggregatorV3Interface[] memory oracles
    )
        public
        view
        returns (
            uint256 collateralAddedTimestamp,
            uint256 debtAddedTimestamp,
            uint256 collateralMaxPriceTimestamp,
            uint256 debtClosestPriceTimestamp,
            uint256 debtNextPriceTimestamp,
            uint256 latestPrice,
            uint256 maximumPrice,
            uint256 latestDebtRoundId
        )
    {
        uint80[] memory roundIdsMax = new uint80[](2);
        uint80[] memory roundIdsAdded = new uint80[](2);
        uint256 collateralLatestPriceUint;
        uint256 debtLatestPriceUint;

        {
            roundIdsAdded[0] = stopLossTriggerData.collateralAddedRoundId;
            roundIdsAdded[1] = stopLossTriggerData.debtAddedRoundId;

            roundIdsMax[0] = stopLossOffchainData.collateralMaxPriceRoundId;
            roundIdsMax[1] = stopLossOffchainData.debtClosestPriceRoundId;
        }
        {
            (uint256[] memory prices, , uint256[] memory latestRoundIds) = getLatestRoundData(
                oracles
            );
            collateralLatestPriceUint = prices[0];
            debtLatestPriceUint = prices[1];
            latestDebtRoundId = latestRoundIds[1];
        }
        {
            (, uint256[] memory timestampsAdded) = getRoundData(oracles, roundIdsAdded);
            collateralAddedTimestamp = timestampsAdded[0];
            debtAddedTimestamp = timestampsAdded[1];
        }
        {
            (, , , debtNextPriceTimestamp, ) = oracles[1].getRoundData(
                stopLossOffchainData.debtNextPriceRoundId
            );
        }
        {
            (uint256[] memory maxPrices, uint256[] memory maxTimestamps) = getRoundData(
                oracles,
                roundIdsMax
            );
            collateralMaxPriceTimestamp = maxTimestamps[0];
            debtClosestPriceTimestamp = maxTimestamps[1];

            uint256 collateralMaxPriceUint = maxPrices[0];
            uint256 debtClosestPriceUint = maxPrices[1];

            latestPrice = (collateralLatestPriceUint * CHAINLINK_PRECISION) / debtLatestPriceUint;
            maximumPrice = (collateralMaxPriceUint * CHAINLINK_PRECISION) / debtClosestPriceUint;
        }
    }

    function isExecutionLegal(
        bytes memory triggerData,
        bytes memory offchainData
    ) public view returns (bool) {
        TrailingStopLossTriggerData memory stopLossTriggerData = abi.decode(
            triggerData,
            (TrailingStopLossTriggerData)
        );
        TrailingStopLossOffchainData memory stopLossOffchainData = abi.decode(
            offchainData,
            (TrailingStopLossOffchainData)
        );
        AggregatorV3Interface[] memory oracles = new AggregatorV3Interface[](2);
        // TODO: can we ensure that oracles are for the token we use?
        oracles[0] = AggregatorV3Interface(stopLossTriggerData.collateralOracle);
        oracles[1] = AggregatorV3Interface(stopLossTriggerData.debtOracle);
        return
            isTrailingStopLossEligible(stopLossTriggerData, stopLossOffchainData, oracles) ||
            isStopLossEligible(stopLossTriggerData, oracles);
    }

    /**
     *  @inheritdoc ICommand
     */
    function isExecutionCorrect(bytes memory triggerData) external view override returns (bool) {
        TrailingStopLossTriggerData memory stopLossTriggerData = abi.decode(
            triggerData,
            (TrailingStopLossTriggerData)
        );

        (uint256 totalCollateralBase, uint256 totalDebtBase, , , , ) = lendingPool
            .getUserAccountData(stopLossTriggerData.commonTriggerData.positionAddress);

        return !(totalCollateralBase > 0 && totalDebtBase > 0);
    }

    /**
     *  @inheritdoc ICommand
     */
    function execute(
        bytes calldata executionData,
        bytes memory triggerData
    ) external override nonReentrant onlyBot {
        TrailingStopLossTriggerData memory stopLossTriggerData = abi.decode(
            triggerData,
            (TrailingStopLossTriggerData)
        );
        ExecutionDataWithOffchainData memory executionDataWithOffchainData = abi.decode(
            executionData,
            (ExecutionDataWithOffchainData)
        );

        if (!isExecutionLegal(triggerData, executionDataWithOffchainData.offchainData)) {
            revert ExecutionNotLegal();
        }

        uint16[] memory triggerTypes = new uint16[](2);
        triggerTypes[0] = AAVE_V3_TRAILING_SL;
        _validateTriggerType(triggerTypes, stopLossTriggerData.commonTriggerData.triggerType);
        _validateSelector(
            operationExecutor.executeOp.selector,
            executionDataWithOffchainData.executionData
        );
        bytes32 operationName = IAccountImplementation(
            stopLossTriggerData.commonTriggerData.positionAddress
        ).execute(address(operationExecutor), executionDataWithOffchainData.executionData);

        _validateOperationName(stopLossTriggerData.commonTriggerData.operationName, operationName);
    }

    /**
     *  @inheritdoc ICommand
     */
    function isTriggerDataValid(
        bool isContinuous,
        bytes memory triggerData
    ) external view override returns (bool) {
        TrailingStopLossTriggerData memory stopLossTriggerData = abi.decode(
            triggerData,
            (TrailingStopLossTriggerData)
        );
        AggregatorV3Interface[] memory oracles = new AggregatorV3Interface[](2);
        // TODO: can we ensure that oracles are for the token we use?
        oracles[0] = AggregatorV3Interface(stopLossTriggerData.collateralOracle);
        oracles[1] = AggregatorV3Interface(stopLossTriggerData.debtOracle);
        uint80[] memory roundIdsAdded = new uint80[](2);

        roundIdsAdded[0] = stopLossTriggerData.collateralAddedRoundId;
        roundIdsAdded[1] = stopLossTriggerData.debtAddedRoundId;
        (, uint256[] memory latestTimestamps, ) = getLatestRoundData(oracles);
        (, uint256[] memory addedTimestamps) = getRoundData(oracles, roundIdsAdded);

        // assure that the timestamps are valid - the lastest and added timestamps are greater than 0 and the added and latest timestamps are the same
        bool isTimestampsValid = addedTimestamps[0] > 0 &&
            addedTimestamps[1] > 0 &&
            latestTimestamps[0] > 0 &&
            latestTimestamps[1] > 0 &&
            addedTimestamps[0] == latestTimestamps[0] &&
            addedTimestamps[1] == latestTimestamps[1];

        // assure that the trigger type is the correct one
        uint16[] memory triggerTypes = new uint16[](1);
        triggerTypes[0] = AAVE_V3_TRAILING_SL;
        bool triggerTypeCorrect = _isTriggerTypeValid(
            triggerTypes,
            stopLossTriggerData.commonTriggerData.triggerType
        );

        return isTimestampsValid && triggerTypeCorrect && !isContinuous;
    }

    /**
     *  @inheritdoc ICommand
     */
    function isExecutionLegal(bytes memory) external pure override returns (bool) {
        return true;
    }

    function getTriggerType(bytes calldata triggerData) external view override returns (uint16) {
        TrailingStopLossTriggerData memory stopLossTriggerData = abi.decode(
            triggerData,
            (TrailingStopLossTriggerData)
        );
        if (!this.isTriggerDataValid(false, triggerData)) {
            return 0;
        }
        return stopLossTriggerData.commonTriggerData.triggerType;
    }

    function checkIfNextRound(
        uint80 _proxyRoundId,
        uint256 _nextProxyRoundId,
        AggregatorV3Interface _proxy
    ) public view returns (bool) {
        return _checkIfNextRound(_proxyRoundId, _nextProxyRoundId, _proxy);
    }
}

