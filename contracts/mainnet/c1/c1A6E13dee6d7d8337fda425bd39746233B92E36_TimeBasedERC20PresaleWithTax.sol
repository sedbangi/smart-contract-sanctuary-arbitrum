import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "../interfaces/ICamelotV2Router.sol";
import "../interfaces/IWeth.sol";
import "../struct/Structs.sol";

contract TimeBasedERC20PresaleWithTax is ERC20, ReentrancyGuard, Ownable {
    struct Participant {
        address addr;
        uint256 ethAmount;
    }

    uint256 public constant LP_SHARE = 49;
    uint256 public constant PRESALE_SHARE = 49;
    uint256 public constant OWNER_SHARE = 2;
    address public constant CAMELOT_V2_ROUTER =
        0xc873fEcbd354f5A56E00E710B90EF4201db2448d;
    address public constant WETH = 0x82aF49447D8a07e3bd95BD0d56f35241523fBab1;
    address public constant TAX_RECEIVER =
        0x857eFc6c1280778b20B14af709C857E8164E731D;

    uint256 public maxParticipants = 801;
    uint256 public lastParticipantId = 1;
    uint256 public presaleEndTs;
    uint256 public presaleShare;
    uint256 public snipeAmount;

    bool public isEnded = false;

    address public initalCreator;

    // user => id
    mapping(address => uint256) public participantId;
    // id => Participant
    mapping(uint256 => Participant) public participants;

    constructor(
        TokenParams memory params,
        uint256 presaleSecondsDuration
    ) ERC20(params.name, params.symbol) {
        _mint(address(this), params.initialSupply * 10 ** 18);
        presaleEndTs = block.timestamp + presaleSecondsDuration;
        initalCreator = tx.origin;
        presaleShare =
            ((params.initialSupply * 10 ** 18) / 100) *
            PRESALE_SHARE;
    }

    modifier isPresaleActive() {
        require(!isEnded, "TimeBasedERC20PresaleWithTax: presale ended");
        _;
    }

    modifier isAirdropTs() {
        require(
            block.timestamp >= presaleEndTs,
            "TimeBasedERC20PresaleWithTax: presale !started"
        );
        _;
    }

    function setSnipeAmount() external payable isPresaleActive onlyOwner {
        snipeAmount += msg.value;
    }

    function withdrawSnipeAmount() external payable onlyOwner nonReentrant {
        require(
            snipeAmount > 0,
            "TimeBasedERC20PresaleWithTax: nothing to withdraw"
        );
        uint256 snipeAmountCopy = snipeAmount;
        snipeAmount = 0;
        payable(owner()).transfer(snipeAmountCopy);
    }

    function withdrawNative() external nonReentrant isPresaleActive {
        uint256 id = participantId[msg.sender];
        Participant storage participant = participants[id];
        uint256 amount = participant.ethAmount;
        participant.ethAmount = 0;
        if (getPresaleSecondsTimeleft() > 0) {
            uint256 fee = amount / 100;

            payable(TAX_RECEIVER).transfer(fee);
            payable(msg.sender).transfer(amount - fee);
        } else {
            payable(msg.sender).transfer(amount);
        }
    }

    function rescueFunds(
        address token,
        address to,
        uint256 amount
    ) external onlyOwner {
        require(
            token != address(this),
            "!TimeBasedERC20PresaleWithTax: !address"
        );
        IERC20(token).transfer(to, amount);
    }

    function airdropAll() external isPresaleActive isAirdropTs nonReentrant {
        // project raise fees
        payable(TAX_RECEIVER).transfer(
            ((address(this).balance - snipeAmount) / 100) * 1
        );

        // airdrop to owner
        _transfer(
            address(this),
            initalCreator,
            (balanceOf(address(this)) / 100) * OWNER_SHARE
        );

        // airdrop to presale participants
        for (uint256 i = 1; i < lastParticipantId; i++) {
            Participant memory participant = participants[i];

            if (participant.ethAmount > 0) {
                _transfer(
                    address(this),
                    participant.addr,
                    getUserShare(participant.ethAmount)
                );
            }
        }

        _addAndBurnLiqudity();

        if (snipeAmount > 0) {
            _snipe();
            snipeAmount = 0;
        }
        
        isEnded = true;
    }

    function getPresaleSecondsTimeleft()
        public
        view
        isPresaleActive
        returns (uint256)
    {
        return
            presaleEndTs > block.timestamp ? presaleEndTs - block.timestamp : 0;
    }

    function getUserShare(uint256 ethAmount) public view returns (uint256) {
        uint256 tokensToDistribute = (ethAmount * presaleShare) /
            (address(this).balance - snipeAmount);

        return tokensToDistribute;
    }

    function _addAndBurnLiqudity() private {
        IWeth(WETH).deposit{value: address(this).balance - snipeAmount}();
        IWeth(WETH).approve(
            CAMELOT_V2_ROUTER,
            IWeth(WETH).balanceOf(address(this))
        );
        TimeBasedERC20PresaleWithTax(payable(address(this))).approve(
            CAMELOT_V2_ROUTER,
            balanceOf(address(this))
        );

        ICamelotV2Router(CAMELOT_V2_ROUTER).addLiquidity(
            address(this),
            WETH,
            balanceOf(address(this)),
            IWeth(WETH).balanceOf(address(this)),
            0,
            0,
            address(0),
            block.timestamp
        );
    }

    function _snipe() private {
        address[] memory path = new address[](2);
        path[0] = WETH;
        path[1] = address(this);

        ICamelotV2Router(CAMELOT_V2_ROUTER)
            .swapExactETHForTokensSupportingFeeOnTransferTokens{
            value: snipeAmount
        }(0, path, owner(), address(0), block.timestamp);
    }

    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal virtual override {
        if (isEnded) {
            uint256 tax = (amount / 1000) * 3; // .3% swap tax

            super._transfer(sender, recipient, amount - tax);
            super._transfer(sender, TAX_RECEIVER, tax);
        } else {
            super._transfer(sender, recipient, amount);
        }
    }

    receive() external payable isPresaleActive {
        require(
            lastParticipantId <= maxParticipants,
            "TimeBasedERC20PresaleWithTax: max participants limit reached"
        );
        if (participantId[msg.sender] == 0) {
            participantId[msg.sender] = lastParticipantId;

            participants[lastParticipantId] = Participant(
                msg.sender,
                msg.value
            );

            lastParticipantId += 1;
        } else {
            uint256 id = participantId[msg.sender];
            participants[id].ethAmount += msg.value;
        }
    }
}

