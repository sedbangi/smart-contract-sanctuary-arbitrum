pragma solidity ^0.8.20;

import "@openzeppelin/contracts-upgradeable/utils/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "hardhat/console.sol";

    struct Prediction {
        string content;
        string firstSelection;
        string secondSelection;
        uint closeSelectionDate;
        uint finalResultDate;
        uint selectionWinner; // 1 is first, 2 is second
        uint256 firstSelectionValue;
        uint256 secondSelectionValue;
        address creator;
        uint status; // 0 unapproved is 1 is public 2 is closed
    }

    struct Bit {
        uint selection;
        uint256 value;
        address creator;
    }

    struct PredictionResponse {
        uint id;
        Prediction prediction;
        Bit ownedBit;
        bool claimed;
    }

contract Fubit is Initializable, PausableUpgradeable, OwnableUpgradeable {

    uint private constant LIMIT_PER_PAGE = 20;

    address private _admin;

    uint private _creatorFeePercentage; // divided by 100000;
    uint private _adminFeePercentage; // divided by 100000;
    address private _feeReceiver;

    Prediction[] private _predictions;
    Bit[] private _bits;

    // Mapping prediction id => bit id
    mapping(uint => uint[]) private _bitMapping;
    // Joined bit
    mapping(address => mapping(uint => uint)) private _bitOwners;

    mapping(address => uint[]) private _predictionOwners;

    mapping(address => mapping(uint => bool)) private _claimedMapping;

    modifier onlyAdmin() {
        if (_admin != _msgSender()) {
            revert("AdminUnauthorizedAccount");
        }
        _;
    }

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize(uint creatorFee, uint adminFee, address feeReceiver, address admin) initializer public {
        __Pausable_init();
        __Ownable_init(_msgSender());
        _creatorFeePercentage = creatorFee;
        _adminFeePercentage = adminFee;
        _admin = admin;
        _feeReceiver = feeReceiver;
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function setAdmin(address addr) external onlyOwner {
        _admin = addr;
    }

    function setCreatorFee(uint _fee) external onlyOwner {
        _creatorFeePercentage = _fee;
    }

    function setAdminFee(uint _fee) external onlyOwner {
        _adminFeePercentage = _fee;
    }

    function setFeeReceiverAddress(address _receiver) public onlyOwner {
        _feeReceiver = _receiver;
    }

    function isAdmin(address _usr) public view returns(bool) {
        return _usr == _admin;
    }

    function createNewBit(
        string memory _content,
        string memory _firstSelection,
        string memory _secondSelection,
        uint _closeSelectionDate,
        uint _finalResultDate
    ) external {
        require(_closeSelectionDate <= _finalResultDate, "Invalid close date");
        require(_closeSelectionDate > block.timestamp, "Invalid date");
        Prediction memory _newBit = Prediction({
            content: _content,
            firstSelection: _firstSelection,
            secondSelection: _secondSelection,
            closeSelectionDate: _closeSelectionDate,
            finalResultDate: _finalResultDate,
            creator: _msgSender(),
            selectionWinner: 0,
            firstSelectionValue: 0,
            secondSelectionValue: 0,
            status: 0
        });
        _predictions.push(_newBit);
        _predictionOwners[_msgSender()].push(_predictions.length - 1);
    }

    function approvePrediction(uint id) public onlyAdmin {
        require(id < _predictions.length, "InvalidPredictionId");
        Prediction storage _pr = _predictions[id];
        require(_pr.status == 0, "Prediction approved or closed");
        _pr.status = 1;
    }

    function setPredictionWinner(uint id, uint winner) public onlyAdmin {
        require(id < _predictions.length, "InvalidPredictionId");
        Prediction storage _pr = _predictions[id];
        require(_pr.closeSelectionDate < block.timestamp
        && _pr.selectionWinner == 0
        && _pr.status == 1, "InvalidPrediction");
        _pr.selectionWinner = winner;
    }

    function predict(uint id, uint selection) external payable {
        require((selection == 1 || selection == 2) && msg.value > 0, "InvalidParams");
        require(id < _predictions.length, "InvalidPredictionId");
        if (_bits.length > 0) {
            uint bitIndex = _bitOwners[_msgSender()][id];
            require(!includes(_bitMapping[id], bitIndex) || _bits[bitIndex].creator != _msgSender(), "BitCreated");
        }
        Prediction storage _pr = _predictions[id];
        require(_pr.closeSelectionDate > block.timestamp && _pr.status == 1, "PredictionNotOpen");
        Bit memory newBit = Bit({
            selection: selection,
            value: msg.value,
            creator : _msgSender()
        });
        if (selection == 1) {
            _pr.firstSelectionValue += msg.value;
        } else {
            _pr.secondSelectionValue += msg.value;
        }
//        _pr.totalValue += msg.value;
        _bits.push(newBit);
        _bitMapping[id].push(_bits.length - 1);
        _bitOwners[_msgSender()][id] = _bits.length - 1;
        uint256 _creatorFeeValue = msg.value * _creatorFeePercentage / 100000;
        uint256 _adminFeeValue = msg.value * _adminFeePercentage / 100000;
        (bool sent1,) = _pr.creator.call{value : _creatorFeeValue}("");
        require(sent1, "FailedOwner");
        (bool sent2,) = _feeReceiver.call{value : _adminFeeValue}("");
        require(sent2, "FailedAdmin");
    }

    function getPredictions() external view returns(PredictionResponse[] memory results) {
//        uint maxSize = LIMIT_PER_PAGE;
//        if (LIMIT_PER_PAGE > _predictions.length) {
//            maxSize = _predictions.length;
//        }
        results = new PredictionResponse[](_predictions.length);
//        uint startIndex = (page - 1) * LIMIT_PER_PAGE;
//        uint endIndex = page * LIMIT_PER_PAGE;
//        if (endIndex > _predictions.length) {
//            endIndex = _predictions.length;
//        }
        uint index = 0;
        for (uint i = 0; i < _predictions.length; i++) {
            uint bitIndex = _bitOwners[_msgSender()][i];
            Bit memory _bit = Bit({
                selection : 0,
                value : 0,
                creator : address(0)
            });
            if (_bits.length > bitIndex && includes(_bitMapping[i], bitIndex) && _bits[bitIndex].creator == _msgSender()) {
                _bit = _bits[bitIndex];
            }
            bool _claimed = _claimedMapping[_msgSender()][bitIndex];
            results[index] = PredictionResponse({
                id: i,
                prediction: _predictions[i],
                ownedBit: _bit,
                claimed : _claimed
            });
            ++index;
        }
        return results;
    }

    function getPrediction(uint id) public view returns(PredictionResponse memory resp) {
        Prediction memory _pr = _predictions[id];
        uint bitIndex = _bitOwners[_msgSender()][id];
        Bit memory _bit = Bit({
            selection : 0,
            value : 0,
            creator : address(0)
        });
        if (_bits.length > bitIndex && includes(_bitMapping[id], bitIndex) && _bits[bitIndex].creator == _msgSender()) {
            _bit = _bits[bitIndex];
        }
        bool _claimed = _claimedMapping[_msgSender()][bitIndex];
        return PredictionResponse({
            id: id,
            prediction : _pr,
            ownedBit : _bit,
            claimed : _claimed
        });
    }

    function includes(uint[] memory arr, uint value) public pure returns(bool) {
        for (uint i = 0; i < arr.length; i++) {
            if (arr[i] == value)
                return true;
        }
        return false;
    }

    function claimReward(uint id) public {
        require(id < _predictions.length, "InvalidId");
        Prediction memory _pr = _predictions[id];
        require(_pr.status == 1
                && _pr.closeSelectionDate < block.timestamp
                && _pr.selectionWinner != 0, "InvalidPrediction");
        uint _bitIndex = _bitOwners[_msgSender()][id];
        Bit memory _bit = _bits[_bitIndex];
        require(_bits.length > _bitIndex && includes(_bitMapping[id], _bitIndex) && _bit.creator == _msgSender(), "InvalidBit");
        require(_pr.selectionWinner == _bit.selection, "YouLose");
        require(_claimedMapping[_msgSender()][_bitIndex] == false, "YouClaimed");
        uint256 _value = 0;
        if (_pr.selectionWinner == 1) {
            _value = _bit.value * (_pr.firstSelectionValue + _pr.secondSelectionValue) / _pr.firstSelectionValue;
        } else {
            _value = _bit.value * (_pr.firstSelectionValue + _pr.secondSelectionValue) / _pr.secondSelectionValue;
        }
        _value = _value * (100000 - (_creatorFeePercentage + _adminFeePercentage)) / 100000;
        (bool sent1,) = _msgSender().call{value : _value}("");
        require(sent1, "FailedToClaim");
        _claimedMapping[_msgSender()][_bitIndex] = true;
    }
}

