pragma solidity 0.8.25;

import { OwnableRoles } from "solady/src/auth/OwnableRoles.sol";
import { Initializable } from "solady/src/utils/Initializable.sol";
import { MerkleProofLib } from "solady/src/utils/MerkleProofLib.sol";
import {
    ERC721Upgradeable
} from "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";

import { Neander } from "./library/Types.sol";
import { PrimarySkill } from "./library/SkillTypes.sol";
import { INeandersmolRender } from "./interface/INeandersmolRender.sol";

import {
    INeandersmolTraitsStorage
} from "./interface/INeandersmolTraitsStorage.sol";

error TokenIsStaked();
error MaxSupplyReached();

contract Neandersmol is ERC721Upgradeable, OwnableRoles {
    INeandersmolRender public render;

    uint256 public totalSupply;
    uint256 constant MAX_SUPPLY = 5678;

    mapping(uint256 => bool) public staked;

    uint256 public constant OWNER_ROLE = uint256(keccak256("OWNER_ROLE"));
    uint256 public constant STAKING_ROLE = uint256(keccak256("STAKER_ROLE"));
    uint256 public constant MINTERS_ROLE = uint256(keccak256("MINTERS_ROLE"));

    function initialize() public initializer {
        __ERC721_init("Neandersmol", "neandersmol");
        _initializeOwner(msg.sender);
    }

    function mint(
        address _to,
        uint256 _tokenId
    ) external onlyRoles(MINTERS_ROLE) {
        if (totalSupply > MAX_SUPPLY) revert MaxSupplyReached();
        _mint(_to, _tokenId);
        totalSupply++;
    }

    function stakeHandler(
        uint256 _tokenId,
        bool _state
    ) external onlyRoles(STAKING_ROLE) {
        staked[_tokenId] = _state;
        emit Staked(_tokenId, _state);
    }

    function transferFrom(
        address _from,
        address _to,
        uint256 _tokenId
    ) public virtual override {
        if (staked[_tokenId]) revert TokenIsStaked();
        super.transferFrom(_from, _to, _tokenId);
    }

    function setRenderAddress(address _render) external onlyOwner {
        render = INeandersmolRender(_render);
    }

    function tokensOfOwner(
        address _owner
    ) external view returns (uint256[] memory) {
        uint256 count;
        uint256 walletBalance = balanceOf(_owner);
        uint256[] memory tokens = new uint256[](walletBalance);

        for (uint256 i; i < MAX_SUPPLY; ++i) {
            if (count == walletBalance) return tokens;

            if (_ownerOf(i) != address(0) && ownerOf(i) == _owner) {
                tokens[count] = i;
                count++;
            }
        }
        return tokens;
    }

    function supportsInterface(
        bytes4 interfaceId
    ) public view virtual override(ERC721Upgradeable) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    function tokenURI(
        uint256 _tokenId
    ) public view virtual override returns (string memory) {
        if (_ownerOf(_tokenId) == address(0))
            revert ERC721NonexistentToken(_tokenId);

        return render.tokenURI(_tokenId);
    }

    event Staked(uint256 indexed tokenId, bool indexed state);
}

