pragma solidity ^0.8.25;

import {AccessManagedUpgradeable} from "@openzeppelin/contracts-upgradeable/access/manager/AccessManagedUpgradeable.sol";
import {PausableUpgradeable} from "@openzeppelin/contracts-upgradeable/utils/PausableUpgradeable.sol";
import {UUPSUpgradeable} from "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import {Initializable} from "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import {IERC20} from "@openzeppelin/contracts/token/ERC20/ERC20.sol";

import {CCTPTokenMessengerLibrary} from "./lib/TokenMessengerLibrary.sol";
import {AccessManagerLibrary} from "./lib/AccessManagerLibrary.sol";
import {USDCLibrary} from "./lib/USDCLibrary.sol";

interface ITokenMessenger {
  function depositForBurn(uint256 amount, uint32 destinationDomain, bytes32 mintRecipient, address burnToken) external returns (uint64 _nonce);
}

contract CCTPManager is Initializable, AccessManagedUpgradeable, UUPSUpgradeable, PausableUpgradeable {
  using CCTPTokenMessengerLibrary for uint256;

  uint256 constant public BASIS_POINTS = 10_000;

  address public tokenMessenger;
  address public usdc;
  address public feeRecipient;

  // fee is taken in basis points. 100 = 1%
  mapping(uint32 domain => uint256 _fee) public fee;

  event FeeWithdrawn(address indexed from, address indexed to, uint256 amount);
  event FeeTaken(address indexed from, uint256 basisPoints, uint256 amount);
  event Bridged(address indexed from, address indexed to, uint32 destinationDomain, uint256 amount);

  error InvalidAmount();
  error ZeroAddress();
  error ZeroBalance();
  error ValueUnchanged();
  error FeeRecipientNotSet();
  error InvalidChainId(uint256);

  /// @custom:oz-upgrades-unsafe-allow constructor
  constructor() {
    _disableInitializers();
  }

  function initialize() public initializer {
    __Pausable_init();
    __AccessManaged_init(AccessManagerLibrary.manager());
    __UUPSUpgradeable_init();
    usdc = USDCLibrary.usdc();
    tokenMessenger = CCTPTokenMessengerLibrary.messenger();
    IERC20(usdc).approve(tokenMessenger, type(uint256).max);
    block.chainid.getDomain();
  }

  function updateUsdc(address usdc_) external restricted {
    if (usdc_ == usdc) revert ValueUnchanged();
    if (usdc_ == address(0)) revert ZeroAddress();
    usdc = usdc_;
  }

  function updateTokenMessenger(address tokenMessenger_) external restricted {
    if (tokenMessenger_ == tokenMessenger) revert ValueUnchanged();
    if (tokenMessenger_ == address(0)) revert ZeroAddress();
    tokenMessenger = tokenMessenger_;
  }

  function updateFee(uint32 domain, uint256 fee_) external restricted {
    if (fee_ > 0 && feeRecipient == address(0)) revert FeeRecipientNotSet();
    if (fee[domain] == fee_) revert ValueUnchanged();
    fee[domain] = fee_;
  }

  function updateFees(uint32[] calldata domains, uint256[] calldata fees) external restricted {
    if (domains.length == 0) revert ValueUnchanged();
    if (domains.length != fees.length) revert InvalidAmount();
    if (feeRecipient == address(0)) revert FeeRecipientNotSet();
    for (uint256 i = 0; i < domains.length; i++) {
      if (fee[domains[i]] == fees[i]) revert ValueUnchanged();
      fee[domains[i]] = fees[i];
    }
  }

  function withdraw() external {
    uint256 balance = IERC20(usdc).balanceOf(address(this));
    if (balance == 0) revert ZeroBalance();
    IERC20(usdc).transfer(feeRecipient, balance);
    emit FeeWithdrawn(msg.sender, feeRecipient, balance);
  }

  function setFeeRecipient(address feeRecipient_) external restricted {
    if (feeRecipient == feeRecipient_) revert ValueUnchanged();
    if (feeRecipient_ == address(0)) revert ZeroAddress();
    feeRecipient = feeRecipient_;
  }

  function bridge(uint256 amount, uint32 destinationDomain, address recipient) public returns (uint64) {
    if (amount == 0) revert InvalidAmount();
    uint256 remainder = _takeFee(destinationDomain, amount);
    IERC20(usdc).transferFrom(msg.sender, address(this), remainder);

    emit Bridged(msg.sender, recipient, destinationDomain, remainder);

    // Call the underlying function on the token messenger
    return ITokenMessenger(tokenMessenger).depositForBurn(amount, destinationDomain, bytes32(uint256(uint160(recipient))), usdc);
  }

  function bridgeByChainId(uint256 amount, uint256 chainId, address recipient) public returns (uint64) {
    return bridge(amount, chainId.getDomain(), recipient);
  }

  function bridgeToBase(uint256 amount, address recipient) public returns (uint64) {
    return bridgeByChainId(amount, 8453, recipient);
  }

  function bridgeToEthereum(uint256 amount, address recipient) public returns (uint64) {
    return bridgeByChainId(amount, 1, recipient);
  }

  function bridgeToOptimism(uint256 amount, address recipient) public returns (uint64) {
    return bridgeByChainId(amount, 10, recipient);
  }

  function bridgeToArbitrum(uint256 amount, address recipient) public returns (uint64) {
    return bridgeByChainId(amount, 42161, recipient);
  }

  function bridgeToPolygon(uint256 amount, address recipient) public returns (uint64) {
    return bridgeByChainId(amount, 137, recipient);
  }

  function bridgeToAvalanche(uint256 amount, address recipient) public returns (uint64) {
    return bridgeByChainId(amount, 43114, recipient);
  }

  function _takeFee(uint32 domain, uint256 amount) internal returns (uint256) {
    uint256 __fee = fee[domain];
    if (__fee > 0) {
      if (feeRecipient == address(0)) revert FeeRecipientNotSet();
      uint256 take = amount * __fee / BASIS_POINTS;
      uint256 remainder = amount - take;
      IERC20(usdc).transferFrom(msg.sender, address(this), take);
      emit FeeTaken(msg.sender, __fee, take);
      return remainder;
    }
    return amount;
  }

  function _authorizeUpgrade(address newImplementation) internal virtual override restricted {
    if (newImplementation == address(0)) revert ZeroAddress();
  }
}

