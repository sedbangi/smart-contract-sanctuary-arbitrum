        uint256 startStakingTime;
        uint256 endStakingTime;
        uint256 stakeDuration;
        uint256 endTime;
        uint256 stakingCycle;
    }

    struct StakeInfo {
        uint256 stakeId;
        address staker;
        uint256 stakeAmount;
        uint256 claimAmount;
        uint256 startAt;
        bool isEnd;
    }

    struct PaymentInfo {
        uint256 fee;
        IERC20 paymentToken;
        address receiver;
    }

    struct RewardInfo {
        uint256 stakeId;
        uint256 totalReward;
        uint256 availableReward;
        uint256 claimedAmount;
    }

    modifier notZero(address param) {
        require(param != address(0), "address is zero");
        _;
    }

    modifier isStakeTime() {
        require(stakingPeriod.startStakingTime < block.timestamp, "not start");
        require(stakingPeriod.endStakingTime > block.timestamp, "is end");
        _;
    }

    modifier isFinish() {
        require(stakingPeriod.endTime < block.timestamp, "not finish");
        _;
    }

    modifier lock() {
        require(!islock, "is lock");
        islock = true;
        _;
        islock = false;
    }

    event Stake(
        uint256 stakeId,
        address user,
        uint256 amount,
        uint256 stakingCycle
    );

    event Withdraw(
        uint256 stakeId,
        address user,
        uint256 amount,
        uint256 stakingCycle
    );

    function initialize(
        IERC20 _stakingToken,
        IERC20 _rewardToken,
        address _rewardManager
    ) public initializer {
        __Ownable_init(msg.sender);
        __UUPSUpgradeable_init();
        stakingToken = _stakingToken;
        rewardToken = _rewardToken;
        rewardManager = _rewardManager;
        minClaimAmount =
            10 ** IStakingRewardManager(address(rewardToken)).decimals();
    }

    function _authorizeUpgrade(
        address newImplementation
    ) internal override onlyOwner {}

    function setToken(
        IERC20 _stakingToken,
        IERC20 _rewardToken
    )
        external
        notZero(address(_stakingToken))
        notZero(address(_rewardToken))
        onlyOwner
    {
        require(
            stakingPeriod.startStakingTime > block.timestamp,
            "stake is start"
        );
        stakingToken = _stakingToken;
        rewardToken = _rewardToken;
    }

    function setRewardManager(
        address _rewardManage
    ) external notZero(_rewardManage) onlyOwner lock {
        rewardManager = _rewardManage;
    }

    function setPaymentInfo(
        PaymentInfo memory info
    )
        external
        notZero(address(info.receiver))
        notZero(address(info.paymentToken))
        onlyOwner
        lock
    {
        paymentInfo = info;
    }

    function setMinClaimAmount(uint256 _minAmount) external onlyOwner {
        minClaimAmount = _minAmount;
    }

    function setStakingPeriod(
        uint256 startStakingTime,
        uint256 endStakingTime,
        uint256 _stakeDuration,
        uint256 _stakingCycle
    ) public onlyOwner {
        require(_stakeDuration > 0, "stakeDuration must gt 0");
        require(startStakingTime < endStakingTime, "startTime must lt endTime");
        stakingPeriod = StakingPeriod(
            startStakingTime,
            endStakingTime,
            _stakeDuration,
            endStakingTime + _stakeDuration,
            _stakingCycle
        );
    }

    function lastTimeRewardApplicable() public view returns (uint) {
        return _min(stakingPeriod.endTime, block.timestamp);
    }

    function addReward(address sender, uint256 reward) external onlyOwner {
        require(totalStake > 0, "no one stake");
        require(block.timestamp > rewardTime + 300, "added too often");
        require(block.timestamp < stakingPeriod.endTime, "stake is end");
        rewardToken.transferFrom(sender, address(this), reward);
        rewardIndex += (reward * 1e18) / totalStake;
        rewardTime = block.timestamp;
    }

    function _calculateRewards(uint256 stakeId) private view returns (uint) {
        uint shares = stakeInfo[stakeId].stakeAmount;
        return (shares * (rewardIndex - rewardIndexOf[stakeId])) / 1e18;
    }

    function getReward(uint256 stakeId) public view returns (uint) {
        if (stakeInfo[stakeId].isEnd) {
            return earned[stakeId];
        }
        return earned[stakeId] + _calculateRewards(stakeId);
    }

    function getUserTotalReward(
        address user
    ) public view returns (uint reward) {
        for (uint i = 0; i < userStakeIdList[user].length(); i++) {
            reward += getReward(userStakeIdList[user].at(i));
        }
    }

    function getTotalStakingRewardsList(
        address user
    ) public view returns (RewardInfo[] memory rewardList) {
        rewardList = new RewardInfo[](userStakeIdList[user].length());
        for (uint i = 0; i < userStakeIdList[user].length(); i++) {
            rewardList[i] = RewardInfo(
                userStakeIdList[user].at(i),
                getReward(userStakeIdList[user].at(i)),
                getReward(userStakeIdList[user].at(i)),
                stakeInfo[userStakeIdList[user].at(i)].claimAmount
            );
        }
    }

    function _updateRewards(uint256 stakeId) private {
        earned[stakeId] += _calculateRewards(stakeId);
        rewardIndexOf[stakeId] = rewardIndex;
    }

    function stake(
        address staker,
        uint256 amount
    ) external notZero(staker) isStakeTime lock {
        require(
            amount >= IStakingRewardManager(rewardManager).stateAmount(0),
            "stake amount not enough min standard"
        );
        stakingToken.transferFrom(msg.sender, address(this), amount);
        _stakeId++;
        _updateRewards(_stakeId);
        userStakeIdList[staker].add(_stakeId);
        totalStakeList.add(_stakeId);
        userTotalStakeAmount[staker] += amount;
        totalStake += amount;
        updateAt[_stakeId] = stakingPeriod.endStakingTime;
        stakeInfo[_stakeId] = StakeInfo(
            _stakeId,
            staker,
            amount,
            0,
            block.timestamp,
            false
        );
        emit Stake(_stakeId, staker, amount, stakingPeriod.stakingCycle);
    }

    function withdraw(uint256 stakeId) public isFinish lock {
        require(userStakeIdList[msg.sender].contains(stakeId), "invalid id");
        require(!stakeInfo[stakeId].isEnd, "id is end");
        if (paymentInfo.fee > 0) {
            paymentInfo.paymentToken.transferFrom(
                msg.sender,
                paymentInfo.receiver,
                paymentInfo.fee
            );
        }
        _updateRewards(_stakeId);
        uint256 amount = stakeInfo[stakeId].stakeAmount;
        userTotalStakeAmount[msg.sender] -= amount;
        _claim(stakeId);
        totalStake -= amount;
        stakeInfo[stakeId].isEnd = true;
        stakingToken.transfer(msg.sender, amount);
        emit Withdraw(stakeId, msg.sender, amount, stakingPeriod.stakingCycle);
    }

    function withdrawAll() external {
        for (uint i = 0; i < userStakeIdList[msg.sender].length(); i++) {
            uint256 stakeId = userStakeIdList[msg.sender].at(i);
            withdraw(stakeId);
        }
    }

    function getUserStakeIdList(
        address user
    ) public view returns (uint256[] memory stakeIdList) {
        stakeIdList = userStakeIdList[user].values();
    }

    function getUserStakeInfos(
        address user
    ) public view returns (StakeInfo[] memory stakeInfos) {
        stakeInfos = new StakeInfo[](userStakeIdList[user].length());
        for (uint i = 0; i < userStakeIdList[user].length(); i++) {
            uint256 stakeId = userStakeIdList[user].at(i);
            stakeInfos[i] = stakeInfo[stakeId];
        }
    }

    function getTotalStakeList() public view returns (uint256[] memory list) {
        list = totalStakeList.values();
    }

    function claim(uint256 stakeId) public lock {
        require(userStakeIdList[msg.sender].contains(stakeId), "invalid id");
        _claim(stakeId);
    }

    function claimAll(uint256[] memory stakeIdList) external {
        for (uint i = 0; i < stakeIdList.length; i++) {
            claim(stakeIdList[i]);
        }
    }

    function _claim(uint256 stakeId) private returns (uint) {
        _updateRewards(stakeId);
        address staker = stakeInfo[stakeId].staker;
        uint reward = earned[stakeId];
        if (reward > 0) {
            stakeInfo[stakeId].claimAmount += earned[stakeId];
            earned[stakeId] = 0;
            rewardToken.transfer(staker, reward);
        }
        return reward;
    }

    function _min(uint x, uint y) private pure returns (uint) {
        return x <= y ? x : y;
    }
}

interface IStakingRewardManager {
    function stateAmount(uint256) external view returns (uint256);

    function decimals() external view returns (uint256);
}

