pragma solidity ^0.8.13;

import { Ownable } from "solady/src/auth/Ownable.sol";
import { SafeTransferLib } from "solady/src/utils/SafeTransferLib.sol";
import {
    ReentrancyGuardUpgradeable,
    Initializable
} from "@openzeppelin/contracts-upgradeable/utils/ReentrancyGuardUpgradeable.sol";
import {
    IERC1155
} from "@openzeppelin/contracts-upgradeable/token/ERC1155/ERC1155Upgradeable.sol";
import {
    IERC721,
    IERC165
} from "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";

import {
    IERC20
} from "@openzeppelin/contracts-upgradeable/token/ERC20/ERC20Upgradeable.sol";

import { Error } from "./library/Error.sol";

import {
    MarketOffer,
    FeeRecipient,
    TokenApprovalStatus,
    AcceptBidParams,
    BuyItemParams,
    TokenBid,
    ListingParams
} from "./library/Types.sol";

contract SmolAgeNftMarketplace is ReentrancyGuardUpgradeable, Ownable {
    using SafeTransferLib for address;

    bytes4 private constant INTERFACE_ID_ERC721 = 0x80ac58cd;
    bytes4 private constant INTERFACE_ID_ERC1155 = 0xd9b67a26;

    uint256 public constant BASIS_POINTS = 10000;

    uint256 public constant MAX_FEE = 5000;

    uint256 public constant MAX_COLLECTION_FEE = 5000;

    uint256 public constant MIN_PRICE = 1e9;

    IERC20 public weth;

    uint256 public fee;
    address public feeRecipient;
    uint256 public feeWithCollectionOwner;

    mapping(bytes32 => MarketOffer) private listings;

    mapping(bytes32 => mapping(address => TokenBid)) private tokenBids;
    mapping(address => mapping(uint256 => mapping(address => TokenBid)))
        private tokenBidsWithoutListing;

    mapping(address => bool) public isTokenAllowed;

    mapping(address => TokenApprovalStatus) public tokenApprovals;
    mapping(address => FeeRecipient) public collectionToCollectionOwnerFee;

    mapping(address => mapping(uint256 => bool)) public isListed;

    function initialize(
        uint256 _initialFee,
        address _initialFeeRecipient
    ) external initializer {
        _initializeOwner(msg.sender);
        __ReentrancyGuard_init_unchained();
        setFee(_initialFee, _initialFee);
        setFeeRecipient(_initialFeeRecipient);
    }

    function createListing(
        ListingParams calldata _listingParams
    ) external nonReentrant {
        bytes32 listingId = keccak256(
            abi.encode(
                _listingParams.nftAddress,
                _listingParams.tokenIds,
                msg.sender
            )
        );

        TokenApprovalStatus status = tokenApprovals[_listingParams.nftAddress];
        if (status == TokenApprovalStatus.NOT_APPROVED) {
            revert Error.TokenIsNotApproved(_listingParams.nftAddress);
        }

        if (status == TokenApprovalStatus.ERC_721_APPROVED)
            validateAndMarkERC721Listed(_listingParams);

        if (status == TokenApprovalStatus.ERC_1155_APPROVED) {
            if (listings[listingId].quantity != 0) revert Error.AlreadyListed();

            validateERC1155OwnershipAndApproval(_listingParams);
        }

        createListingHelper(_listingParams, listingId);
        emit ItemListed(
            msg.sender,
            listingId,
            _listingParams.nftAddress,
            _listingParams.tokenIds,
            _listingParams.quantity,
            _listingParams.price,
            _listingParams.expirationTime,
            _listingParams.paymentToken
        );
    }

    function updateListing(
        ListingParams calldata _listingParams,
        bytes32 _listingId
    ) external nonReentrant {
        if (listings[_listingId].quantity == 0) revert Error.TokenIsNotListed();
        if (listings[_listingId].owner != msg.sender) revert Error.UnauthorizedActionByTokenOwner();

        createListingHelper(_listingParams, _listingId);
        emit ItemListed(
            msg.sender,
            _listingId,
            _listingParams.nftAddress,
            _listingParams.tokenIds,
            _listingParams.quantity,
            _listingParams.price,
            _listingParams.expirationTime,
            _listingParams.paymentToken
        );
    }

    function validateAndMarkERC721Listed(
        ListingParams calldata _listingParams
    ) internal {
        IERC721 nft = IERC721(_listingParams.nftAddress);
        if (!nft.isApprovedForAll(msg.sender, address(this))) {
            revert Error.TokenNotApprovedForAll();
        }

        for (uint256 i = 0; i < _listingParams.tokenIds.length; ++i) {
            uint256 tokenId = _listingParams.tokenIds[i];
            if (nft.ownerOf(tokenId) != msg.sender) {
                revert Error.NotYourToken();
            }
            if (isListed[_listingParams.nftAddress][tokenId]) {
                revert Error.TokenAlreadyListed();
            }
            isListed[_listingParams.nftAddress][tokenId] = true;
        }
    }

    function validateERC1155OwnershipAndApproval(
        ListingParams calldata _listingParams
    ) internal view {
        IERC1155 nft = IERC1155(_listingParams.nftAddress);
        if (!nft.isApprovedForAll(msg.sender, address(this))) {
            revert Error.TokenNotApprovedForAll();
        }
        if (
            nft.balanceOf(msg.sender, _listingParams.tokenIds[0]) <
            _listingParams.quantity
        ) {
            revert Error.InsufficientBalance();
        }
    }

    function createListingHelper(
        ListingParams calldata _listingParams,
        bytes32 _listingId
    ) internal {
        validateListingTerms(
            _listingParams.paymentToken,
            _listingParams.price,
            _listingParams.expirationTime
        );

        listings[_listingId] = MarketOffer({
            nftAddress: _listingParams.nftAddress,
            currencyAddress: _listingParams.paymentToken,
            owner: msg.sender,
            tokenIds: _listingParams.tokenIds,
            price: _listingParams.price,
            quantity: _listingParams.quantity,
            expiryTime: _listingParams.expirationTime
        });
    }

    function validateListingTerms(
        address _paymentToken,
        uint128 _pricePerItem,
        uint64 _expirationTime
    ) internal view {
        if (!isTokenAllowed[_paymentToken])
            revert Error.PaymentTokenIsNotApprovedForSale();

        if (_pricePerItem < MIN_PRICE) revert Error.BelowMinimumPrice();

        if (_expirationTime < block.timestamp) revert Error.TimeExpired();
    }

    function cancelListing(bytes32 _listingId) public nonReentrant {
        MarketOffer memory listing = listings[_listingId];
        if (listing.owner != msg.sender) revert Error.NotYourToken();

        emit ItemCancelled(
            msg.sender,
            _listingId
        );
        updateListingAfterSale(_listingId);
    }

    function cancelTokenBidForListing(
        bytes32 _listingId
    ) external nonReentrant {
        if (tokenBids[_listingId][msg.sender].quantity > 0) {
            delete tokenBids[_listingId][msg.sender];
            emit TokenBidCancelled(msg.sender, address(0), 0, _listingId);
        } else revert Error.BidDoesNotExist();
    }

    function cancelTokenBidForNonListing(
        address _nftAddress,
        uint256 _tokenId
    ) external nonReentrant {
        if (
            tokenBidsWithoutListing[_nftAddress][_tokenId][msg.sender]
                .quantity > 0
        ) {
            delete tokenBidsWithoutListing[_nftAddress][_tokenId][msg.sender];
            emit TokenBidCancelled(
                msg.sender,
                _nftAddress,
                _tokenId,
                bytes32(0)
            );
        } else revert Error.BidDoesNotExist();
    }

    function createTokenBidForNonListing(
        address _nftAddress,
        uint256 _tokenId,
        uint64 _quantity,
        uint128 _price,
        uint64 _expirationTime,
        address _paymentToken
    ) external nonReentrant {
        createBid(
            _nftAddress,
            _quantity,
            _price,
            _expirationTime,
            _paymentToken,
            tokenBidsWithoutListing[_nftAddress][_tokenId][msg.sender]
        );

        emit TokenBidCreatedOrUpdated(
            msg.sender,
            _nftAddress,
            _tokenId,
            bytes32(0),
            _price,
            _expirationTime,
            _paymentToken
        );
    }

    function createTokenBidForListing(
        uint128 _price,
        uint64 _expirationTime,
        address _paymentToken,
        bytes32 _listingId
    ) external nonReentrant {
        MarketOffer memory listing = listings[_listingId];
        if (listing.owner == address(0) && _listingId != bytes32(0))
            revert Error.TokenIsNotListed();
        if (listing.owner == msg.sender)
            revert Error.UnauthorizedActionByTokenOwner();

        createBid(
            listing.nftAddress,
            listing.quantity,
            _price,
            _expirationTime,
            _paymentToken,
            tokenBids[_listingId][msg.sender]
        );

        emit TokenBidCreatedOrUpdated(
            msg.sender,
            address(0),
            0,
            _listingId,
            _price,
            _expirationTime,
            _paymentToken
        );
    }

    function createBid(
        address _nftAddress,
        uint64 _quantity,
        uint128 _price,
        uint64 _expirationTime,
        address _paymentToken,
        TokenBid storage _bid
    ) internal {
        TokenApprovalStatus status = tokenApprovals[_nftAddress];

        if (status == TokenApprovalStatus.NOT_APPROVED)
            revert Error.TokenIsNotApproved(_nftAddress);
        if (status == TokenApprovalStatus.ERC_721_APPROVED && _quantity != 1)
            revert Error.InvalidQuantity();
        if (status == TokenApprovalStatus.ERC_1155_APPROVED && _quantity == 0)
            revert Error.InvalidQuantity();

        createBidHelper(
            _quantity,
            _price,
            _expirationTime,
            _paymentToken,
            _bid
        );
    }

    function createBidHelper(
        uint64 _quantity,
        uint128 _pricePerItem,
        uint64 _expirationTime,
        address _paymentToken,
        TokenBid storage _bid
    ) internal {
        if (_expirationTime < block.timestamp) revert Error.TimeExpired();
        if (_pricePerItem < MIN_PRICE) revert Error.InvalidPrice();

        if (!isTokenAllowed[_paymentToken]) revert Error.InvalidPaymentToken();

        IERC20 token = IERC20(_paymentToken);

        uint256 _totalAmountNeeded = _pricePerItem * _quantity;

        if (token.allowance(msg.sender, address(this)) < _totalAmountNeeded)
            revert Error.InsufficientAllowance();
        if (token.balanceOf(msg.sender) < _totalAmountNeeded)
            revert Error.InsufficientBalance();

        _bid.quantity = _quantity;
        _bid.price = _pricePerItem;
        _bid.expiryTime = _expirationTime;
        _bid.paymentToken = _paymentToken;
        _bid.bidder = msg.sender;
    }

    function acceptTokenBidForListing(
        AcceptBidParams calldata _acceptBidParams
    ) external nonReentrant {
        MarketOffer memory listing = listings[_acceptBidParams.listingId];
        TokenBid memory bid = tokenBids[_acceptBidParams.listingId][
            _acceptBidParams.bidder
        ];

        if (listing.owner != msg.sender || bid.bidder == msg.sender)
            revert Error.UnauthorizedActionByTokenOwner();

        acceptBid(
            bid,
            _acceptBidParams.quantity,
            _acceptBidParams.nftAddress,
            listing.tokenIds
        );

        emit BidAccepted(
            msg.sender,
            _acceptBidParams.bidder,
            _acceptBidParams.nftAddress,
            _acceptBidParams.tokenId,
            _acceptBidParams.listingId,
            bid.quantity,
            bid.price,
            bid.paymentToken
        );
        delete tokenBids[_acceptBidParams.listingId][_acceptBidParams.bidder];
        updateListingAfterSale(_acceptBidParams.listingId);
    }

    function acceptTokenBidForNonListing(
        AcceptBidParams calldata _acceptBidParams
    ) external nonReentrant {
        if (_acceptBidParams.listingId != bytes32(0))
            revert Error.TokenIsNotListed();
        TokenBid memory bid = tokenBidsWithoutListing[
            _acceptBidParams.nftAddress
        ][_acceptBidParams.tokenId][_acceptBidParams.bidder];

        if (_acceptBidParams.quantity > 1) {
            if (
                IERC1155(_acceptBidParams.nftAddress).balanceOf(
                    msg.sender,
                    _acceptBidParams.tokenId
                ) < _acceptBidParams.quantity
            ) revert Error.InvalidQuantity();
        }
        uint[] memory tokenIds = new uint[](1);
        tokenIds[0] = _acceptBidParams.tokenId;

        acceptBid(
            bid,
            _acceptBidParams.quantity,
            _acceptBidParams.nftAddress,
            tokenIds
        );

        emit BidAccepted(
            msg.sender,
            _acceptBidParams.bidder,
            _acceptBidParams.nftAddress,
            _acceptBidParams.tokenId,
            _acceptBidParams.listingId,
            bid.quantity,
            bid.price,
            bid.paymentToken
        );
        if (bid.quantity - _acceptBidParams.quantity == 0) {
            delete tokenBidsWithoutListing[_acceptBidParams.nftAddress][
                _acceptBidParams.tokenId
            ][_acceptBidParams.bidder];
        } else {
            tokenBidsWithoutListing[_acceptBidParams.nftAddress][
                _acceptBidParams.tokenId
            ][_acceptBidParams.bidder].quantity -= 1;
        }
    }

    function acceptBid(
        TokenBid memory bid,
        uint64 _quantity,
        address nftAddress,
        uint[] memory tokenIds
    ) internal {
        if (bid.expiryTime < block.timestamp) revert Error.TimeExpired();
        if (bid.price == 0) revert Error.InvalidPrice();

        executeTransfer(
            bid.bidder,
            msg.sender,
            nftAddress,
            _quantity,
            tokenIds
        );

        uint256 amount = bid.price * _quantity;

        payFees(
            amount,
            nftAddress,
            bid.bidder,
            msg.sender,
            bid.paymentToken,
            false
        );
    }

    function buyItems(
        BuyItemParams[] calldata _buyItemParams
    ) external payable nonReentrant {
        uint256 _ethAmountRequired;
        for (uint256 i = 0; i < _buyItemParams.length; i++) {
            _ethAmountRequired += buyItem(_buyItemParams[i]);
        }

        if (msg.value != _ethAmountRequired) revert Error.InsufficientEth();
    }

    function buyItem(
        BuyItemParams calldata _buyItemParams
    ) internal returns (uint256) {
        MarketOffer memory listing = listings[_buyItemParams.listingId];
        validateBuyItemTransaction(_buyItemParams);

        processFees(
            listing,
            listing.owner,
            listing.nftAddress,
            _buyItemParams.paymentToken,
            _buyItemParams.usingEth
        );

        executeTransfer(
            msg.sender,
            listing.owner,
            listing.nftAddress,
            listing.quantity,
            listing.tokenIds
        );

        emit ItemSold(
            listing.owner,
            msg.sender,
            listing.nftAddress,
            _buyItemParams.listingId,
            listing.tokenIds,
            listing.quantity,
            listing.price,
            _buyItemParams.paymentToken
        );

        updateListingAfterSale(_buyItemParams.listingId);

        return _buyItemParams.usingEth ? listing.quantity * listing.price : 0;
    }

    function validateBuyItemTransaction(
        BuyItemParams calldata _buyItemParams
    ) internal view {
        MarketOffer memory listing = listings[_buyItemParams.listingId];
        if (msg.sender == listing.owner)
            revert Error.UnauthorizedActionByTokenOwner();

        if (listing.expiryTime < block.timestamp) revert Error.TimeExpired();
        if (listing.quantity == 0 || listing.price == 0)
            revert Error.TokenIsNotListed();

        if (listing.currencyAddress != _buyItemParams.paymentToken)
            revert Error.InvalidPaymentToken();

        if (_buyItemParams.usingEth && listing.currencyAddress != address(weth))
            revert Error.OnlyEthIsAllowed();

        TokenApprovalStatus status = tokenApprovals[listing.nftAddress];
        if (status == TokenApprovalStatus.NOT_APPROVED)
            revert Error.TokenIsNotApproved(listing.nftAddress);
    }

    function executeTransfer(
        address _to,
        address _owner,
        address _nftAddress,
        uint64 _quantity,
        uint256[] memory _tokenIds
    ) internal {
        TokenApprovalStatus status = tokenApprovals[_nftAddress];
        if (status == TokenApprovalStatus.ERC_721_APPROVED) {
            for (uint i; i < _tokenIds.length; i++) {
                IERC721(_nftAddress).safeTransferFrom(
                    _owner,
                    _to,
                    _tokenIds[i]
                );
            }
        } else if (status == TokenApprovalStatus.ERC_1155_APPROVED) {
            IERC1155(_nftAddress).safeTransferFrom(
                _owner,
                _to,
                _tokenIds[0],
                _quantity,
                bytes("")
            );
        } else {
            revert Error.TokenIsNotApprovedForSale();
        }
    }

    function processFees(
        MarketOffer memory _listing,
        address _owner,
        address _nftAddress,
        address _paymentToken,
        bool _usingEth
    ) internal {
        payFees(
            _listing.quantity * _listing.price,
            _nftAddress,
            msg.sender,
            _owner,
            _paymentToken,
            _usingEth
        );
    }

    function updateListingAfterSale(bytes32 _listingId) internal {
        MarketOffer memory listing = listings[_listingId];
        if (
            tokenApprovals[listing.nftAddress] ==
            TokenApprovalStatus.ERC_721_APPROVED
        ) {
            for (uint256 i; i < listing.tokenIds.length; ++i) {
                isListed[listing.nftAddress][listing.tokenIds[i]] = false;
            }
        }

        delete listings[_listingId];
    }

    function payFees(
        uint256 _totalPrice,
        address _collectionAddress,
        address _from,
        address _to,
        address _paymentTokenAddress,
        bool _usingEth
    ) internal {
        IERC20 _paymentToken = IERC20(_paymentTokenAddress);

        FeeRecipient memory collectionFeeInfo = collectionToCollectionOwnerFee[
            _collectionAddress
        ];
        address collectionFeeRecipient = collectionFeeInfo.recipient;

        uint256 protocolFee = collectionFeeRecipient != address(0)
            ? feeWithCollectionOwner
            : fee;
        uint256 collectionFee = collectionFeeRecipient != address(0)
            ? collectionFeeInfo.fee
            : 0;

        uint256 protocolFeeAmount = (_totalPrice * protocolFee) / BASIS_POINTS;
        uint256 collectionFeeAmount = (_totalPrice * collectionFee) /
            BASIS_POINTS;

        transferAmount(
            _from,
            feeRecipient,
            protocolFeeAmount,
            _paymentToken,
            _usingEth
        );

        if (collectionFeeRecipient != address(0)) {
            transferAmount(
                _from,
                collectionFeeRecipient,
                collectionFeeAmount,
                _paymentToken,
                _usingEth
            );
        }

        uint256 amountToTransfer = _totalPrice -
            protocolFeeAmount -
            collectionFeeAmount;

        transferAmount(_from, _to, amountToTransfer, _paymentToken, _usingEth);
    }

    function transferAmount(
        address _from,
        address _to,
        uint256 _amount,
        IERC20 _paymentToken,
        bool _usingEth
    ) internal {
        if (_amount == 0) return;

        _usingEth
            ? _to.safeTransferETH(_amount)
            : address(_paymentToken).safeTransferFrom(_from, _to, _amount);
    }

    function setFee(
        uint256 _newFee,
        uint256 _newFeeWithCollectionOwner
    ) public onlyOwner {
        if (_newFee > MAX_FEE || _newFeeWithCollectionOwner > MAX_FEE)
            revert Error.ExceedsMaximumAllowedFee();

        fee = _newFee;
        feeWithCollectionOwner = _newFeeWithCollectionOwner;

        emit UpdateFee(_newFee);
        emit UpdateFeeWithCollectionOwner(_newFeeWithCollectionOwner);
    }

    function setCollectionOwnerFee(
        address _collectionAddress,
        FeeRecipient calldata _collectionOwnerFee
    ) external onlyOwner {
        if (
            tokenApprovals[_collectionAddress] !=
            TokenApprovalStatus.ERC_1155_APPROVED &&
            tokenApprovals[_collectionAddress] !=
            TokenApprovalStatus.ERC_721_APPROVED
        ) revert Error.TokenIsNotApprovedForSale();
        if (_collectionOwnerFee.fee > MAX_COLLECTION_FEE)
            revert Error.InvalidCollectionFee();

        collectionToCollectionOwnerFee[
            _collectionAddress
        ] = _collectionOwnerFee;

        emit UpdateCollectionOwnerFee(
            _collectionAddress,
            _collectionOwnerFee.recipient,
            _collectionOwnerFee.fee
        );
    }

    function getListings(
        bytes32 _listingId
    ) external view returns (MarketOffer memory) {
        return listings[_listingId];
    }

    function getTokenBid(
        address _bidder,
        bytes32 _listingId
    ) external view returns (TokenBid memory) {
        return tokenBids[_listingId][_bidder];
    }

    function getTokenBidsWithoutListing(
        address _nftAddress,
        uint256 _tokenId,
        address _owner
    ) external view returns (TokenBid memory) {
        return tokenBidsWithoutListing[_nftAddress][_tokenId][_owner];
    }

    function setFeeRecipient(address _newFeeRecipient) public onlyOwner {
        if (_newFeeRecipient == address(0)) revert Error.ZeroAddressError();
        feeRecipient = _newFeeRecipient;
        emit UpdateFeeRecipient(_newFeeRecipient);
    }

    function setPaymentToken(
        address _paymentToken,
        bool _state
    ) external onlyOwner {
        isTokenAllowed[_paymentToken] = _state;
    }

    function setTokenApprovalStatus(
        address _nft,
        TokenApprovalStatus _status
    ) external onlyOwner {
        if (
            _status == TokenApprovalStatus.ERC_721_APPROVED &&
            !IERC165(_nft).supportsInterface(INTERFACE_ID_ERC721)
        ) revert Error.InvalidERC721();
        if (
            _status == TokenApprovalStatus.ERC_1155_APPROVED &&
            !IERC165(_nft).supportsInterface(INTERFACE_ID_ERC1155)
        ) revert Error.InvalidERC1155();

        tokenApprovals[_nft] = _status;

        emit TokenApprovalStatusUpdated(_nft, _status);
    }

    function setWeth(address _wethAddress) external onlyOwner {
        if (address(weth) != address(0)) revert Error.ZeroAddressError();

        weth = IERC20(_wethAddress);
    }

    event UpdateFee(uint256 fee);

    event UpdateFeeWithCollectionOwner(uint256 fee);

    event UpdateCollectionOwnerFee(
        address _collection,
        address _recipient,
        uint256 _fee
    );

    event UpdateFeeRecipient(address feeRecipient);

    event TokenApprovalStatusUpdated(address nft, TokenApprovalStatus status);

    event TokenBidCreatedOrUpdated(
        address bidder,
        address nftAddress,
        uint256 tokenId,
        bytes32 listingId,
        uint128 pricePerItem,
        uint64 expirationTime,
        address paymentToken
    );

    event TokenBidCancelled(
        address bidder,
        address nftAddress,
        uint256 tokenId,
        bytes32 listingId
    );

    event BidAccepted(
        address seller,
        address bidder,
        address nftAddress,
        uint256 tokenId,
        bytes32 listingId,
        uint64 quantity,
        uint256 pricePerItem,
        address paymentToken
    );

    event ItemListed(
        address seller,
        bytes32 listingId,
        address nftAddress,
        uint256[] tokenIds,
        uint64 quantity,
        uint128 pricePerItem,
        uint64 expirationTime,
        address paymentToken
    );

    event ItemCancelled(
        address indexed seller,
        bytes32 indexed listingId
    );

    event ItemSold(
        address seller,
        address buyer,
        address nftAddress,
        bytes32 listingId,
        uint256[] tokenIds,
        uint64 quantity,
        uint128 pricePerItem,
        address paymentToken
    );
}

