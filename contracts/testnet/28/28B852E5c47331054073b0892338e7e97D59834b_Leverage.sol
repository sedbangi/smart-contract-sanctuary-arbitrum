pragma solidity ^0.8.20;

import "./interfaces/IWrappedTokenGatewayV3.sol";
import "@openzeppelin/contracts/utils/math/SafeCast.sol";
import "@aave/core-v3/contracts/interfaces/IPool.sol";

contract Leverage {
    using SafeCast for uint256;

    IWrappedTokenGatewayV3 immutable i_wethGateway;
    IPool immutable i_pool;
    address immutable i_poolproxy;
    address immutable i_usdcAddress;

    constructor(
        address _wethGateway,
        address _proxyAddress,
        address _usdcAddress
    ) {
        i_wethGateway = IWrappedTokenGatewayV3(_wethGateway);
        i_pool = IPool(_proxyAddress);
        i_poolproxy = _proxyAddress;
        i_usdcAddress = _usdcAddress;
    }

    // redefine amount
    function recursiveBorrow(uint256 amount) public payable {
        i_wethGateway.depositETH{value: msg.value}(
            i_poolproxy,
            msg.sender,
            uint16(0)
        );
        i_pool.borrow(i_usdcAddress, amount, 2, 0, msg.sender);
    }
}