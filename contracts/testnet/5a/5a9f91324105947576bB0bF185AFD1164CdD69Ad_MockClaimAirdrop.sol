import "../../interfaces/IPostReserveBase.sol";
import "../../interfaces/IVePost.sol";
import "../../interfaces/IPendingLP.sol";

import "hardhat/console.sol";

contract ClaimAirdrop is OwnableUpgradeable, ReentrancyGuardUpgradeable {
    event RewardEpochClaimed(address indexed beneficiary, uint256 amount);

    IPostReserveBase public airdropTreasury;
    IVePost public vePost;
    IPendingLP public pendingLP;

    IERC20 public postToken;
    uint64 public durationLock;

    uint256 public startTimeFreeClaim;
    uint256 public endTimeFreeClaim;

    uint256 public startTimePreClaim;
    uint256 public endTimePreClaim;

    mapping(address => address) public userToVestWallet;

    bool public isVestingMethod;

    mapping(address => bool) public claimed;
    mapping(address => uint256) public remainingClaim;
    mapping(address => uint256) public remainingClaimEpoch1;

    bytes32 public merkleRoot;

    function initialize(
        uint256 _startTimePreClaim,
        uint256 _startTimeFreeClaim,
        address _pendingLP,
        address _airdropTreasury,
        address _vePost,
        address _postToken
    ) external initializer {
        __Ownable_init(msg.sender);
        __ReentrancyGuard_init();

        durationLock = 60 * 60 * 24 * 30 * 6;

        startTimePreClaim = _startTimePreClaim;
        endTimePreClaim = _startTimePreClaim + 60 * 60 * 24 * 14;

        startTimeFreeClaim = _startTimeFreeClaim;
        endTimeFreeClaim = _startTimeFreeClaim + 60 * 60 * 24 * 30;

        vePost = IVePost(_vePost);
        pendingLP = IPendingLP(_pendingLP);
        airdropTreasury = IPostReserveBase(_airdropTreasury);

        postToken = IERC20(_postToken);

        postToken.approve(address(vePost), type(uint256).max);
    }

    function preClaim(
        uint256 claimAmount,
        uint256 durationLock,
        uint256 amountEpoch1,
        uint256 amountEpoch25,
        bytes32[] calldata merkleProof
    ) external payable nonReentrant {
        require(
            block.timestamp >= startTimePreClaim &&
                block.timestamp < endTimePreClaim,
            "not in time fre claim"
        );

        address beneficiary = msg.sender;
        require(!claimed[beneficiary], "Airdrop: Already claimed.");
        claimed[beneficiary] = true;
        // Verify the merkle proof.
        _verify(beneficiary, amountEpoch1, amountEpoch25, merkleProof);

        bool isAddLP = false;

        if (msg.value > 0) {
            isAddLP = true;
        }

        airdropTreasury.transferOut(address(this), claimAmount);

        if (claimAmount < amountEpoch1) {
            remainingClaimEpoch1[beneficiary] = amountEpoch1 - claimAmount;
            remainingClaim[beneficiary] = amountEpoch25;
        } else if (claimAmount >= amountEpoch1) {
            remainingClaim[beneficiary] =
                amountEpoch25 -
                (claimAmount - amountEpoch1);
        }

        if (isAddLP) {
            pendingLP.addPendingLP{value: msg.value}(
                claimAmount,
                beneficiary,
                durationLock
            );
            postToken.transfer(address(pendingLP), claimAmount);
        } else {
            vePost.create_lock_for(claimAmount, durationLock, beneficiary);
        }
    }

    function freeClaim(
        uint256 amountEpoch1,
        uint256 amountEpoch25,
        bytes32[] calldata merkleProof
    ) external nonReentrant {
        require(
            block.timestamp >= startTimeFreeClaim &&
                block.timestamp < endTimeFreeClaim,
            "not in time free claim"
        );
        address beneficiary = msg.sender;

        if (claimed[beneficiary]) {
            uint256 claimAmount = remainingClaim[beneficiary];
            amountEpoch1 = remainingClaimEpoch1[beneficiary];
            remainingClaimEpoch1[beneficiary] = 0;

            if (claimAmount == 0) {
                return;
            }
            remainingClaim[beneficiary] = 0;
            amountEpoch25 = claimAmount;
        } else {
            // Verify the merkle proof.
            _verify(beneficiary, amountEpoch1, amountEpoch25, merkleProof);
            claimed[beneficiary] = true;
        }

        if (amountEpoch1 > 0) {
            airdropTreasury.transferOut(beneficiary, amountEpoch1);
        }
        if (isVestingMethod) {
            require(
                userToVestWallet[beneficiary] == address(0),
                "already claim"
            );
            VestingWallet vestWallet = new VestingWallet(
                beneficiary,
                uint64(block.timestamp),
                durationLock
            );
            airdropTreasury.transferOut(address(vestWallet), amountEpoch25);
            userToVestWallet[beneficiary] = address(vestWallet);
            emit RewardEpochClaimed(beneficiary, amountEpoch25);
        } else {
            airdropTreasury.transferOut(beneficiary, amountEpoch25);
        }
    }

    function increasePendingLP(uint256 amount) external payable {
        uint256 amountEth = msg.value;
        address beneficiary = msg.sender;
        require(amountEth > 0 && amount > 0, "amountEth or amountPost is zero");
        require(claimed[beneficiary], "Airdrop: need claimed.");
        pendingLP.increasePendingLP{value: amountEth}(beneficiary, amount);
        postToken.transfer(address(pendingLP), amount);
    }

    function _verify(
        address beneficiary,
        uint256 amountEpoch1,
        uint256 amountEpoch25,
        bytes32[] calldata merkleProof
    ) internal view virtual {
        bytes32 node = keccak256(
            abi.encodePacked(beneficiary, amountEpoch1, amountEpoch25)
        );
        require(
            MerkleProof.verify(merkleProof, merkleRoot, node),
            "Airdrop: Invalid proof."
        );
    }

    function releasable(
        address beneficiary
    )
        external
        view
        returns (
            uint256 releaseAble,
            IERC20 postToken,
            IVestingWallet vestWallet
        )
    {
        IVestingWallet vestWallet = IVestingWallet(
            userToVestWallet[beneficiary]
        );
        return (
            vestWallet.releasable(address(postToken)),
            postToken,
            vestWallet
        );
    }

    function released(
        address beneficiary
    )
        external
        view
        returns (
            uint256 releaseAble,
            IERC20 postToken,
            IVestingWallet vestWallet
        )
    {
        IVestingWallet vestWallet = IVestingWallet(
            userToVestWallet[beneficiary]
        );
        return (vestWallet.released(address(postToken)), postToken, vestWallet);
    }

    function end(
        address beneficiary
    )
        external
        view
        returns (uint256 releaseAble, IERC20 post, IVestingWallet vestWallet)
    {
        IVestingWallet vestWallet = IVestingWallet(
            userToVestWallet[beneficiary]
        );
        return (vestWallet.end(), postToken, vestWallet);
    }

    function setMethodVesting(bool isVesting) external onlyOwner {
        isVestingMethod = isVesting;
    }

    function setAirdropTreasury(address _airdropTreasury) external onlyOwner {
        airdropTreasury = IPostReserveBase(_airdropTreasury);
    }
}

