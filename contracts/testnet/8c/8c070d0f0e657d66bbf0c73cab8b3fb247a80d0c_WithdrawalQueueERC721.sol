pragma solidity ^0.8.19;

import {IERC20Permit} from "@openzeppelin/contracts/token/ERC20/extensions/IERC20Permit.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/interfaces/IERC4626.sol";
import "@openzeppelin-upgradeable/contracts/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin-upgradeable/contracts/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "./interfaces/IGStake.sol";
import "./interfaces/IWithdrawalQueue.sol";

contract WithdrawalQueueERC721 is Initializable, IWithdrawalQueue, ERC721EnumerableUpgradeable {
    using EnumerableSet for EnumerableSet.UintSet;

    /// @notice precision base for share rate
    uint256 internal constant E27_PRECISION_BASE = 1e27;

    // keccak256(abi.encode(uint256(keccak256("gstake.storage.WithdrawalQueueERC721")) - 1)) & ~bytes32(uint256(0xff))
    bytes32 private constant WQStorageLocation = 0x9f576ac24ccf143479c94bbd8375f02090cebbcc057fa89501c6c7d09ba14e00;

    enum WRStatus {
        Processing,
        Undelegating,
        Claimable
    }

    struct WithdrawalRequest {
        uint256 wstGRT; // total wstGRT
        uint256 amountOfGRT; //
        uint256 undelegateId;
        WRStatus status;
        uint40 timestamp;
    }

    struct UndelegateInfo {
        uint256 lockedGRT;
        uint256 leftGRT;
        uint40 timestamp;
        uint40 undelegationId;
        bool isFinished;
    }

    /// @custom:storage-location erc7201:gstake.storage.GStake
    struct WQStorage {
        address GRT;
        address wstGRT;
        uint256 lastRequestID;
        uint256 lockedGRT;
        uint256 maxRequestPendingTime;
        mapping(uint256 => WithdrawalRequest) withdrawRequests;
        mapping(uint256 => UndelegateInfo) undelegations;
        EnumerableSet.UintSet matching;
    }

    function _getWQStorage() private pure returns (WQStorage storage $) {
        assembly {
            $.slot := WQStorageLocation
        }
    }

    /// @dev Contains both stGRT token amount and its corresponding shares amount
    event WithdrawalRequested(uint256 tokenId, address owner, uint256 wstGRT, uint256 grt, uint256 timestamp);
    event Matched(uint256 tokenId);
    event WithdrawalClaimed(uint256 tokenId, address owner, uint256 timestamp);
    event Undelegate(uint40 undelegateId, uint256 totalGRT, uint256 lockedGRT, uint256 timestamp);
    event LockGRT(uint256 amount);
    event ClaimGRT(address recipient, uint256 amount);

    error ZeroAmount();
    error InvalidState();
    error InvalidParam();
    error InvalidUndelegateId();
    error NotEnoughGRT();
    error NotNeedUndelegate();
    error RequestAmountTooSmall();
    error NotClaimable();
    error UndelegateError(uint256 tokenId);

    function initialize(address GRT_, address wstGRT_) public initializer {
        __ERC721_init("GStake WithdrawQueue", "GStakeWQ");
        WQStorage storage $ = _getWQStorage();
        $.GRT = GRT_;
        $.wstGRT = wstGRT_;
        $.maxRequestPendingTime = 12 hours;
    }

    // create request
    function requestWithdrawal(uint256 amountOfWstGRT, uint256 amountOfGRT) public returns (uint256 tokenId) {
        WQStorage storage $ = _getWQStorage();
        address _wstGRT = $.wstGRT;
        address _owner = msg.sender;
        if (amountOfWstGRT == 0) {
            if (amountOfGRT == 0) {
                revert ZeroAmount();
            } else {
                amountOfWstGRT = IERC4626(_wstGRT).withdraw(amountOfGRT, address(this), _owner);
            }
        } else {
            amountOfGRT = IERC4626(_wstGRT).redeem(amountOfWstGRT, address(this), _owner);
        }
        _checkWithdrawalRequestAmount(amountOfGRT);
        tokenId = _enqueue(amountOfWstGRT, amountOfGRT, _owner);
        matchOrder(1);
    }

    function requestWithdrawalsWithPermit(uint256 amountOfWstGRT, uint256 amountOfGRT, PermitInput calldata _permit)
        external
        returns (uint256 tokenId)
    {
        WQStorage storage $ = _getWQStorage();
        IERC20Permit($.wstGRT).permit(
            msg.sender, address(this), _permit.value, _permit.deadline, _permit.v, _permit.r, _permit.s
        );
        return requestWithdrawal(amountOfWstGRT, amountOfGRT);
    }

    function _enqueue(uint256 _amountOfWstGRT, uint256 amountOfGRT, address _owner)
        internal
        returns (uint256 tokenId)
    {
        WQStorage storage $ = _getWQStorage();
        tokenId = $.lastRequestID;
        $.lastRequestID = ++tokenId;
        _safeMint(_owner, tokenId);
        WithdrawalRequest memory newRequest =
            WithdrawalRequest(_amountOfWstGRT, amountOfGRT, 0, WRStatus.Processing, uint40(block.timestamp));
        $.withdrawRequests[tokenId] = newRequest;
        $.matching.add(tokenId);
        emit WithdrawalRequested(tokenId, _owner, _amountOfWstGRT, amountOfGRT, block.timestamp);
    }

    //claim
    /// @notice returns claimable GRT under the request. Returns 0 if request is claimed, canceled or processing.
    function _getClaimableGRT(uint256 tokenId) internal view returns (uint256) {
        WQStorage storage $ = _getWQStorage();
        WithdrawalRequest memory request = $.withdrawRequests[tokenId];
        if (request.status == WRStatus.Claimable) return request.amountOfGRT;
        if (request.status == WRStatus.Undelegating && $.undelegations[request.undelegateId].isFinished) {
            return request.amountOfGRT;
        }
        return 0;
    }

    function _claim(uint256 tokenId) internal {
        WQStorage storage $ = _getWQStorage();
        address _owner = ownerOf(tokenId);
        _checkAuthorized(_owner, msg.sender, tokenId);

        uint256 claimable = _getClaimableGRT(tokenId);
        if (claimable == 0) revert NotClaimable();

        uint256 amountOfGRT = $.withdrawRequests[tokenId].amountOfGRT;
        _deleteRequest(tokenId);
        sendGRT(_owner, amountOfGRT);
        emit WithdrawalClaimed(tokenId, _owner, amountOfGRT);
    }

    function getClaimableGRT(uint256[] calldata _requestIds)
        external
        view
        returns (uint256[] memory claimableEthValues)
    {
        WQStorage storage $ = _getWQStorage();
        claimableEthValues = new uint256[](_requestIds.length);
        for (uint256 i = 0; i < _requestIds.length; ++i) {
            WithdrawalRequest memory request = $.withdrawRequests[i];
            if (request.status == WRStatus.Claimable) claimableEthValues[i] = request.amountOfGRT;
        }
    }

    function claimWithdrawalsTo(uint256[] calldata _requestIds) external {
        for (uint256 i = 0; i < _requestIds.length; ++i) {
            _claim(_requestIds[i]);
        }
    }

    function _deleteRequest(uint256 tokenId) internal {
        WQStorage storage $ = _getWQStorage();
        delete $.withdrawRequests[tokenId];
        _burn(tokenId);
    }

    // exchange
    function matchOrder(uint256 maxNum) public {
        WQStorage storage $ = _getWQStorage();
        address _wstGRT = $.wstGRT;
        uint256 availGRT = IGStake(_wstGRT).maxPending();
        if (availGRT == 0) return;

        uint256 queueLength = $.matching.length();
        if (maxNum > queueLength) maxNum = queueLength;
        if (maxNum == 0) return;
        uint256[] memory ids = new uint256[](maxNum);
        uint256 i = 0;
        while (i < queueLength) {
            ids[i] = $.matching.at(queueLength - i - 1);
            ++i;
        }
        i = 0;
        uint256 totalGRT = 0;
        while (i < maxNum) {
            uint256 _tokenId = ids[i];
            WithdrawalRequest memory request = $.withdrawRequests[_tokenId];
            if (request.amountOfGRT + totalGRT <= availGRT && request.status == WRStatus.Processing) {
                totalGRT += request.amountOfGRT;
                $.withdrawRequests[_tokenId].status = WRStatus.Claimable;
                $.matching.remove(_tokenId);
                emit Matched(_tokenId);
            }
            if (totalGRT == availGRT) {
                break;
            }
            ++i;
        }
        if (totalGRT > 0) {
            IGStake(_wstGRT).claimFromPending(totalGRT);
            checkNewGRT(totalGRT);
        }
    }

    function undelegate(uint256[] memory ids) external {
        WQStorage storage $ = _getWQStorage();
        address _wstGRT = $.wstGRT;
        uint256 _timestamp = block.timestamp;
        uint256 deadline = _timestamp - $.maxRequestPendingTime;
        uint40 _undelegateId = IGStake(_wstGRT).nextUndelegateTaskId();
        uint256 maxNum = ids.length;
        if (maxNum == 0) revert InvalidParam();
        uint256 i = 0;
        uint256 totalGRT = 0;
        while (i < maxNum) {
            uint256 _tokenId = ids[i];
            WithdrawalRequest memory request = $.withdrawRequests[_tokenId];
            if (request.timestamp < deadline && request.status == WRStatus.Processing) revert UndelegateError(_tokenId);
            totalGRT += request.amountOfGRT;
            $.withdrawRequests[_tokenId].status = WRStatus.Undelegating;
            $.withdrawRequests[_tokenId].undelegateId = _undelegateId;
            $.matching.remove(_tokenId);
            ++i;
        }
        if (totalGRT == 0) {
            revert NotNeedUndelegate();
        }
        (uint40 undelegateId, uint256 _lockedGRT) = IGStake(_wstGRT).requestUndelegate(totalGRT);
        checkNewGRT(_lockedGRT);
        if (undelegateId != _undelegateId) revert InvalidState();
        UndelegateInfo storage info = $.undelegations[_undelegateId];
        if (info.timestamp == 0) {
            $.undelegations[_undelegateId] =
                UndelegateInfo(_lockedGRT, totalGRT - _lockedGRT, uint40(_timestamp), _undelegateId, false);
        } else {
            info.lockedGRT += _lockedGRT;
            info.leftGRT += totalGRT - _lockedGRT;
        }
        emit Undelegate(_undelegateId, totalGRT, _lockedGRT, _timestamp);
    }

    function completeUmdelegation(uint40 undelegationId) external override {
        WQStorage storage $ = _getWQStorage();
        UndelegateInfo memory info = $.undelegations[undelegationId];
        if (info.isFinished == true) revert InvalidUndelegateId();
        checkNewGRT(info.leftGRT);
        $.undelegations[undelegationId].isFinished = true;
    }

    function checkNewGRT(uint256 amount) private {
        WQStorage storage $ = _getWQStorage();
        uint256 _lockedGRT = $.lockedGRT;
        uint256 _bal = IERC20($.GRT).balanceOf(address(this));
        if (_bal < _lockedGRT + amount) revert NotEnoughGRT();
        $.lockedGRT = _lockedGRT + amount;
        emit LockGRT(amount);
    }

    function sendGRT(address _recipient, uint256 amount) private {
        WQStorage storage $ = _getWQStorage();
        $.lockedGRT -= amount;
        IERC20($.GRT).transfer(_recipient, amount);
        emit ClaimGRT(_recipient, amount);
    }

    function _checkWithdrawalRequestAmount(uint256 amountOfGRT) internal pure {
        if (amountOfGRT < 100 * 1e18) revert RequestAmountTooSmall();
    }
}

