// SPDX-License-Identifier: MIT

pragma solidity ^0.8.4;

import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "../MicroUtility.sol";
import "./ERC721.sol";

contract MicroNFT is ERC721, Ownable, ReentrancyGuard, MicroUtility {
    using Strings for uint256;

    string public notRevealedUri;
    bool private initialized;
    uint224 private _currentTokenId;
    uint256 private constant STATIC_GAS_LIMIT = 210_000;

    struct SaleConfiguration {
        uint64 editionSize;
        uint16 profitSharing;
        address payable fundsRecipient;
        uint256 publicSalePrice;
        uint32 maxSalePurchasePerAddress;
        uint64 publicSaleStart;
        uint64 publicSaleEnd;
        uint64 presaleStart;
        uint64 presaleEnd;
        bytes32 presaleMerkleRoot;
        bool cancelable;
    }

    mapping(address => bool) public admin;
    mapping(address => uint256) public totalMintsByAddress;
    mapping(address => uint256) public presaleMintsByAddress;
    mapping(address => bool) public sharingByAddress;

    SaleConfiguration public saleConfig;

    event BridgeIn(address indexed _to, uint256 _dstChainId, uint256 _tokenId);

    event BridgeOut(
        address indexed _from,
        uint256 _dstChainId,
        uint256 _tokenId
    );

    event Purchase(
        address indexed to,
        uint256 indexed quantity,
        uint256 indexed price,
        uint256 firstMintedTokenId
    );

    event OpenMintFinalized(address indexed sender, uint256 editionSize);

    event CancelSaleEdition(address indexed sender, uint256 lastTimeUpdated);

    event PublicSaleCollection(address indexed sender, uint256 lastTimeUpdated);

    event FundsWithdrawn(
        address indexed sender,
        address indexed fundsRecipient,
        uint256 fund
    );

    event SharingWithdrawn(
        address indexed sender,
        address indexed fundsRecipient,
        uint256 fund
    );

    event FundsRecipientChanged(
        address indexed fundsRecipient,
        address indexed sender
    );

    constructor() ERC721("", "") {}

    modifier onlyBridge() {
        require(
            microManager.microBridge(_msgSender()),
            "MicroNFT: bridge only call"
        );
        _;
    }

    modifier onlyAdmin() {
        require(admin[_msgSender()], "MicroNFT: admin only call");
        _;
    }

    modifier onlyCancelable() {
        require(!saleConfig.cancelable, "MicroNFT: Sale is canceled");
        _;
    }

    modifier canMintTokens(uint256 quantity) {
        if (
            saleConfig.editionSize != 0 &&
            quantity + _currentTokenId > saleConfig.editionSize
        ) {
            revert("Sold_Out");
        }
        _;
    }

    modifier onlyPublicSaleActive() {
        if (!_publicSaleActive()) {
            revert("Sale_Inactive");
        }
        _;
    }

    modifier onlyPresaleActive() {
        if (!_presaleActive()) {
            revert("Presale_Inactive");
        }
        _;
    }

    function init(bytes memory initPayload) external returns (bool) {
        require(!initialized, "MicroNFT: Already initialized");
        (
            string memory _baseURI,
            string memory _notRevealedURI,
            string memory _name,
            string memory _symbol,
            address _owner,
            address _admin,
            address _manager
        ) = abi.decode(
                initPayload,
                (string, string, string, string, address, address, address)
            );
        _setManager(_manager);
        notRevealedUri = _notRevealedURI;
        _setBaseURI(_baseURI);
        transferOwnership(_owner);
        admin[_admin] = true;
        initialized = true;
        _setNameAndSymbol(_name, _symbol);
        return true;
    }

    function tokenURI(
        uint256 tokenId
    ) public view virtual override returns (string memory) {
        require(
            _exists(tokenId),
            "ERC721AMetadata: URI query for nonexistent token"
        );

        if (bytes(notRevealedUri).length > 0) {
            return notRevealedUri;
        }

        string memory currentBaseURI = baseURI();

        return
            bytes(currentBaseURI).length > 0
                ? string(
                    abi.encodePacked(
                        currentBaseURI,
                        tokenId.toString(),
                        ".json"
                    )
                )
                : "";
    }

    function setBaseURI(string memory _newBaseURI) public onlyAdmin {
        _setBaseURI(_newBaseURI);
    }

    function setNotRevealedURI(string memory _notRevealedURI) public onlyAdmin {
        notRevealedUri = _notRevealedURI;
    }

    /**
     * Owner, Admin FUNCTIONS
     * non state changing
     */

    function adminMint(
        address recipient,
        uint256 quantity
    ) external onlyAdmin canMintTokens(quantity) returns (uint256) {
        _mintNFTs(recipient, quantity);

        return _currentTokenId;
    }

    function adminMintAirdrop(
        address[] calldata recipients
    ) external onlyAdmin canMintTokens(recipients.length) returns (uint256) {
        unchecked {
            for (uint256 i = 0; i < recipients.length; i++) {
                _mintNFTs(recipients[i], 1);
            }
        }

        return _currentTokenId;
    }

    function setAdmin(address _admin, bool _value) external onlyAdmin {
        admin[_admin] = _value;
    }

    function setSaleDetail(bytes memory initPayload) external onlyAdmin {
        if (
            saleConfig.presaleStart != 0 &&
            saleConfig.publicSaleStart > block.timestamp
        ) {
            revert("Sale_Active_Can_Not_Update");
        }

        if (
            saleConfig.presaleStart != 0 &&
            saleConfig.presaleStart > block.timestamp
        ) {
            revert("PreSale_Active_Can_Not_Update");
        }

        SaleConfiguration memory config = abi.decode(
            initPayload,
            (SaleConfiguration)
        );

        require(
            config.publicSaleStart > block.timestamp &&
                config.publicSaleEnd > config.publicSaleStart,
            "MicroNFT: Invalid timings"
        );

        require(
            config.profitSharing <= 50,
            "MicroNFT: Can not share over 50 percent"
        );

        require(
            config.fundsRecipient != address(0),
            "MicroNFT: Zero project owner address"
        );

        saleConfig = SaleConfiguration({
            editionSize: config.editionSize,
            profitSharing: config.profitSharing,
            fundsRecipient: config.fundsRecipient,
            publicSalePrice: config.publicSalePrice,
            maxSalePurchasePerAddress: config.maxSalePurchasePerAddress,
            publicSaleStart: config.publicSaleStart,
            publicSaleEnd: config.publicSaleEnd,
            presaleStart: config.presaleStart,
            presaleEnd: config.presaleEnd,
            presaleMerkleRoot: config.presaleMerkleRoot,
            cancelable: false
        });

        emit PublicSaleCollection(_msgSender(), block.timestamp);
    }

    function setFundsRecipient(
        address payable newRecipientAddress
    ) external onlyAdmin {
        require(
            newRecipientAddress != address(0),
            "MicroNFT: Funds Recipient cannot be 0 address"
        );
        saleConfig.fundsRecipient = newRecipientAddress;
        emit FundsRecipientChanged(newRecipientAddress, _msgSender());
    }

    function finalizeOpenEdition() external onlyAdmin {
        require(
            saleConfig.editionSize == type(uint64).max,
            "Admin_UnableToFinalizeNotOpenEdition"
        );
        saleConfig.editionSize = uint64(_currentTokenId);
        emit OpenMintFinalized(_msgSender(), saleConfig.editionSize);
    }

    function cancelSaleEdition() external onlyAdmin {
        require(!saleConfig.cancelable, "MicroNFT: Sale is canceled");
        saleConfig.cancelable = true;
        emit CancelSaleEdition(_msgSender(), block.timestamp);
    }

    /**
     * EXTERNAL FUNCTIONS
     * state changing
     */
    function purchase(
        uint256 quantity
    )
        external
        payable
        onlyCancelable
        canMintTokens(quantity)
        onlyPublicSaleActive
        nonReentrant
        returns (uint256)
    {
        uint256 salePrice = saleConfig.publicSalePrice;
        uint256 microProtocolFee = _usdToWei(microManager.microProtocolFee());

        if (msg.value < (salePrice + microProtocolFee) * quantity) {
            // The error will display the wrong price that was sent in USD
            revert("Purchase_WrongPrice");
        }

        uint256 remainder = msg.value -
            ((salePrice + microProtocolFee) * quantity);

        // If max purchase per address == 0 there is no limit.
        // Any other number, the per address mint limit is that.
        if (
            saleConfig.maxSalePurchasePerAddress != 0 &&
            totalMintsByAddress[_msgSender()] +
                quantity -
                presaleMintsByAddress[_msgSender()] >
            saleConfig.maxSalePurchasePerAddress
        ) {
            revert("Purchase_TooManyForAddress");
        }

        // First mint the NFTs
        _mintNFTs(_msgSender(), quantity);

        //Add total minted
        totalMintsByAddress[_msgSender()] += quantity;

        // Then send the Micro fee to the recipient (currently the Micro Treasury)
        _payoutMicroFee(quantity);

        // Then send the funding raise to the recipient
        _payoutFundingRaise(salePrice, quantity);

        uint256 chainPrepend = sourceGetChainPrepend();

        uint256 firstMintedTokenId = (chainPrepend +
            uint256(_currentTokenId - quantity)) + 1;

        emit Purchase({
            to: _msgSender(),
            quantity: quantity,
            price: salePrice,
            firstMintedTokenId: firstMintedTokenId
        });

        // Refund any overpayment
        if (remainder > 0) {
            (bool success, ) = _msgSender().call{
                value: remainder,
                gas: gasleft() > STATIC_GAS_LIMIT ? STATIC_GAS_LIMIT : gasleft()
            }("");
            require(success, "MicroNFT: Refund failed");
        }

        return firstMintedTokenId;
    }

    function purchasePresale(
        uint256 quantity,
        uint256 maxQuantity,
        uint256 pricePerToken,
        bytes32[] calldata merkleProof
    )
        external
        payable
        onlyCancelable
        nonReentrant
        canMintTokens(quantity)
        onlyPresaleActive
        returns (uint256)
    {
        if (
            !MerkleProof.verify(
                merkleProof,
                saleConfig.presaleMerkleRoot,
                keccak256(
                    // address, uint256, uint256
                    abi.encode(_msgSender(), maxQuantity, pricePerToken)
                )
            )
        ) {
            revert("Presale_MerkleNotApproved");
        }

        uint256 salePrice = pricePerToken;

        uint256 microProtocolFee = _usdToWei(microManager.microProtocolFee());

        if (msg.value < (salePrice + microProtocolFee) * quantity) {
            revert("Purchase_WrongPrice");
        }

        uint256 remainder = msg.value -
            ((salePrice + microProtocolFee) * quantity);

        if (presaleMintsByAddress[_msgSender()] > maxQuantity) {
            revert("Presale_TooManyForAddress");
        }

        // First mint the NFTs
        _mintNFTs(_msgSender(), quantity);

        presaleMintsByAddress[_msgSender()] += quantity;

        // Then send the Micro fee to the recipient (currently the Micro Treasury)
        _payoutMicroFee(quantity);

        // Then send the funding raise to the recipient
        _payoutFundingRaise(salePrice, quantity);

        uint256 chainPrepend = sourceGetChainPrepend();

        uint256 firstMintedTokenId = (chainPrepend +
            uint256(_currentTokenId - quantity)) + 1;

        emit Purchase({
            to: _msgSender(),
            quantity: quantity,
            price: salePrice,
            firstMintedTokenId: firstMintedTokenId
        });

        // Refund any overpayment
        if (remainder > 0) {
            (bool success, ) = _msgSender().call{
                value: remainder,
                gas: gasleft() > STATIC_GAS_LIMIT ? STATIC_GAS_LIMIT : gasleft()
            }("");
            require(success, "MicroNFT: Refund failed");
        }

        return firstMintedTokenId;
    }

    function withdrawProfitSharing() external nonReentrant {
        require(
            saleConfig.profitSharing > 0,
            "MicroNFT: Profit sharing is not allowed"
        );

        if (!saleConfig.cancelable) {
            require(
                block.timestamp >= saleConfig.publicSaleEnd,
                "MicroNFT: Requesting before end time"
            );
        }

        address sender = _msgSender();

        require(
            !sharingByAddress[sender],
            "MicroNFT: Access withdraw is not allowed"
        );

        // Number of owner
        uint256 mintedNumber = totalMintsByAddress[sender];

        // Check if withdraw is allowed for sender
        require(mintedNumber > 0, "MicroNFT: You don't own any tokens");

        // Get the profit sharing of sender
        uint256 rewards = ((saleConfig.publicSalePrice * mintedNumber) *
            saleConfig.profitSharing) / 100;

        if (rewards > 0) {
            // Payout recipient
            (bool successFunds, ) = payable(sender).call{
                value: rewards,
                gas: gasleft() > STATIC_GAS_LIMIT ? STATIC_GAS_LIMIT : gasleft()
            }("");
            require(successFunds, "MicroNFT: Withdraw Funds Send Failure");
        }

        sharingByAddress[sender] = true;

        // Emit event for indexing
        emit SharingWithdrawn(sender, sender, rewards);
    }

    function bridgeOut(
        address _from,
        uint64 _dstChainId,
        uint256 _tokenId
    ) external onlyBridge {
        require(
            _isApprovedOrOwner(_msgSender(), _tokenId),
            "MicroNFT: send caller is not owner nor approved"
        );
        require(
            ERC721.ownerOf(_tokenId) == _msgSender(),
            "MicroNFT: send from incorrect owner"
        );
        _burn(_tokenId);
        emit BridgeOut(_from, _dstChainId, _tokenId);
    }

    function bridgeIn(
        address _toAddress,
        uint64 _dstChainId,
        uint256 _tokenId
    ) external onlyBridge {
        require(
            !_exists(_tokenId) ||
                (_exists(_tokenId) && ERC721.ownerOf(_tokenId) == address(this))
        );
        if (!_exists(_tokenId)) {
            _safeMint(_toAddress, _tokenId);
        } else {
            _transfer(address(this), _toAddress, _tokenId);
        }
        emit BridgeIn(_toAddress, _dstChainId, _tokenId);
    }

    /**
     * INTERNAL FUNCTIONS
     * state changing
     */
    function _mintNFTs(address recipient, uint256 quantity) internal {
        uint256 chainPrepend = sourceGetChainPrepend();
        uint256 tokenId = 0;
        for (uint256 i = 0; i < quantity; i++) {
            _currentTokenId += 1;
            while (_exists(chainPrepend + uint256(_currentTokenId))) {
                _currentTokenId += 1;
            }
            tokenId = uint256(
                bytes32(
                    keccak256(abi.encodePacked(block.chainid, _currentTokenId))
                )
            );
            _safeMint(recipient, tokenId);
        }
    }

    function sourceGetChainPrepend() internal view returns (uint256) {
        return
            uint256(
                bytes32(
                    keccak256(
                        abi.encodePacked(uint32(block.chainid), uint224(0))
                    )
                )
            );
    }

    function _publicSaleActive() internal view returns (bool) {
        return
            saleConfig.publicSaleStart <= block.timestamp &&
            saleConfig.publicSaleEnd > block.timestamp;
    }

    function _presaleActive() internal view returns (bool) {
        return
            saleConfig.presaleStart <= block.timestamp &&
            saleConfig.presaleEnd > block.timestamp;
    }

    function _payoutFundingRaise(
        uint256 salePrice,
        uint256 quantity
    ) internal returns (bool) {
        if (saleConfig.fundsRecipient == address(0) || salePrice == 0) {
            return false;
        }

        //Calculate the totalPurchase
        uint256 totalPurchase = salePrice * quantity;
        //Calculate the profitSharing
        uint256 profitSharing = (totalPurchase * saleConfig.profitSharing) /
            100;

        uint256 funds = totalPurchase > profitSharing
            ? totalPurchase - profitSharing
            : 0;

        if (funds > 0) {
            // Payout recipient
            (bool successFunds, ) = saleConfig.fundsRecipient.call{
                value: funds,
                gas: gasleft() > STATIC_GAS_LIMIT ? STATIC_GAS_LIMIT : gasleft()
            }("");
            require(successFunds, "Withdraw_FundsSendFailure");
        }

        emit FundsWithdrawn(_msgSender(), saleConfig.fundsRecipient, funds);

        return true;
    }
}