contract MultiCallWithReason is AccessControl {
    uint256 private constant REVERT_REASON_MAX_LEN = 2048;

    struct Call {
        address target;
        bytes callData;
    }

    struct Result {
        bool success;
        bytes returnData;
    }

    struct Call3 {
        address target;
        bool allowFailure;
        bytes callData;
    }

    struct Call3Value {
        address target;
        bool allowFailure;
        uint256 value;
        bytes callData;
    }

    error MultiCall__ValueMismatch();
    error MultiCall__CallFailed(uint256 i);
