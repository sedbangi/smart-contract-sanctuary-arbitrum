        Call[] calldata calls
    ) public payable onlyRole(Constants.KEEPER_ROLE) returns (uint256 blockNumber, bytes[] memory returnData) {
        blockNumber = block.number;
        uint256 length = calls.length;
        returnData = new bytes[](length);
        Call calldata call;
        for (uint256 i; i < length; ) {
            bool success;
            call = calls[i];
