pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "../../../libs/SafeMath.sol";
import "../../../interfaces/IVePost.sol";
import "../../../interfaces/IPostTechStaking.sol";
import "../../../interfaces/IPostTechReferral.sol";

import "hardhat/console.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/PausableUpgradeable.sol";

contract PostTechStaking is
    IPostTechStaking,
    OwnableUpgradeable,
    ReentrancyGuardUpgradeable,
    PausableUpgradeable
{
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    /* ========== STATE VARIABLES ========== */

    IERC20 public postToken;
    IVePost public vePost;

    // Position referral contract address.
    IPostTechReferral public postTechReferral;
    // Referral commission rate in basis points.
    uint16 public referralCommissionRate;
    // Max referral commission rate: 10%.
    uint16 public MAXIMUM_REFERRAL_COMMISSION_RATE;

    uint256 public periodFinish;
    uint256 public rewardRate;
    uint256 public rewardsDuration;
    uint256 public lastUpdateTime;
    uint256 public rewardPerTokenStored;

    mapping(address => uint256) public userRewardPerTokenPaid;
    mapping(address => uint256) public rewards;

    uint256 private _totalStake;
    mapping(address => uint256) private _balances;

    /// time tracking
    mapping(address => uint) public lastTimeAction;

    mapping(uint256 => Stake) public stakes;

    // Greylisting of bad addresses
    mapping(address => bool) public greylist;

    // user =>  nftId[]
    mapping(address => uint256[]) public userNfts;
    // nftid =>  its index in userNft
    mapping(uint256 => uint256) public nftOwnedIndex;

    address public rewardsDistribution;

    bool public stakingPaused; // For emergencies

    struct Stake {
        uint256 vePostId;
        address user;
        uint128 amount;
        uint128 amountBuffer; // buffer with boostMultiplier: amount * boostMultiplier / 10_000
        uint32 boostMultiplier; /// base on 10_000
    }

    /* ========== CONSTRUCTOR ========== */

    function initialize(
        address _rewardsDistribution,
        address _postToken,
        address _vePost
    ) external initializer {
        __Ownable_init(msg.sender);
        __ReentrancyGuard_init();
        __Pausable_init();

        postToken = IERC20(_postToken);
        vePost = IVePost(_vePost);
        rewardsDistribution = _rewardsDistribution;

        MAXIMUM_REFERRAL_COMMISSION_RATE = 1_000; // 10%

        periodFinish = 0;
        rewardRate = 0;
        rewardsDuration = 30 days;
    }

    /* ========== VIEWS ========== */

    function rewardsToken() external view returns (address) {
        return address(postToken);
    }

    function totalStake() external view returns (uint256) {
        return _totalStake;
    }

    function balanceOf(address account) external view returns (uint256) {
        return _balances[account];
    }

    function lastTimeRewardApplicable() public view returns (uint256) {
        return block.timestamp < periodFinish ? block.timestamp : periodFinish;
    }

    function rewardPerToken() public view returns (uint256) {
        if (_totalStake == 0) {
            return rewardPerTokenStored;
        }
        return
            rewardPerTokenStored.add(
                lastTimeRewardApplicable()
                    .sub(lastUpdateTime)
                    .mul(rewardRate)
                    .mul(1e18)
                    .div(_totalStake)
            );
    }

    function claimableReward(address account) public view returns (uint256) {
        console.log("userNfts[account].length: ", userNfts[account].length);
        if (userNfts[account].length == 0 || userNfts[account].length == 1) {
            return 0;
        }
        uint256 averageBalance = _balances[account].div(
            userNfts[account].length - 1
        );
        uint256 averageWeight = averageWeight(account);

        uint256 faction = averageWeight.mul(1e18).div(averageBalance);

        console.log(
            "[claimableReward] _balances[account]: ",
            _balances[account]
        );
        console.log("[claimableReward] rewardPerToken(): ", rewardPerToken());
        console.log(
            "[claimableReward] userRewardPerTokenPaid[account]: ",
            userRewardPerTokenPaid[account]
        );
        console.log("[claimableReward] rewards[account]: ", rewards[account]);
        uint256 pendingReward = _balances[account]
            .mul(rewardPerToken().sub(userRewardPerTokenPaid[account]))
            .div(1e18)
            .add(rewards[account]);

        return pendingReward.mul(faction).div(1e18);
    }

    function averageWeight(address user) public view returns (uint256) {
        uint256[] memory _nftIds = userNfts[user];
        return
            vePost.averageMultiWeightInTime(
                _nftIds,
                lastTimeAction[user],
                block.timestamp
            );
    }

    function getRewardForDuration() external view returns (uint256) {
        return rewardRate.mul(rewardsDuration);
    }

    function getUserNfts(address user) external view returns (uint256[] memory) {
        return userNfts[user];
    }

    /* ========== MUTATIVE FUNCTIONS ========== */

    function stake(
        uint256 tokenId,
        bool isClaimReward
    )
        external
        nonReentrant
        whenNotPaused
        updateReward(msg.sender, isClaimReward)
    {
        address user = msg.sender;
        _stake(tokenId, user);
    }

    function withdraw(
        uint256 tokenId,
        bool isClaimReward
    ) public nonReentrant updateReward(msg.sender, isClaimReward) {
        _withdraw(tokenId, msg.sender);
    }

    function stakeMultiple(
        uint256[] memory tokenIds,
        bool isClaimReward
    )
        external
        nonReentrant
        whenNotPaused
        updateReward(msg.sender, isClaimReward)
    {
        for (uint i = 0; i < tokenIds.length; i++) {
            _stake(tokenIds[i], msg.sender);
        }
    }

    function withdrawMultiple(
        uint256[] memory tokenIds,
        bool isClaimReward
    ) external nonReentrant updateReward(msg.sender, isClaimReward) {
        for (uint i = 0; i < tokenIds.length; i++) {
            _withdraw(tokenIds[i], msg.sender);
        }
    }

    function claimReward() public nonReentrant updateReward(msg.sender, true) {}

    function claimRewardFor(
        address account
    ) public nonReentrant updateReward(account, true) {}

    function exit() external nonReentrant updateReward(msg.sender, false) {
        address user = msg.sender;

        uint256[] memory _userNfts = userNfts[user];
        for (uint i = 0; i < _userNfts.length; i++) {
            _withdraw(_userNfts[i], user);
        }
        _claimReward(msg.sender);
    }

    /* ========== INTERNAL FUNCTIONS ========== */

    function _stake(uint256 tokenId, address user) internal {
        (
            uint128 liquidity,
            uint start,
            uint end,
            uint32 boostMultiplier
        ) = _getLiquidityAndDurationLockByVePostId(tokenId);

        _validateStake(user, tokenId, liquidity);

        // Pull the tokens from the source_address
        _lockStake(tokenId, user);

        uint128 bufferLiquidity = (liquidity * boostMultiplier) / 10_000;

        _totalStake = _totalStake.add(bufferLiquidity);
        _balances[user] = _balances[user].add(liquidity);

        /// storage the stake data
        stakes[tokenId] = Stake({
            vePostId: tokenId,
            user: user,
            amount: liquidity,
            amountBuffer: bufferLiquidity,
            boostMultiplier: boostMultiplier
        });

        uint256[] storage _nftIds = userNfts[user];
        if (_nftIds.length == 0) {
            _nftIds.push(0);
            nftOwnedIndex[0] = 0;
        }
        _nftIds.push(tokenId);
        nftOwnedIndex[tokenId] = _nftIds.length - 1;

        emit Staked(user, tokenId);
    }

    function _lockStake(uint256 tokenId, address user) internal {
        vePost.lockWhenStake(tokenId);
    }

    function _unLockStake(uint256 tokenId) internal {
        vePost.unLockWhenStake(tokenId);
    }

    function _withdraw(uint256 tokenId, address user) internal {
        Stake memory _stake = stakes[tokenId];

        _totalStake = _totalStake.sub(_stake.amountBuffer);
        _balances[user] = _balances[user].sub(_stake.amount);
        _unLockStake(tokenId);
        _removeNftFromUser(tokenId, user);
        emit Withdrawn(user, tokenId);
    }

    function _claimReward(address user) internal {
        uint256 reward = rewards[user];
        if (reward > 0) {
            rewards[user] = 0;
            postToken.safeTransfer(user, reward);
            _payReferralCommission(user, reward);
            emit RewardPaid(user, reward);
        }
    }

    // Pay referral commission to the referrer who referred this user.
    function _payReferralCommission(address _user, uint256 _reward) internal {
        if (
            address(postTechReferral) != address(0) &&
            referralCommissionRate > 0
        ) {
            address referrer = postTechReferral.getReferrer(_user);
            uint256 commissionAmount = _reward.mul(referralCommissionRate).div(
                10_000
            );

            if (referrer != address(0) && commissionAmount > 0) {
                postToken.transfer(referrer, commissionAmount);
                postTechReferral.recordReferralCommission(
                    referrer,
                    commissionAmount
                );
                emit ReferralCommissionPaid(_user, referrer, commissionAmount);
            }
        }
    }

    function _removeNftFromUser(uint256 _nftId, address _userAddress) internal {
        uint256[] memory _nftIds = userNfts[_userAddress];
        uint256 nftIndex = nftOwnedIndex[_nftId];
        require(_nftIds[nftIndex] == _nftId, "not gegoId owner");
        uint256 _nftArrLength = _nftIds.length - 1;
        uint256 tailId = _nftIds[_nftArrLength];
        userNfts[_userAddress][nftIndex] = tailId;
        userNfts[_userAddress][_nftArrLength] = 0;
        userNfts[_userAddress].pop();
        nftOwnedIndex[tailId] = nftIndex;
        nftOwnedIndex[_nftId] = 0;
    }

    function _getLiquidityAndDurationLockByVePostId(
        uint256 tokenId
    ) internal view returns (uint128, uint, uint, uint32) {
        (int128 amount, uint start, uint end, uint32 boostMultiplier) = vePost
            .locked(tokenId);

        return (uint128(amount), start, end, boostMultiplier);
    }

    function _validateStake(
        address stakerAddress,
        uint256 tokenId,
        uint256 liquidity
    ) internal virtual {
        require(!stakingPaused, "Staking paused or in migration");
        require(liquidity > 0, "Must stake more than zero");
        require(vePost.ownerOf(tokenId) == stakerAddress, "not vePost owner");
        require(!vePost.isLockVeWhenStake(tokenId), "vePost already locked");
//        require()
        require(
            greylist[stakerAddress] == false,
            "Address has been greylisted"
        );
    }

    /* ========== RESTRICTED FUNCTIONS ========== */

    function notifyRewardAmount(
        uint256 reward
    ) external onlyRewardsDistribution updateReward(address(0), false) {
        require(reward > 0, "Reward is zero");
        if (block.timestamp >= periodFinish) {
            rewardRate = reward.div(rewardsDuration);
        } else {
            uint256 remaining = periodFinish.sub(block.timestamp);
            uint256 leftover = remaining.mul(rewardRate);
            rewardRate = reward.add(leftover).div(rewardsDuration);
        }
        postToken.transferFrom(msg.sender, address(this), reward);

        // Ensure the provided reward amount is not more than the balance in the contract.
        // This keeps the reward rate in the right range, preventing overflows due to
        // very high values of rewardRate in the earned and rewardsPerToken functions;
        // Reward + leftover must be less than 2^256 / 10^18 to avoid overflow.
        uint balance = postToken.balanceOf(address(this));
        //  console.log("rewardRate: ", rewardRate);
        require(
            rewardRate <= balance.div(rewardsDuration),
            "Provided reward too high"
        );

        lastUpdateTime = block.timestamp;
        periodFinish = block.timestamp.add(rewardsDuration);
        emit RewardAdded(reward);
    }

    // Added to support recovering LP Rewards from other systems such as BAL to be distributed to holders
    function recoverERC20(
        address tokenAddress,
        uint256 tokenAmount
    ) external onlyOwner {
        IERC20(tokenAddress).safeTransfer(owner(), tokenAmount);
        emit Recovered(tokenAddress, tokenAmount);
    }

    function setRewardsDistribution(
        address _rewardsDistribution
    ) external onlyOwner {
        rewardsDistribution = _rewardsDistribution;
    }

    function setRewardsDuration(uint256 _rewardsDuration) external onlyOwner {
        require(
            block.timestamp > periodFinish,
            "Previous rewards period must be complete before changing the duration for the new period"
        );
        rewardsDuration = _rewardsDuration;
        emit RewardsDurationUpdated(rewardsDuration);
    }

    function setVePost(address _vePost) external onlyOwner {
        vePost = IVePost(_vePost);
    }

    // Update the postToken referral contract address by the owner
    function setPositionReferral(
        IPostTechReferral _postTechReferral
    ) public onlyOwner {
        postTechReferral = _postTechReferral;
    }

    // Update referral commission rate by the owner
    function setReferralCommissionRate(
        uint16 _referralCommissionRate
    ) public onlyOwner {
        require(
            _referralCommissionRate <= MAXIMUM_REFERRAL_COMMISSION_RATE,
            "invalid referral commission"
        );
        referralCommissionRate = _referralCommissionRate;
    }

    function onERC721Received(
        address operator,
        address from,
        uint256 tokenId,
        bytes memory data
    ) public returns (bytes4) {
        emit NFTReceived(operator, from, tokenId, data);
        return
            bytes4(
                keccak256("onERC721Received(address,address,uint256,bytes)")
            );
    }

    /* ========== MODIFIERS ========== */

    modifier updateReward(address account, bool isClaimReward) {
        rewardPerTokenStored = rewardPerToken();
        lastUpdateTime = lastTimeRewardApplicable();
        if (account != address(0)) {
            rewards[account] = claimableReward(account);
            lastTimeAction[account] = block.timestamp;
            userRewardPerTokenPaid[account] = rewardPerTokenStored;

            if (isClaimReward) {
                _claimReward(account);
            }
        }
        _;
    }

    modifier onlyRewardsDistribution() {
        require(
            msg.sender == rewardsDistribution,
            "Caller is not RewardsDistribution contract"
        );
        _;
    }

    /* ========== EVENTS ========== */

    event RewardAdded(uint256 reward);
    event Staked(address indexed user, uint256 amount);
    event Withdrawn(address indexed user, uint256 amount);
    event RewardPaid(address indexed user, uint256 reward);
    event RewardsDurationUpdated(uint256 newDuration);
    event Recovered(address token, uint256 amount);
    event NFTReceived(
        address operator,
        address from,
        uint256 tokenId,
        bytes data
    );
    event ReferralCommissionPaid(
        address indexed user,
        address indexed referrer,
        uint256 commissionAmount
    );
}

